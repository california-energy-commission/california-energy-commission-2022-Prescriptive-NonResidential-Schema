                                                <xsd:enumeration value="HospitalExamTreatment"/>
                                                <xsd:enumeration value="HospitalImaging"/>
                                                <xsd:enumeration value="HospitalMedicalSupply"/>
                                                <xsd:enumeration value="HospitalNursery"/>
                                                <xsd:enumeration value="HospitalNurseStation"/>
                                                <xsd:enumeration value="HospitalOperatingRoom"/>
