                                          <xsd:documentation source="FieldText"/>
                                          <xsd:documentation source="AdditionalRequirements">
                                             <d:i><d:sup>2</d:sup> Must be at least 40% to comply with 
                                                <d:t24refnolink/>141.0(b)2I(iii) /
                                                <d:t24refnolink/>180.2(b)4biv</d:i>
                                          </xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
