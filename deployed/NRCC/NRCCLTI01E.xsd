                     <xsd:simpleType>
                        <xsd:restriction base="com:WholeBuildingOccupancy">
                           <xsd:enumeration value="AllOthersBuildings"/>
                           <xsd:enumeration value="AuditoriumBuilding"/>
                           <xsd:enumeration value="ClassroomBuilding"/>
                           <xsd:enumeration value="CommercialIndustrialBuilding"/>
                           <xsd:enumeration value="ConventionCenterBuilding"/>
                           <xsd:enumeration value="DataCenterBuilding"/>
                           <xsd:enumeration value="FinancialInstitutionBuilding"/>
                           <xsd:enumeration value="GroceryStoreBuilding"/>
                           <xsd:enumeration value="GymnasiumBuilding"/>
                           <xsd:enumeration value="HighRiseResidential"/>
                           <xsd:enumeration value="HospitalBuilding"/>
                           <xsd:enumeration value="HotelMotel"/>
                           <xsd:enumeration value="LibraryBuilding"/>
                           <xsd:enumeration value="MedicalClinicBuilding"/>
                           <xsd:enumeration value="MultiFamilyMixedUseGTE4"/>
                           <xsd:enumeration value="OfficeBuilding"/>
                           <xsd:enumeration value="ParkingGarageBuilding"/>
                           <xsd:enumeration value="ReligiousFacilityBuilding"/>
                           <xsd:enumeration value="RelocatablePublicSchoolBuilding"/>
                           <xsd:enumeration value="RestaurantBuilding"/>
                           <xsd:enumeration value="RetailBuilding"/>
                           <xsd:enumeration value="SchoolBuilding"/>
                           <xsd:enumeration value="SportsArenaBuilding"/>
                           <xsd:enumeration value="SupportArea"/>
                           <xsd:enumeration value="TheaterBuilding"/>
                           <xsd:enumeration value="Warehouse"/>
                        </xsd:restriction>
                     </xsd:simpleType>
                  </xsd:element>
                  <xsd:element name="A04_TotalConditionedFloorArea" type="com:TotalConditionedFloorArea">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Total Conditioned Floor Area (ft<d:sup>2</d:sup>)</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">User input  which can include 0;  If user doesn't enter anything the cell is blank</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="A05_TotalUnconditionedFloorArea" type="com:TotalUnconditionedFloorArea">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Total Unconditioned Floor Area (ft<d:sup>2</d:sup>)</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">User input  which can include 0;  If user doesn't enter anything the cell is blank</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="A06_AboveGradeStoryCount" type="env:AboveGradeStoryCount">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"># of Stories (Habitable Above Grade)</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">User input</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="A07_RequestedMMB" type="comp:RequestedMMB" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">NOT REPORTED IN PDF</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">This is a flag set by the agent that generates the report XML data to indicate that the user requested a Mandatory Measures Note Block which is placed after the DeclarationBlock in the XML output.</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="A08_ProjectScopeNR">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">NOT REPORTED IN PDF</xsd:documentation>
                        <xsd:appinfo source="displayterm">
                           <dtyp:displayterm value="AdditionComplete">Addition</dtyp:displayterm>
                           <dtyp:displayterm value="ExistingAdditionAndAlteration">Addition Alteration</dtyp:displayterm>
                           <dtyp:displayterm value="ExistingAlteration">Alteration</dtyp:displayterm>
                           <dtyp:displayterm value="NewComplete">New</dtyp:displayterm>
                        </xsd:appinfo>
                     </xsd:annotation>
                     <xsd:simpleType>
                        <xsd:restriction base="comp:ProjectScopeNR">
                           <xsd:enumeration value="AdditionComplete"/>
                           <xsd:enumeration value="ExistingAdditionAndAlteration"/>
                           <xsd:enumeration value="ExistingAlteration"/>
                           <xsd:enumeration value="NewComplete"/>
                        </xsd:restriction>
                     </xsd:simpleType>
                  </xsd:element>
                  <xsd:element name="AErrorMessage_SectionErrorMessage" type="comp:SectionErrorMessage" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"/>
                     </xsd:annotation>
                  </xsd:element>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
         <xsd:element name="Section_B" minOccurs="1" maxOccurs="1">
            <xsd:annotation>
               <xsd:documentation source="FieldText">
                  <d:b>B. PROJECT SCOPE</d:b>
               </xsd:documentation>
               <xsd:documentation source="MinOccurs">This section is required.</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="B_BeginNote1" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"/>
                        <xsd:documentation source="AdditionalRequirements">
                           <d:i>This table includes any lighting systems that are within the scope of the permit application and are demonstrating compliance using the prescriptive path outlined in 
                              <d:t24refnolink/>140.6 / 
                              <d:t24refnolink/>170.2(e) or 
                              <d:t24refnolink/>141.0(b)2 / 
                              <d:t24refnolink/>180.2(b)4 for alterations. </d:i>
                        </xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="B_ColumnHeading01" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"/>
                        <xsd:documentation source="AdditionalRequirements">
                           <d:b>Scope of Work</d:b>
                        </xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="B_ColumnSubHeading01" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"/>
                        <xsd:documentation source="AdditionalRequirements">My Project Consists of (check all that apply):</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="B_ColumnHeading02_03" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"/>
                        <xsd:documentation source="AdditionalRequirements">
                           <d:b>Conditioned Spaces</d:b>
                        </xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="B_ColumnFieldText02Cond" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"/>
                        <xsd:documentation source="AdditionalRequirements">Calculation Method</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="B_ColumnFieldText03Cond" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"/>
                        <xsd:documentation source="AdditionalRequirements">Area (ft<d:sup>2</d:sup>
                        </xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="B_ColumnHeading04_05" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"/>
                        <xsd:documentation source="AdditionalRequirements">
                           <d:b>Unconditioned Spaces</d:b>
                        </xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="B_ColumnFieldText02Uncond" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"/>
                        <xsd:documentation source="AdditionalRequirements">Calculation Method</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="B_ColumnFieldText03Uncond" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"/>
                        <xsd:documentation source="AdditionalRequirements">Area (ft<d:sup>2</d:sup>
                        </xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="Bnew01_LightingProjectScopeNR">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">New Lighting System</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">If user checks New Lighting System result = NewLightingSystem  Else result = NotApplicableLightingScope</xsd:documentation>
                     </xsd:annotation>
                     <xsd:simpleType>
                        <xsd:restriction base="comp:LightingProjectScopeNR">
                           <xsd:enumeration value="NewLightingSystem"/>
                           <xsd:enumeration value="NotApplicableLightingScope"/>
                        </xsd:restriction>
                     </xsd:simpleType>
                  </xsd:element>
                  <xsd:element name="NewLighting" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"/>
                        <xsd:documentation source="CalculationsAndRules">This subsection is required if LightingProjectScopeNR ==  NewLightingSystem</xsd:documentation>
                     </xsd:annotation>
                     <xsd:complexType>
                        <xsd:sequence>
                           <xsd:element name="Bnew02_LightingPrescriptiveMethodNR">
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText">Calculation Method</xsd:documentation>
                                 <xsd:documentation source="CalculationsAndRules">If new lighting system does not include conditioned space, result = NotApplicableLightingMethod, Else user selects calculation method,</xsd:documentation>
                              </xsd:annotation>
                              <xsd:simpleType>
                                 <xsd:restriction base="comp:LightingPrescriptiveMethodNR">
                                    <xsd:enumeration value="AreaCategory"/>
                                    <xsd:enumeration value="AreaCategoryAndTailored"/>
                                    <xsd:enumeration value="CompleteBuilding"/>
                                    <xsd:enumeration value="NotApplicableLightingMethod"/>
                                    <xsd:enumeration value="Tailored"/>
                                 </xsd:restriction>
                              </xsd:simpleType>
                           </xsd:element>
                           <xsd:element name="Bnew03_NewLightingConditionedFloorArea" type="lit:NewLightingConditionedFloorArea" minOccurs="0">
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText">Area (ft<d:sup>2</d:sup>)</xsd:documentation>
                                 <xsd:documentation source="CalculationsAndRules">If  Bnew02_LightingPrescriptiveMethodNR == NotApplicableLightingMethod  field is blank and not required; Else user inputs value.</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                           <xsd:element name="Bnew04_LightingPrescriptiveMethodNR">
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText">Calculation Method</xsd:documentation>
                                 <xsd:documentation source="CalculationsAndRules">If new lighting system does not include unconditioned space, result = NotApplicableLightingMethod, Else If Bnew02_LightingPrescriptiveMethodNR  == CompleteBuilding, only value in list to select is CompleteBuilding, Else 
 user selects calculation method from dropdown list,</xsd:documentation>
                              </xsd:annotation>
                              <xsd:simpleType>
                                 <xsd:restriction base="comp:LightingPrescriptiveMethodNR">
                                    <xsd:enumeration value="AreaCategory"/>
                                    <xsd:enumeration value="AreaCategoryAndTailored"/>
                                    <xsd:enumeration value="CompleteBuilding"/>
                                    <xsd:enumeration value="NotApplicableLightingMethod"/>
                                    <xsd:enumeration value="Tailored"/>
                                 </xsd:restriction>
                              </xsd:simpleType>
                           </xsd:element>
                           <xsd:element name="Bnew05_NewLightingUnconditionedFloorArea" type="lit:NewLightingUnconditionedFloorArea" minOccurs="0">
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText">Area (ft<d:sup>2</d:sup>)</xsd:documentation>
                                 <xsd:documentation source="CalculationsAndRules">If  Bnew04_LightingPrescriptiveMethodNR == NotApplicableLightingMethod field is blank and not required; Else user inputs value.</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                        </xsd:sequence>
                     </xsd:complexType>
                  </xsd:element>
                  <xsd:element name="Bpark01_LightingProjectScopeNR">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">New Lighting System - Parking Garage</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">If user checks New Lighting System - Parking Garage result = NewParkingGarageLighting  Else result = NotApplicableLightingScope</xsd:documentation>
                     </xsd:annotation>
                     <xsd:simpleType>
                        <xsd:restriction base="comp:LightingProjectScopeNR">
                           <xsd:enumeration value="NewParkingGarageLighting"/>
                           <xsd:enumeration value="NotApplicableLightingScope"/>
                        </xsd:restriction>
                     </xsd:simpleType>
                  </xsd:element>
                  <xsd:element name="NewLightingGarage" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"/>
                        <xsd:documentation source="CalculationsAndRules">This subsection is required if LightingProjectScopeNR ==  NewParkingGarageLighting</xsd:documentation>
                     </xsd:annotation>
                     <xsd:complexType>
                        <xsd:sequence>
                           <xsd:element name="Bpark02_LightingPrescriptiveMethodNR">
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText">Calculation Method</xsd:documentation>
                                 <xsd:documentation source="CalculationsAndRules">If new parking garage lighting system does not include conditioned space, result = NotApplicableLightingMethod, Else result = CompleteBuilding</xsd:documentation>
                              </xsd:annotation>
                              <xsd:simpleType>
                                 <xsd:restriction base="comp:LightingPrescriptiveMethodNR">
                                    <xsd:enumeration value="CompleteBuilding"/>
                                    <xsd:enumeration value="NotApplicableLightingMethod"/>
                                 </xsd:restriction>
                              </xsd:simpleType>
                           </xsd:element>
                           <xsd:element name="Bpark03_NewLightingConditionedFloorArea" type="lit:NewLightingConditionedFloorArea" minOccurs="0">
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText">Area (ft<d:sup>2</d:sup>
                                 </xsd:documentation>
                                 <xsd:documentation source="CalculationsAndRules">If  Bpark02_LightingPrescriptiveMethodNR == NotApplicableLightingMethod field is blank and not required; Else user inputs value.</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                           <xsd:element name="Bpark04_LightingPrescriptiveMethodNR">
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText">Calculation Method</xsd:documentation>
                                 <xsd:documentation source="CalculationsAndRules">If new parking garage lighting system does not include unconditioned space, result = NotApplicableLightingMethod, Else result = CompleteBuilding</xsd:documentation>
                              </xsd:annotation>
                              <xsd:simpleType>
                                 <xsd:restriction base="comp:LightingPrescriptiveMethodNR">
                                    <xsd:enumeration value="CompleteBuilding"/>
                                    <xsd:enumeration value="NotApplicableLightingMethod"/>
                                 </xsd:restriction>
                              </xsd:simpleType>
                           </xsd:element>
                           <xsd:element name="Bpark05_NewLightingUnconditionedFloorArea" type="lit:NewLightingUnconditionedFloorArea" minOccurs="0">
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText">Area (ft<d:sup>2</d:sup>
                                 </xsd:documentation>
                                 <xsd:documentation source="CalculationsAndRules">If  Bpark04_LightingPrescriptiveMethodNR == NotApplicableLightingMethod  field is blank and not required; Else user inputs value.</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                        </xsd:sequence>
                     </xsd:complexType>
                  </xsd:element>
                  <xsd:element name="Baltered01_LightingProjectScopeNR">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Altered Lighting System</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">If user checks Altered Lighting System result = AlteredLightingSystem  Else result = NotApplicableLightingScope</xsd:documentation>
                     </xsd:annotation>
                     <xsd:simpleType>
                        <xsd:restriction base="comp:LightingProjectScopeNR">
                           <xsd:enumeration value="AlteredLightingSystem"/>
                           <xsd:enumeration value="NotApplicableLightingScope"/>
                        </xsd:restriction>
                     </xsd:simpleType>
                  </xsd:element>
                  <xsd:element name="TableAlteredLighting" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"/>
                        <xsd:documentation source="CalculationsAndRules">This table is required if LightingProjectScopeNR == AlteredLightingSystem</xsd:documentation>
                     </xsd:annotation>
                     <xsd:complexType>
                        <xsd:sequence>
                           <xsd:element name="Row" minOccurs="1" maxOccurs="unbounded">
                              <xsd:complexType>
                                 <xsd:sequence>
                                    <xsd:element name="Baltered02_LightingPrescriptiveMethodNR" type="comp:LightingPrescriptiveMethodNR">
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Calculation Method</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">If altered lighting system does not include conditioned space, result = NotApplicableLightingMethod, Else user selects calculation method,</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="Baltered03_AlteredLightingConditionedFloorArea" type="lit:AlteredLightingConditionedFloorArea" minOccurs="0">
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Area (ft<d:sup>2</d:sup>
                                          </xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">If Baltered02_LightingPrescriptiveMethodNR  == NotApplicableLightingMethod field is blank and not required; Else user inputs value.</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="Baltered04_LightingPrescriptiveMethodNR" type="comp:LightingPrescriptiveMethodNR">
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Calculation Method</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">If altered lighting system does not include unconditioned space, result = NotApplicableLightingMethod, Else If Baltered02_LightingPrescriptiveMethodNR == CompleteBuilding, only value in list to select is CompleteBuilding, Else 
 user selects calculation method from dropdown list,</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="Baltered05_AlteredLightingUnconditionedFloorArea" type="lit:AlteredLightingUnconditionedFloorArea" minOccurs="0">
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Area (ft<d:sup>2</d:sup>
                                          </xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">If  Baltered04_LightingPrescriptiveMethodNR == NotApplicableLightingMethod field is blank and not required; Else user inputs value.</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                 </xsd:sequence>
                              </xsd:complexType>
                           </xsd:element>
                        </xsd:sequence>
                     </xsd:complexType>
                  </xsd:element>
                  <xsd:element name="BTotalsRowLabel" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">
                           <d:b>Total Area of Work (ft<d:sup>2</d:sup>)</d:b>
                        </xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="BTotals03_LightingProjectTotalConditionedFloorArea" type="comp:LightingProjectTotalConditionedFloorArea" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"/>
                        <xsd:documentation source="CalculationsAndRules">Calculated value: If Bnew03_NewLightingConditionedFloorArea IsNull And Bpark03_NewLightingConditionedFloorArea IsNull And  All Baltered03_AlteredLightingConditionedFloorArea IsNull result = field blank and not required;  Else for Bnew03, Bpark03 and all Baltered03  values NotNull result =  SUM  ( Bnew03_NewLightingConditionedFloorArea, Bpark03_NewLightingConditionedFloorArea, Baltered03_AlteredLightingConditionedFloorArea)
 
If result !=  A04_TotalConditionedFloorArea Then
D03  ExceptionalConditionMessageCCLTIE += FloorAreasInconsistentValues</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="BTotals05_LightingProjectTotalUnconditionedFloorArea" type="comp:LightingProjectTotalUnconditionedFloorArea" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"/>
                        <xsd:documentation source="CalculationsAndRules">Calculated value: If Bnew05_NewLightingUnconditionedFloorArea IsNull And Bpark05_NewLightingUnconditionedFloorArea IsNull And  Baltered05_AlteredLightingUnconditionedFloorArea IsNull result = field blank and not required;  Else for Bnew05, Bpark05 and all Baltered05  values NotNull result = SUM ( Bnew05_NewLightingUnconditionedFloorArea, Bpark05_NewLightingUnconditionedFloorArea, Baltered05_AlteredLightingUnconditionedFloorArea)
 
If result !=  A05_TotalUnconditionedFloorArea Then
D03  ExceptionalConditionMessageCCLTIE += FloorAreasInconsistentValues</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="BalteredMessage02_04" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"/>
                        <xsd:documentation source="AdditionalRequirements">
                           <d:b>
                              <d:i>ALERT! The Rated Power Reduction Method for alterations may only be used for one-for-one luminaire alterations within a building or tenant space of 5,000 ft<d:sup>2</d:sup> or less per 
                                 <d:t24refnolink/>141.0(b)2liii /
                                 <d:t24refnolink/>180.2(b)4.</d:i>
                           </d:b>
                        </xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">This text is displayed to the user If Baltered02_LightingPrescriptiveMethodNR Or Baltered04_LightingPrescriptiveMethodNR == RatedPowerReduction And Baltered03_AlteredLightingConditionedFloorArea Or Baltered05_AlteredLightingUnconditionedFloorArea Or SUM (Baltered03_AlteredLightingConditionedFloorArea, Baltered05_AlteredLightingUnconditionedFloorArea) GT 5000</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
         <xsd:element name="Section_C" minOccurs="1">
            <xsd:annotation>
               <xsd:documentation source="FieldText">
                  <d:b>C. COMPLIANCE RESULTS</d:b>
               </xsd:documentation>
               <xsd:documentation source="MinOccurs">This section is required.</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="C_BeginNote1" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"/>
                        <xsd:documentation source="AdditionalRequirements">
                           <d:i> If any cell on this table says  "DOES NOT COMPLY" or "COMPLIES with Exceptional Conditions" refer to Table D. for guidance.</d:i>
                        </xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="C_BeginNote2" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"/>
                        <xsd:documentation source="AdditionalRequirements">
                           <d:i>Lighting in conditioned and unconditioned spaces must not be combined for compliance per 
                              <d:t24refnolink/>140.6(b)1 /
                              <d:t24refnolink/>170.2(e)</d:i>
                        </xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="C_ColumnHeading01To05" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"/>
                        <xsd:documentation source="AdditionalRequirements">
                           <d:b>Allowed Lighting Power per 
                              <d:t24refnolink/>140.6(b) /
                              <d:t24refnolink/>170.2(e) (Watts)</d:b>
                        </xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="C_ColumnHeading06To08" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"/>
                        <xsd:documentation source="AdditionalRequirements">
                           <d:b>Adjusted Lighting Power per 
                              <d:t24refnolink/>140.6(a) /
                              <d:t24refnolink/>170.2(e) (Watts)</d:b>
                        </xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="C_ColumnHeading09" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"/>
                        <xsd:documentation source="AdditionalRequirements">
                           <d:b>Compliance Results</d:b>
                        </xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="C_ColumnSubheading07" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"/>
                        <xsd:documentation source="AdditionalRequirements">Adjustments</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="C_RowHeading01" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"/>
                        <xsd:documentation source="AdditionalRequirements">
                           <d:b>Conditioned</d:b>
                        </xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="C_RowHeading02" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"/>
                        <xsd:documentation source="AdditionalRequirements">
                           <d:b>Unconditioned</d:b>
                        </xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="C01_ColumnLabel1" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Complete Building 
                           <d:t24refnolink/>140.6(c)1</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="C01_ColumnLabel2" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">(See Table I)</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:choice maxOccurs="1">
                     <xsd:element name="C01Row01_BuildingLightingPowerAllowedConditioned" type="comp:BuildingLightingPowerAllowedConditioned">
                        <xsd:annotation>
                           <xsd:documentation source="FieldText"/>
                           <xsd:documentation source="CalculationsAndRules">If  a project has Bnew02_LightingPrescriptiveMethodNR Or Bpark02_LightingPrescriptiveMethodNR Or Baltered02_LightingPrescriptiveMethodNR ==  CompleteBuilding this field is required and  auto-filled result = IcTotals05AllowedWatt_LightingPowerAllowanceTotalConditioned; Else result = BlankGrayedOut stored in NotApplicableMessage and field is grayed out and blank</xsd:documentation>
                        </xsd:annotation>
                     </xsd:element>
                     <xsd:element name="C01Row01_NotApplicableMessage">
                        <xsd:annotation>
                           <xsd:documentation source="FieldText"/>
                           <xsd:documentation source="CalculationsAndRules">If  a project has Bnew02_LightingPrescriptiveMethodNR Or Bpark02_LightingPrescriptiveMethodNR Or Baltered02_LightingPrescriptiveMethodNR ==  CompleteBuilding this field is required and  auto-filled result = IcTotals05AllowedWatt_LightingPowerAllowanceTotalConditioned; Else result = BlankGrayedOut stored in NotApplicableMessage and field is grayed out and blank</xsd:documentation>
                        </xsd:annotation>
                        <xsd:simpleType>
                           <xsd:restriction base="comp:NotApplicableMessage">
                              <xsd:enumeration value="BlankGrayedOut"/>
                           </xsd:restriction>
                        </xsd:simpleType>
                     </xsd:element>
