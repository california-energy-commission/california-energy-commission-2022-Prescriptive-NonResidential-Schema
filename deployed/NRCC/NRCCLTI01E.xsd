                                                <xsd:enumeration value="ExerciseCenterGymnasiumArea"/>
                                                <xsd:enumeration value="ExhibitMuseumArea"/>
                                                <xsd:enumeration value="FamilyDiningArea"/>
                                                <xsd:enumeration value="FastfoodDining"/>
