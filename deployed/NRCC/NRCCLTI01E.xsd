         <xsd:element name="Section_P" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation source="FieldText">
                  <d:b>P. POWER ADJUSTMENT: LIGHTING CONTROL CREDIT  (POWER ADJUSTMENT FACTOR (PAF))</d:b>
               </xsd:documentation>
               <xsd:documentation source="CalculationsAndRules">This section applies if section I is required and in section I at least one value in Ic06b_AdditionalAllowanceForPAF Or Iu06b_AdditionalAllowanceForPAF == true Or if section K is required and at least one value in Kc09_AdditionalAllowanceForPAF or Ku09_AdditionalAllowanceForPAF == true</xsd:documentation>
               <xsd:documentation source="MinOccurs">If this section doesn't apply, do not display anything for this section</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="P_BeginNote1" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"/>
                        <xsd:documentation source="AdditionalRequirements">
                           <d:i>This table includes all areas indicated in Table I or Table K as using a PAF credit described in 
                              <d:t24refnolink/>140.6(a)2 /
                              <d:t24refnolink/>170.2(e)2B.</d:i>
                        </xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="P_ColumnHeading02" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">PAF per 
                           <d:t24refnolink/>140.6(a)2 / 
                           <d:t24refnolink/>170.2(e)2B<d:sup>1</d:sup>
                           <d:line1/> (*Can be used in conjunction with other PAF'S)</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="P_ColumnHeading03To06" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Luminaires Controlled for PAF Credit</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="Pc_Conditioned" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"/>
                        <xsd:documentation source="CalculationsAndRules">This subsection is required if at least one value in Ic06b_AdditionalAllowanceForPAF Or Kc09_AdditionalAllowanceForPAF == true</xsd:documentation>
                     </xsd:annotation>
                     <xsd:complexType>
                        <xsd:sequence>
