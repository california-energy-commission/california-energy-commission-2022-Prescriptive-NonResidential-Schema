                                             <xsd:element name="JcTableAddAllowance">
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText"/>
                                                </xsd:annotation>
                                                <xsd:complexType>
                                                   <xsd:sequence>
                                                      <xsd:element name="Row" minOccurs="1" maxOccurs="unbounded">
                                                         <xsd:complexType>
                                                            <xsd:sequence>
                                                               <xsd:element name="Jc03_AreaCategoryAdditionalAllowanceType" type="comp:AreaCategoryAdditionalAllowanceType">
                                                                  <xsd:annotation>
                                                                     <xsd:documentation source="FieldText">Applicable Qualifying Lighting System from Table 140.6-C</xsd:documentation>
                                                                     <xsd:documentation source="CalculationsAndRules">User selects from dropdown list values dependent on Jc02_SpaceFunction as follows:  
 If Jc02_SpaceFunction == AuditoriumArea or ExhibitMuseumArea : list DecorativeDisplayF; 
Else If Jc02_SpaceFunction == AutoRepairArea, ElectricalMechancialTelephoneRoom, GeneralCommercialIndustrialWorkAreaHighBay, Or  GeneralCommercialIndustrialWorkAreaLowBay:  list DetailedTaskA;   
 
Else If Jc02_SpaceFunction == AudienceSeatingArea, CivicMeetingPlaceArea, ConventionCenterAudienceSeatingArea, CorridorArea, HotelFunctionArea,   FastfoodDining, FamilyDiningArea,  FinancialTransactionsArea,  LibraryReadingArea, LobbyMainEntry, LoungeBreakRoom, ReligiousWorshipArea, RetailMallConcourse, TheaterMotionPicture, TheaterPerformance, MallAndAtria, ConferenceMultipurposeMeetingMF, ConventionConferenceMultipurposeMeetingCenterArea or  list DecorativeDisplayC;  
Else If Jc02_SpaceFunction ==  BeautySalonArea: list DecorativeDisplayC and DetailedTaskB;  
Else If Jc02_SpaceFunction == ClassroomLectureTrainingVocationalArea: list WhiteChalkBoardLighting;   
Else If Jc02_SpaceFunction == TransportationFunctionTicketing Or LowVisionRestroom: list DecorativeDisplayB;  
Else If Jc02_SpaceFunction ==  GeneralCommercialIndustrialWorkAreaPrecision:  list PrecisionLightingIndustrial;   
Else If Jc02_SpaceFunction ==  Office250FtOrLess, OfficeGreaterThan250Ft Or OfficeOpenPlan:  list PortableOfficeLighting and DecorativeDisplayB;  
Else If Jc02_SpaceFunction == ParkingGarageBuildingParkingArea: list ParkingGarageATM; 
Else If Jc02_SpaceFunction == PharmacyArea Or LaboratoryScientific:  list SpecializedTaskLighting;  
Else If Jc02_SpaceFunction == GrocerySalesArea, BarLoungeFineDiningArea, Restroom, Stairwell:  Or RetailSalesArea: Or RetailMerchandiseSalesWholesaleShowroom list DecorativeDisplayE;  
Else If Jc02_SpaceFunction == RetailFittingRoom: list MirrorLightingExternal and MirrorLightingInternal;  
Else If Jc02_SpaceFunction == HospitalNursery,  HospitalPhysicalTherapy  Or HospitalRecovery Or NurseStationMF Or PhysicalTherapyMF:  list TunableWhiteLuminaires;  
Else If Jc02_SpaceFunction == HospitalImaging:  list TunableWhiteLuminaires and DecorativeDisplayB;
Else If Jc02_SpaceFunction == HospitalNurseStation:  list TunableWhiteLuminaires and DetailedTaskA; 
Else If Jc02_SpaceFunction == HospitalPatientRoom:  list TunableWhiteLuminaires and DecorativeDisplayA;  
Else If Jc02_SpaceFunction == LowVisionWaitingArea,  LowVisionMultipurposeRoom, LowVisionReligiousWorshipArea Or LowVisionDiningArea:   list TunableWhiteLuminaires and DecorativeDisplayD;
Else If Jc02_SpaceFunction == VideoConferencingStudio: list VideoConferencing;  
Else If Jc02_SpaceFunction ==  LowVisionLobbyMainEntry: list TunableWhiteLuminaires,  DecorativeDisplayD, LowVisionMainEntryTransition
Else If Jc02_SpaceFunction == LowVisionCorridorArea or LowVisionStairwell Or LowVisionWaitingAreaMF Or LowVisionDiningAreaMF Or LowVisionMultipurposeRoomMF  :  list DecorativeDisplayD.
Else If Jc02_SpaceFunction == MuseumRestoration:  list  DetailedTaskC; 
End If</xsd:documentation>
                                                                  </xsd:annotation>
                                                               </xsd:element>
                                                               <xsd:choice maxOccurs="1">
                                                                  <xsd:element name="Jc04_AdditionalAllowedLPD" type="comp:AdditionalAllowedLPD">
                                                                     <xsd:annotation>
                                                                        <xsd:documentation source="FieldText">Allowed Density (W/ft<d:sup>2</d:sup> or W/lf or W/unit)</xsd:documentation>
                                                                        <xsd:documentation source="CalculationsAndRules">Calculated field:  Assigned results are W/ft2 stored in AdditionalAllowedLPD  Switch (Jc03_ AreaCategoryAdditionalAllowanceType) 
case DecorativeDisplayA: result = 0.15;
case DecorativeDisplayB: result = 0.20;
case DecorativeDisplayC: result = 0.25;   
case DecorativeDisplayD: result = 0.30; 
case DecorativeDisplayE: result = 0.35; 
case DecorativeDisplayF: result = 0.45; 
case DetailedTaskA  result = 0.20;  
case DetailedTaskB  result = 0.30; 
case DetailedTaskC  result = 0.35; 
case PrecisionLightingIndustrial: result = 0.70;   
case PortableOfficeLighting: result = 0.20;  
case SpecializedTaskLighting result = 0.35; 
case TunableWhiteLuminaires: result = 0.10; 
case VideoConferencing: result = 1.00;
case LowVisionMainEntryTransition: result =  0.95;
End Switch</xsd:documentation>
                                                                     </xsd:annotation>
                                                                  </xsd:element>
                                                                  <xsd:element name="Jc04_AdditionalAllowedPowerPerFoot" type="comp:AdditionalAllowedPowerPerFoot">
                                                                     <xsd:annotation>
                                                                        <xsd:documentation source="FieldText">Allowed Density (W/ft<d:sup>2</d:sup> or W/lf or W/unit)</xsd:documentation>
                                                                        <xsd:documentation source="CalculationsAndRules">Calculated field:  Assigned results are W/ft stored in AdditionalAllowedPowerPerFoot; Switch (Jc03_ AreaCategoryAdditionalAllowanceType) 
case WhiteChalkBoardLighting: result = 7;
End Switch</xsd:documentation>
                                                                     </xsd:annotation>
                                                                  </xsd:element>
                                                                  <xsd:element name="Jc04_AdditionalAllowedPowerPerUnit" type="comp:AdditionalAllowedPowerPerUnit">
                                                                     <xsd:annotation>
                                                                        <xsd:documentation source="FieldText">Allowed Density (W/ft<d:sup>2</d:sup> or W/lf or W/unit)</xsd:documentation>
                                                                        <xsd:documentation source="CalculationsAndRules">Calculated field:  Assigned results are W/unit stored in  AdditionalAllowedPowerPerUnit; Switch (Jc03_ AreaCategoryAdditionalAllowanceType) 
case MirrorLightingExternal: result = 40;  
case MirrorLightingInternal: result = 120;
End Switch</xsd:documentation>
                                                                     </xsd:annotation>
                                                                  </xsd:element>
                                                                  <xsd:element name="Jc04_NotApplicableMessage">
                                                                     <xsd:annotation>
                                                                        <xsd:documentation source="FieldText">Allowed Density (W/ft<d:sup>2</d:sup> or W/lf or W/unit)</xsd:documentation>
                                                                        <xsd:documentation source="CalculationsAndRules">Calculated field:  If Jc03_ AreaCategoryAdditionalAllowanceType == ParkingGarageATM result = BlankGrayedOut stored in NotApplicableMessage;</xsd:documentation>
                                                                     </xsd:annotation>
                                                                     <xsd:simpleType>
                                                                        <xsd:restriction base="comp:NotApplicableMessage">
                                                                           <xsd:enumeration value="BlankGrayedOut"/>
                                                                        </xsd:restriction>
                                                                     </xsd:simpleType>
                                                                  </xsd:element>
                                                               </xsd:choice>
                                                               <xsd:choice maxOccurs="1">
                                                                  <xsd:element name="Jc05_SpaceFloorArea" type="com:SpaceFloorArea">
                                                                     <xsd:annotation>
                                                                        <xsd:documentation source="FieldText">Ltg Area, Length or ATM/Mirror  (ft<d:sup>2</d:sup>, lf or #)</xsd:documentation>
                                                                        <xsd:documentation source="CalculationsAndRules">If Jc04_AdditionalAllowedLPD NotNull User enters floor area;</xsd:documentation>
                                                                     </xsd:annotation>
                                                                  </xsd:element>
                                                                  <xsd:element name="Jc05_BoardLightingLength" type="lit:BoardLightingLength">
                                                                     <xsd:annotation>
                                                                        <xsd:documentation source="FieldText">Ltg Area, Length or ATM/Mirror (ft<d:sup>2</d:sup>, lf or #)</xsd:documentation>
                                                                        <xsd:documentation source="CalculationsAndRules">If Jc04_AdditionalAllowedPowerPerFoot NotNull User enters the length of the white/chalk board.</xsd:documentation>
                                                                     </xsd:annotation>
                                                                  </xsd:element>
                                                                  <xsd:element name="Jc05_MirrorsCount" type="lit:MirrorsCount">
                                                                     <xsd:annotation>
                                                                        <xsd:documentation source="FieldText">Ltg Area, Length or ATM/Mirror (ft<d:sup>2</d:sup>, lf or #)</xsd:documentation>
                                                                        <xsd:documentation source="CalculationsAndRules">If Jc03_ AreaCategoryAdditionalAllowanceType == MirrorLightingExternal Or MirrorLightingInternal User enters number of mirrors</xsd:documentation>
                                                                     </xsd:annotation>
                                                                  </xsd:element>
                                                                  <xsd:element name="Jc05_BankingKioskCount" type="lit:BankingKioskCount">
                                                                     <xsd:annotation>
                                                                        <xsd:documentation source="FieldText">Ltg Area, Length or ATM/Mirror  (ft<d:sup>2</d:sup>, lf or #)</xsd:documentation>
                                                                        <xsd:documentation source="CalculationsAndRules">If Jc03_ AreaCategoryAdditionalAllowanceType == ParkingGarageATM:  User enters number of ATM's or Ticket machines in area</xsd:documentation>
                                                                     </xsd:annotation>
                                                                  </xsd:element>
                                                               </xsd:choice>
                                                               <xsd:element name="Jc06_ExtraPowerCalculatedForArea" type="comp:ExtraPowerCalculatedForArea">
                                                                  <xsd:annotation>
                                                                     <xsd:documentation source="FieldText">Extra Allowance (Watts)</xsd:documentation>
                                                                     <xsd:documentation source="CalculationsAndRules">Calculated value:  If Jc04_AdditionalAllowedLPD NotNull  
result = Jc04_AdditionalAllowedLPD x Jc05_SpaceFloorArea; 
Else If Jc04_AdditionalAllowedPowerPerFoot NotNull 
result = Jc04_AdditionalAllowedPowerPerFoot x Jc05_BoardLightingLength; Else If  Jc03_ AreaCategoryAdditionalAllowanceType == MirrorLightingExternal Or   MirrorLightingInternal 
result = Jc04_AdditionalAllowedPowerPerUnit x Jc05_MirrorsCount; 
Else If Jc03_ AreaCategoryAdditionalAllowanceType == ParkingGarageATM
result = 100 + (Jc05_BankingKioskCount-1) x 50)
End If</xsd:documentation>
                                                                  </xsd:annotation>
                                                               </xsd:element>
                                                               <xsd:element name="JcTableLuminaires">
                                                                  <xsd:annotation>
                                                                     <xsd:documentation source="FieldText"/>
                                                                  </xsd:annotation>
                                                                  <xsd:complexType>
                                                                     <xsd:sequence>
