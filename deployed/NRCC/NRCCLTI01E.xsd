                                                         <xsd:annotation>
                                                            <xsd:documentation source="FieldText">Total Watts</xsd:documentation>
                                                            <xsd:documentation source="CalculationsAndRules">Calculated value: result = Q11_LuminairePower x  Q13_LuminaireCountByType</xsd:documentation>
