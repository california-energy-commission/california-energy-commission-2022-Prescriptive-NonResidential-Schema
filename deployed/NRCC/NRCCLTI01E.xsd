                                                <xsd:enumeration value="RetailMallConcourse"/>
                                                <xsd:enumeration value="RetailMerchandiseSalesWholesaleShowroom"/>
                                                <xsd:enumeration value="RetailSalesArea"/>
                                                <xsd:enumeration value="SportsArenaSeatsGTE5000ClassI"/>
                                                <xsd:enumeration value="SportsArenaSeatsLimitedClassIV"/>
                                                <xsd:enumeration value="SportsArenaSeatsLT2000ClassIII"/>
                                                <xsd:enumeration value="SportsArenaSeatsLT5000ClassII"/>
                                                <xsd:enumeration value="Stairwell"/>
                                                <xsd:enumeration value="StorageMF"/>
                                                <xsd:enumeration value="TheaterMotionPicture"/>
                                                <xsd:enumeration value="TheaterPerformance"/>
                                                <xsd:enumeration value="TransportationFunctionConcourseBaggage"/>
                                                <xsd:enumeration value="TransportationFunctionTicketing"/>
                                                <xsd:enumeration value="VideoConferencingStudio"/>
                                             </xsd:restriction>
                                          </xsd:simpleType>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:element name="R03_SpaceFloorArea" type="com:SpaceFloorArea">
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Area (ft<d:sup>2</d:sup>)</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">User Input</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="R04_AllowedLPD" type="comp:AllowedLPD">
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Allowed Density (W/ft<d:sup>2</d:sup>)</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">Auto-filled with allowed LPD for  building or space type value in R02. If R02_WholeBuilding NotNull  Switch ( R02_WholeBuildingOccupancy)   
case LibraryBuilding: case ReligiousFacilityBuilding:  result = 0.70; 
case AssemblyBuilding: case FinancialInstitutionBuilding: case MuseumBuilding: case RestaurantBuilding: result = 0.65;  
case GymnasiumBuilding: case OfficeBuilding: case SchoolBuilding case  ManufacturingFacilityBuilding: case MotionPictureBuilding: result = 0.60; 
case ParkingGarageBuilding: result = 0.13; 
case GroceryStoreBuilding: case HospitalBuilding: case RetailBuilding: result = 0.90; 
case PerformingArtsTheaterBuilding:  case SportsArenaBuilding: result =0.75; 
case AllOthersBuildings:  result = 0.4;  
End Switch
 
Else If R02_SpaceFunction NotNull Switch (R02_SpaceFunction) 
ParkingGarageBuildingParkingArea: result = 0.10;
 
case CommercialIndustrialWarehouse: case CorridorArea: case FamilyDiningArea:  case ElectricalMechancialTelephoneRoom:  case TransportationFunctionConcourseBaggage:  case AllOthersSpace:  result = 0.40;    
 
case FastfoodDining: case BarLoungeFineDiningArea: case LockerDressingRoom: case Laundry: case StorageMF: case TransportationFunctionTicketing: result = 0.45;
 
case AudienceSeatingArea: case CopyRoom:  case ExerciseCenterGymnasiumArea: case TheaterMotionPicture: result = 0.50;   
 
case AutoRepairArea: case HospitalMedicalSupply:   case LoungeBreakRoom: result = 0.55;  
 
case ClassroomLectureTrainingVocationalArea:  case CommercialIndustrialStorageArea:  case ExhibitMuseumArea: case  GeneralCommercialIndustrialWorkAreaLowBay: case HospitalImaging:  case OfficeGreaterThan250Ft:  case RetailFittingRoom: case RetailMallConcourse:  case Stairwell MallAndAtria result = 0.60;  
 
case GeneralCommercialIndustrialWorkAreaHighBay:  case Office250FtOrLess: case Restroom:  result = 0.65;  
 
case AuditoriumArea: case BeautySalonArea:  case FinancialTransactionsArea:   case HospitalPatientRoom: case LobbyMainEntry: case LowVisionCorridorArea: case MuseumRestoration:    result = 0.70; 
 
case ConventionConferenceMultipurposeMeetingCenterArea: case ConferenceMultipurposeMeetingMF: case HospitalPhysicalTherapy:  case NurseStationMF: case SportsArenaSeatsLimitedClassIV:  case  result = 0.75;
 
case LibraryReadingArea: case LowVisionStairwell:  case LowVisionDiningArea: LowVisionWaitingArea: case HospitalNursery: case TheaterPerformance:   result = 0.80;  
 
case  HotelFunctionArea: case GeneralCommercialIndustrialWorkAreaPrecision:   case HospitalNurseStation: case LowVisionLobbyMainEntry: case LowVisionMultipurposeRoom: case PhysicalTherapyMF:  result = 0.85
 
case CivicMeetingPlaceArea:   case HospitalRecovery: case LaboratoryScientific: case  VideoConferencingStudio:    result = 0.90;  
 
case KitchenCommercialFoodPreparation:   case ReligiousWorshipArea:  case RetailSalesArea: RetailMerchandiseSalesWholesaleShowroom: result = 0.95;        
 
case GrocerySalesArea: case LibraryStacks:  case LowVisionReligiousWorshipArea: case LowVisionRestroom:  case ParkingGarageAreaDaylightAdaptationZone:  case PharmacyArea:   result = 1.00; 
 
case SportsArenaSeatsLT2000ClassIII:  result =1.10;  
case HospitalExamTreatment:  result = 1.15;  
 
case SportsArenaSeatsLT5000ClassII:  result = 1.45; 
 
case HospitalOperatingRoom:  result = 1.90;   
 
case SportsArenaSeatsGTE5000ClassI:  result = 2.25;  
End Switch
End If</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="R05_LightingPowerAllowance" type="comp:LightingPowerAllowance">
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Allowance (Watts)</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">Calculated value:  result = PRODUCT (R03_ SpaceFloorArea, R04_AllowedLPD)</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="RTableLuminaires">
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText"/>
                                       </xsd:annotation>
                                       <xsd:complexType>
                                          <xsd:sequence>
                                             <xsd:element name="Row" minOccurs="1" maxOccurs="unbounded">
                                                <xsd:complexType>
                                                   <xsd:sequence>
                                                      <xsd:element name="R06_LuminaireName" type="lit:LuminaireName">
                                                         <xsd:annotation>
                                                            <xsd:documentation source="FieldText">Luminaire Name or Item Tag</xsd:documentation>
                                                            <xsd:documentation source="CalculationsAndRules">User input: Dropdown list auto-filled from values for Fc01_LuminaireName  and Fu01_LuminaireName</xsd:documentation>
                                                         </xsd:annotation>
                                                      </xsd:element>
                                                      <xsd:element name="R07_LuminairePower" type="lit:LuminairePower">
                                                         <xsd:annotation>
                                                            <xsd:documentation source="FieldText">Watts per Luminaire</xsd:documentation>
                                                            <xsd:documentation source="CalculationsAndRules">autofilled result = Fc05_LuminairePower Or Fu05_LuminairePower for luminaire selected in R06_LuminaireName</xsd:documentation>
                                                         </xsd:annotation>
                                                      </xsd:element>
                                                      <xsd:element name="R08_LuminaireCountByType" type="lit:LuminaireCountByType">
                                                         <xsd:annotation>
                                                            <xsd:documentation source="FieldText"># of Luminaires</xsd:documentation>
                                                            <xsd:documentation source="CalculationsAndRules">User input</xsd:documentation>
                                                         </xsd:annotation>
                                                      </xsd:element>
                                                      <xsd:element name="R09_LuminairePowerByType" type="lit:LuminairePowerByType">
                                                         <xsd:annotation>
                                                            <xsd:documentation source="FieldText">Total Design Watts</xsd:documentation>
                                                            <xsd:documentation source="CalculationsAndRules">Calculated value:   result = PRODUCT (R07_LuminairePower, R08_LuminaireCountByType)</xsd:documentation>
                                                         </xsd:annotation>
                                                      </xsd:element>
                                                   </xsd:sequence>
                                                </xsd:complexType>
                                             </xsd:element>
                                          </xsd:sequence>
                                       </xsd:complexType>
                                    </xsd:element>
                                 </xsd:sequence>
                              </xsd:complexType>
                           </xsd:element>
                        </xsd:sequence>
                     </xsd:complexType>
                  </xsd:element>
                  <xsd:element name="RTotals05_LightingPowerAllowedTotal" type="comp:LightingPowerAllowedTotal">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Total Allowance (Watts) for all Areas:</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">Calculated value:   result = SUM ALL (R05_LightingPowerAllowance)</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="RTotals09_LightingPowerInstalledTotal" type="comp:LightingPowerInstalledTotal">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Total Design Watts for all Areas:</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">Calculated value:   result = SUM ALL (R09_LuminairePowerByType)</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="R09bPercentAllowance_LightingPowerInstalledPowerAllowedRatio" type="comp:LightingPowerInstalledPowerAllowedRatio">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Percent of Indoor Lighting Power Allowance:</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">Calculated value:   result = RTotals09_LightingPowerInstalledTotal / RTotals05_LightingPowerAllowedTotal. Display of percent = result x 100.  
If result GT .80  
D03_ ExceptionalConditionMessageCCLTIE += LP80PercentAlterationExceededInR
End If</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
         <xsd:element name="Section_S" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation source="FieldText">
                  <d:b>S. DAYLIGHT DESIGN POWER ADJUSTMENT FACTOR (PAF)</d:b>
               </xsd:documentation>
               <xsd:documentation source="CalculationsAndRules">This section is required If in section P Pc08 Or Pu08 == true</xsd:documentation>
               <xsd:documentation source="MinOccurs">If this section doesn't apply, do not display anything for this section</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="S_BeginNote1" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"/>
                        <xsd:documentation source="AdditionalRequirements">
                           <d:i>This table documents clerestories, horizontal slats or light shelves meet the requirements in 
                              <d:t24refnolink/>140.3(d) /
                              <d:t24refnolink/>170.2(e)2B if a Power Adjustment Factor was claimed on Table P.  These features must be documented on the architectural plans or where appropriate within the construction documents.  This PAF also must be verified in the field with an acceptance test per Table U. </d:i>
                        </xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="S01_PAFCreditType" maxOccurs="unbounded">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Compliance Strategy</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">Use selects from dropdown list any that apply: Clerestories, Horizontal Slats, Light Shelves</xsd:documentation>
                     </xsd:annotation>
                     <xsd:simpleType>
                        <xsd:restriction base="comp:PAFCreditType">
                           <xsd:enumeration value="Clerestories"/>
                           <xsd:enumeration value="HorizontalSlats"/>
                           <xsd:enumeration value="LightShelves"/>
                        </xsd:restriction>
                     </xsd:simpleType>
                  </xsd:element>
                  <xsd:element name="Clerestories" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">
                           <d:b>Clerestory Fenestration</d:b>
                        </xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">This subsection is required if S01_PAFCreditType includes Clerestories Or Light Shelves</xsd:documentation>
                     </xsd:annotation>
                     <xsd:complexType>
                        <xsd:sequence>
                           <xsd:element name="S02_ClerestoryLocation" type="comp:ClerestoryLocation">
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText">Requirement</xsd:documentation>
                                 <xsd:documentation source="AdditionalRequirements">Installed on East, West or South facade</xsd:documentation>
                                 <xsd:documentation source="CalculationsAndRules">User input: If Yes checked result = true Else if Yes is unchecked result = false</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                           <xsd:element name="S03_ClerestoryHeadHeight" type="comp:ClerestoryHeadHeight">
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText">Requirement</xsd:documentation>
                                 <xsd:documentation source="AdditionalRequirements">Head height 
                                    <d:gte/>10ft above finished floor</xsd:documentation>
                                 <xsd:documentation source="CalculationsAndRules">User input: If Yes checked result = true Else if Yes is unchecked result = false</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                           <xsd:element name="S04_ClerestoryGlazingHeight" type="comp:ClerestoryGlazingHeight">
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText">Requirement</xsd:documentation>
                                 <xsd:documentation source="AdditionalRequirements">Glazing height 
                                    <d:gte/>0.10 x head height</xsd:documentation>
                                 <xsd:documentation source="CalculationsAndRules">User input: If Yes checked result = true Else if Yes is unchecked result = false</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                           <xsd:element name="S05_ClerestoryShading" type="comp:ClerestoryShading">
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText">Requirement</xsd:documentation>
                                 <xsd:documentation source="AdditionalRequirements">Operable shading on clerestory is controlled separately from other shading</xsd:documentation>
                                 <xsd:documentation source="CalculationsAndRules">User input: If Yes is checked result = true Else If Not Applicable is checked result = false display term Not Applicable</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                        </xsd:sequence>
                     </xsd:complexType>
                  </xsd:element>
                  <xsd:element name="HorizontalSlats" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">
                           <d:b>Interior and Exterior Horizontal Slats</d:b>
                        </xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">This subsection is required if S01_PAFCreditType includes HorizontalSlats</xsd:documentation>
                     </xsd:annotation>
                     <xsd:complexType>
                        <xsd:sequence>
                           <xsd:element name="S06_HorizontalSlatsLocation" type="comp:HorizontalSlatsLocation">
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText">Requirement</xsd:documentation>
                                 <xsd:documentation source="AdditionalRequirements">Installed on East or West facade with 20-30% WWR</xsd:documentation>
                                 <xsd:documentation source="CalculationsAndRules">User input: If Yes checked result = true Else if Yes is unchecked result = false</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                           <xsd:element name="S07_HorizontalSlatsMounting" type="comp:HorizontalSlatsMounting">
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText">Requirement</xsd:documentation>
                                 <xsd:documentation source="AdditionalRequirements">Exterior slats level or sloped downward from fenestration.  Interior slats level or sloped upward from fenestration.  Slats are permanently mounted and not adjustable</xsd:documentation>
                                 <xsd:documentation source="CalculationsAndRules">User input: If Yes checked result = true Else if Yes is unchecked result = false</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                           <xsd:element name="S08_HorizontalSlatsProjection" type="comp:HorizontalSlatsProjection">
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText">Requirement</xsd:documentation>
                                 <xsd:documentation source="AdditionalRequirements">Slats have a Projection Factor per Table 140.3-E / 170.2-N Daylighting Devices and extend the entire height of vertical fenestration and beyond each side of the window jamb by a distance 
                                    <d:gte/> their horizontal projection.  Slats do not need to extend beyond the jamb if they are located entirely within the fenestration rough opening or a fin is located at the jamb and extends vertically the entire height of the jamb and horizontally the entire depth of the projection.</xsd:documentation>
                                 <xsd:documentation source="CalculationsAndRules">User input: If Yes checked result = true Else if Yes is unchecked result = false</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                           <xsd:element name="S09_HorizontalSlatsDistanceFactor" type="comp:HorizontalSlatsDistanceFactor">
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText">Requirement</xsd:documentation>
                                 <xsd:documentation source="AdditionalRequirements">Slats have a minimum Distance Factor of 0.3 calculated per 
                                    <d:t24refnolink/>140.3(d) /
                                    <d:t24refnolink/>170.2(e)2Bxii</xsd:documentation>
                                 <xsd:documentation source="CalculationsAndRules">User input: If Yes is checked result = true Else If Not Applicable is checked result = false display term Not Applicable</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                           <xsd:element name="S10_HorizontalSlatsVisibleReflectance" type="comp:HorizontalSlatsVisibleReflectance">
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText">Requirement</xsd:documentation>
                                 <xsd:documentation source="AdditionalRequirements">Slats have a minimum Visible Reflectance of 0.50 tested in accordance with ASTM E903, and are either opaque or have a maximum Visible Transmittance of 0.03 tested in accordance with ASTM E1175</xsd:documentation>
                                 <xsd:documentation source="CalculationsAndRules">User input: If Yes checked result = true Else if Yes is unchecked result = false</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                           <xsd:element name="S11_BuildingPlanName" type="comp:BuildingPlanName">
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText">Location of slat design in construction documents:</xsd:documentation>
                                 <xsd:documentation source="AdditionalRequirements">User input string</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                        </xsd:sequence>
                     </xsd:complexType>
                  </xsd:element>
                  <xsd:element name="LightShelves" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">
                           <d:b>Interior and Exterior Light Shelves</d:b>
                        </xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">This subsection is required if S01_PAFCreditType includes  LightShelves</xsd:documentation>
                     </xsd:annotation>
                     <xsd:complexType>
                        <xsd:sequence>
                           <xsd:element name="S12_LightShelvesInteriorExterior" type="comp:LightShelvesInteriorExterior">
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText">Requirement</xsd:documentation>
                                 <xsd:documentation source="AdditionalRequirements">Where there is vertical fenestration area below the light shelf, both interior and exterior light shelves shall be installed.</xsd:documentation>
                                 <xsd:documentation source="CalculationsAndRules">User input: If Yes is checked result = true Else If Not Applicable is checked result = false display term Not Applicable</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                           <xsd:element name="S13_LightShelvesLocation" type="comp:LightShelvesLocation">
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText">Requirement</xsd:documentation>
                                 <xsd:documentation source="AdditionalRequirements">Light shelves installed adjacent to clerestory fenestration on south facade with 
                                    <d:gt/> 30% WWR and with a head height 
                                    <d:lte/> 1ft below the finished ceiling.</xsd:documentation>
                                 <xsd:documentation source="CalculationsAndRules">User input: If Yes checked result = true Else if Yes is unchecked result = false</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                           <xsd:element name="S14_LightShelvesMounting" type="comp:LightShelvesMounting">
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText">Requirement</xsd:documentation>
                                 <xsd:documentation source="AdditionalRequirements">Exterior light shelves are sloped downward from fenestration.  Interior light shelves are sloped upward from fenestration.</xsd:documentation>
                                 <xsd:documentation source="CalculationsAndRules">User input: If Yes checked result = true Else if Yes is unchecked result = false</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                           <xsd:element name="S15_LightShelvesProjection" type="comp:LightShelvesProjection">
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText">Requirement</xsd:documentation>
                                 <xsd:documentation source="AdditionalRequirements">Shelves have a Projection Factor per Table 140.3-D Daylighting Devices and extend beyond each side of the window jamb by a distance 
                                    <d:gte/> their horizontal projection.</xsd:documentation>
                                 <xsd:documentation source="CalculationsAndRules">User input: If Yes checked result = true Else if Yes is unchecked result = false</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                           <xsd:element name="S16_LightShelvesDistanceFactor" type="comp:LightShelvesDistanceFactor">
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText">Requirement</xsd:documentation>
                                 <xsd:documentation source="AdditionalRequirements">Shelves have a minimum Distance Factor of 0.3 calculated per 
                                    <d:t24refnolink/>140.3(d) /
                                    <d:t24refnolink/>170.2(e)2Bxii</xsd:documentation>
                                 <xsd:documentation source="CalculationsAndRules">User input: If Yes checked result = true Else if Yes is unchecked result = false</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                           <xsd:element name="S17_LightShelvesVisibleReflectance" type="comp:LightShelvesVisibleReflectance">
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText">Requirement</xsd:documentation>
                                 <xsd:documentation source="AdditionalRequirements">Shelves have a minimum Visible Reflectance of 0.50 tested in accordance with ASTM E903, or an exterior light shelf installed 
                                    <d:gt/> 2ft below the clerestory sill.</xsd:documentation>
                                 <xsd:documentation source="CalculationsAndRules">User input: If Yes checked result = true Else if Yes is unchecked result = false</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                           <xsd:element name="S18_BuildingPlanName" type="comp:BuildingPlanName">
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText">Location of light shelve design in construction documents:</xsd:documentation>
                                 <xsd:documentation source="AdditionalRequirements">User input string</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                        </xsd:sequence>
                     </xsd:complexType>
                  </xsd:element>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
         <xsd:element name="Section_T" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation source="FieldText">
                  <d:b>T. DWELLING UNIT LIGHTING</d:b>
               </xsd:documentation>
               <xsd:documentation source="CalculationsAndRules">This section is required If A03_WholeBuildingOccupancy includes MultiFamilyMixedUseGTE4 or HighRiseResidential</xsd:documentation>
               <xsd:documentation source="MinOccurs">If this section doesn't apply, do not display anything for this section</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="T_BeginNote1" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"/>
                        <xsd:documentation source="AdditionalRequirements">
                           <d:i>This table documents compliance with mandatory requirements for dwelling unit indoor lighting per 
                              <d:t24refnolink/>160.5(a) and 
                              <d:t24refnolink/>180.2(b)4A for alterations.  Lighting systems in common use areas providing shared provisions for living, eating, cooking or sanitation to dwelling units that would otherwise lack these provisions may document compliance with  
                              <d:t24refnolink/>160.5(a) in this table or may document compliance with lighting for nonresidential occupancies in other tables of this form. </d:i>
                        </xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="T01Label" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Indicate lighting and electrical components included in multifamily dwelling units:</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="T01a_IncludesLuminaire" type="comp:IncludesLuminaire">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Luminaires</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="T01b_IncludesBlankElecBox" type="comp:IncludesBlankElecBox">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Blank Electrical Boxes</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="Luminaires" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">
                           <d:b>Luminaires</d:b>
                        </xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">If T01a_IncludesLuminaire == true
this subsection is required</xsd:documentation>
                     </xsd:annotation>
                     <xsd:complexType>
                        <xsd:sequence>
                           <xsd:element name="T02_HighLuminousEfficacyReq" type="comp:HighLuminousEfficacyReq">
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText">Requirement</xsd:documentation>
                                 <xsd:documentation source="AdditionalRequirements">All installed luminaires shall meet the requirements in TABLE 160.5-A except lighting integral to exhaust fans, kitchen range hoods, bath vanity mirrors, and garage door openers, and non-removable lighting attached to ceiling fans; lighting internal to drawers, cabinetry, and linen closets with an efficacy of 45 lumens per watt or greater.</xsd:documentation>
                                 <xsd:documentation source="CalculationsAndRules">User input: If Yes checked result = true Else if Yes is unchecked result = false</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                           <xsd:element name="T03_ScrewBasedReq" type="comp:ScrewBasedReq">
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText">Requirement</xsd:documentation>
                                 <xsd:documentation source="AdditionalRequirements">Screw based luminaires shall contain lamps that comply with Reference Joint Appendix JA8.</xsd:documentation>
                                 <xsd:documentation source="CalculationsAndRules">User input: If Yes is checked result = true Else If Not Applicable is checked result = false display term Not Applicable</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                           <xsd:element name="T04_RecessedDownlightReq" type="comp:RecessedDownlightReq">
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText">Requirement</xsd:documentation>
                                 <xsd:documentation source="AdditionalRequirements">Recessed Downlight Luminaires other than those marked for use in fire-rated installations and other than recessed luminaires installed in non-insulated ceilings shall meet all of the following requirements:
                                    <d:line1/> - Shall not contain screw base lamp sockets; and 
                                    <d:line1/> - Have a label that certifies the luminaire is airtight with air leakage less than 2.0 cfm at 75 Pascals when tested in accordance with ASTM E283. An exhaust fan   housing with integral light shall not be required to be certified airtight; and 
                                    <d:line1/> - Be sealed with a gasket or caulk between the luminaire housing and ceiling, and have all air leak paths between conditioned and unconditioned spaces sealed with a gasket or caulk, or be installed per manufacturer's instructions to maintain airtightness between the luminaire housing and ceiling; and 
                                    <d:line1/> - Meet the clearance and installation requirements of California Electrical Code Section 410.116 for recessed luminaires.</xsd:documentation>
                                 <xsd:documentation source="CalculationsAndRules">User input: If Yes is checked result = true Else If Not Applicable is checked result = false display term Not Applicable</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                           <xsd:element name="T05_EnclosedTempReq" type="comp:EnclosedTempReq">
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText">Requirement</xsd:documentation>
                                 <xsd:documentation source="AdditionalRequirements">Light Sources in Enclosed or Recessed Luminaires. Lamps and other separable light sources that are not compliant with the JA8 elevated temperature requirements, including marking requirements, shall not be installed in enclosed or recessed luminaires.</xsd:documentation>
                                 <xsd:documentation source="CalculationsAndRules">User input: If Yes is checked result = true Else If Not Applicable is checked result = false display term Not Applicable</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                        </xsd:sequence>
                     </xsd:complexType>
                  </xsd:element>
                  <xsd:element name="Controls" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">
                           <d:b>Lighting Controls</d:b>
                        </xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">If T01a_IncludesLuminaire == true
this subsection is required</xsd:documentation>
                     </xsd:annotation>
                     <xsd:complexType>
                        <xsd:sequence>
                           <xsd:element name="T06_WallControlsReq" type="comp:WallControlsReq">
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText">Requirement</xsd:documentation>
                                 <xsd:documentation source="AdditionalRequirements">Lighting shall have readily accessible wall-mounted controls that allow the lighting to be manually turned ON and OFF. Ceiling fans may provide control of integrated lighting via a remote control.</xsd:documentation>
                                 <xsd:documentation source="CalculationsAndRules">User input: If Yes checked result = true Else if Yes is unchecked result = false</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                           <xsd:element name="T07_MandatoryLightControlsReq" type="comp:MandatoryLightControlsReq">
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText">Requirement</xsd:documentation>
                                 <xsd:documentation source="AdditionalRequirements">Lighting controls shall comply with the applicable requirements of Section 110.9.</xsd:documentation>
                                 <xsd:documentation source="CalculationsAndRules">User input: If Yes checked result = true Else if Yes is unchecked result = false</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                           <xsd:element name="T08_NoBypassDimmerSensorReq" type="comp:NoBypassDimmerSensorReq">
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText">Requirement</xsd:documentation>
                                 <xsd:documentation source="AdditionalRequirements">No controls shall bypass a dimmer, occupant sensor or vacancy sensor function where that dimmer or sensor has been installed to comply with Section160.5(a)2.</xsd:documentation>
                                 <xsd:documentation source="CalculationsAndRules">User input: If Yes is checked result = true Else If Not Applicable is checked result = false display term Not Applicable</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                           <xsd:element name="T09_AutoManualControlsReq" type="comp:AutoManualControlsReq">
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText">Requirement</xsd:documentation>
                                 <xsd:documentation source="AdditionalRequirements">An Energy Management Control System (EMCS) or a multiscene programmable controller may be used to comply with dimming, occupancy, and lighting control requirements in Section 160.5(a)2 if it provides the functionality of the specified controls in accordance with Section 110.9, and the physical controls specified in in Section 160.5(a)2A.</xsd:documentation>
                                 <xsd:documentation source="CalculationsAndRules">User input: If Yes is checked result = true Else If Not Applicable is checked result = false display term Not Applicable</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                           <xsd:element name="T10_SupportAreasSensorReq" type="comp:SupportAreasSensorReq">
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText">Requirement</xsd:documentation>
                                 <xsd:documentation source="AdditionalRequirements">In bathrooms, garages, laundry rooms, utility rooms, and walk-in closets, at least one installed luminaire shall be controlled by an occupancy or vacancy sensor providing automatic-off functionality.</xsd:documentation>
                                 <xsd:documentation source="CalculationsAndRules">User input: If Yes checked result = true Else if Yes is unchecked result = false</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                           <xsd:element name="T11_CabinetLightControlReq" type="comp:CabinetLightControlReq">
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText">Requirement</xsd:documentation>
                                 <xsd:documentation source="AdditionalRequirements">For lighting internal to drawers and cabinetry with opaque fronts or doors, controls that turn light off when the drawer or door is closed shall be provided.</xsd:documentation>
                                 <xsd:documentation source="CalculationsAndRules">User input: If Yes is checked result = true Else If Not Applicable is checked result = false display term Not Applicable</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                           <xsd:element name="T12_HabitableAreaControlsReq" type="comp:HabitableAreaControlsReq">
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText">Requirement</xsd:documentation>
                                 <xsd:documentation source="AdditionalRequirements">Lighting in habitable spaces, including but not limited to living rooms, dining rooms, kitchens, and bedrooms, shall have readily accessible wall-mounted dimming controls that allow the lighting to be manually adjusted up and down OR luminaires must be controlled by an occupancy or vacancy sensor providing automatic-off functionality. Forward phase cut dimmers controlling LED light sources shall comply with NEMA SSL 7A. 
Ceiling fans may provide control of integrated lighting via a remote control.
Lighting internal to drawers and cabinetry with opaque fronts or doors or with automatic off controls does not need to provide dimming controls.</xsd:documentation>
                                 <xsd:documentation source="CalculationsAndRules">User input: If Yes checked result = true Else if Yes is unchecked result = false</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                           <xsd:element name="T13_SeparateControlsReq" type="comp:SeparateControlsReq">
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText">Requirement</xsd:documentation>
                                 <xsd:documentation source="AdditionalRequirements">Integrated lighting of exhaust fans shall be controlled independently from the fans. The following shall be controlled separately from ceiling-installed lighting such that one can be turned on without turning on the other:
 - Undercabinet lighting
 - Undershelf lighting
 - Interior lighting of display cabinets
 - Switched outlets</xsd:documentation>
                                 <xsd:documentation source="CalculationsAndRules">User input: If Yes checked result = true Else if Yes is unchecked result = false</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                        </xsd:sequence>
                     </xsd:complexType>
                  </xsd:element>
                  <xsd:element name="ElecBoxes" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">
                           <d:b>Blank Electrical Boxes</d:b>
                        </xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">If T01b_IncludesBlankElecBox == true this subsection is required</xsd:documentation>
                     </xsd:annotation>
                     <xsd:complexType>
                        <xsd:sequence>
                           <xsd:element name="T14_BlankElecBoxReq" type="comp:BlankElecBoxReq">
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText">Requirement</xsd:documentation>
                                 <xsd:documentation source="AdditionalRequirements">The number of electrical boxes that are more than 5 feet above the finished floor and do not contain a luminaire or other device shall be no greater than the number of bedrooms. These electrical boxes must be served by a dimmer, vacancy sensor-control, low voltage wiring or fan speed control.</xsd:documentation>
                                 <xsd:documentation source="CalculationsAndRules">User input: If Yes checked result = true Else if Yes is unchecked result = false</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                        </xsd:sequence>
                     </xsd:complexType>
                  </xsd:element>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
         <xsd:element name="Section_Nrci" minOccurs="1">
            <xsd:annotation>
               <xsd:documentation source="FieldText">
                  <d:b>U.  DECLARATION OF REQUIRED CERTIFICATES OF INSTALLATION</d:b>
               </xsd:documentation>
               <xsd:documentation source="CalculationsAndRules">If any selection is changed by the user, 
Then
D03  ExceptionalConditionMessageCCLTIE += UserChangedSelectionInCI
End If</xsd:documentation>
               <xsd:documentation source="MinOccurs">This section is required.</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="Nrci_BeginNote1" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"/>
                        <xsd:documentation source="AdditionalRequirements">
                           <d:i> Selections have been made based on information provided in this document. If any selections have been changed by permit applicant, an explanation should be included in Table E. Additional Remarks.  These documents must be provided to the building inspector during construction and can be found online  </d:i>
                        </xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="Nrci01_RequireNRCILti01E" type="comp:RequireNRCILti01E">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Form/Title</xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">NRCI-LTI-E - Must be submitted for all buildings</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">Result = true and can not be changed by user</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="Nrci01a_VerifedByInspector" type="comp:VerifedByInspector" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Field Inspector</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
         <xsd:element name="Section_Nrca" minOccurs="1">
            <xsd:annotation>
               <xsd:documentation source="FieldText">
                  <d:b>V.  DECLARATION OF REQUIRED CERTIFICATES OF ACCEPTANCE</d:b>
               </xsd:documentation>
               <xsd:documentation source="CalculationsAndRules">If any selection is changed by the user, 
Then
D03  ExceptionalConditionMessageCCLTIE += UserChangedSelectionInCA
End If</xsd:documentation>
               <xsd:documentation source="MinOccurs">This section is required.</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="Nrca_BeginNote1" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"/>
                        <xsd:documentation source="AdditionalRequirements">
                           <d:i>Selections have been made based on information provided in this document.  If any selections have been changed by the permit applicant, an explanation should be included in Table E. Additional Remarks. These documents must be provided to the building inspector during construction and any with "-A" in the form name must be completed through an Acceptance Test Technician Certification Provider (ATTCP). For more information visit: http://www.energy.ca.gov/title24/attcp/providers.html</d:i>
                        </xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="Nrca01_RequireNRCALti02A" type="comp:RequireNRCALti02A">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Form/Title</xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">NRCA-LTI-02-A - Must be submitted for occupancy sensors and automatic time switch controls.</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">Calculated Boolean value based on user input in previous sections of this form. By default result = false represented as a selected No radio button.  
 
If H02_ShutOffControlsBuilding ==WholeBuildingAutoTimeSwitch 
Or H08_ShutOffControlsArea == AutomaticTimerSwitch Or OccupancySensor Or PartialOnShutOffControl Or PartialOffShutOffControl
Or If Q18_ShutOffControlsBuilding == WholeBuildingAutoTimeSwitch Or WholeBuildingOccupancySensors 
Or Q18_ShutOffControlsArea == AutomaticTimerSwitch Or OccupancySensor Or PartialOffShutOffControl Or PartialOnShutOffControl 
result = true represented as a selected Yes radio button.  
End If
 
 If user changes this value to false, an explanation is required in Section E Additional Remarks.</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="Nrca01_SystemFieldVerified" type="comp:SystemFieldVerified" minOccurs="0" maxOccurs="unbounded">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Systems/Spaces To Be Field Verified</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">If Nrca01_RequireNRCALti02A == true; this field is loaded with systems/spaces that satisfy any of these conditions:
 
If H02_ShutOffControlsBuilding ==WholeBuildingAutoTimeSwitch 
Or H08_ShutOffControlsArea == AutomaticTimerSwitch Or OccupancySensor
 load value in H04_SpaceDescription
End If
 
If Q18_ShutOffControlsBuilding == WholeBuildingAutoTimeSwitch Or WholeBuildingOccupancySensors 
Or Q18_ShutOffControlsArea == AutomaticTimerSwitch Or OccupancySensor  
  load value in Q15_SpaceDescription
End If</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="Nrca01_SystemsToFieldVerify" type="comp:SystemsToFieldVerify" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Systems/Spaces To Be Field Verified</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">If Nrca01_RequireNRCALti02A == true this field is displayed and loaded with a string that concatenates systems/spaces from Nrca01_SystemFieldVerified</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="Nrca01a_VerifedByInspector" type="comp:VerifedByInspector" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Field Inspector</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="Nrca02_RequireNRCALti03A" type="comp:RequireNRCALti03A">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Form/Title</xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">NRCA-LTI-03-A - Must be submitted for automatic daylight controls.</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">Calculated Boolean value based on user input in previous sections of this form. By default result = false represented as a selected No radio button. 
 
If all values for H09_PrimarySkylitDaylightControlsIncluded Or H10_SecondaryDaylightControlsIncluded == Included result = true represented as a selected Yes radio button; Else result = false   If user changes this value an explanation is required in Section E Additional Remarks.</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="Nrca02_SystemFieldVerified" type="comp:SystemFieldVerified" minOccurs="0" maxOccurs="unbounded">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Systems/Spaces To Be Field Verified</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">If Nrca02_RequireNRCALti03A == true; this field is loaded with systems/spaces that satisfy this condition:
 
If  value for H09_PrimarySkylitDaylightControlsIncluded Or H10_SecondaryDaylightControlsIncluded == Included
 load value in H04_SpaceDescription
End If</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="Nrca02_SystemsToFieldVerify" type="comp:SystemsToFieldVerify" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Systems/Spaces To Be Field Verified</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">If Nrca02_RequireNRCALti03A == true this field is displayed and loaded with a string that concatenates systems/spaces from Nrca02_SystemFieldVerified</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="Nrca02a_VerifedByInspector" type="comp:VerifedByInspector" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Field Inspector</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="Nrca03_RequireNRCALti04A" type="comp:RequireNRCALti04A">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Form/Title</xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">NRCA-LTI-04-A - Must be submitted for demand responsive lighting controls.</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">Calculated Boolean value based on user input in previous sections of this form. By default result = false represented as a selected No radio button.. 
 
If H01_MandatoryLightingDemandResponseNR  ==RequiredGTE4000W  Or H11_InterlockedLightingSystems == true 
result = true represented as a selected Yes radio button.  If user changes the value an explanation is required in Section E Additional Remarks.</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="Nrca03_SystemFieldVerified" type="comp:SystemFieldVerified" minOccurs="0" maxOccurs="unbounded">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Systems/Spaces To Be Field Verified</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">If Nrca03_RequireNRCALti04A == true; this field is loaded with systems/spaces that satisfy this condition:
 
If H01_MandatoryLightingDemandResponseNR  == RequiredGTE4000W  Or H11_InterlockedLightingSystems == true
 load value in H04_SpaceDescription
End If</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="Nrca03_SystemsToFieldVerify" type="comp:SystemsToFieldVerify" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Systems/Spaces To Be Field Verified</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">If Nrca03_RequireNRCALti04A == true this field is displayed and loaded with a string that concatenates systems/spaces from Nrca03_SystemFieldVerified</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="Nrca03a_VerifedByInspector" type="comp:VerifedByInspector" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Field Inspector</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="Nrca04_RequireNRCALti05A" type="comp:RequireNRCALti05A">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Form/Title</xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">NRCA-LTI-05-A. - Must be submitted for institutional tuning power adjustment factor (PAF)</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">Calculated Boolean value based on user input in previous sections of this form. By default result = false represented as a selected No radio button.. 
 
If Pc02e_ApplyInstitutionalTuningNonDaylit Or Pc02f_ApplyInstitutionalTuningDaylit == true   Or Pu02e_ApplyInstitutionalTuningNonDaylit Or Pu02f_ApplyInstitutionalTuningDaylit == true  
result = true represented as a selected Yes radio button. If user changes the value an explanation is required in Section E Additional Remarks.</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="Nrca04_SystemFieldVerified" type="comp:SystemFieldVerified" minOccurs="0" maxOccurs="unbounded">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Systems/Spaces To Be Field Verified</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">If Nrca04_RequireNRCALti05A == true; this field is loaded with systems/spaces that satisfy any of these conditions:
 
If Pc02e_ApplyInstitutionalTuningNonDaylit Or Pc02f_ApplyInstitutionalTuningDaylit == true   
load value in Pc01_SpaceDescription
End If
 
If Pu02e_ApplyInstitutionalTuningNonDaylit Or Pu02f_ApplyInstitutionalTuningDaylit == true  
 load value in Pu01_SpaceDescription
End If</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="Nrca04_SystemsToFieldVerify" type="comp:SystemsToFieldVerify" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Systems/Spaces To Be Field Verified</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">If Nrca04_RequireNRCALti05A == true this field is displayed and loaded with a string that concatenates systems/spaces from Nrca04_SystemFieldVerified</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="Nrca04a_VerifedByInspector" type="comp:VerifedByInspector" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Field Inspector</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="Nrca05_RequireNRCAEnv03F" type="comp:RequireNRCAEnv03F">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Form/Title</xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">NRCA-ENV-03-F. - Must be submitted for daylighting design power adjustment factors (PAF)</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">Calculated Boolean value based on user input in previous sections of this form. By default result = false represented as a selected No radio button. . 
 
If Pc02h_ApplyClerestoryFenestrationPAF, Pc02i_ApplyHorizontalSlatsPAF Or Pc02j_ApplyLightShelvesPAF == true Or If Pu02h_ApplyClerestoryFenestrationPAF, Pu02i_ApplyHorizontalSlatsPAF Or Pu02j_ApplyLightShelvesPAF == true 
result = true represented as a selected Yes radio button.   If user changes the value an explanation is required in Section E Additional Remarks.</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="Nrca05_SystemFieldVerified" type="comp:SystemFieldVerified" minOccurs="0" maxOccurs="unbounded">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Systems/Spaces To Be Field Verified</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">If Nrca05_RequireNRCAEnv03F == true; this field is loaded with systems/spaces that satisfy any of these conditions:
 
If Pc02h_ApplyClerestoryFenestrationPAF, Pc02i_ApplyHorizontalSlatsPAF Or Pc02j_ApplyLightShelvesPAF == true
load value in Pc01_SpaceDescription
End If
 
If Pu02h_ApplyClerestoryFenestrationPAF, Pu02i_ApplyHorizontalSlatsPAF Or Pu02j_ApplyLightShelvesPAF == true 
load value in Pu01_SpaceDescription
End If</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="Nrca05_SystemsToFieldVerify" type="comp:SystemsToFieldVerify" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Systems/Spaces To Be Field Verified</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">If Nrca05_RequireNRCAEnv03F == true this field is displayed and loaded with a string that concatenates systems/spaces from Nrca05_SystemFieldVerified</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="Nrca05a_VerifedByInspector" type="comp:VerifedByInspector" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Field Inspector</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
      </xsd:sequence>
   </xsd:complexType>
   <xsd:element name="nRCCLTI01E" type="NRCCLTI01E">
      <xsd:annotation>
         <xsd:documentation>This element contains all of the data and text required to generate the NRCCLTI01E compliance report.</xsd:documentation>
      </xsd:annotation>
   </xsd:element>
</xsd:schema>
