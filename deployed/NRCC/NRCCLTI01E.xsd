                                                   <xsd:documentation source="FieldText">4*</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">User input: checked result = true, unchecked result = false.  If result == true add 0.05 to PAF value used in calculation for Pc07_PAF_AllowedInArea</xsd:documentation>
                                                </xsd:annotation>
                                             </xsd:element>
