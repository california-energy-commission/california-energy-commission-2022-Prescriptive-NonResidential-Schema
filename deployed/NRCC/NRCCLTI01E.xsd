                                    <xsd:element name="QEndNote1" minOccurs="0">
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText"/>
                                          <xsd:documentation source="AdditionalRequirements">
                                             <d:i><d:sup>1</d:sup>FOOTNOTE: Authority Having Jurisdiction may ask for Luminaire cut sheets to confirm wattage used for compliance per 
                                                <d:t24refnolink/>130.0(c) /
                                                <d:t24refnolink/>1160.5(b)1</d:i>
                                          </xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="QEndNote2" minOccurs="0">
