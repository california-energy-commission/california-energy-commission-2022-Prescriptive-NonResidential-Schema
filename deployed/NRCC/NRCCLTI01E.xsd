         <xsd:element name="Section_Q" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation source="FieldText">
                  <d:b>Q. RATED POWER REDUCTION COMPLIANCE FOR ONE-FOR-ONE ALTERATIONS</d:b>
               </xsd:documentation>
               <xsd:documentation source="CalculationsAndRules">This section is required if in section B there is at least one value in  Baltered02_LightingPrescriptiveMethodNR Or Baltered04_LightingPrescriptiveMethodNR  == RatedPowerReduction</xsd:documentation>
               <xsd:documentation source="MinOccurs">If this section doesn't apply, do not display anything for this section</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="Q_BeginNote1" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"/>
                        <xsd:documentation source="AdditionalRequirements">
                           <d:i>Indoor lighting alterations complying prescriptively with 
                              <d:t24refnolink/>141.0(b)2I(iii) /
                              <d:t24refnolink/>180.2(b)4biv are documented in this table. Any control option having a * will include a note in the Notes section of this table detailing how compliance is achieved, otherwise the compliance status in Table C will say "DOES NOT COMPLY". </d:i>
                        </xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="Q_ColumnHeading03To14" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"/>
                        <xsd:documentation source="AdditionalRequirements">
                           <d:b>Fixture Schedule </d:b> (Includes all luminaires being altered in the project).</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="Q_ColumnSubheading03To08" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"/>
                        <xsd:documentation source="AdditionalRequirements">Pre-alteration Luminaire Information</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="Q_ColumnSubheading09To14" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"/>
                        <xsd:documentation source="AdditionalRequirements">Post-alteration Luminaire Information</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="Q_ColumnHeading15To19" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"/>
                        <xsd:documentation source="AdditionalRequirements">
                           <d:b>Mandatory Controls</d:b>
                        </xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="Q01_RatedPowerReductionScopeApplies" type="comp:RatedPowerReductionScopeApplies">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Alteration scope includes a one-for-one luminaire alteration within a building or tenant space of 5,000 ft<d:sup>2</d:sup> or less per 
                           <d:t24refnolink/>141.0(b)2I(iii) /
                           <d:t24refnolink/>180.2(b)4biv.</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">Boolean Checked result = true, unchecked result = false.
If result == true,all fields Q03 - 18 fields are displayed and enabled; 
Else If result == false Q03 through Q14Total fields are not required and are blank and grayed out  And Mandatory Controls table with fields Q15 - 18 fields are required and are enabled;
End If</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="Q02_RatedPowerReductionException6AppliesSome" type="comp:RatedPowerReductionException6AppliesSome">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"/>
                        <xsd:documentation source="AdditionalRequirements">At least one floor or complete tenant space includes a one-for-one luminaire alteration of 50 or less luminaires, per annum.  These spaces do not need to comply with Part 6 requirements and therefore do not need to be included in tables below per Exception 6 to 
                           <d:t24refnolink/>141.0(b)2I /
                           <d:t24refnolink/>180.2(b)4biv.</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">User input: If checked result = true and Q02a data is required</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="Q02a_RatedPowerReductionException6Spaces" type="comp:RatedPowerReductionException6Spaces" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Applicable Spaces</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">If Q02_RatedPowerReductionException6AppliesSome == true
user input text listing all applicable spaces; 
Else no data is required in this field.</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="Q02b_RatedPowerReductionException6AppliesToAll" type="comp:RatedPowerReductionException6AppliesToAll">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"/>
                        <xsd:documentation source="AdditionalRequirements">Exception 6 applies to all spaces within the permit application</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">User input: If checked result = true And, all fields Q03-Q14Total fields are not required and are blank and grayed out</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="ChoiceQTables">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"/>
                     </xsd:annotation>
                     <xsd:complexType>
                        <xsd:choice>
                           <xsd:element name="QTablesNA_NotApplicableMessage">
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText"/>
                                 <xsd:documentation source="CalculationsAndRules">If Q01_RatedPowerReductionScopeApplies == false Or If Q02b_RatedPowerReductionException6AppliesToAll == true result = BlankGrayedOut stored in NotApplicableMessage applied to Fixture Table and following totals fields Q08Total_TotalPowerPreAlteration and  Q14Total_TotalPowerPostAlteration And TableControlsBySpace
End If</xsd:documentation>
                              </xsd:annotation>
                              <xsd:simpleType>
                                 <xsd:restriction base="comp:NotApplicableMessage">
                                    <xsd:enumeration value="BlankGrayedOut"/>
                                 </xsd:restriction>
                              </xsd:simpleType>
                           </xsd:element>
                           <xsd:element name="QTablesRequired">
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText"/>
                                 <xsd:documentation source="CalculationsAndRules">If Q01_RatedPowerReductionScopeApplies  == true And Q02b_RatedPowerReductionException6AppliesToAll ==  false
Fixture Table and following totals fields Q08Total_TotalPowerPreAlteration and  Q14Total_TotalPowerPostAlteration and And TableControlsBySpace are required.</xsd:documentation>
                              </xsd:annotation>
                              <xsd:complexType>
                                 <xsd:sequence>
                                    <xsd:element name="TableAllLuminairesAltered">
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText"/>
                                       </xsd:annotation>
                                       <xsd:complexType>
                                          <xsd:sequence>
                                             <xsd:element name="Row" minOccurs="1" maxOccurs="unbounded">
                                                <xsd:complexType>
                                                   <xsd:sequence>
                                                      <xsd:element name="Q03_LuminaireName" type="lit:LuminaireName">
                                                         <xsd:annotation>
                                                            <xsd:documentation source="FieldText">Name or Item Tag</xsd:documentation>
                                                            <xsd:documentation source="CalculationsAndRules">User input</xsd:documentation>
                                                         </xsd:annotation>
                                                      </xsd:element>
                                                      <xsd:element name="Q04_LuminaireDescription" type="lit:LuminaireDescription">
                                                         <xsd:annotation>
                                                            <xsd:documentation source="FieldText">Complete Luminaire Description</xsd:documentation>
                                                            <xsd:documentation source="CalculationsAndRules">User input</xsd:documentation>
                                                         </xsd:annotation>
