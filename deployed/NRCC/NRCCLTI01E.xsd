                                                                  <xsd:annotation>
                                                                     <xsd:documentation source="FieldText">Applicable Qualifying Lighting System from Table 140.6-C</xsd:documentation>
                                                                     <xsd:documentation source="CalculationsAndRules">User selects from dropdown list values dependent on Ju02_SpaceFunction as follows:   
If Ju02_SpaceFunction == AuditoriumArea or ExhibitMuseumArea : list DecorativeDisplayF; 
Else If Ju02_SpaceFunction == AutoRepairArea, ElectricalMechancialTelephoneRoom, GeneralCommercialIndustrialWorkAreaHighBay, Or  GeneralCommercialIndustrialWorkAreaLowBay:  list DetailedTaskA;   
 
Else If Ju02_SpaceFunction == AudienceSeatingArea, CivicMeetingPlaceArea, ConventionCenterAudienceSeatingArea, CorridorArea, HotelFunctionArea,   FastfoodDining, FamilyDiningArea,  FinancialTransactionsArea,  LibraryReadingArea, LobbyMainEntry, LoungeBreakRoom, ReligiousWorshipArea, RetailMallConcourse, TheaterMotionPicture, TheaterPerformance MallAndAtria, ConferenceMultipurposeMeetingMF or ConventionConferenceMultipurposeMeetingCenterArea list DecorativeDisplayC;  
 
Else If Ju02_SpaceFunction ==  BeautySalonArea: list DecorativeDisplayC and DetailedTaskB;  
Else If Ju02_SpaceFunction == ClassroomLectureTrainingVocationalArea: list WhiteChalkBoardLighting;   
Else If Ju02_SpaceFunction == TransportationFunctionTicketing Or LowVisionRestroom: list DecorativeDisplayB;  
Else If Ju02_SpaceFunction ==  GeneralCommercialIndustrialWorkAreaPrecision:  list PrecisionLightingIndustrial;   
Else If Ju02_SpaceFunction ==  Office250FtOrLess, OfficeGreaterThan250Ft Or OfficeOpenPlan:  list PortableOfficeLighting and DecorativeDisplayB;  
Else If Ju02_SpaceFunction == ParkingGarageBuildingParkingArea: list ParkingGarageATM; 
Else If Ju02_SpaceFunction == PharmacyArea Or LaboratoryScientific:  list SpecializedTaskLighting;  
Else If Ju02_SpaceFunction == GrocerySalesArea, BarLoungeFineDiningArea, Restroom, Stairwell:  Or RetailSalesArea: Or RetailMerchandiseSalesWholesaleShowroom: list DecorativeDisplayE;  
Else If Ju02_SpaceFunction == RetailFittingRoom : list MirrorLightingExternal and MirrorLightingInternal;  
Else If Ju02_SpaceFunction == HospitalNursery,  HospitalPhysicalTherapy  Or HospitalRecovery Or NurseStationMF Or PhysicalTherapyMF:  list TunableWhiteLuminaires;  
Else If Ju02_SpaceFunction == HospitalImaging:  list TunableWhiteLuminaires and DecorativeDisplayB;
Else If Ju02_SpaceFunction == HospitalNurseStation:  list TunableWhiteLuminaires and DetailedTaskA; 
Else If Ju02_SpaceFunction == HospitalPatientRoom:  list TunableWhiteLuminaires and DecorativeDisplayA;  
Else If Ju02_SpaceFunction == LowVisionWaitingArea,  LowVisionMultipurposeRoom, LowVisionReligiousWorshipArea Or LowVisionDiningArea:   list TunableWhiteLuminaires and DecorativeDisplayD;
Else If Ju02_SpaceFunction == VideoConferencingStudio: list VideoConferencing;  
Else If Ju02_SpaceFunction ==  LowVisionLobbyMainEntry: list TunableWhiteLuminaires, DecorativeDisplayD and LowVisionMainEntryTransition;  
Else If Ju02_SpaceFunction == LowVisionCorridorArea or LowVisionStairwell Or LowVisionWaitingAreaMF Or LowVisionDiningAreaMF Or LowVisionMultipurposeRoomMF:   list DecorativeDisplayD.
Else If Ju02_SpaceFunction == MuseumRestoration:  list  DetailedTaskC; 
End If</xsd:documentation>
                                                                  </xsd:annotation>
                                                               </xsd:element>
                                                               <xsd:choice maxOccurs="1">
                                                                  <xsd:element name="Ju04_AdditionalAllowedLPD" type="comp:AdditionalAllowedLPD">
                                                                     <xsd:annotation>
                                                                        <xsd:documentation source="FieldText">Allowed Density (W/ft<d:sup>2</d:sup> or W/lf or W/unit)</xsd:documentation>
                                                                        <xsd:documentation source="CalculationsAndRules">Calculated field:  Assigned results are W/ft2 stored in AdditionalAllowedLPD  Switch (Ju03_ AreaCategoryAdditionalAllowanceType) 
case DecorativeDisplayA: result = 0.15;
case DecorativeDisplayB: result = 0.20;
case DecorativeDisplayC: result = 0.25;   
case DecorativeDisplayD: result = 0.30; 
case DecorativeDisplayE: result = 0.35; 
case DecorativeDisplayF: result = 0.45; 
case DetailedTaskA  result = 0.20;  
case DetailedTaskB  result = 0.30; 
case DetailedTaskC  result = 0.35; 
case PrecisionLightingIndustrial: result = 0.70;   
case PortableOfficeLighting: result = 0.20;  
case SpecializedTaskLighting result = 0.35; 
case TunableWhiteLuminaires: result = 0.10; 
case VideoConferencing: result = 1.00;
case LowVisionMainEntryTransition: result = 0.95; 
End Switch</xsd:documentation>
                                                                     </xsd:annotation>
                                                                  </xsd:element>
                                                                  <xsd:element name="Ju04_AdditionalAllowedPowerPerFoot" type="comp:AdditionalAllowedPowerPerFoot">
                                                                     <xsd:annotation>
                                                                        <xsd:documentation source="FieldText">Allowed Density (W/ft<d:sup>2</d:sup> or W/lf or W/unit)</xsd:documentation>
                                                                        <xsd:documentation source="CalculationsAndRules">Calculated field:  Assigned results are W/ft stored in AdditionalAllowedPowerPerFoot; Switch (Ju03_ AreaCategoryAdditionalAllowanceType) 
case WhiteChalkBoardLighting: result = 7;
End Switch</xsd:documentation>
                                                                     </xsd:annotation>
                                                                  </xsd:element>
                                                                  <xsd:element name="Ju04_AdditionalAllowedPowerPerUnit" type="comp:AdditionalAllowedPowerPerUnit">
                                                                     <xsd:annotation>
                                                                        <xsd:documentation source="FieldText">Allowed Density (W/ft<d:sup>2</d:sup> or W/lf or W/unit)</xsd:documentation>
                                                                        <xsd:documentation source="CalculationsAndRules">Calculated field:  Assigned results are W/unit stored in  AdditionalAllowedPowerPerUnit; Switch (Ju03_ AreaCategoryAdditionalAllowanceType) 
case MirrorLightingExternal: result = 40;  
case MirrorLightingInternal: result = 120;
End Switch</xsd:documentation>
                                                                     </xsd:annotation>
                                                                  </xsd:element>
                                                                  <xsd:element name="Ju04_NotApplicableMessage">
                                                                     <xsd:annotation>
                                                                        <xsd:documentation source="FieldText">Allowed Density (W/ft<d:sup>2</d:sup> or W/lf or W/unit)</xsd:documentation>
                                                                        <xsd:documentation source="CalculationsAndRules">Calculated field:  If Ju03_ AreaCategoryAdditionalAllowanceType == ParkingGarageATM 
result = BlankGrayedOut stored in NotApplicableMessage;
End If</xsd:documentation>
                                                                     </xsd:annotation>
                                                                     <xsd:simpleType>
                                                                        <xsd:restriction base="comp:NotApplicableMessage">
                                                                           <xsd:enumeration value="BlankGrayedOut"/>
                                                                        </xsd:restriction>
                                                                     </xsd:simpleType>
                                                                  </xsd:element>
                                                               </xsd:choice>
                                                               <xsd:choice maxOccurs="1">
                                                                  <xsd:element name="Ju05_SpaceFloorArea" type="com:SpaceFloorArea">
                                                                     <xsd:annotation>
                                                                        <xsd:documentation source="FieldText">Ltg Area, Length or ATMs  (ft<d:sup>2</d:sup>, lf or #)</xsd:documentation>
                                                                        <xsd:documentation source="CalculationsAndRules">If Ju04_AdditionalAllowedLPD NotNull User enters floor area</xsd:documentation>
                                                                     </xsd:annotation>
                                                                  </xsd:element>
                                                                  <xsd:element name="Ju05_BoardLightingLength" type="lit:BoardLightingLength">
                                                                     <xsd:annotation>
                                                                        <xsd:documentation source="FieldText">Ltg Area, Length or ATMs  (ft<d:sup>2</d:sup>, lf or #)</xsd:documentation>
                                                                        <xsd:documentation source="CalculationsAndRules">If Ju04_AdditionalAllowedPowerPerFoot NotNull User enters the length of the white/chalk board</xsd:documentation>
                                                                     </xsd:annotation>
                                                                  </xsd:element>
                                                                  <xsd:element name="Ju05_MirrorsCount" type="lit:MirrorsCount">
                                                                     <xsd:annotation>
                                                                        <xsd:documentation source="FieldText">Ltg Area, Length or ATM/Mirror (ft<d:sup>2</d:sup>, lf or #)</xsd:documentation>
                                                                        <xsd:documentation source="CalculationsAndRules">If Ju03_ AreaCategoryAdditionalAllowanceType == MirrorLightingExternal Or MirrorLightingInternal User enters number of mirrors</xsd:documentation>
                                                                     </xsd:annotation>
                                                                  </xsd:element>
                                                                  <xsd:element name="Ju05_BankingKioskCount" type="lit:BankingKioskCount">
                                                                     <xsd:annotation>
                                                                        <xsd:documentation source="FieldText">Ltg Area, Length or ATMs  (ft<d:sup>2</d:sup>, lf or #)</xsd:documentation>
                                                                        <xsd:documentation source="CalculationsAndRules">If Ju03_ AreaCategoryAdditionalAllowanceType == ParkingGarageATM:  User enters number of ATMs</xsd:documentation>
                                                                     </xsd:annotation>
                                                                  </xsd:element>
                                                               </xsd:choice>
                                                               <xsd:element name="Ju06_ExtraPowerCalculatedForArea" type="comp:ExtraPowerCalculatedForArea">
                                                                  <xsd:annotation>
                                                                     <xsd:documentation source="FieldText">Extra Allowance (Watts)</xsd:documentation>
                                                                     <xsd:documentation source="CalculationsAndRules">Calculated value:  If Ju04_AdditionalAllowedLPD NotNull  
result = Ju04_AdditionalAllowedLPD x Ju05_SpaceFloorArea; 
Else If Ju04_AdditionalAllowedPowerPerFoot NotNull 
result = Ju04_AdditionalAllowedPowerPerFoot x Ju05_BoardLightingLength; 
Else If  Ju03_ AreaCategoryAdditionalAllowanceType == MirrorLightingExternal Or   MirrorLightingInternal 
result = Ju04_AdditionalAllowedPowerPerUnit x Ju05_MirrorsCount; 
Else If Ju03_ AreaCategoryAdditionalAllowanceType == ParkingGarageATM: 
result = 100 + ( (Ju05_BankingKioskCount-1)  x 50);
End If</xsd:documentation>
                                                                  </xsd:annotation>
                                                               </xsd:element>
                                                               <xsd:element name="JuTableLuminaires">
                                                                  <xsd:annotation>
                                                                     <xsd:documentation source="FieldText"/>
                                                                  </xsd:annotation>
                                                                  <xsd:complexType>
                                                                     <xsd:sequence>
                                                                        <xsd:element name="Row" minOccurs="1" maxOccurs="unbounded">
                                                                           <xsd:complexType>
                                                                              <xsd:sequence>
                                                                                 <xsd:element name="Ju07_LuminaireName" type="lit:LuminaireName">
                                                                                    <xsd:annotation>
                                                                                       <xsd:documentation source="FieldText">Luminaire Name or Item Tag</xsd:documentation>
                                                                                       <xsd:documentation source="CalculationsAndRules">User input: Dropdown list auto-filled from values for Fu01_LuminaireName</xsd:documentation>
                                                                                    </xsd:annotation>
                                                                                 </xsd:element>
                                                                                 <xsd:element name="Ju08_LuminairePower" type="lit:LuminairePower">
                                                                                    <xsd:annotation>
                                                                                       <xsd:documentation source="FieldText">Watts  per Luminaire</xsd:documentation>
                                                                                       <xsd:documentation source="CalculationsAndRules">autofilled result = Fu05_LuminairePower for luminaire selected in Ju07__LuminaireName</xsd:documentation>
                                                                                    </xsd:annotation>
                                                                                 </xsd:element>
                                                                                 <xsd:element name="Ju09_LuminaireCountByType" type="lit:LuminaireCountByType">
                                                                                    <xsd:annotation>
                                                                                       <xsd:documentation source="FieldText">Number of Luminaires</xsd:documentation>
                                                                                       <xsd:documentation source="CalculationsAndRules">User input</xsd:documentation>
                                                                                    </xsd:annotation>
                                                                                 </xsd:element>
                                                                                 <xsd:element name="Ju10_LuminairePowerByType" type="lit:LuminairePowerByType">
                                                                                    <xsd:annotation>
                                                                                       <xsd:documentation source="FieldText">Total Design Watts</xsd:documentation>
                                                                                       <xsd:documentation source="CalculationsAndRules">Calculated value:   result = PRODUCT (Ju08_LuminairePower, Ju09_LuminaireCountByType)</xsd:documentation>
                                                                                    </xsd:annotation>
                                                                                 </xsd:element>
                                                                              </xsd:sequence>
                                                                           </xsd:complexType>
                                                                        </xsd:element>
                                                                     </xsd:sequence>
                                                                  </xsd:complexType>
                                                               </xsd:element>
                                                            </xsd:sequence>
                                                         </xsd:complexType>
                                                      </xsd:element>
                                                   </xsd:sequence>
                                                </xsd:complexType>
                                             </xsd:element>
                                             <xsd:element name="Ju10a_AllLuminairePowerInArea" type="lit:AllLuminairePowerInArea">
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">Total Design Watts</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">Calculated value: result = SUM All ( Ju10_LuminairePowerByType)</xsd:documentation>
                                                </xsd:annotation>
                                             </xsd:element>
                                             <xsd:element name="Ju10b_AllExtraPowerCalculatedForArea" type="comp:AllExtraPowerCalculatedForArea">
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">Calculated Allowance (Watts):</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">Calculated value: result = SUM All ( Ju06_ExtraPowerCalculatedForArea</xsd:documentation>
                                                </xsd:annotation>
                                             </xsd:element>
                                             <xsd:element name="Ju10c_AllExtraPowerAllowedForArea" type="comp:AllExtraPowerAllowedForArea">
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">Total Additional Allowance for this area:</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">Calculated value: result = Smallest (Ju10a_AllLuminairePowerInArea,  ju10b_AllExtraPowerCalculatedForArea)</xsd:documentation>
                                                </xsd:annotation>
                                             </xsd:element>
                                          </xsd:sequence>
                                       </xsd:complexType>
                                    </xsd:element>
                                 </xsd:sequence>
                              </xsd:complexType>
                           </xsd:element>
                           <xsd:element name="Ju11_AreaAdditionalLightingPowerAllowedUnconditioned" type="comp:AreaAdditionalLightingPowerAllowedUnconditioned">
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText">
                                    <d:b>Total Additional Allowance (Watts) UNCONDITIONED SPACES</d:b>
                                 </xsd:documentation>
                                 <xsd:documentation source="CalculationsAndRules">Calculated value: result = SUM All ( Ju10c_AllExtraPowerAllowedForArea)</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                           <xsd:element name="JuErrorMessage_SectionErrorMessage" type="comp:SectionErrorMessage">
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText"/>
                              </xsd:annotation>
                           </xsd:element>
                        </xsd:sequence>
                     </xsd:complexType>
                  </xsd:element>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
         <xsd:element name="Section_K" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation source="FieldText">
                  <d:b>K. TAILORED METHOD GENERAL LIGHTING POWER ALLOWANCE</d:b>
               </xsd:documentation>
               <xsd:documentation source="CalculationsAndRules">This section applies if in section B at least one value for Bnew02_LightingPrescriptiveMethodNR, Bnew04_LightingPrescriptiveMethodNR,   Baltered02_LightingPrescriptiveMethodNR Or Baltered04_LightingPrescriptiveMethodNR == Tailored Or AreaCategoryAndTailored</xsd:documentation>
               <xsd:documentation source="MinOccurs">If this section doesn't apply, do not display anything for this section</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="K_BeginNote1" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"/>
                        <xsd:documentation source="AdditionalRequirements">
                           <d:i>This table includes areas using the Tailored Method per 
                              <d:t24refnolink/>140.6(c)3 /
                              <d:t24refnolink/>170.2(e)4B.  If additional Tailored Method "use it or lose it" allowances are used it is also indicated.  Lighting that qualifies for one of the additional allowances shall not qualify for another additional allowance. </d:i>
                        </xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="K_ColumnHeading00cTo00f" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"/>
                        <xsd:documentation source="AdditionalRequirements">Additional "Use it or lose it" Allowance (Watts)</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="K_ColumnHeading02To09" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"/>
                        <xsd:documentation source="AdditionalRequirements">Calculated General Lighting Power Allowance</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="Kc_Conditioned" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Conditioned Spaces</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">This subsection is required if in section B at least one value for Bnew02_LightingPrescriptiveMethodNR Or Baltered02_LightingPrescriptiveMethodNR == Tailored Or AreaCategoryAndTailored And at least one value for Bnew03_NewLightingConditionedFloorArea  Or Baltered03_AlteredLightingConditionedFloorArea GT 0</xsd:documentation>
                     </xsd:annotation>
                     <xsd:complexType>
                        <xsd:sequence>
                           <xsd:element name="Kc_Label" minOccurs="0">
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText">
                                    <d:b>Conditioned Spaces</d:b>
                                 </xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                           <xsd:element name="Kc00a_TailoredPowerAllowedGeneral" type="comp:TailoredPowerAllowedGeneral">
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText">General Lighting Power Allowance   Table K(below)</xsd:documentation>
                                 <xsd:documentation source="CalculationsAndRules">Auto-filled result =  Kc10_TailoredPowerAllowedGeneral</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                           <xsd:element name="Kc00c1_TailoredWallDisplay" type="comp:TailoredWallDisplay">
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText">Wall Display Lighting</xsd:documentation>
                                 <xsd:documentation source="CalculationsAndRules">User input; If user checks box, Section L is required.</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                           <xsd:element name="Kc00c2_PowerAllowedWallDisplay" type="comp:PowerAllowedWallDisplay">
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText">Table L</xsd:documentation>
                                 <xsd:documentation source="CalculationsAndRules">Calculated value:  If Kc00c1_TailoredWallDisplay == true then result =  Lc12_PowerAllowedWallDisplay; Else result = 0</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                           <xsd:element name="Kc00d1_TailoredFloorDisplayOrTask" type="comp:TailoredFloorDisplayOrTask">
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText">Floor Display &amp;amp; Task Lighting</xsd:documentation>
                                 <xsd:documentation source="CalculationsAndRules">User input; If user checks box, Section M is required.</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                           <xsd:element name="Kc00d2_PowerAllowedFloorDisplayOrTask" type="comp:PowerAllowedFloorDisplayOrTask">
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText">Table M</xsd:documentation>
                                 <xsd:documentation source="CalculationsAndRules">Calculated value:  If Kc00d1_TailoredFloorDisplayOrTask == true then result = Mc12_PowerAllowedFloorDisplayOrTask; Else result = 0</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                           <xsd:element name="Kc00e1_TailoredOrnamentalSpecialEffects" type="comp:TailoredOrnamentalSpecialEffects">
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText">Decorative &amp;amp; Special Effects</xsd:documentation>
                                 <xsd:documentation source="CalculationsAndRules">User input; If user checks box, Section N is required.</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                           <xsd:element name="Kc00e2_PowerAllowedOrnamentalSpecialEffects" type="comp:PowerAllowedOrnamentalSpecialEffects">
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText">Table N</xsd:documentation>
                                 <xsd:documentation source="CalculationsAndRules">Calculated value:  If Kc00e1_TailoredOrnamentalSpecialEffects == true then result = Nc10_PowerAllowedOrnamentalSpecialEffects; Else result = 0</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                           <xsd:element name="Kc00f1_TailoredVeryValuableDisplay" type="comp:TailoredVeryValuableDisplay">
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText">Very Valuable Merchandise</xsd:documentation>
                                 <xsd:documentation source="CalculationsAndRules">User input; If user checks box, Section O is required.</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                           <xsd:element name="Kc00f2_PowerAllowedVeryValuableDisplay" type="comp:PowerAllowedVeryValuableDisplay">
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText">Table O</xsd:documentation>
                                 <xsd:documentation source="CalculationsAndRules">Calculated value:  If Kc00f1_TailoredVeryValuableDisplay == true then result = Oc14_PowerAllowedVeryValuableDisplay; Else result = 0</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                           <xsd:element name="Kc01_TailoredTotalLightingPowerAllowedConditioned" type="comp:TailoredTotalLightingPowerAllowedConditioned">
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText">Total Allowed Watts using Tailored Method</xsd:documentation>
                                 <xsd:documentation source="CalculationsAndRules">Calculated value:  result = SUM ( Kc00a_TailoredPowerAllowedGeneral, Kc00c2_PowerAllowedWallDisplay,  Kc00d2_PowerAllowedFloorDisplayOrTask, Kc00e2_PowerAllowedOrnamentalSpecialEffects, Kc00f2_PowerAllowedVeryValuableDisplay)</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                           <xsd:element name="KcTableTailoredGeneral">
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText"/>
                              </xsd:annotation>
                              <xsd:complexType>
                                 <xsd:sequence>
                                    <xsd:element name="Row" minOccurs="1" maxOccurs="unbounded">
                                       <xsd:complexType>
                                          <xsd:sequence>
                                             <xsd:element name="Kc02_SpaceDescription" type="com:SpaceDescription">
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">Area Description (per room)</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">User input.</xsd:documentation>
                                                </xsd:annotation>
                                             </xsd:element>
                                             <xsd:element name="Kc03_SpaceFunctionTailored" type="comp:SpaceFunctionTailored">
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">Primary Function Area per Table 140.6-D/ 170.2-N</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">User selects from dropdown list loaded with space functions allowed in tailored method: AuditoriumArea, ConferenceArea, ConferenceAreaMF, ConventionArea, MeetingCenterArea, MeetingCenterAreaMF, MultipurposeArea, MultipurposeAreaMF, DiningArea, DiningAreaMF, ExhibitMuseumArea, HotelBallroomEventsArea, HotelLobby, MainEntryLobby, MainEntryLobbyMF, MotionPictureTheater, PerformanceTheater, ReligiousWorshipArea, GroceryStoreArea, RetailMerchandiseSales, ShowroomArea.</xsd:documentation>
                                                </xsd:annotation>
                                             </xsd:element>
                                             <xsd:element name="Kc04_SpaceIlluminationLevel" type="lit:SpaceIlluminationLevel">
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">Illuminance Value (LUX)</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">Calculated value: 
If Kc03_SpaceFunctionTailored == AuditoriumArea, ConferenceArea, ConferenceAreaMF, ConventionArea, MeetingCenterArea, MeetingCenterAreaMF, MultipurposeArea, MultipurposeAreaMF Or ReligiousWorshipArea  
result = 300; 
Else if Kc03_SpaceFunctionTailored == DiningArea, DiningAreaMF, HotelLobby, MainEntryLobby, MainEntryLobbyMF, MotionPictureTheater Or PerformanceTheater 
result = 200;  
Else if  Kc03_SpaceFunctionTailored == ExhibitMuseumArea  then 
result = 150; 
Else If Kc03_SpaceFunctionTailored == GroceryStoreArea  then 
result = 600; 
Else If Kc03_SpaceFunctionTailored == HotelBallroomEventsArea  
result = 400; 
Else If Kc03_SpaceFunctionTailored == RetailMerchandiseSales Or ShowroomArea 
result = 500;
End If</xsd:documentation>
                                                </xsd:annotation>
                                             </xsd:element>
                                             <xsd:element name="Kc05_IsSpaceRectangular" type="com:IsSpaceRectangular">
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">Room Configuration</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">User input.  Dropdown list displays Rectangular for true value and Non-Rectangular for false value.</xsd:documentation>
                                                </xsd:annotation>
                                             </xsd:element>
                                             <xsd:choice maxOccurs="1">
                                                <xsd:element name="Kc05a_SpaceLength" type="com:SpaceLength">
                                                   <xsd:annotation>
                                                      <xsd:documentation source="FieldText">Room Length (ft)</xsd:documentation>
                                                      <xsd:documentation source="CalculationsAndRules">If IsSpaceRectangular == true user input value stored in Kc05a_SpaceLength; Else result = NotApplicable stored in the NotApplicableMessage.</xsd:documentation>
                                                   </xsd:annotation>
                                                </xsd:element>
                                                <xsd:element name="Kc05a_NotApplicableMessage" type="comp:NotApplicableMessage">
                                                   <xsd:annotation>
                                                      <xsd:documentation source="FieldText">Not Applicable</xsd:documentation>
                                                      <xsd:documentation source="CalculationsAndRules">If IsSpaceRectangular = true user input value stored in Kc05a_SpaceLength; Else result =  NotApplicable stored in the NotApplicableMessage.</xsd:documentation>
                                                   </xsd:annotation>
                                                </xsd:element>
                                             </xsd:choice>
                                             <xsd:choice maxOccurs="1">
                                                <xsd:element name="Kc05b_SpaceWidth" type="com:SpaceWidth">
                                                   <xsd:annotation>
                                                      <xsd:documentation source="FieldText">Room Width (ft)</xsd:documentation>
                                                      <xsd:documentation source="CalculationsAndRules">If IsSpaceRectangular == true, user enters the width of the space stored in Kc05b_SpaceWidth; Else user enters the perimeter of the space stored in Kc05b_SpacePerimeter.</xsd:documentation>
                                                   </xsd:annotation>
                                                </xsd:element>
                                                <xsd:element name="Kc05b_SpacePerimeter" type="com:SpacePerimeter">
                                                   <xsd:annotation>
                                                      <xsd:documentation source="FieldText">Perimeter (ft)</xsd:documentation>
                                                      <xsd:documentation source="CalculationsAndRules">If IsSpaceRectangular == true, user enters the width of the space stored in Kc05b_SpaceWidth; Else user enters the perimeter of the space stored in Kc05b_SpacePerimeter.</xsd:documentation>
                                                   </xsd:annotation>
                                                </xsd:element>
                                             </xsd:choice>
                                             <xsd:element name="Kc05c_Height" type="dtyp:Height">
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">Height (ft)<d:sup>1</d:sup>
                                                   </xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">User input</xsd:documentation>
                                                </xsd:annotation>
                                             </xsd:element>
                                             <xsd:element name="Kc05d_RoomCavityRatio" type="com:RoomCavityRatio">
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">Room Cavity Ratio (RCR)</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">Calculated value using equations in TABLE 140.6-F:  If kc05_IsSpaceRectanfular == true then result = (5 x Kc05c_Height x (Kc05a_SpaceLength + Kc05b_SpaceWidth)) / (Kc05a_SpaceLength x Kc05b_SpaceWidth); Else result = 2.5 x Kc05c_Height x Kc05b_SpacePerimeter / Kc07_SpaceFloorArea.</xsd:documentation>
                                                </xsd:annotation>
                                             </xsd:element>
                                             <xsd:element name="Kc06_TailoredGeneralLPD" type="comp:TailoredGeneralLPD">
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">Allowed Density (W/ft<d:sup>2</d:sup>)</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">Calculated value based on values for Kc04_SpaceIlluminationLevel, referred to as lux in formula and Kc05d_RoomCavityRatio, referred to as rcr in formula. In formula LTE stands for less than or equal to and GT stands for greater than.   
Formula:   Switch (Kc04_SpaceIlluminationLevel) 
case 150: 
If rcr LTE 2.0  result = 0.35; Else If rcr LTE 3.5 result = 0.40; Else If  rcr LTE 7.0  result = 0.50; Else If  rcr GT 7.0 result = 0.65;  
End If
case 200: 
If rcr LTE 2.0 result = 0.40; Else If  rcr LTE 3.5 result = 0.50; Else If  rcr LTE 7.0 result = 0.65;  Else If rcr GT 7.0 result = 0.85;  
End If
case 300: 
If rcr LTE 2.0 result = 0.55; Else If  rcr LTE 3.5 result = 0.70; Else If  rcr LTE 7.0 result = 0.85; Else If  rcr GT 7.0 result = 1.20;  
End If
case 400: 
If rcr LTE 2.0 result = 0.65; Else If  rcr LTE 3.5 result = 0.80; Else If  rcr LTE 7.0 result = 1.05; Else If  rcr GT 7.0 result = 1.25;  
End If
case 500: 
If rcr LTE 2.0 result = 0.80; Else If  rcr LTE 3.5 result = 0.90; Else If  rcr LTE 7.0 result = 1.25; Else If  rcr GT 7.0 result = 1.55; 
End If
case 600: 
If rcr LTE 2.0 result = 0.90; Else If  rcr LTE 3.5 result = 1.05; Else If  rcr LTE 7.0 result = 1.40; Else If  rcr GT 7.0 result = 2.00;
End If
End Switch</xsd:documentation>
                                                </xsd:annotation>
                                             </xsd:element>
                                             <xsd:element name="Kc07_SpaceFloorArea" type="com:SpaceFloorArea">
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">Area (ft<d:sup>2</d:sup>)</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">User input</xsd:documentation>
                                                </xsd:annotation>
                                             </xsd:element>
                                             <xsd:element name="Kc08_TailoredPowerAllowedForArea" type="comp:TailoredPowerAllowedForArea">
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">Extra Allowance (Watts)</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">Calculated value:  result = PRODUCT (Kc06_TailoredGeneralLPD, Kc07_SpaceFloorArea)</xsd:documentation>
                                                </xsd:annotation>
                                             </xsd:element>
                                             <xsd:element name="Kc09_AdditionalAllowanceForPAF" type="comp:AdditionalAllowanceForPAF">
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">Power Adjustment   (See Table P)   
                                                      <d:line1/>  PAF</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">User input. Checked result = true and unchecked result = false.  If result == true, section P is required.</xsd:documentation>
                                                </xsd:annotation>
                                             </xsd:element>
                                          </xsd:sequence>
                                       </xsd:complexType>
                                    </xsd:element>
                                 </xsd:sequence>
                              </xsd:complexType>
                           </xsd:element>
                           <xsd:element name="Kc10_TailoredPowerAllowedGeneral" type="comp:TailoredPowerAllowedGeneral">
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText">
                                    <d:b>Total General Power Allowance CONDITIONED SPACES:</d:b>
                                 </xsd:documentation>
                                 <xsd:documentation source="CalculationsAndRules">Calculated value:  result = SUM All (Kc08_TailoredPowerAllowedForArea)</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                           <xsd:element name="KcErrorMessage_SectionErrorMessage" type="comp:SectionErrorMessage" minOccurs="0">
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText"/>
                              </xsd:annotation>
                           </xsd:element>
                        </xsd:sequence>
                     </xsd:complexType>
                  </xsd:element>
                  <xsd:element name="Ku_Unconditioned" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Unconditioned Spaces</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">This subsection is required if in section B at least one value for Bnew04_LightingPrescriptiveMethodNR Or Baltered04_LightingPrescriptiveMethodNR == Tailored Or AreaCategoryAndTailored And at least one value for Bnew05_NewLightingUnconditionedFloorArea Or Baltered05_AlteredLightingUnconditionedFloorArea GT 0</xsd:documentation>
                     </xsd:annotation>
                     <xsd:complexType>
                        <xsd:sequence>
                           <xsd:element name="Ku_UnconditionedLabel" minOccurs="0">
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText">
                                    <d:b>Unconditioned Spaces</d:b>
                                 </xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                           <xsd:element name="Ku00a_TailoredPowerAllowedGeneral" type="comp:TailoredPowerAllowedGeneral">
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText">General Lighting Power Allowance   Table K(below)</xsd:documentation>
                                 <xsd:documentation source="CalculationsAndRules">Auto-filled result =  Ku10_TailoredPowerAllowedGeneral</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                           <xsd:element name="Ku00c1_TailoredWallDisplay" type="comp:TailoredWallDisplay">
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText">Wall Display Lighting</xsd:documentation>
                                 <xsd:documentation source="CalculationsAndRules">User input; If user checks box, Section L is required.</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                           <xsd:element name="Ku00c2_PowerAllowedWallDisplay" type="comp:PowerAllowedWallDisplay">
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText">Table L</xsd:documentation>
                                 <xsd:documentation source="CalculationsAndRules">Calculated value:  If Ku00c1_TailoredWallDisplay == true then result =  Lu12_PowerAllowedWallDisplay; Else result = 0</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                           <xsd:element name="Ku00d1_TailoredFloorDisplayOrTask" type="comp:TailoredFloorDisplayOrTask">
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText">Floor Display &amp;amp; Task Lighting</xsd:documentation>
                                 <xsd:documentation source="CalculationsAndRules">User input; If user checks box, Section M is required.</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                           <xsd:element name="Ku00d2_PowerAllowedFloorDisplayOrTask" type="comp:PowerAllowedFloorDisplayOrTask">
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText">Table M</xsd:documentation>
                                 <xsd:documentation source="CalculationsAndRules">Calculated value:  If Ku00d1_TailoredFloorDisplayOrTask == true then result = Mu12_PowerAllowedFloorDisplayOrTask; Else result = 0</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                           <xsd:element name="Ku00e1_TailoredOrnamentalSpecialEffects" type="comp:TailoredOrnamentalSpecialEffects">
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText">Decorative &amp;amp; Special Effects</xsd:documentation>
                                 <xsd:documentation source="CalculationsAndRules">User input; If user checks box, Section N is required.</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                           <xsd:element name="Ku00e2_PowerAllowedOrnamentalSpecialEffects" type="comp:PowerAllowedOrnamentalSpecialEffects">
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText">Table N</xsd:documentation>
                                 <xsd:documentation source="CalculationsAndRules">Calculated value:  If Ku00e1_TailoredOrnamentalSpecialEffects == true then result = Nu10_PowerAllowedOrnamentalSpecialEffects; Else result = 0</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                           <xsd:element name="Ku00f1_TailoredVeryValuableDisplay" type="comp:TailoredVeryValuableDisplay">
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText">Very Valuable Merchandise</xsd:documentation>
                                 <xsd:documentation source="CalculationsAndRules">User input; If user checks box, Section O is required.</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                           <xsd:element name="Ku00f2_PowerAllowedVeryValuableDisplay" type="comp:PowerAllowedVeryValuableDisplay">
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText">Table O</xsd:documentation>
                                 <xsd:documentation source="CalculationsAndRules">Calculated value:  If Ku00f1_TailoredVeryValuableDisplay == true then result = Ou14_PowerAllowedVeryValuableDisplay; Else result = 0</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                           <xsd:element name="Ku01_TailoredTotalLightingPowerAllowedConditioned" type="comp:TailoredTotalLightingPowerAllowedConditioned">
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText">Total Allowed Watts using Tailored Method</xsd:documentation>
                                 <xsd:documentation source="CalculationsAndRules">Calculated value:  result = SUM ( Ku00a_TailoredPowerAllowedGeneral, Ku00c2_PowerAllowedWallDisplay,  Ku00d2_PowerAllowedFloorDisplayOrTask, Ku00e2_PowerAllowedOrnamentalSpecialEffects, Ku00f2_PowerAllowedVeryValuableDisplay);</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                           <xsd:element name="KuTableTailoredGeneral">
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText"/>
                              </xsd:annotation>
                              <xsd:complexType>
                                 <xsd:sequence>
                                    <xsd:element name="Row" minOccurs="1" maxOccurs="unbounded">
                                       <xsd:complexType>
                                          <xsd:sequence>
                                             <xsd:element name="Ku02_SpaceDescription" type="com:SpaceDescription">
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">Area Description (per room)</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">User input.</xsd:documentation>
                                                </xsd:annotation>
                                             </xsd:element>
                                             <xsd:element name="Ku03_SpaceFunctionTailored" type="comp:SpaceFunctionTailored">
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">Primary Function Area per Table 140.6-D/ 170.2-N</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">User selects from dropdown list loaded with space functions allowed in tailored method: AuditoriumArea, ConferenceArea, ConferenceAreaMF, ConventionArea, MeetingCenterArea, MeetingCenterAreaMF, MultipurposeArea, MultipurposeAreaMF, DiningArea, DiningAreaMF, ExhibitMuseumArea, HotelBallroomEventsArea, HotelLobby, MainEntryLobby, MainEntryLobbyMF, MotionPictureTheater, PerformanceTheater, ReligiousWorshipArea, GroceryStoreArea, RetailMerchandiseSales, ShowroomArea.</xsd:documentation>
                                                </xsd:annotation>
                                             </xsd:element>
                                             <xsd:element name="Ku04_SpaceIlluminationLevel" type="lit:SpaceIlluminationLevel">
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">Illuminance Value (LUX)</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">Calculated value: 
If Ku03_SpaceFunctionTailored == AuditoriumArea, ConferenceArea, ConferenceAreaMF, ConventionArea, MeetingCenterArea, MeetingCenterAreaMF, MultipurposeArea, MultipurposeAreaMF Or ReligiousWorshipArea  
result = 300; 
Else if Ku03_SpaceFunctionTailored == DiningArea, DiningAreaMF, HotelLobby, MainEntryLobby, MainEntryLobbyMF, MotionPictureTheater Or PerformanceTheater 
result = 200;  
Else if  Ku03_SpaceFunctionTailored == ExhibitMuseumArea  
result = 150; 
Else If Ku03_SpaceFunctionTailored == GroceryStoreArea  
result = 600; 
Else If Ku03_SpaceFunctionTailored == HotelBallroomEventsArea  
result = 400; 
Else If Ku03_SpaceFunctionTailored == RetailMerchandiseSales Or ShowroomArea 
result = 500;
End If</xsd:documentation>
                                                </xsd:annotation>
                                             </xsd:element>
                                             <xsd:element name="Ku05_IsSpaceRectangular" type="com:IsSpaceRectangular">
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">Room Configuration</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">User input.  Dropdown list displays Rectangular for true value and Non-Rectangular for false value.</xsd:documentation>
                                                </xsd:annotation>
                                             </xsd:element>
                                             <xsd:choice maxOccurs="1">
                                                <xsd:element name="Ku05a_SpaceLength" type="com:SpaceLength">
                                                   <xsd:annotation>
                                                      <xsd:documentation source="FieldText">Length of Room (ft)</xsd:documentation>
                                                      <xsd:documentation source="CalculationsAndRules">If IsSpaceRectangular == true user input value stored in Ku05a_SpaceLength; Else result =  NotApplicable stored in the NotApplicableMessage.</xsd:documentation>
                                                   </xsd:annotation>
                                                </xsd:element>
                                                <xsd:element name="Ku05a_NotApplicableMessage" type="comp:NotApplicableMessage">
                                                   <xsd:annotation>
                                                      <xsd:documentation source="FieldText">Not Applicable</xsd:documentation>
                                                      <xsd:documentation source="CalculationsAndRules">If IsSpaceRectangular == true user input value stored in Ku05a_SpaceLength; Else result =  NotApplicable stored in the NotApplicableMessage.</xsd:documentation>
                                                   </xsd:annotation>
                                                </xsd:element>
                                             </xsd:choice>
                                             <xsd:choice maxOccurs="1">
                                                <xsd:element name="Ku05b_SpaceWidth" type="com:SpaceWidth">
                                                   <xsd:annotation>
                                                      <xsd:documentation source="FieldText">Room Width (ft)</xsd:documentation>
                                                      <xsd:documentation source="CalculationsAndRules">If IsSpaceRectangular == true, user enters the width of the space stored in Ku05b_SpaceWidth; Else user enters the perimeter of the space stored in Ku05b_SpacePerimeter.</xsd:documentation>
                                                   </xsd:annotation>
                                                </xsd:element>
                                                <xsd:element name="Ku05b_SpacePerimeter" type="com:SpacePerimeter">
                                                   <xsd:annotation>
                                                      <xsd:documentation source="FieldText">Perimeter (ft)</xsd:documentation>
                                                      <xsd:documentation source="CalculationsAndRules">If IsSpaceRectangular == true, user enters the width of the space stored in Ku05b_SpaceWidth; Else user enters the perimeter of the space stored in Ku05b_SpacePerimeter.</xsd:documentation>
                                                   </xsd:annotation>
                                                </xsd:element>
                                             </xsd:choice>
                                             <xsd:element name="Ku05c_Height" type="dtyp:Height">
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">Height (ft)<d:sup>1</d:sup>
                                                   </xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">User input</xsd:documentation>
                                                </xsd:annotation>
                                             </xsd:element>
                                             <xsd:element name="Ku05d_RoomCavityRatio" type="com:RoomCavityRatio">
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">Room Cavity Ratio (RCR)</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">Calculated value using equations in TABLE 140.6-F:  If Ku05_IsSpaceRectanfular == true then result = (5 x Ku05c_Height x (Ku05a_SpaceLength + Ku05b_SpaceWidth)) / (Ku05a_SpaceLength x Ku05b_SpaceWidth); Else result = 2.5 x Ku05c_Height x Ku05b_SpacePerimeter / Ku07_SpaceFloorArea.</xsd:documentation>
                                                </xsd:annotation>
                                             </xsd:element>
                                             <xsd:element name="Ku06_TailoredGeneralLPD" type="comp:TailoredGeneralLPD">
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">Allowed Density (W/ft<d:sup>2</d:sup>)</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">Calculated value based on values for Ku04_SpaceIlluminationLevel, referred to as lux in formula and Ku05d_RoomCavityRatio, referred to as rcr in formula. In formula LTE stands for less than or equal to and GT stands for greater than.   Formula:   Switch (Ku04_SpaceIlluminationLevel)
case 150: 
If rcr LTE 2.0  result = 0.35; Else If rcr LTE 3.5 result = 0.40; Else If  rcr LTE 7.0  result = 0.50; Else If  rcr GT 7.0 result = 0.65;  
End If
case 200: 
If rcr LTE 2.0 result = 0.40; Else If  rcr LTE 3.5 result = 0.50; Else If  rcr LTE 7.0 result = 0.65;  Else If rcr GT 7.0 result = 0.85;  
End If
case 300: 
If rcr LTE 2.0 result = 0.55; Else If  rcr LTE 3.5 result = 0.70; Else If  rcr LTE 7.0 result = 0.85; Else If  rcr GT 7.0 result = 1.20;  
End If
case 400: 
If rcr LTE 2.0 result = 0.65; Else If  rcr LTE 3.5 result = 0.80; Else If  rcr LTE 7.0 result = 1.05; Else If  rcr GT 7.0 result = 1.25;  
End If
case 500: 
If rcr LTE 2.0 result = 0.80; Else If  rcr LTE 3.5 result = 0.90; Else If  rcr LTE 7.0 result = 1.25; Else If  rcr GT 7.0 result = 1.55; 
End If
case 600: 
If rcr LTE 2.0 result = 0.90; Else If  rcr LTE 3.5 result = 1.05; Else If  rcr LTE 7.0 result = 1.40; Else If  rcr GT 7.0 result = 2.00;
End If
End Switch</xsd:documentation>
                                                </xsd:annotation>
                                             </xsd:element>
                                             <xsd:element name="Ku07_SpaceFloorArea" type="com:SpaceFloorArea">
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">Area (ft<d:sup>2</d:sup>)</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">User input</xsd:documentation>
                                                </xsd:annotation>
                                             </xsd:element>
                                             <xsd:element name="Ku08_TailoredPowerAllowedForArea" type="comp:TailoredPowerAllowedForArea">
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">Extra Allowance (Watts)</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">Calculated value:  result = PRODUCT (Ku06_TailoredGeneralLPD, Ku07_SpaceFloorArea)</xsd:documentation>
                                                </xsd:annotation>
                                             </xsd:element>
                                             <xsd:element name="Ku09_AdditionalAllowanceForPAF" type="comp:AdditionalAllowanceForPAF">
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">Power Adjustment   (See Table P)  
                                                      <d:line1/>  PAF</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">User input. Checked result = true and unchecked result = false.  If result == true, section P is required.</xsd:documentation>
                                                </xsd:annotation>
                                             </xsd:element>
                                          </xsd:sequence>
                                       </xsd:complexType>
                                    </xsd:element>
                                 </xsd:sequence>
                              </xsd:complexType>
                           </xsd:element>
                           <xsd:element name="Ku10_TailoredPowerAllowedGeneral" type="comp:TailoredPowerAllowedGeneral">
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText">
                                    <d:b>Total General Power Allowance UNCONDITIONED SPACES:</d:b>
                                 </xsd:documentation>
                                 <xsd:documentation source="CalculationsAndRules">Calculated value:  result = SUM All (Ku08_TailoredPowerAllowedForArea)</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                           <xsd:element name="KuErrorMessage_SectionErrorMessage" type="comp:SectionErrorMessage" minOccurs="0">
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText"/>
                              </xsd:annotation>
                           </xsd:element>
                        </xsd:sequence>
                     </xsd:complexType>
                  </xsd:element>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
         <xsd:element name="Section_L" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation source="FieldText">
                  <d:b>L. ADDITIONAL LIGHTING ALLOWANCE: TAILORED WALL DISPLAY</d:b>
               </xsd:documentation>
               <xsd:documentation source="CalculationsAndRules">This section applies if in section K Kc00c1_TailoredWallDisplay or Ku00c1_TailoredWallDisplay == true</xsd:documentation>
               <xsd:documentation source="MinOccurs">If this section doesn't apply, do not display anything for this section</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="L_ColumnHeading02To04" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">
                           <d:b>CALCULATED ALLOWANCE(Watts)</d:b>
                        </xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="L_ColumnHeading05To10" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">
                           <d:b>DESIGN WATTS</d:b>
                        </xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="Lc_Conditioned" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Conditioned Spaces</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">This subsection is required if in section K Kc00c1_TailoredWallDisplay == true And If at least one values for Bnew03_NewLightingConditionedFloorArea Or Baltered03_AlteredLightingConditionedFloorArea GT 0</xsd:documentation>
                     </xsd:annotation>
                     <xsd:complexType>
                        <xsd:sequence>
                           <xsd:element name="Lc_ConditionedLabel" minOccurs="0">
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText">
                                    <d:b>Conditioned Spaces</d:b>
                                 </xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                           <xsd:element name="LcTableTailoredWall">
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText"/>
                              </xsd:annotation>
                              <xsd:complexType>
                                 <xsd:sequence>
                                    <xsd:element name="Row" minOccurs="1" maxOccurs="unbounded">
                                       <xsd:complexType>
                                          <xsd:sequence>
                                             <xsd:element name="Lc01_SpaceDescription" type="com:SpaceDescription">
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">Area Description</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">User selects from dropdown list filled with values for Kc02_SpaceDescription from section K where Kc00c1_TailoredWallDisplay == true</xsd:documentation>
                                                </xsd:annotation>
                                             </xsd:element>
                                             <xsd:element name="Lc02_WallDisplayLength" type="lit:WallDisplayLength">
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">Wall Display Length (lf)</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">User input</xsd:documentation>
                                                </xsd:annotation>
                                             </xsd:element>
                                             <xsd:element name="Lc03_TailoredWallLightingPowerPerFoot" type="comp:TailoredWallLightingPowerPerFoot">
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">Wall Display Density (W/lf)</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">Calculated value: wall display lighting density rules for the space function type  in Kc03 for the area selected in Lc01 as follows: 
If Kc03_SpaceFunctionTailored == AuditoriumArea result = 3.00; 
Else If Kc03_SpaceFunctionTailored == ConventionArea, ConferenceArea, ConferenceAreaMF, MultipurposeArea, MultipurposeAreaMF, MeetingCenterArea, MeetingCenterAreaMF Or MotionPictureTheater  result = 2.00; 
 Else If Kc03_SpaceFunctionTailored == DiningArea or DiningAreaMF result = 1.25; 
Else If Kc03_SpaceFunctionTailored == HotelLobby, MainEntryLobby  result = 3.40;  
Else If Kc03_SpaceFunctionTailored == MainEntryLobbyMF result = 3.50; 
Else If Kc03_SpaceFunctionTailored == ReligiousWorshipArea result = 1.30; 
Else If Kc03_SpaceFunctionTailored == RetailMerchandiseSales Or ShowroomArea  result = 11.50;  
Else If Kc03_SpaceFunctionTailored == ExhibitMuseumArea  result = 11.20;  
Else If Kc03_SpaceFunctionTailored == GroceryStoreArea result = 6.60;  
Else If Kc03_SpaceFunctionTailored == HotelBallroomEventsArea result = 1.80;  
Else If Kc03_SpaceFunctionTailored == PerformanceTheater result = 7.30.
End If</xsd:documentation>
                                                </xsd:annotation>
                                             </xsd:element>
                                             <xsd:element name="Lc04_TailoredPowerCalculatedForArea" type="comp:TailoredPowerCalculatedForArea">
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">Extra Allowance (Watts)</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">Calculated value:  result = Lc02_WallDisplayLength x Lc03_TailoredWallLightingPowerPerFoot</xsd:documentation>
                                                </xsd:annotation>
                                             </xsd:element>
                                             <xsd:element name="LcTableLuminaires">
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText"/>
                                                </xsd:annotation>
                                                <xsd:complexType>
                                                   <xsd:sequence>
                                                      <xsd:element name="Row" minOccurs="1" maxOccurs="unbounded">
                                                         <xsd:complexType>
                                                            <xsd:sequence>
                                                               <xsd:element name="Lc05_LuminaireName" type="lit:LuminaireName">
                                                                  <xsd:annotation>
                                                                     <xsd:documentation source="FieldText">Luminaire Name or Item Tag</xsd:documentation>
                                                                     <xsd:documentation source="CalculationsAndRules">User input: selects from dropdown list filled with values from Fc02_LuminaireName field F02</xsd:documentation>
                                                                  </xsd:annotation>
                                                               </xsd:element>
                                                               <xsd:element name="Lc06_TailoredMountingHeight" type="comp:TailoredMountingHeight">
                                                                  <xsd:annotation>
                                                                     <xsd:documentation source="FieldText">Mounting Height (ft)</xsd:documentation>
                                                                     <xsd:documentation source="CalculationsAndRules">User input: selects from dropdown list:</xsd:documentation>
                                                                     <xsd:appinfo source="displayterm">
                                                                        <dtyp:displayterm value="LT10Ft7In">
                                                                           <d:lte/> 10'-6"</dtyp:displayterm>
                                                                        <dtyp:displayterm value="10Ft7InTo14Ft">
                                                                           <d:gt/> 10'6" to 14'</dtyp:displayterm>
                                                                     </xsd:appinfo>
                                                                  </xsd:annotation>
                                                               </xsd:element>
                                                               <xsd:element name="Lc07_MountingHeightFactor" type="lit:MountingHeightFactor">
                                                                  <xsd:annotation>
                                                                     <xsd:documentation source="FieldText">Mounting Height Factor</xsd:documentation>
                                                                     <xsd:documentation source="CalculationsAndRules">Calculated value: 
If Lc06_TailoredMountingHeight == LT10Ft7In result = 1.00; 
Else If Lc06_TailoredMountingHeight == 10Ft7InTo14Ft  result = 0.85; 
Else If Lc06_TailoredMountingHeight == GT14FtTo18Ft  result = 0.75;  
Else  If Lc06_TailoredMountingHeight == GTE18Ft result = 0.70.
End If</xsd:documentation>
                                                                  </xsd:annotation>
                                                               </xsd:element>
                                                               <xsd:element name="Lc08_LuminairePower" type="lit:LuminairePower">
                                                                  <xsd:annotation>
                                                                     <xsd:documentation source="FieldText">Watts per Luminaire</xsd:documentation>
                                                                     <xsd:documentation source="CalculationsAndRules">Auto-filled with Fc05_LuminairePower for luminaire selected  in Lc05_LuminaireName</xsd:documentation>
                                                                  </xsd:annotation>
                                                               </xsd:element>
                                                               <xsd:element name="Lc09_LuminaireCountByType" type="lit:LuminaireCountByType">
                                                                  <xsd:annotation>
                                                                     <xsd:documentation source="FieldText"># of Luminaires</xsd:documentation>
                                                                     <xsd:documentation source="CalculationsAndRules">User input</xsd:documentation>
                                                                  </xsd:annotation>
                                                               </xsd:element>
                                                               <xsd:element name="Lc10a_LuminairePowerByType" type="lit:LuminairePowerByType">
                                                                  <xsd:annotation>
                                                                     <xsd:documentation source="FieldText">Total Design Watts</xsd:documentation>
                                                                     <xsd:documentation source="CalculationsAndRules">Calculated value:  result = Lc08_LuminairePower x Lc09_LuminaireCountByType</xsd:documentation>
                                                                  </xsd:annotation>
                                                               </xsd:element>
                                                            </xsd:sequence>
                                                         </xsd:complexType>
                                                      </xsd:element>
                                                   </xsd:sequence>
                                                </xsd:complexType>
                                             </xsd:element>
                                             <xsd:element name="Lc10b_TailoredPowerInArea" type="comp:TailoredPowerInArea">
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">Total Design Watts for this Area:</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">Calculated value:  result = SUM All (Lc10a_LuminairePowerByType)</xsd:documentation>
                                                </xsd:annotation>
                                             </xsd:element>
                                             <xsd:element name="Lc11_TailoredPowerAllowedForArea" type="comp:TailoredPowerAllowedForArea">
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">Additional Allowance (Watts)</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">Calculated value: 
If there is only 1 row in the LcTableLuminaires 
result = Smallest (Lc10b_TailoredPowerInArea, ( Lc04_TailoredPowerCalculatedForArea + (Lc10b_TailoredPowerInArea * (1 - Lc07_MountingHeightFactor)))) 
 
Else  for each row in LcTableLuminaires 
add (Lc10a_LuminairePowerByType * (1 - Lc07_MountingHeightFactor) to a temporary variable:
     Var1 += (Lc10a_LuminairePowerByType * (1 - Lc07_MountingHeightFactor))
      result = Smallest (Lc10b_TailoredPowerInArea, ( Lc04_TailoredPowerCalculatedForArea +  Var1))
End If</xsd:documentation>
                                                </xsd:annotation>
                                             </xsd:element>
                                          </xsd:sequence>
                                       </xsd:complexType>
                                    </xsd:element>
                                 </xsd:sequence>
                              </xsd:complexType>
                           </xsd:element>
                           <xsd:element name="Lc12_PowerAllowedWallDisplay" type="comp:PowerAllowedWallDisplay">
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText">
                                    <d:b>Total Additional Allowance (Watts) CONDITIONED SPACES:</d:b>
                                 </xsd:documentation>
                                 <xsd:documentation source="CalculationsAndRules">Calculated value:  result = SUM All (Lc11_TailoredPowerAllowedForArea)</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                           <xsd:element name="LcErrorMessage_SectionErrorMessage" type="comp:SectionErrorMessage" minOccurs="0">
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText"/>
                              </xsd:annotation>
                           </xsd:element>
                        </xsd:sequence>
                     </xsd:complexType>
                  </xsd:element>
                  <xsd:element name="Lu_Unconditioned" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Unconditioned Spaces</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">This subsection is required if in section L Lu00c1_TailoredWallDisplay == true And If at least one values for Bnew05_NewLightingUnconditionedFloorArea Or Baltered05_AlteredLightingUnconditionedFloorArea GT 0</xsd:documentation>
                     </xsd:annotation>
                     <xsd:complexType>
                        <xsd:sequence>
                           <xsd:element name="Lu_UnconditionedLabel" minOccurs="0">
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText">
                                    <d:b>Unconditioned Spaces</d:b>
                                 </xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                           <xsd:element name="LuTableTailoredWall">
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText"/>
                              </xsd:annotation>
                              <xsd:complexType>
                                 <xsd:sequence>
                                    <xsd:element name="Row" minOccurs="1" maxOccurs="unbounded">
                                       <xsd:complexType>
                                          <xsd:sequence>
                                             <xsd:element name="Lu01_SpaceDescription" type="com:SpaceDescription">
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">Area Description</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">User selects from dropdown list filled with values for Ku02_SpaceDescription from section K where Ku00c1_TailoredWallDisplay == true</xsd:documentation>
                                                </xsd:annotation>
                                             </xsd:element>
                                             <xsd:element name="Lu02_WallDisplayLength" type="lit:WallDisplayLength">
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">Wall Display Length (lf)</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">User input</xsd:documentation>
                                                </xsd:annotation>
                                             </xsd:element>
                                             <xsd:element name="Lu03_TailoredWallLightingPowerPerFoot" type="comp:TailoredWallLightingPowerPerFoot">
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">Wall Display Density (W/lf)</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">Calculated value: wall display lighting density rules for the space function type  in Ku03 for the area selected in Lu01 as follows:  
If Ku03_SpaceFunctionTailored == AuditoriumArea result = 3.00; 
Else If Ku03_SpaceFunctionTailored == ConventionArea, ConferenceArea, ConferenceAreaMF, MultipurposeArea, MultipurposeAreaMF, MeetingCenterArea, MeetingCenterAreaMF Or MotionPictureTheater  result = 2.00; 
 Else If Ku03_SpaceFunctionTailored == DiningArea or DiningAreaMF result = 1.25; 
Else If Ku03_SpaceFunctionTailored == HotelLobby, MainEntryLobby  result = 3.40;  
Else If Ku03_SpaceFunctionTailored == MainEntryLobbyMF result = 3.50; 
Else If Ku03_SpaceFunctionTailored == ReligiousWorshipArea result = 1.30; 
Else If Ku03_SpaceFunctionTailored == RetailMerchandiseSales Or ShowroomArea  result = 11.50;  
Else If Ku03_SpaceFunctionTailored == ExhibitMuseumArea  result = 11.20;  
Else If Ku03_SpaceFunctionTailored == GroceryStoreArea result = 6.60;  
Else If Ku03_SpaceFunctionTailored == HotelBallroomEventsArea result = 1.80;  
Else If Ku03_SpaceFunctionTailored == PerformanceTheater result = 7.30.
End If</xsd:documentation>
                                                </xsd:annotation>
                                             </xsd:element>
                                             <xsd:element name="Lu04_TailoredPowerCalculatedForArea" type="comp:TailoredPowerCalculatedForArea">
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">Extra Allowance (Watts)</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">Calculated value:  result = Lu02_WallDisplayLength x Lu03_TailoredWallLightingPowerPerFoot;</xsd:documentation>
                                                </xsd:annotation>
                                             </xsd:element>
                                             <xsd:element name="LuTableLuminaires">
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText"/>
                                                </xsd:annotation>
                                                <xsd:complexType>
                                                   <xsd:sequence>
                                                      <xsd:element name="Row" minOccurs="1" maxOccurs="unbounded">
                                                         <xsd:complexType>
                                                            <xsd:sequence>
                                                               <xsd:element name="Lu05_LuminaireName" type="lit:LuminaireName">
                                                                  <xsd:annotation>
                                                                     <xsd:documentation source="FieldText">Luminaire Name or Item Tag</xsd:documentation>
                                                                     <xsd:documentation source="CalculationsAndRules">User input: selects from dropdown list filled with values from section F Fu02__LuminaireName</xsd:documentation>
                                                                  </xsd:annotation>
                                                               </xsd:element>
                                                               <xsd:element name="Lu06_TailoredMountingHeight" type="comp:TailoredMountingHeight">
                                                                  <xsd:annotation>
                                                                     <xsd:documentation source="FieldText">Mounting Height (ft)</xsd:documentation>
                                                                     <xsd:documentation source="CalculationsAndRules">User input: selects from dropdown list.</xsd:documentation>
                                                                  </xsd:annotation>
                                                               </xsd:element>
                                                               <xsd:element name="Lu07_MountingHeightFactor" type="lit:MountingHeightFactor">
                                                                  <xsd:annotation>
                                                                     <xsd:documentation source="FieldText">Mounting Height Factor</xsd:documentation>
                                                                     <xsd:documentation source="CalculationsAndRules">Calculated value: If Lu06_TailoredMountingHeight == LT10Ft7In result = 1.00; Else If Lu06_TailoredMountingHeight == 10Ft7InTo14Ft  result = 0.85; Else If Lu06_TailoredMountingHeight == GT14FtTo18Ft  result = 0.75;  Else  If Lu06_TailoredMountingHeight == GTE18Ft result = 0.70.</xsd:documentation>
                                                                  </xsd:annotation>
                                                               </xsd:element>
                                                               <xsd:element name="Lu08_LuminairePower" type="lit:LuminairePower">
                                                                  <xsd:annotation>
                                                                     <xsd:documentation source="FieldText">Watts per Luminaire</xsd:documentation>
                                                                     <xsd:documentation source="CalculationsAndRules">Auto-filled with Fc05_LuminairePower for luminaire selected  in Lu05_LuminaireName</xsd:documentation>
                                                                  </xsd:annotation>
                                                               </xsd:element>
                                                               <xsd:element name="Lu09_LuminaireCountByType" type="lit:LuminaireCountByType">
                                                                  <xsd:annotation>
                                                                     <xsd:documentation source="FieldText"># of Luminaires</xsd:documentation>
                                                                     <xsd:documentation source="CalculationsAndRules">User input</xsd:documentation>
                                                                  </xsd:annotation>
                                                               </xsd:element>
                                                               <xsd:element name="Lu10a_LuminairePowerByType" type="lit:LuminairePowerByType">
                                                                  <xsd:annotation>
                                                                     <xsd:documentation source="FieldText">Total Design Watts</xsd:documentation>
                                                                     <xsd:documentation source="CalculationsAndRules">Calculated value:  result = PRODUCT (Lu08_LuminairePower, Lu09_LuminaireCountByType)</xsd:documentation>
                                                                  </xsd:annotation>
                                                               </xsd:element>
                                                            </xsd:sequence>
                                                         </xsd:complexType>
                                                      </xsd:element>
                                                   </xsd:sequence>
                                                </xsd:complexType>
                                             </xsd:element>
                                             <xsd:element name="Lu10b_TailoredPowerInArea" type="comp:TailoredPowerInArea">
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">Total Design Watts for this Area:</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">Calculated value:  result = SUM All (Lu10a_LuminairePowerByType)</xsd:documentation>
                                                </xsd:annotation>
                                             </xsd:element>
                                             <xsd:element name="Lu11_TailoredPowerAllowedForArea" type="comp:TailoredPowerAllowedForArea">
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">Additional Allowance (Watts)</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">Calculated value: 
If there is only 1 row in the LuTableLuminaires 
result = Smallest (Lu10b_TailoredPowerInArea, ( Lu04_TailoredPowerCalculatedForArea + (Lu10b_TailoredPowerInArea * (1 - Lu07_MountingHeightFactor)))) 
 
Else  for each row in LuTableLuminaires 
add (Lu10a_LuminairePowerByType * (1 - Lu07_MountingHeightFactor) to a temporary variable:
     Var1 += (Lu10a_LuminairePowerByType * (1 - Lu07_MountingHeightFactor))
      result = Smallest (Lu10b_TailoredPowerInArea, ( Lu04_TailoredPowerCalculatedForArea +  Var1))
End If</xsd:documentation>
                                                </xsd:annotation>
                                             </xsd:element>
                                          </xsd:sequence>
                                       </xsd:complexType>
                                    </xsd:element>
                                 </xsd:sequence>
                              </xsd:complexType>
                           </xsd:element>
                           <xsd:element name="Lu12_PowerAllowedWallDisplay" type="comp:PowerAllowedWallDisplay">
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText">
                                    <d:b>Total Additional Allowance (Watts) UNCONDITIONED SPACES:</d:b>
                                 </xsd:documentation>
                                 <xsd:documentation source="CalculationsAndRules">Calculated value:  result = SUM All (Lu11_TailoredPowerAllowedForArea)</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                           <xsd:element name="LuErrorMessage_SectionErrorMessage" type="comp:SectionErrorMessage" minOccurs="0">
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText"/>
                              </xsd:annotation>
                           </xsd:element>
                        </xsd:sequence>
                     </xsd:complexType>
                  </xsd:element>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
         <xsd:element name="Section_M" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation source="FieldText">
                  <d:b>M. ADDITIONAL LIGHTING ALLOWANCE: TAILORED FLOOR AND TASK LIGHTING</d:b>
               </xsd:documentation>
               <xsd:documentation source="CalculationsAndRules">This section applies if in section K Kc00d1_TailoredFloorDisplayOrTask or Ku00d1_TailoredFloorDisplayOrTask == true</xsd:documentation>
               <xsd:documentation source="MinOccurs">If this section doesn't apply, do not display anything for this section</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="M_ColumnHeading02To04" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">
                           <d:b>CALCULATED ALLOWANCE (Watts)</d:b>
                        </xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="M_ColumnHeading05To10" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">
                           <d:b>DESIGN WATTS</d:b>
                        </xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="Mc_Conditioned" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Conditioned Spaces</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">This subsection is required if Kc00d1_TailoredFloorDisplayOrTask  == true And at least one value in Bnew03_NewLightingConditionedFloorArea or Baltered03_AlteredLightingConditionedFloorArea GT 0</xsd:documentation>
                     </xsd:annotation>
                     <xsd:complexType>
                        <xsd:sequence>
                           <xsd:element name="Mc_ConditionedLabel" minOccurs="0">
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText">
                                    <d:b>Conditioned Spaces</d:b>
                                 </xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                           <xsd:element name="McTableTailoredFloor">
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText"/>
                              </xsd:annotation>
                              <xsd:complexType>
                                 <xsd:sequence>
                                    <xsd:element name="Row" minOccurs="1" maxOccurs="unbounded">
                                       <xsd:complexType>
                                          <xsd:sequence>
                                             <xsd:element name="Mc01_SpaceDescription" type="com:SpaceDescription">
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">Area Description</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">User input: selects from dropdown list filled with values from section K Kc02_SpaceDescription field</xsd:documentation>
                                                </xsd:annotation>
                                             </xsd:element>
                                             <xsd:element name="Mc02_SpaceFloorArea" type="com:SpaceFloorArea">
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">Area (ft<d:sup>2</d:sup>)</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">User input</xsd:documentation>
                                                </xsd:annotation>
                                             </xsd:element>
                                             <xsd:element name="Mc03_TailoredFloorTaskLPD" type="comp:TailoredFloorTaskLPD">
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">Allowed Density (W/ft<d:sup>2</d:sup>)</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">Calculated value: floor and task lighting density rules for the space function type  in Kc03 for the area selected in Mc01 as follows:  
If Kc03_SpaceFunctionTailored == AuditoriumArea, HotelLobby, MainEntryLobby, MotionPictureTheater Or PerformanceTheater  result = 0.20; 
Else If Kc03_SpaceFunctionTailored == ConferenceAreaMF, DiningAreaMF, MainEntryLobbyMF, MultipurposeAreaMF Or MeetingCenterAreaMF result = 0.25; 
Else If Kc03_SpaceFunctionTailored == ConventionArea, ConferenceArea, MultipurposeArea Or MeetingCenterArea result = 0.30;  
Else If Kc03_SpaceFunctionTailored == DiningArea result = 0.45; 
Else If Kc03_SpaceFunctionTailored == ReligiousWorshipArea result = 0.40; 
Else If Kc03_SpaceFunctionTailored == ExhibitMuseumArea, RetailMerchandiseSale Or ShowroomArea  result = 0.70;
Else If Kc03_SpaceFunctionTailored == GroceryStoreArea result = 0.60;  
Else If Kc03_SpaceFunctionTailored == HotelBallroomEventsArea result = 0.12;
End If</xsd:documentation>
                                                </xsd:annotation>
                                             </xsd:element>
                                             <xsd:element name="Mc04_TailoredPowerCalculatedForArea" type="comp:TailoredPowerCalculatedForArea">
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">Extra Allowance (Watts)</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">Calculated value:  result = PRODUCT ( Mc02_SpaceFloorArea, Mc03_TailoredFloorTaskLPD)</xsd:documentation>
                                                </xsd:annotation>
                                             </xsd:element>
                                             <xsd:element name="McTableLuminaires">
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText"/>
                                                </xsd:annotation>
                                                <xsd:complexType>
                                                   <xsd:sequence>
                                                      <xsd:element name="Row" minOccurs="1" maxOccurs="unbounded">
                                                         <xsd:complexType>
                                                            <xsd:sequence>
                                                               <xsd:element name="Mc05_LuminaireName" type="lit:LuminaireName">
                                                                  <xsd:annotation>
                                                                     <xsd:documentation source="FieldText">Luminaire Name or Item Tag</xsd:documentation>
                                                                     <xsd:documentation source="CalculationsAndRules">User input: selects from dropdown list filled with values from section F Fc02_LuminaireName</xsd:documentation>
                                                                  </xsd:annotation>
                                                               </xsd:element>
                                                               <xsd:element name="Mc06_TailoredMountingHeight" type="comp:TailoredMountingHeight">
                                                                  <xsd:annotation>
                                                                     <xsd:documentation source="FieldText">Mounting Height (ft)</xsd:documentation>
                                                                     <xsd:documentation source="CalculationsAndRules">User input: selects from dropdown list.</xsd:documentation>
                                                                  </xsd:annotation>
                                                               </xsd:element>
                                                               <xsd:element name="Mc07_MountingHeightFactor" type="lit:MountingHeightFactor">
                                                                  <xsd:annotation>
                                                                     <xsd:documentation source="FieldText">Mounting Height Factor</xsd:documentation>
                                                                     <xsd:documentation source="CalculationsAndRules">Calculated value: If Mc06_TailoredMountingHeight == LT10Ft7In result = 1.00; Else If Mc06_TailoredMountingHeight == 10Ft7InTo14Ft  result = 0.85; Else If Mc06_TailoredMountingHeight == GT14FtTo18Ft  result = 0.75;  Else  If Mc06_TailoredMountingHeight == GTE18Ft result = 0.70.</xsd:documentation>
                                                                  </xsd:annotation>
                                                               </xsd:element>
                                                               <xsd:element name="Mc08_LuminairePower" type="lit:LuminairePower">
                                                                  <xsd:annotation>
                                                                     <xsd:documentation source="FieldText">Watts per Luminaire</xsd:documentation>
                                                                     <xsd:documentation source="CalculationsAndRules">Auto-filled with Fc05_LuminairePower in section F for selected value in Mc05</xsd:documentation>
                                                                  </xsd:annotation>
                                                               </xsd:element>
                                                               <xsd:element name="Mc09_LuminaireCountByType" type="lit:LuminaireCountByType">
                                                                  <xsd:annotation>
                                                                     <xsd:documentation source="FieldText"># of Luminaires</xsd:documentation>
                                                                     <xsd:documentation source="CalculationsAndRules">User input</xsd:documentation>
                                                                  </xsd:annotation>
                                                               </xsd:element>
                                                               <xsd:element name="Mc10a_LuminairePowerByType" type="lit:LuminairePowerByType">
                                                                  <xsd:annotation>
                                                                     <xsd:documentation source="FieldText">Total Design Watts</xsd:documentation>
                                                                     <xsd:documentation source="CalculationsAndRules">Calculated value:  result = PRODUCT ( Mc08_LuminairePower, Mc09_LuminaireCountByType)</xsd:documentation>
                                                                  </xsd:annotation>
                                                               </xsd:element>
                                                            </xsd:sequence>
                                                         </xsd:complexType>
                                                      </xsd:element>
                                                   </xsd:sequence>
                                                </xsd:complexType>
                                             </xsd:element>
                                             <xsd:element name="Mc10b_TailoredPowerInArea" type="comp:TailoredPowerInArea">
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">Total Design Watts for this Area:</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">Calculated value:  result = SUM All ( Mc10a_LuminairePowerByType)</xsd:documentation>
                                                </xsd:annotation>
                                             </xsd:element>
                                             <xsd:element name="Mc11_TailoredPowerAllowedForArea" type="comp:TailoredPowerAllowedForArea">
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">Additional Allowance (Watts)</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">Calculated value: 
If there is only 1 row in the McTableLuminaires 
result = Smallest (Mc10b_TailoredPowerInArea, ( Mc04_TailoredPowerCalculatedForArea + (Mc10b_TailoredPowerInArea * (1 - Mc07_MountingHeightFactor)))) 
 
Else  for each row in McTableLuminaires 
add (Mc10a_LuminairePowerByType  * (1 - Mc07_MountingHeightFactor) to a temporary variable:
     Var1 += (Mc10a_LuminairePowerByType  * (1 - Mc07_MountingHeightFactor))
      result = Smallest (Mc10b_TailoredPowerInArea, ( Mc04_TailoredPowerCalculatedForArea +  Var1))
End If</xsd:documentation>
                                                </xsd:annotation>
                                             </xsd:element>
                                          </xsd:sequence>
                                       </xsd:complexType>
                                    </xsd:element>
                                 </xsd:sequence>
                              </xsd:complexType>
                           </xsd:element>
                           <xsd:element name="Mc12_PowerAllowedFloorDisplayOrTask" type="comp:PowerAllowedFloorDisplayOrTask">
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText">
                                    <d:b>Total Additional Allowance (Watts) CONDITIONED SPACES:</d:b>
                                 </xsd:documentation>
                                 <xsd:documentation source="CalculationsAndRules">Calculated value:  result = SUM All ( Mc11_TailoredPowerAllowedForArea)</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                           <xsd:element name="McErrorMessage_SectionErrorMessage" type="comp:SectionErrorMessage" minOccurs="0">
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText"/>
                              </xsd:annotation>
                           </xsd:element>
                        </xsd:sequence>
                     </xsd:complexType>
                  </xsd:element>
                  <xsd:element name="Mu_Unconditioned" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Unconditioned Spaces</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">This subsection is required if Bnew05_NewLightingUnconditionedFloorArea or Bpark05_NewLightingUnconditionedFloorArea or Baltered05_AlteredLightingUnconditionedFloorArea GT 0</xsd:documentation>
                     </xsd:annotation>
                     <xsd:complexType>
                        <xsd:sequence>
                           <xsd:element name="Mu_UnconditionedLabel" minOccurs="0">
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText">
                                    <d:b>Unconditioned Spaces</d:b>
                                 </xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                           <xsd:element name="MuTableTailoredFloor">
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText"/>
                              </xsd:annotation>
                              <xsd:complexType>
                                 <xsd:sequence>
                                    <xsd:element name="Row" minOccurs="1" maxOccurs="unbounded">
                                       <xsd:complexType>
                                          <xsd:sequence>
                                             <xsd:element name="Mu01_SpaceDescription" type="com:SpaceDescription">
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">Area Description</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">User input: selects from dropdown list filled with values from section K Ku02_SpaceDescription field</xsd:documentation>
                                                </xsd:annotation>
                                             </xsd:element>
                                             <xsd:element name="Mu02_SpaceFloorArea" type="com:SpaceFloorArea">
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">Area (ft<d:sup>2</d:sup>)</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">User input</xsd:documentation>
                                                </xsd:annotation>
                                             </xsd:element>
                                             <xsd:element name="Mu03_TailoredFloorTaskLPD" type="comp:TailoredFloorTaskLPD">
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">Allowed Density (W/ft<d:sup>2</d:sup>)</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">Calculated value: floor and task lighting density rules for the space function type  in Ku03 for the area selected in Mc01 as follows: 
If Ku03_SpaceFunctionTailored == AuditoriumArea, HotelLobby, MainEntryLobby, MotionPictureTheater Or PerformanceTheater  result = 0.20; 
Else If Ku03_SpaceFunctionTailored == ConferenceAreaMF, DiningAreaMF, MainEntryLobbyMF, MultipurposeAreaMF Or MeetingCenterAreaMF result = 0.25; 
Else If Ku03_SpaceFunctionTailored == ConventionArea, ConferenceArea, MultipurposeArea Or MeetingCenterArea result = 0.30;  
Else If Ku03_SpaceFunctionTailored == DiningArea result = 0.45; 
Else If Ku03_SpaceFunctionTailored == ReligiousWorshipArea result = 0.40; 
Else If Ku03_SpaceFunctionTailored == ExhibitMuseumArea, RetailMerchandiseSale Or ShowroomArea  result = 0.70;
Else If Ku03_SpaceFunctionTailored == GroceryStoreArea result = 0.60;  
Else If Ku03_SpaceFunctionTailored == HotelBallroomEventsArea result = 0.12;
End If</xsd:documentation>
                                                </xsd:annotation>
                                             </xsd:element>
                                             <xsd:element name="Mu04_TailoredPowerCalculatedForArea" type="comp:TailoredPowerCalculatedForArea">
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">Extra Allowance (Watts)</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">Calculated value:  result =  Mu02_SpaceFloorArea x Mu03_TailoredFloorTaskLPD</xsd:documentation>
                                                </xsd:annotation>
                                             </xsd:element>
                                             <xsd:element name="MuTableLuminaires">
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText"/>
                                                </xsd:annotation>
                                                <xsd:complexType>
                                                   <xsd:sequence>
                                                      <xsd:element name="Row" minOccurs="1" maxOccurs="unbounded">
                                                         <xsd:complexType>
                                                            <xsd:sequence>
                                                               <xsd:element name="Mu05_LuminaireName" type="lit:LuminaireName">
                                                                  <xsd:annotation>
                                                                     <xsd:documentation source="FieldText">Luminaire Name or Item Tag</xsd:documentation>
                                                                     <xsd:documentation source="CalculationsAndRules">User input: selects from dropdown list filled with values from section F Fu02_LuminaireName</xsd:documentation>
                                                                  </xsd:annotation>
                                                               </xsd:element>
                                                               <xsd:element name="Mu06_TailoredMountingHeight" type="comp:TailoredMountingHeight">
                                                                  <xsd:annotation>
                                                                     <xsd:documentation source="FieldText">Mounting Height (ft)</xsd:documentation>
                                                                     <xsd:documentation source="CalculationsAndRules">User input: selects from dropdown list.</xsd:documentation>
                                                                  </xsd:annotation>
                                                               </xsd:element>
                                                               <xsd:element name="Mu07_MountingHeightFactor" type="lit:MountingHeightFactor">
                                                                  <xsd:annotation>
                                                                     <xsd:documentation source="FieldText">Mounting Height Factor</xsd:documentation>
                                                                     <xsd:documentation source="CalculationsAndRules">Calculated value: If Mu06_TailoredMountingHeight == LT10Ft7In result = 1.00; Else If Mu06_TailoredMountingHeight == 10Ft7InTo14Ft  result = 0.85; Else If Mu06_TailoredMountingHeight == GT14FtTo18Ft  result = 0.75;  Else  If Mu06_TailoredMountingHeight == GTE18Ft result = 0.70.</xsd:documentation>
                                                                  </xsd:annotation>
                                                               </xsd:element>
                                                               <xsd:element name="Mu08_LuminairePower" type="lit:LuminairePower">
                                                                  <xsd:annotation>
                                                                     <xsd:documentation source="FieldText">Watts per Luminaire</xsd:documentation>
                                                                     <xsd:documentation source="CalculationsAndRules">Auto-filled with Fc05_LuminairePower in section F for selected value in Mu05</xsd:documentation>
                                                                  </xsd:annotation>
                                                               </xsd:element>
                                                               <xsd:element name="Mu09_LuminaireCountByType" type="lit:LuminaireCountByType">
                                                                  <xsd:annotation>
                                                                     <xsd:documentation source="FieldText"># of Luminaires</xsd:documentation>
                                                                     <xsd:documentation source="CalculationsAndRules">User input</xsd:documentation>
                                                                  </xsd:annotation>
                                                               </xsd:element>
                                                               <xsd:element name="Mu10a_LuminairePowerByType" type="lit:LuminairePowerByType">
                                                                  <xsd:annotation>
                                                                     <xsd:documentation source="FieldText">Total Design Watts</xsd:documentation>
                                                                     <xsd:documentation source="CalculationsAndRules">Calculated value:  result = PRODUCT (Mu08_LuminairePower, Mu09_LuminaireCountByType)</xsd:documentation>
                                                                  </xsd:annotation>
                                                               </xsd:element>
                                                            </xsd:sequence>
                                                         </xsd:complexType>
                                                      </xsd:element>
                                                   </xsd:sequence>
                                                </xsd:complexType>
                                             </xsd:element>
                                             <xsd:element name="Mu10b_TailoredPowerInArea" type="comp:TailoredPowerInArea">
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">Total Design Watts for this Area:</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">Calculated value:  result = SUM All ( Mu10a_LuminairePowerByType)</xsd:documentation>
                                                </xsd:annotation>
                                             </xsd:element>
                                             <xsd:element name="Mu11_TailoredPowerAllowedForArea" type="comp:TailoredPowerAllowedForArea">
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">Additional Allowance (Watts)</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">Calculated value: 
If there is only 1 row in the MuTableLuminaires 
result = Smallest (Mu10b_TailoredPowerInArea, ( Mu04_TailoredPowerCalculatedForArea + (Mu10b_TailoredPowerInArea * (1 - Mu07_MountingHeightFactor)))) 
 
Else  for each row in MuTableLuminaires 
add (Mu10a_LuminairePowerByType  * (1 - Mu07_MountingHeightFactor) to a temporary variable:
     Var1 += (Mu10a_LuminairePowerByType  * (1 - Mu07_MountingHeightFactor))
      result = Smallest (Mu10b_TailoredPowerInArea, ( Mu04_TailoredPowerCalculatedForArea +  Var1))
End If</xsd:documentation>
                                                </xsd:annotation>
                                             </xsd:element>
                                          </xsd:sequence>
                                       </xsd:complexType>
                                    </xsd:element>
                                 </xsd:sequence>
                              </xsd:complexType>
                           </xsd:element>
                           <xsd:element name="Mu12_PowerAllowedFloorDisplayOrTask" type="comp:PowerAllowedFloorDisplayOrTask">
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText">
                                    <d:b>Total Additional Allowance (Watts) UNCONDITIONED SPACES:</d:b>
                                 </xsd:documentation>
                                 <xsd:documentation source="CalculationsAndRules">Calculated value:  result = SUM All ( Mu11_TailoredPowerAllowedForArea)</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                           <xsd:element name="muErrorMessage_SectionErrorMessage" type="comp:SectionErrorMessage" minOccurs="0">
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText"/>
                              </xsd:annotation>
                           </xsd:element>
                        </xsd:sequence>
                     </xsd:complexType>
                  </xsd:element>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
         <xsd:element name="Section_N" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation source="FieldText">
                  <d:b>N. ADDITIONAL LIGHTING ALLOWANCE: TAILORED DECORATIVE /SPECIAL EFFECTS</d:b>
               </xsd:documentation>
               <xsd:documentation source="CalculationsAndRules">This section applies if in section K Kc00e1_TailoredOrnamentalSpecialEffects or Ku00e1_TailoredOrnamentalSpecialEffects  == true</xsd:documentation>
               <xsd:documentation source="MinOccurs">If this section doesn't apply, do not display anything for this section</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="N_ColumnHeading02To04" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">
                           <d:b>CALCULATED ALLOWANCE (Watts)</d:b>
                        </xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="N_ColumnHeading05To08" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">
                           <d:b>DESIGN WATTS</d:b>
                        </xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="Nc_Conditioned" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Conditioned Spaces</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">This subsection is required if in section K Kc00e1_TailoredOrnamentalSpecialEffects == true And at least one value for  Bnew03_NewLightingConditionedFloorArea or Baltered03_AlteredLightingConditionedFloorArea GT 0</xsd:documentation>
                     </xsd:annotation>
                     <xsd:complexType>
                        <xsd:sequence>
                           <xsd:element name="Nc_ConditionedLabel" minOccurs="0">
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText">
                                    <d:b>Conditioned Spaces</d:b>
                                 </xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                           <xsd:element name="NcTableTailoredOrnamental">
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText"/>
                              </xsd:annotation>
                              <xsd:complexType>
                                 <xsd:sequence>
                                    <xsd:element name="Row" minOccurs="1" maxOccurs="unbounded">
                                       <xsd:complexType>
                                          <xsd:sequence>
                                             <xsd:element name="Nc01_SpaceDescription" type="com:SpaceDescription">
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">Area Description</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">User input: selects from dropdown list filled with values from section K field Kc02_SpaceDescription</xsd:documentation>
                                                </xsd:annotation>
                                             </xsd:element>
                                             <xsd:element name="Nc02_SpaceFloorArea" type="com:SpaceFloorArea">
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">Area (ft<d:sup>2</d:sup>)</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">User input</xsd:documentation>
                                                </xsd:annotation>
                                             </xsd:element>
                                             <xsd:element name="Nc03_TailoredOrnamentalLPD" type="comp:TailoredOrnamentalLPD">
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">Allowed Density (W/ft<d:sup>2</d:sup>)</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">Calculated value: For all primary function types result = 0.35;</xsd:documentation>
                                                </xsd:annotation>
                                             </xsd:element>
                                             <xsd:element name="Nc04_TailoredPowerCalculatedForArea" type="comp:TailoredPowerCalculatedForArea">
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">Extra Allowance (Watts)</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">Calculated value:  result = PRODUCT ( Nc02_SpaceFloorArea, Nc03_TailoredOrnamentalLPD)</xsd:documentation>
                                                </xsd:annotation>
                                             </xsd:element>
                                             <xsd:element name="NcTableLuminaires">
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText"/>
                                                </xsd:annotation>
                                                <xsd:complexType>
                                                   <xsd:sequence>
                                                      <xsd:element name="Row" minOccurs="1" maxOccurs="unbounded">
                                                         <xsd:complexType>
                                                            <xsd:sequence>
                                                               <xsd:element name="Nc05_LuminaireName" type="lit:LuminaireName">
                                                                  <xsd:annotation>
                                                                     <xsd:documentation source="FieldText">Luminaire Name or Item Tag</xsd:documentation>
                                                                     <xsd:documentation source="CalculationsAndRules">User input: selects from dropdown list filled with values from section F Fc02_LuminaireName</xsd:documentation>
                                                                  </xsd:annotation>
                                                               </xsd:element>
                                                               <xsd:element name="Nc06_LuminairePower" type="lit:LuminairePower">
                                                                  <xsd:annotation>
                                                                     <xsd:documentation source="FieldText">Watts per Luminaire</xsd:documentation>
                                                                     <xsd:documentation source="CalculationsAndRules">Auto-filled with Fc05_LuminairePower in section F for selected value in Nc05</xsd:documentation>
                                                                  </xsd:annotation>
                                                               </xsd:element>
                                                               <xsd:element name="Nc07_LuminaireCountByType" type="lit:LuminaireCountByType">
                                                                  <xsd:annotation>
                                                                     <xsd:documentation source="FieldText"># of Luminaires</xsd:documentation>
                                                                     <xsd:documentation source="CalculationsAndRules">User input</xsd:documentation>
                                                                  </xsd:annotation>
                                                               </xsd:element>
                                                               <xsd:element name="Nc08a_LuminairePowerByType" type="lit:LuminairePowerByType">
                                                                  <xsd:annotation>
                                                                     <xsd:documentation source="FieldText">Total Design Watts</xsd:documentation>
                                                                     <xsd:documentation source="CalculationsAndRules">Calculated value:  result = PRODUCT (Nc06_LuminairePower, Nc07_LuminaireCountByType)</xsd:documentation>
                                                                  </xsd:annotation>
                                                               </xsd:element>
                                                            </xsd:sequence>
                                                         </xsd:complexType>
                                                      </xsd:element>
                                                   </xsd:sequence>
                                                </xsd:complexType>
                                             </xsd:element>
                                             <xsd:element name="Nc08b_TailoredPowerInArea" type="comp:TailoredPowerInArea">
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">Total Design Watts for this Area:</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">Calculated value:  result = SUM All (Nc08a_LuminairePowerByType)</xsd:documentation>
                                                </xsd:annotation>
                                             </xsd:element>
                                             <xsd:element name="Nc09_TailoredPowerAllowedForArea" type="comp:TailoredPowerAllowedForArea">
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">Additional Allowance (Watts)</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">Calculated value:  result = Smallest (Nc08b_TailoredPowerInArea, Nc04_TailoredPowerCalculatedForArea)</xsd:documentation>
                                                </xsd:annotation>
                                             </xsd:element>
                                          </xsd:sequence>
                                       </xsd:complexType>
                                    </xsd:element>
                                 </xsd:sequence>
                              </xsd:complexType>
                           </xsd:element>
                           <xsd:element name="Nc10_PowerAllowedOrnamentalSpecialEffects" type="comp:PowerAllowedOrnamentalSpecialEffects">
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText">
                                    <d:b>Total Additional Allowance (Watts) CONDITIONED SPACES:</d:b>
                                 </xsd:documentation>
                                 <xsd:documentation source="CalculationsAndRules">Calculated value:  result = SUM All ( Nc09_TailoredPowerAllowedForArea)</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                           <xsd:element name="NcErrorMessage_SectionErrorMessage" type="comp:SectionErrorMessage" minOccurs="0">
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText"/>
                              </xsd:annotation>
                           </xsd:element>
                        </xsd:sequence>
                     </xsd:complexType>
                  </xsd:element>
                  <xsd:element name="Nu_Unconditioned" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Unconditioned Spaces</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">This subsection is required if in section K Ku00e1_TailoredOrnamentalSpecialEffects == true And at least one value for  Bnew03_NewLightingConditionedFloorArea or Baltered03_AlteredLightingConditionedFloorArea GT 0</xsd:documentation>
                     </xsd:annotation>
                     <xsd:complexType>
                        <xsd:sequence>
                           <xsd:element name="Nu_UnconditionedLabel" minOccurs="0">
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText">
                                    <d:b>Unconditioned Spaces</d:b>
                                 </xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                           <xsd:element name="NuTableTailoredOrnamental">
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText"/>
                              </xsd:annotation>
                              <xsd:complexType>
                                 <xsd:sequence>
                                    <xsd:element name="Row" minOccurs="1" maxOccurs="unbounded">
                                       <xsd:complexType>
                                          <xsd:sequence>
                                             <xsd:element name="Nu01_SpaceDescription" type="com:SpaceDescription">
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">Area Description</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">User input: selects from dropdown list filled with values from section K field Ku02_SpaceDescription</xsd:documentation>
                                                </xsd:annotation>
                                             </xsd:element>
                                             <xsd:element name="Nu02_SpaceFloorArea" type="com:SpaceFloorArea">
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">Area (ft<d:sup>2</d:sup>)</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">User input</xsd:documentation>
                                                </xsd:annotation>
                                             </xsd:element>
                                             <xsd:element name="Nu03_TailoredOrnamentalLPD" type="comp:TailoredOrnamentalLPD">
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">Allowed Density (W/ft<d:sup>2</d:sup>)</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">Calculated value: For all primary function types result = 0.35;</xsd:documentation>
                                                </xsd:annotation>
                                             </xsd:element>
                                             <xsd:element name="Nu04_TailoredPowerCalculatedForArea" type="comp:TailoredPowerCalculatedForArea">
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">Extra Allowance (Watts)</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">Calculated value: result =  PRODUCT (Nu02_SpaceFloorArea, Nu03_TailoredOrnamentalLPD)</xsd:documentation>
                                                </xsd:annotation>
                                             </xsd:element>
                                             <xsd:element name="NuTableLuminaires">
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText"/>
                                                </xsd:annotation>
                                                <xsd:complexType>
                                                   <xsd:sequence>
                                                      <xsd:element name="Row" minOccurs="1" maxOccurs="unbounded">
                                                         <xsd:complexType>
                                                            <xsd:sequence>
                                                               <xsd:element name="Nu05_LuminaireName" type="lit:LuminaireName">
                                                                  <xsd:annotation>
                                                                     <xsd:documentation source="FieldText">Luminaire Name or Item Tag</xsd:documentation>
                                                                     <xsd:documentation source="CalculationsAndRules">User input: selects from dropdown list filled with values from section F Fu02_LuminaireName</xsd:documentation>
                                                                  </xsd:annotation>
                                                               </xsd:element>
                                                               <xsd:element name="Nu06_LuminairePower" type="lit:LuminairePower">
                                                                  <xsd:annotation>
                                                                     <xsd:documentation source="FieldText">Watts per Luminaire</xsd:documentation>
                                                                     <xsd:documentation source="CalculationsAndRules">Auto-filled result = Fu05_LuminairePower in section F  for selected value in Nu05</xsd:documentation>
                                                                  </xsd:annotation>
                                                               </xsd:element>
                                                               <xsd:element name="Nu07_LuminaireCountByType" type="lit:LuminaireCountByType">
                                                                  <xsd:annotation>
                                                                     <xsd:documentation source="FieldText"># of Luminaires</xsd:documentation>
                                                                     <xsd:documentation source="CalculationsAndRules">User input</xsd:documentation>
                                                                  </xsd:annotation>
                                                               </xsd:element>
                                                               <xsd:element name="Nu08a_LuminairePowerByType" type="lit:LuminairePowerByType">
                                                                  <xsd:annotation>
                                                                     <xsd:documentation source="FieldText">Total Design Watts</xsd:documentation>
                                                                     <xsd:documentation source="CalculationsAndRules">Calculated value: result = PRODUCT ( Nu06_LuminairePower, Nu07_LuminaireCountByType)</xsd:documentation>
                                                                  </xsd:annotation>
                                                               </xsd:element>
                                                            </xsd:sequence>
                                                         </xsd:complexType>
                                                      </xsd:element>
                                                   </xsd:sequence>
                                                </xsd:complexType>
                                             </xsd:element>
                                             <xsd:element name="Nu08b_TailoredPowerInArea" type="comp:TailoredPowerInArea">
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">Total Design Watts for this Area:</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">Calculated value:  result = SUM All (Nu08aLuminairePowerByType)</xsd:documentation>
                                                </xsd:annotation>
                                             </xsd:element>
                                             <xsd:element name="Nu09_TailoredPowerAllowedForArea" type="comp:TailoredPowerAllowedForArea">
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">Additional Allowance (Watts)</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">Calculated value:  result = Smallest (Nu08b_TailoredPowerInArea, Nu04_TailoredPowerCalculatedForArea)</xsd:documentation>
                                                </xsd:annotation>
                                             </xsd:element>
                                          </xsd:sequence>
                                       </xsd:complexType>
                                    </xsd:element>
                                 </xsd:sequence>
                              </xsd:complexType>
                           </xsd:element>
                           <xsd:element name="Nu10_PowerAllowedOrnamentalSpecialEffects" type="comp:PowerAllowedOrnamentalSpecialEffects">
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText">
                                    <d:b>Total Additional Allowance (Watts) UNCONDITIONED SPACES:</d:b>
                                 </xsd:documentation>
                                 <xsd:documentation source="CalculationsAndRules">Calculated value:  result = SUM All (Nu09_TailoredPowerAllowedForArea)</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                           <xsd:element name="NuErrorMessage_SectionErrorMessage" type="comp:SectionErrorMessage" minOccurs="0">
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText"/>
                              </xsd:annotation>
                           </xsd:element>
                        </xsd:sequence>
                     </xsd:complexType>
                  </xsd:element>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
         <xsd:element name="Section_O" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation source="FieldText">
                  <d:b>O. ADDITIONAL LIGHTING ALLOWANCE: TAILORED VERY VALUABLE MERCHANDISE</d:b>
               </xsd:documentation>
               <xsd:documentation source="CalculationsAndRules">This section applies if in section K Kc00f1_TailoredVeryValuableDisplay or  Ku00f1_TailoredVeryValuableDisplay == true</xsd:documentation>
               <xsd:documentation source="MinOccurs">If this section doesn't apply, do not display anything for this section</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="O_BeginNote1" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"/>
                        <xsd:documentation source="AdditionalRequirements">
                           <d:i>This category includes case lighting in retail merchandise sales, museums, and religious settings, where lighting of small art objects, artifacts, or valuable collections is required. The display case shall contain jewelry, coins, fine china, fine crystal, precious stones, silver, small art objects or artifacts, and/or valuable collections the display of which involves customer inspection of very fine detail from outside of a locked case. Lighting in this category must comply with the applicable requirements in 
                              <d:t24refnolink/>140.6(c)3J /
                              <d:t24refnolink/>170.2(e)4B.</d:i>
                        </xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="O_ColumnHeading03To05" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">
                           <d:b>QUALIFYING FLOOR AREA (Watts)</d:b>
                        </xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="O_ColumnHeading06To08" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">
                           <d:b>QUALIFYING DISPLAY CASE AREA </d:b>
                        </xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="O_ColumnHeading09To12" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">
                           <d:b>DESIGN WATTS</d:b>
                        </xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="Oc_Conditioned" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"/>
                        <xsd:documentation source="CalculationsAndRules">This subsection is required if Kc00f1_TailoredVeryValuableDisplay == true And at least one value in Bnew03_NewLightingConditionedFloorArea Or Baltered03_AlteredLightingConditionedFloorArea GT 0</xsd:documentation>
                     </xsd:annotation>
                     <xsd:complexType>
                        <xsd:sequence>
                           <xsd:element name="Oc_ConditionedLabel" minOccurs="0">
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText">
                                    <d:b>Conditioned Spaces</d:b>
                                 </xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                           <xsd:element name="OcTableTailoredValuables">
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText"/>
                              </xsd:annotation>
                              <xsd:complexType>
                                 <xsd:sequence>
                                    <xsd:element name="Row" minOccurs="1" maxOccurs="unbounded">
                                       <xsd:complexType>
                                          <xsd:sequence>
                                             <xsd:element name="Oc01_SpaceDescription" type="com:SpaceDescription">
                                                <xsd:annotation>
