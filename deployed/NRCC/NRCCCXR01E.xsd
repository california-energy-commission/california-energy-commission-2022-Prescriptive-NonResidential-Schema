<?xml version="1.0" encoding="UTF-8"?>
<xsd:schema attributeFormDefault="unqualified" elementFormDefault="qualified" targetNamespace="http://www.lmonte.com/besm/NRCCCXR01E" version="2022.0.000" xmlns="http://www.lmonte.com/besm/NRCCCXR01E" xmlns:altova="http://www.altova.com/xml-schema-extensions" xmlns:bld="http://www.lmonte.com/besm/bld" xmlns:com="http://www.lmonte.com/besm/com" xmlns:comp="http://www.lmonte.com/besm/comp" xmlns:d="http://www.lmonte.com/besm/d" xmlns:dtyp="http://www.lmonte.com/besm/dtyp" xmlns:env="http://www.lmonte.com/besm/env" xmlns:hvac="http://www.lmonte.com/besm/hvac" xmlns:lit="http://www.lmonte.com/besm/lit" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
   <xsd:import namespace="http://www.lmonte.com/besm/bld" schemaLocation="../base/NrPresBuilding.xsd"/>
   <xsd:import namespace="http://www.lmonte.com/besm/com" schemaLocation="../base/NrPresCommon.xsd"/>
   <xsd:import namespace="http://www.lmonte.com/besm/comp" schemaLocation="../base/NrPresCompliance.xsd"/>
   <xsd:import namespace="http://www.lmonte.com/besm/dtyp" schemaLocation="../base/DataTypes.xsd"/>
   <xsd:import namespace="http://www.lmonte.com/besm/env" schemaLocation="../base/NrPresEnvelope.xsd"/>
   <xsd:import namespace="http://www.lmonte.com/besm/hvac" schemaLocation="../base/NrPresHvac.xsd"/>
   <xsd:import namespace="http://www.lmonte.com/besm/lit" schemaLocation="../base/NrPresLighting.xsd"/>
   <xsd:element name="ComplianceDocumentPackage">
      <xsd:complexType>
         <xsd:sequence>
            <xsd:element name="DocID" minOccurs="0">
               <xsd:complexType>
                  <xsd:attribute name="doc" type="comp:ComplianceDocumentTagNR" fixed="NRCCCXRE"/>
                  <xsd:attribute name="docType" type="comp:ComplianceDocumentTypeNR" fixed="CERTIFICATE OF COMPLIANCE NR"/>
                  <xsd:attribute name="docTitle" type="comp:ComplianceDocumentTitleNR" fixed="Nonresidential Building Commissioning"/>
               </xsd:complexType>
            </xsd:element>
            <xsd:element name="Payload" type="comp:Payload"/>
            <xsd:element name="DocumentData">
               <xsd:complexType>
                  <xsd:sequence>
                     <xsd:element name="Header" type="comp:HeaderNRCC"/>
                     <xsd:element ref="nRCCCXR01E"/>
                     <xsd:element name="DeclarationsBlock" type="comp:DeclarationsNRCC" maxOccurs="unbounded"/>
                     <xsd:element name="Section_MandatoryMeasureBlock" minOccurs="0">
                        <xsd:annotation>
                           <xsd:documentation source="FieldText">
                              <d:b>
                                 <d:i>Building Commissioning Mandatory Measures:</d:i>
                              </d:b>
                           </xsd:documentation>
                           <xsd:documentation source="CalculationsAndRules">If NRCC-CXR-E is included in scope And 
A07_RequestedMMB == true
this Mandatory Measures Noteblock is required 

Else it is not required and nothing is captured or displayed
End If</xsd:documentation>
                           <xsd:documentation source="MinOccurs">If this section doesn't apply, do not display anything for this section</xsd:documentation>
                        </xsd:annotation>
                        <xsd:complexType>
                           <xsd:sequence>
                              <xsd:element name="MM_BeforeNote1" minOccurs="0">
                                 <xsd:annotation>
                                    <xsd:documentation source="FieldText">
                                       <d:b>
                                          <d:i>2022 Nonresidential Energy Standards Compliance (Title 24, Part 6 Ch.1)</d:i>
                                       </d:b>
                                    </xsd:documentation>
                                 </xsd:annotation>
                              </xsd:element>
                              <xsd:element name="MM_BeginNote1" minOccurs="0">
                                 <xsd:annotation>
                                    <xsd:documentation source="FieldText">
                                       <d:i>The following mandatory measures have been identified as applicable to the project and must be met in order to demonstrate compliance with Title 24, Part 6</d:i>
                                    </xsd:documentation>
                                 </xsd:annotation>
                              </xsd:element>
                              <xsd:element name="MM120_8_MeasureApplies" type="comp:MeasureApplies">
                                 <xsd:annotation>
                                    <xsd:documentation source="FieldText">
                                       <d:b>
                                          <d:t24refnolink/>120.8 BUILDING COMMISSIONING</d:b>
                                    </xsd:documentation>
                                    <xsd:documentation source="AdditionalRequirements">
                                       <d:b>NONRESIDENTIAL BUILDINGS, OTHER THAN HEALTHCARE FACILITIES, WITH CONDITIONED SPACE
                                          <d:gte/>10,000 FT2 OR COMPLEX MECHANICAL SYSTEMS, OR
                                          <d:line1/>HIGH-RISE RESIDENTIAL
                                          <d:gte/>4 STORIES, HOTEL/MOTEL, AND MIXED-USE BUILDINGS WITH NONRESIDENTIAL CONDITIONED SPACES
                                          <d:gte/>10,000 FT2</d:b>
                                       <d:line1/>NOTE: MECHANICAL SYSTEMS OTHER THAN UNITARY OR PACKAGED EQUIPMENT EACH SERVING ONE ZONE, OR TWO-PIPE HEATING ONLY SYSTEMS ARE CONSIDERED COMPLEX AND MUST MEET ALL REQUIREMENTS OUTLINED BELOW.
                                       <d:line1/>COMMISSIONING SHALL INCLUDE COMPLETION OF THE FOLLOWING:
                                       <d:line1/>
                                       <d:t24refnolink/>120.8(b) OWNER'S PROJECT REQUIREMENTS (OPR) SHALL BE DOCUMENTED BEFORE THE DESIGN PHASE BEGINS.  THE OPR SHALL INCLUDE THE OWNER'S GOALS, REQUIREMENTS AND EXPECTATIONS FOR EVERYTHING RELATED TO ENERGY CONSUMPTION AND OPERATION.
                                       <d:line1/>
                                       <d:t24refnolink/>120.8(c) THE BASIS OF DESIGN (BOD) SHALL BE DOCUMENTED AT THE DESIGN PHASE AND UPDATED THROUGHOUT DESIGN AND CONSTRUCTION AS NEEDED. THE BOD SHALL DOCUMENT THE DESIGN ELEMENTS SUCH AS CALCULATIONS AND PRODUCT SELECTIONS THAT MEET THE OWNER'S PROJECT REQUIREMENTS AND APPLICABLE REGULATORY REQUIREMENTS.
                                       <d:line1/>
                                       <d:t24refnolink/>120.8(d) DURING SCHEMATIC DESIGN A DESIGN REVIEW KICKOFF MEETING SHALL BE HELD TO ESTABLISH WHO WILL PLAY THE ROLE OF THE DESIGN REVIEWER, THE PROJECT SCHEDULE, AND IDENTIFY THE OWNER'S REQUIREMENTS. DURING THE DESIGN PHASE, THE DESIGN REVIEWER(S) SHALL REVIEW THE CONSTRUCTION DOCUMENTS FOR CLARITY, COMPLETENESS, AND ADHERENCE TO THE OWNER'S GOALS.
                                       <d:line1/>
                                       <d:t24refnolink/>120.8(e) COMMISSIONING MEASURES MUST BE INCLUDED IN THE CONSTRUCTION DOCUMENTS TO FACILITATE THE DESIGN REVIEW AND COMMISSIONING PROCESS.
                                       <d:line1/>
                                       <d:t24refnolink/>120.8(f) A COMMISSIONING PLAN SHALL BE DEVELOPED BY THE COMMISSIONING PROVIDER WITH INPUT FROM THE DESIGNER AND SHALL DEFINE THE SCOPE OF THE COMMISSIONING PROJECT.  THIS SHALL BE DRAFTED DURING DESIGN AND COMPLETED DURING EARLY CONSTRUCTION.
                                       <d:line1/>
                                       <d:t24refnolink/>120.8(g) FUNCTIONAL PERFORMANCE TESTING SHALL BE CONDUCTED ON BUILDING SYSTEMS TO DEMONSTRATE CORRECT INSTALLATION AND OPERATION.
                                       <d:line1/>
                                       <d:t24refnolink/>120.8(h) DOCUMENTATION OF THE OPERATIONAL ASPECTS OF THE BUILDING SHALL BE COMPLETED WITHIN THE SYSTEMS MANUAL AND DELIVERED TO THE BUILDING OWNER OR REPRESENTATIVE AND FACILITIES OPERATOR. TRAINING OF APPROPRIATE MAINTENANCE STAFF SHALL BE DOCUMENTED IN THE COMMISSIONING REPORT.
                                       <d:line2/>
                                       <d:t24refnolink/>120.8(i) A COMPLETE REPORT OF COMMISSIONING PROCESS ACTIVITIES UNDERTAKEN THROUGH THE DESIGN, CONSTRUCTION AND REPORTING RECOMMENDATIONS FOR POST-CONSTRUCTION PHASES OF THE BUILDING PROJECT SHALL BE COMPLETED AND PROVIDED TO THE OWNER OR REPRESENTATIVE.</xsd:documentation>
                                    <xsd:documentation source="CalculationsAndRules">If A05_ConditionedFloorAreaRangeNR ==  10000To49999Ft2 Or GTE50000Ft2
result = true  FieldText and any AdditionalRequirements static text is displayed in the report
Else result = false nothing is displayed in the report
End if</xsd:documentation>
                                 </xsd:annotation>
                              </xsd:element>
                              <xsd:element name="MM120_8gExc_ExceptionApplies" type="comp:ExceptionApplies" minOccurs="0">
                                 <xsd:annotation>
                                    <xsd:documentation source="FieldText">NOT REPORTED IN PDF</xsd:documentation>
                                    <xsd:documentation source="AdditionalRequirements">
                                       <d:b>EXCEPTION</d:b>TO
                                       <d:t24refnolink/>120.8(g)
                                       <d:b>APPLIES</d:b>:

HEALTHCARE FACILITIES.</xsd:documentation>
                                    <xsd:documentation source="CalculationsAndRules">If MM120_8_MeasureApplies == true, 
user input
Else
result = false
End If

If result == true  FieldText and AdditionalRequirements static text is displayed in the report
Else If result == false nothing is displayed in the report
End if</xsd:documentation>
                                 </xsd:annotation>
                              </xsd:element>
                              <xsd:element name="MM120_8_LT10K_MeasureApplies" type="comp:MeasureApplies">
                                 <xsd:annotation>
                                    <xsd:documentation source="FieldText">
                                       <d:b>
                                          <d:t24refnolink/>120.8 BUILDING COMMISSIONING</d:b>
                                    </xsd:documentation>
                                    <xsd:documentation source="AdditionalRequirements">
                                       <d:b>NONRESIDENTIAL BUILDINGS, OTHER THAN HEALTHCARE FACILITIES, WITH CONDITIONED SPACE
                                          <d:lt/>10,000 FT2 AND SIMPLE MECHANICAL SYSTEMS, OR
                                          <d:line1/>HIGH-RISE RESIDENTIAL
                                          <d:gte/>4 STORIES, HOTEL/MOTEL, AND MIXED-USE BUILDINGS WITH NONRESIDENTIAL CONDITIONED SPACES
                                          <d:lt/>10,000 FT2</d:b>
                                       <d:line1/>NOTE: UNITARY OR PACKAGED EQUIPMENT EACH SERVING ONE ZONE AND TWO-PIPE HEATING ONLY SYSTEMS ARE SIMPLE MECHANICAL SYSTEMS.  ALL OTHER HVAC SYSTEM TYPES ARE COMPLEX AND PROJECTS MUST MEET ALL REQUIREMENTS OF
                                       <d:t24refnolink/>120.8.
                                       <d:line1/>COMMISSIONING SHALL INCLUDE COMPLETION OF THE FOLLOWING:
                                       <d:line1/>
                                       <d:t24refnolink/>120.8(d) DURING SCHEMATIC DESIGN A DESIGN REVIEW KICKOFF MEETING SHALL BE HELD TO ESTABLISH WHO WILL PLAY THE ROLE OF THE DESIGN REVIEWER, THE PROJECT SCHEDULE, AND IDENTIFY THE OWNER'S REQUIREMENTS. DURING THE DESIGN PHASE, THE DESIGN REVIEWER(S) SHALL REVIEW THE CONSTRUCTION DOCUMENTS FOR CLARITY, COMPLETENESS, AND ADHERENCE TO THE OWNER'S GOALS.
                                       <d:line1/>
                                       <d:t24refnolink/>120.8(e) COMMISSIONING MEASURES MUST BE INCLUDED IN THE CONSTRUCTION DOCUMENTS TO FACILITATE THE DESIGN REVIEW AND COMMISSIONING PROCESS.</xsd:documentation>
                                    <xsd:documentation source="CalculationsAndRules">If A05_ConditionedFloorAreaRangeNR == LT10000Ft2 
result = true  FieldText and any AdditionalRequirements static text is displayed in the report
Else result = false nothing is displayed in the report
End if</xsd:documentation>
                                 </xsd:annotation>
                              </xsd:element>
                           </xsd:sequence>
                        </xsd:complexType>
                     </xsd:element>
                     <xsd:element name="Footer" type="comp:FooterNRCC"/>
                  </xsd:sequence>
               </xsd:complexType>
            </xsd:element>
            <xsd:element name="Report" type="xsd:base64Binary" minOccurs="0"/>
            <xsd:element name="ReportMMB" type="xsd:base64Binary" minOccurs="0"/>
         </xsd:sequence>
         <xsd:attribute name="revision" use="required" type="xsd:string" fixed="rev 20220101"/>
         <xsd:attribute name="doc" use="required" type="comp:ComplianceDocumentTagNR" fixed="NRCCCXRE"/>
      </xsd:complexType>
   </xsd:element>
   <xsd:complexType name="NRCCCXR01E">
      <xsd:annotation>
         <xsd:documentation>Nonresidential Building Commissioning</xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="Section_A" minOccurs="1" maxOccurs="1">
            <xsd:annotation>
               <xsd:documentation source="FieldText">
                  <d:b>A. GENERAL INFORMATION</d:b>
               </xsd:documentation>
               <xsd:documentation source="MinOccurs">This section is required.</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="A_BeforeNote1" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"/>
                        <xsd:documentation source="AdditionalRequirements">
                           <d:i>This document is used to demonstrate compliance with mandatory commissioning requirements in 
                              <d:t24refnolink/>120.8 for nonresidential buildings and hotel/motel or mixed-use buildings with nonresidential spaces. This document does not demonstrate compliance with commissioning requirements within Title 24, Part 11, which need to be documented separately if they apply.</d:i>
                        </xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">This note should be placed before section A in the header on the first page of the report only</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="A01_ProjectCity" type="comp:ProjectCity">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Project Location (city)</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">User input</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="A02_CommissioningOccupancy" type="comp:CommissioningOccupancy">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Occupancy Type</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">User input selected from occupancy types in dropdown</xsd:documentation>
                        <xsd:appinfo source="displayterm">
                           <dtyp:displayterm value="MultifamilyResidential">Multifamily Residential (R occ only)</dtyp:displayterm>
                           <dtyp:displayterm value="HotelMotel">Hotel/ Motel (w Nonres occ)</dtyp:displayterm>
                        </xsd:appinfo>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="A03_CommissioningProjectScope" type="comp:CommissioningProjectScope">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Project Type</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">User input selected from project types: NewConstruction, ShellCoreOnly, TenantFitOutOnly and AdditionAlteration.
If result == ShellCoreOnly Or , TenantFitOutOnly Then 
D03_ExceptionalConditionMessageCCCXRE += CoreShellOrTenantFitOutInA</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="A04_TotalGrossFloorArea" type="com:TotalGrossFloorArea">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Building Size (ft<d:sup>2</d:sup>)</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">User input</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:choice>
                     <xsd:element name="A05_ConditionedFloorAreaRangeNR">
                        <xsd:annotation>
                           <xsd:documentation source="FieldText">Nonresidential Conditioned Floor Area  (ft<d:sup>2</d:sup>)</xsd:documentation>
                           <xsd:documentation source="CalculationsAndRules">If A02StopMessage and A0203StopMessage are false 
   User input selected from list of floor area ranges:  LT10000Ft2, 10000To49999Ft2 and GTE50000Ft2
   
   Mandatory Measure Triggers:
   If result == 10000To49999Ft2 Or GTE50000Ft2
   MM120_8_MeasureApplies = true and is displayed 
   Else If result == LT10000Ft2
   MM120_8_LT10K_MeasureApplies = true and is displayed
   End If</xsd:documentation>
                        </xsd:annotation>
                        <xsd:simpleType>
                           <xsd:restriction base="env:ConditionedFloorAreaRangeNR">
                              <xsd:enumeration value="10000To49999Ft2"/>
                              <xsd:enumeration value="GTE50000Ft2"/>
                              <xsd:enumeration value="LT10000Ft2"/>
                           </xsd:restriction>
                        </xsd:simpleType>
                     </xsd:element>
                     <xsd:element name="A05_NotApplicableMessage">
                        <xsd:annotation>
                           <xsd:documentation source="FieldText">Nonresidential Conditioned Floor Area  (ft<d:sup>2</d:sup>)</xsd:documentation>
                           <xsd:documentation source="CalculationsAndRules">If A02StopMessage or A0203StopMessage are true
Result = BlankGrayedOut stored in A05_NotApplicableMessage
End if</xsd:documentation>
                        </xsd:annotation>
                        <xsd:simpleType>
                           <xsd:restriction base="comp:NotApplicableMessage">
                              <xsd:enumeration value="BlankGrayedOut"/>
                           </xsd:restriction>
                        </xsd:simpleType>
                     </xsd:element>
                  </xsd:choice>
                  <xsd:choice>
                     <xsd:element name="A06_CommissioningHvacSystem" type="hvac:CommissioningHvacSystem">
                        <xsd:annotation>
                           <xsd:documentation source="FieldText">HVAC System Type</xsd:documentation>
                           <xsd:documentation source="CalculationsAndRules">If A02StopMessage and A0203StopMessage are false
   If User input selected from list of HVAC system types: UnitaryPackagedEquipmentPerZone, TwoPipeHeatingOnly, and ComplexHvacSystem</xsd:documentation>
                        </xsd:annotation>
                     </xsd:element>
                     <xsd:element name="A06_NotApplicableMessage">
                        <xsd:annotation>
                           <xsd:documentation source="FieldText">HVAC System Type</xsd:documentation>
                           <xsd:documentation source="CalculationsAndRules">If A02StopMessage or A0203StopMessage are true
Result = BlankGrayedOut stored in A06_NotApplicableMessage
End if</xsd:documentation>
                        </xsd:annotation>
                        <xsd:simpleType>
                           <xsd:restriction base="comp:NotApplicableMessage">
                              <xsd:enumeration value="BlankGrayedOut"/>
                           </xsd:restriction>
                        </xsd:simpleType>
                     </xsd:element>
                  </xsd:choice>
                  <xsd:element name="A07_ClimateZone" type="com:ClimateZone">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Climate Zone</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">User selects from climate zones 1 -16 the climate zone where the project is located</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="A0203StopMessage" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">
                           <d:b>
                              <d:i>STOP! Occupancy types that are not nonresidential, hotel/motel with nonresidential occupancies, or mixed-use, and project types which are additions or alterations, are not required to comply with commissioning requirements in 
                                 <d:t24refnolink/>120.8 and do not need to complete this compliance document.</d:i>
                           </d:b>
                        </xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">If A02_CommissioningOccupancy ==  AllOthers Or  MultifamilyResidential  Or 
If A03_CommissioningProjectScope ==  AdditionAlteration 
this Stop Message  is displayed to the user
End If</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="A02StopMessage" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">
                           <d:b>
                              <d:i>STOP! Healthcare facilities are not required to comply with the Commissioning requirements in Part 6, but must comply with the applicable requirements in Chapter 7 of Title 24 Part 1 (OSHPD).</d:i>
                           </d:b>
                        </xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">If A02_CommissioningOccupancy == Healthcare 
this Stop Message is displayed to the user
End If</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="A07_RequestedMMB" type="comp:RequestedMMB" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">NOT REPORTED IN PDF</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">This is a flag set by the agent that generates the report XML data to indicate that the user requested a Mandatory Measures Note Block which is placed after the DeclarationBlock in the XML output.</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="A08_ProjectScopeNR">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">NOT REPORTED IN PDF</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">This non-reported field has scope values consistent across all CC's</xsd:documentation>
                        <xsd:appinfo source="displayterm">
                           <dtyp:displayterm value="AdditionComplete">Addition</dtyp:displayterm>
                           <dtyp:displayterm value="ExistingAdditionAndAlteration">Addition Alteration</dtyp:displayterm>
                           <dtyp:displayterm value="ExistingAlteration">Alteration</dtyp:displayterm>
                           <dtyp:displayterm value="NewComplete">New</dtyp:displayterm>
                        </xsd:appinfo>
                     </xsd:annotation>
                     <xsd:simpleType>
                        <xsd:restriction base="comp:ProjectScopeNR">
                           <xsd:enumeration value="AdditionComplete"/>
                           <xsd:enumeration value="ExistingAdditionAndAlteration"/>
                           <xsd:enumeration value="ExistingAlteration"/>
                           <xsd:enumeration value="NewComplete"/>
                        </xsd:restriction>
                     </xsd:simpleType>
                  </xsd:element>
                  <xsd:element name="A09_IsProjectAllElectric" type="comp:IsProjectAllElectric">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">NOT REPORTED IN PDF</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">This non-reported field defines projects that are all electric</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="A10_WholeBuildingOccupancy" maxOccurs="unbounded">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">NOT REPORTED IN PDF</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">This non-reported field that defines finer grained occupancy types in the project consistent across all CC's</xsd:documentation>
                     </xsd:annotation>
                     <xsd:simpleType>
                        <xsd:restriction base="com:WholeBuildingOccupancy">
                           <xsd:enumeration value="AllOthersBuildings"/>
                           <xsd:enumeration value="AuditoriumBuilding"/>
                           <xsd:enumeration value="ClassroomBuilding"/>
                           <xsd:enumeration value="CommercialIndustrialBuilding"/>
                           <xsd:enumeration value="ConventionCenterBuilding"/>
                           <xsd:enumeration value="DataCenterBuilding"/>
                           <xsd:enumeration value="FinancialInstitutionBuilding"/>
                           <xsd:enumeration value="GroceryStoreBuilding"/>
                           <xsd:enumeration value="GymnasiumBuilding"/>
                           <xsd:enumeration value="HighRiseResidential"/>
                           <xsd:enumeration value="HospitalBuilding"/>
                           <xsd:enumeration value="HotelMotel"/>
                           <xsd:enumeration value="LibraryBuilding"/>
                           <xsd:enumeration value="MedicalClinicBuilding"/>
                           <xsd:enumeration value="OfficeBuilding"/>
                           <xsd:enumeration value="ParkingGarageBuilding"/>
                           <xsd:enumeration value="ReligiousFacilityBuilding"/>
                           <xsd:enumeration value="RelocatablePublicSchoolBuilding"/>
                           <xsd:enumeration value="RestaurantBuilding"/>
                           <xsd:enumeration value="RetailBuilding"/>
                           <xsd:enumeration value="SchoolBuilding"/>
                           <xsd:enumeration value="SportsArenaBuilding"/>
                           <xsd:enumeration value="SupportArea"/>
                           <xsd:enumeration value="TheaterBuilding"/>
                           <xsd:enumeration value="Warehouse"/>
                        </xsd:restriction>
                     </xsd:simpleType>
                  </xsd:element>
                  <xsd:element name="A11_GasType" type="com:GasType">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">NOT REPORTED IN PDF</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">This non-reported field that defines whether project has access to Natural Gas, Propane or no gas</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
         <xsd:element name="Section_B" minOccurs="1" maxOccurs="1">
            <xsd:annotation>
               <xsd:documentation source="FieldText">
                  <d:b>B. PROJECT SCOPE</d:b>
               </xsd:documentation>
               <xsd:documentation source="MinOccurs">This section is required.</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="B_BeginNote1" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"/>
                        <xsd:documentation source="AdditionalRequirements">
                           <d:i>Based on project information provided in Table A, Table B indicates which commissioning related requirements apply per  
                              <d:t24refnolink/>120.8.  Table B is not editable by the user.</d:i>
                        </xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="B_Subheading" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"/>
                        <xsd:documentation source="AdditionalRequirements">
                           <d:b>Commissioning Requirements per 
                              <d:t24refnolink/>120.8</d:b>
                        </xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="B01" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Table F: Design Review Kickoff</xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">The design review kickoff meeting establishes who will play the role of the design reviewer, the project schedule and identify owner's requirements.  This meeting should be conducted during schematic design.</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">This  applies to all projects and is never grayed out.</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="B01Ref" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"/>
                        <xsd:documentation source="AdditionalRequirements">
                           <d:t24refnolink/>120.8(d)1 and 
                           <d:t24refnolink/>120.8(d)2</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">This contains one or more  references and is displayed in the cell between the FieldText and AdditionalRequirements documentation elements</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="B02_RequiredOPR" type="comp:RequiredOPR">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Table G: Owner's Project Requirements (OPR)</xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">The owner's project requirements establish the owner's goals, requirements and expectations for everything related to energy consumption and operation.  This should be completed during schematic design.</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">If A05_ConditionedFloorAreaRangeNR != LT10000Ft2 result = true and  AdditionalRequirements is displayed;  Else result = false display term BlankGrayedOut to indicate row is blank and grayed out.</xsd:documentation>
                        <xsd:appinfo source="displayterm">
