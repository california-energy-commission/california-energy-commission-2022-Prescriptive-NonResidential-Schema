                                 <xsd:documentation source="CalculationsAndRules">User enters description of goals for this category</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                           <xsd:element name="F14_GoalsEnvelope" type="comp:GoalsEnvelope">
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText">Envelope  Goals</xsd:documentation>
                                 <xsd:documentation source="CalculationsAndRules">User enters description of goals for this category</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
