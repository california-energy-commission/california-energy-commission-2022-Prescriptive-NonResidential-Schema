                        <xsd:documentation source="FieldText">Design Development</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">User enters the date this stage starts in the format yyyy-mm-dd</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
