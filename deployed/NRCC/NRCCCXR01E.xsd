                        <xsd:documentation source="FieldText">Building Turnover</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">User enters the date this stage is completed in the format yyyy-mm-dd</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="ProjectGoalsEE" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"/>
                        <xsd:documentation source="CalculationsAndRules">If A05_ConditionedFloorAreaRangeNR == LT10000Ft2   this subsection is required; Else user is instructed to proceed to section G</xsd:documentation>
                     </xsd:annotation>
                     <xsd:complexType>
                        <xsd:sequence>
                           <xsd:element name="F10toF20Subheading" minOccurs="0">
