                                 <xsd:documentation source="FieldText">Operational Costs</xsd:documentation>
                                 <xsd:documentation source="CalculationsAndRules">User enters description of goals for this category</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                           <xsd:element name="F11_GoalsBuildingLifespan" type="comp:GoalsBuildingLifespan">
