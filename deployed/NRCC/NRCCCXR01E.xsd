                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="F05b_SchematicDesignEndDate" type="comp:SchematicDesignEndDate">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Schematic Design</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">User enters the date this stage is completed in the format yyyy-mm-dd</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="F06a_DesignDevelopmentBeginDate" type="comp:DesignDevelopmentBeginDate">
