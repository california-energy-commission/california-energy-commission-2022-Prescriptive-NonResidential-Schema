         <xsd:element name="Section_F" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation source="FieldText">
                  <d:b>F. DESIGN REVIEW KICKOFF MEETING</d:b>
               </xsd:documentation>
               <xsd:documentation source="MinOccurs">This section is required when A02StopMessage And A0203StopMessage are false.</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="F_BeginNote1" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"/>
                        <xsd:documentation source="AdditionalRequirements">
                           <d:i>This table indicates that the design reviewer meets the qualification requirements per Title 24, Part 1 Section 10-103(a)1 and demonstrates compliance with design review kickoff requirements per 
                              <d:t24refnolink/>120.8(d)2.  This meeting should occur during the Schematic Design phase of the project. </d:i>
                        </xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="F_ColumnHeading01To09" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"/>
                        <xsd:documentation source="AdditionalRequirements">
                           <d:b>Design Review Kickoff Meeting Details</d:b>
                        </xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="F01_MeetingDate" type="comp:MeetingDate">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Date of Design Review Kickoff Meeting</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">User input  Internal schema  format for date is yyyy-mm-dd</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="F02Subheading" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Meeting Attendees: (one person may play multiple roles)</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="F02a_OwnerRepAttending" type="comp:OwnerRepAttending" maxOccurs="1">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Owner/Facility Manager:</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">User input:  If user checks the box,  F02a_OwnerRepAttending = true and F02a_PersonFullName is required; Else F02a_OwnerRepAttending = false</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="F02a_PersonFullName" type="comp:PersonFullName" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Owner/Facility Manager:</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">If F02a_OwnerRepAttending == false this field is not required and should be disabled, Else user must provide the name of the owner or facility manager attending the meeting</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="F02b_ProjectManagerAttending" type="comp:ProjectManagerAttending" maxOccurs="1">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Project Manager:</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">User input:  If user checks the box,  F02b_ProjectManagerAttending = true and F02b_PersonFullName is required Else F02b_ProjectManagerAttending = false</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="F02b_PersonFullName" type="comp:PersonFullName" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Project Manager:</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">If F02b_ProjectManagerAttending == false this field is not required and should be disabled, Else user must provide the name of the project manager attending the meeting</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="F02c_ContractorAttending" type="comp:ContractorAttending" maxOccurs="1">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Contractor:</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">User input:  If user checks the box,  F02c_ContractorAttending = true and F02c_PersonFullName is required Else F02c_ContractorAttending = false</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="F02c_PersonFullName" type="comp:PersonFullName" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Contractor:</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">If F02c_ContractorAttending == false this field is not required and should be disabled, Else user must provide the name of the contractor attending the meeting</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="F02d_CommissioningProviderAttending" type="comp:CommissioningProviderAttending" maxOccurs="1">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Commissioning Provider:</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">User input:  If user checks the box,  F02d_CommissioningProviderAttending = true and F02d_PersonFullName is required Else F02d_CommissioningProviderAttending = false</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="F02d_PersonFullName" type="comp:PersonFullName" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Commissioning Provider:</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">If F02d_CommissioningProviderAttending == false this field is not required and should be disabled, Else user must provide the name of the commissioning provider attending the meeting</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="F02e_DesignReviewerAttending" type="comp:DesignReviewerAttending" maxOccurs="1">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Design Reviewer(s)</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">User input:  If user checks the box,  F02e_DesignReviewerAttending = true and F02e_PersonFullName is required Else F02e_DesignReviewerAttending = false</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="F02e_PersonFullName" type="comp:PersonFullName" minOccurs="0" maxOccurs="unbounded">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Design Reviewer(s)</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">If F02e_DesignReviewerAttending == false this field is not required and should be disabled, Else user must provide the name(s) of the design  reviewer(s) attending the meeting</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="F02f_DesignArchitectEngineerAttending" type="comp:DesignArchitectEngineerAttending" maxOccurs="1">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Design Architect/ Engineer(s):</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">User input:  If user checks the box,  F02f_DesignArchitectEngineerAttending = true and F02f_PersonFullName is required Else F02f_DesignArchitectEngineerAttending = false</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="F02f_PersonFullName" type="comp:PersonFullName" minOccurs="0" maxOccurs="unbounded">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Design Architect/ Engineer(s):</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">If F02f_DesignArchitectEngineerAttending == false this field is not required and should be disabled, Else user must provide the name(s) of the design architect and or engineer(s) attending the meeting</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="F02g_AcceptanceTestTechAttending" type="comp:AcceptanceTestTechAttending" maxOccurs="1">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Certified Acceptance Test Tech(s):</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">User input:  If user checks the box,  F02g_AcceptanceTestTechAttending = true and F02g_PersonFullName is required Else F02g_AcceptanceTestTechAttending = false</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="F02g_PersonFullName" type="comp:PersonFullName" minOccurs="0" maxOccurs="unbounded">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Certified Acceptance Test Tech(s):</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">If F02g_AcceptanceTestTechAttending == false this field is not required and should be disabled, Else user must provide the name(s) of the certified acceptance test tech(s) attending the meeting</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="F02h_EnergyT24P6ConsultantAttending" type="comp:EnergyT24P6ConsultantAttending" maxOccurs="1">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Energy/ T24 Part 6 Consultant:</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">User input:  If user checks the box,  F02h_EnergyT24P6ConsultantAttending = true and F02h_PersonFullName is required Else F02h_EnergyT24P6ConsultantAttending = false</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="F02h_PersonFullName" type="comp:PersonFullName" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Energy/ T24 Part 6 Consultant:</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">If F02h_EnergyT24P6ConsultantAttending == false this field is not required and should be disabled, Else user must provide the name of the energy or Title 24 Part 6 consultant attending the meeting</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="F03Heading" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"/>
                        <xsd:documentation source="AdditionalRequirements">
                           <d:b>Design Reviewer Qualifications per Title 24 Part 1 Section 10-103(a)1</d:b>
                        </xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="F03Note1" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"/>
                        <xsd:documentation source="AdditionalRequirements">The design reviewer(s) must be licensed professional engineers or licensed architects, or licensed contractors representing services performed by or under the direct supervision of a licensed engineer or architect, as specified in the provisions of Division 3 of the Business and Professions Code.</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:choice maxOccurs="1">
                     <xsd:element name="F03_BldgLT10000Ft2DesignReviewerQualifies" type="comp:BldgLT10000Ft2DesignReviewerQualifies">
                        <xsd:annotation>
                           <xsd:documentation source="FieldText">Do the Design Reviewer(s) meet these qualifications?</xsd:documentation>
                           <xsd:documentation source="AdditionalRequirements">In addition, for buildings with 
                              <d:lt/> 10,000 ft<d:sup>2</d:sup>, the design reviewer(s) may be the engineer or architect of record.  The design reviewer(s) may also be a qualified in-house engineer or architect with no other project involvement or a third party engineer, architect or contractor.</xsd:documentation>
                           <xsd:documentation source="CalculationsAndRules">If A05_ConditionedFloorAreaRangeNR == LT10000Ft2  the correct option to display is F03_BldgLT10000Ft2DesignReviewerQualifies and associated text.  If user selects Yes,  F03_BldgLT10000Ft2DesignReviewerQualifies = true, Else F03_BldgLT10000Ft2DesignReviewerQualifies = false</xsd:documentation>
                        </xsd:annotation>
                     </xsd:element>
                     <xsd:element name="F03_Bldg10000ToLT50000Ft2DesignReviewerQualifies" type="comp:Bldg10000ToLT50000Ft2DesignReviewerQualifies">
                        <xsd:annotation>
                           <xsd:documentation source="FieldText">Do the Design Reviewer(s) meet these qualifications?</xsd:documentation>
                           <xsd:documentation source="AdditionalRequirements">In addition, for buildings with 
                              <d:gte/> 10,000 ft<d:sup>2</d:sup> but 
                              <d:lt/> 50,000<d:sup>2</d:sup>, the design reviewer(s) shall be a qualified in-house engineer or architect with no other project involvement or a third party engineer, architect, or contractor</xsd:documentation>
                           <xsd:documentation source="CalculationsAndRules">If A05_ConditionedFloorAreaRangeNR == 10000ToLT50000Ft2 the correct option to display is F03_Bldg10000To49999Ft2DesignReviewerQualifies and associated text.  If user selects Yes, F03_Bldg10000To49999Ft2DesignReviewerQualifies = true Else F03_Bldg10000To49999Ft2DesignReviewerQualifies = false</xsd:documentation>
                        </xsd:annotation>
                     </xsd:element>
                     <xsd:element name="F03_BldgGTE50000Ft2DesignReviewerQualifies" type="comp:BldgGTE50000Ft2DesignReviewerQualifies">
                        <xsd:annotation>
                           <xsd:documentation source="FieldText">Do the Design Reviewer(s) meet these qualifications?</xsd:documentation>
                           <xsd:documentation source="AdditionalRequirements">In addition, for buildings with 
                              <d:gte/> 50,000 ft<d:sup>2</d:sup>, or complex mechanical systems<d:sup>1</d:sup>, the design reviewer(s) shall be a third party design engineer, architect, or contractor.</xsd:documentation>
                           <xsd:documentation source="CalculationsAndRules">If A05_ConditionedFloorAreaRangeNR == GTE50000Ft2 Or A06_CommissioningHvacSystem == ComplexHvacSystem  the correct option to display is  F03_BldgGTE50000Ft2DesignReviewerQualifies and associated text.  If user selects Yes, F03_BldgGTE50000Ft2DesignReviewerQualifies = true Else F03_BldgGTE50000Ft2DesignReviewerQualifies  = false</xsd:documentation>
                        </xsd:annotation>
                     </xsd:element>
                  </xsd:choice>
                  <xsd:element name="F04_PersonFullName" type="comp:PersonFullName" minOccurs="1" maxOccurs="unbounded">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">The design reviewer(s) for this project will be:</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">User input</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="F04AfterFootnote" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"/>
                        <xsd:documentation source="AdditionalRequirements">
                           <d:i><d:sup>1</d:sup>FOOTNOTE: Complex Mechanical Systems are systems that include 1) fan systems each serving multiple thermostatically controlled zones, or 2) built-up air handler systems (non-unitary or non-packaged HVAC equipment), or 3) hydronic or steam heating systems, or 4) hydronic cooling systems. </d:i>
                        </xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">If A05_ConditionedFloorAreaRangeNR == GTE50000Ft2 Or A06_CommissioningHvacSystem == ComplexHvacSystem then display this footnote</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
