<?xml version="1.0" encoding="UTF-8"?>
<xsd:schema attributeFormDefault="unqualified" elementFormDefault="qualified" targetNamespace="http://www.lmonte.com/besm/NRCILTO01E" version="2022.0.000" xmlns="http://www.lmonte.com/besm/NRCILTO01E" xmlns:altova="http://www.altova.com/xml-schema-extensions" xmlns:bld="http://www.lmonte.com/besm/bld" xmlns:com="http://www.lmonte.com/besm/com" xmlns:comp="http://www.lmonte.com/besm/comp" xmlns:d="http://www.lmonte.com/besm/d" xmlns:dtyp="http://www.lmonte.com/besm/dtyp" xmlns:env="http://www.lmonte.com/besm/env" xmlns:hvac="http://www.lmonte.com/besm/hvac" xmlns:lit="http://www.lmonte.com/besm/lit" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
   <xsd:import namespace="http://www.lmonte.com/besm/bld" schemaLocation="../base/NrPresBuilding.xsd"/>
   <xsd:import namespace="http://www.lmonte.com/besm/com" schemaLocation="../base/NrPresCommon.xsd"/>
   <xsd:import namespace="http://www.lmonte.com/besm/comp" schemaLocation="../base/NrPresCompliance.xsd"/>
   <xsd:import namespace="http://www.lmonte.com/besm/dtyp" schemaLocation="../base/DataTypes.xsd"/>
   <xsd:import namespace="http://www.lmonte.com/besm/env" schemaLocation="../base/NrPresEnvelope.xsd"/>
   <xsd:import namespace="http://www.lmonte.com/besm/hvac" schemaLocation="../base/NrPresHvac.xsd"/>
   <xsd:import namespace="http://www.lmonte.com/besm/lit" schemaLocation="../base/NrPresLighting.xsd"/>
   <xsd:element name="ComplianceDocumentPackage">
      <xsd:complexType>
         <xsd:sequence>
            <xsd:element name="DocID" minOccurs="0">
               <xsd:complexType>
                  <xsd:attribute name="doc" type="comp:ComplianceDocumentTagNR" fixed="NRCILTO01E"/>
                  <xsd:attribute name="docType" type="comp:ComplianceDocumentTypeNR" fixed="CertificateInstallationNR"/>
                  <xsd:attribute name="docTitle" type="comp:ComplianceDocumentTitleNR" fixed="Outdoor Lighting"/>
               </xsd:complexType>
            </xsd:element>
            <xsd:element name="Payload" type="comp:Payload"/>
            <xsd:element name="DocumentData">
               <xsd:complexType>
                  <xsd:sequence>
                     <xsd:element name="Header" type="comp:HeaderNRCI"/>
                     <xsd:element ref="nRCILTO01E"/>
                     <xsd:element name="DocAuthor" type="comp:DocumentAuthor" maxOccurs="unbounded"/>
                     <xsd:element name="RespPerson" type="comp:ResponsiblePersonNRCI" maxOccurs="unbounded"/>
                     <xsd:element name="Footer" type="comp:FooterNRCC"/>
                     <xsd:element name="ComplianceID" minOccurs="0"/>
                  </xsd:sequence>
               </xsd:complexType>
            </xsd:element>
            <xsd:element name="Report" type="xsd:base64Binary" minOccurs="0"/>
         </xsd:sequence>
         <xsd:attribute name="revision" use="required" type="xsd:string" fixed="rev 20220901"/>
         <xsd:attribute name="doc" use="required" type="comp:ComplianceDocumentTagNR" fixed="NRCILTO01E"/>
      </xsd:complexType>
   </xsd:element>
   <xsd:complexType name="NRCILTO01E">
      <xsd:annotation>
         <xsd:documentation>Outdoor Lighting</xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="Section_A" minOccurs="1" maxOccurs="1">
            <xsd:annotation>
               <xsd:documentation source="FieldText">
                  <d:b>A. GENERAL INFORMATION</d:b>
               </xsd:documentation>
               <xsd:documentation source="MinOccurs">This section is required.</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="A_BeforeNote1" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"/>
                        <xsd:documentation source="AdditionalRequirements">
                           <d:i>This Certificate of Installation documents the installation of sign lighting features, materials, components, and manufactured devices required to demonstrate compliance with Title 24, Part 6 per  
                              <d:t24refnolink/>10.103(a)3 for nonresidential, hotel/motel and high-rise residential occupancies. </d:i>
                        </xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="A01_ProjectCity" type="comp:ProjectCity">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Project Location (city)</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">If CC-LTO parent, 
result = A01_ProjectCity
End If</xsd:documentation>
                        <xsd:appinfo source="CIsrc">
                           <dtyp:srcSchema form="CCLTO01E" usage="copy">A01_ProjectCity</dtyp:srcSchema>
                        </xsd:appinfo>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="A02_ProjectZipcode" type="comp:ProjectZipcode">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Zip Code</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">If CC-LTO parent, 
User input
End If</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="A03_PermitSetDate" type="comp:PermitSetDate">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Date of Permit Set used for construction</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">User input</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="A04_AuthorityHavingJurisdiction" type="comp:AuthorityHavingJurisdiction">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Authority Having Jurisdiction</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">User input</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="A05_PermitNumber" type="comp:PermitNumber">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Building Permit #</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">User input</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="A06_AsBuiltSetDate" type="comp:AsBuiltSetDate">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Date of As-built Set</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">User input</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="A07_PermitSetName" type="comp:PermitSetName" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">NOT REPORTED IN PDF</xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">Name of Permit Set used for construction</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">This data is not currently required but may be needed in the future</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="A08_AsBuiltSetName" type="comp:AsBuiltSetName" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">NOT REPORTED IN PDF</xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">Name of As-built Set</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">This data is not currently required but may be needed in the future</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
         <xsd:element name="Section_B" minOccurs="1">
            <xsd:annotation>
               <xsd:documentation source="FieldText">
                  <d:b>B. INSTALLER SCOPE</d:b>
               </xsd:documentation>
               <xsd:documentation source="MinOccurs">This section is required.</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="B_BeginNote1" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"/>
                        <xsd:documentation source="AdditionalRequirements">
                           <d:i>This table indicates the construction systems and materials documented on this Certificate of Installation. </d:i>
                        </xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="B01_ProjectHasLuminaires" type="comp:ProjectHasLuminaires">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Luminaires</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="B02_ProjectHasControls" type="comp:ProjectHasControls">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Controls</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
         <xsd:element name="Section_C" minOccurs="1">
            <xsd:annotation>
               <xsd:documentation source="FieldText">
                  <d:b>C. COMPLIANCE RESULTS</d:b>
               </xsd:documentation>
               <xsd:documentation source="MinOccurs">This section is required.</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="C_BeginNote1" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"/>
                        <xsd:documentation source="AdditionalRequirements">
                           <d:i>This table indicates whether the as-built conditions documented in this form are equal or better than what was documented on the permitted Certificate of Compliance.  If the installation is not equal or better, Section 10-103(a)2B requires the Certificate of Compliance form to be revised accordingly to demonstrate compliance.</d:i>
                        </xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="C01_ComplianceResultCI" type="comp:ComplianceResultCI">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"/>
                        <xsd:documentation source="CalculationsAndRules">Calculated result:  
 If in any required tables or subsections in Section F 
  ComplianceResultNRCI == TableIncomplete
 result = FormNotComplete
 Else If ComplianceResultNRCI == MayNotComply
 result = MayNotComply
  
 Else If All required tables or subsections in Section F  have  a combination of ComplianceResultNRCI ==  ExactMatch or NotInScope
 result = ExactMatch
 Else If All required tables or subsections in Section F  have  a combination of ComplianceResultNRCI ==  EqualBetter or ExactMatch or NotInScope
 result = EqualBetter
 End If</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="C01Message_ComplianceResultMessageCI" type="comp:ComplianceResultMessageCI">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"/>
                        <xsd:documentation source="CalculationsAndRules">Calculated result:  
 If C01_ComplianceResultNRCI == ExactMatch Or EqualBetter
 result = ExactEqualBetterText
 Else If C01_ComplianceResultNRCI == MayNotComply
 result =  MayNotComplyText
 End If</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
         <xsd:element name="Section_D" minOccurs="1">
            <xsd:annotation>
               <xsd:documentation source="FieldText">
                  <d:b>D. EXCEPTIONAL CONDITIONS</d:b>
               </xsd:documentation>
               <xsd:documentation source="MinOccurs">This section is required.</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="D_BeginNote1" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"/>
                        <xsd:documentation source="AdditionalRequirements">
                           <d:i>This table is auto-filled with uneditable comments because of field conditions noted by the installer that may impact requirements documented on the Certificate of Compliance.</d:i>
                        </xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="D01_ExceptionalConditionMessageCILTOE" type="comp:ExceptionalConditionMessageCILTOE" minOccurs="0" maxOccurs="unbounded">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Exceptional Messages</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">Auto-filled with any system generated messages that explain exceptional conditions encountered in the the completed compliance document
If ExceptionalConditionMessageNRCILTOE == ""
result = NoExceptionNote
End If</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
         <xsd:element name="Section_E" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation source="FieldText">
                  <d:b>E. INSTALLER NOTES</d:b>
               </xsd:documentation>
               <xsd:documentation source="CalculationsAndRules">This section is required if there are  comments from the  installer to the Authority Having Jurisdiction</xsd:documentation>
               <xsd:documentation source="MinOccurs">If this section doesn't apply, do not display anything for this section</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="E_BeginNote1" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"/>
                        <xsd:documentation source="AdditionalRequirements">
                           <d:i>This table includes remarks made by the installer to the Authority Having Jurisdiction.</d:i>
                        </xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="E01_CommentsToPermitAuthority" type="comp:CommentsToPermitAuthority" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"/>
                     </xsd:annotation>
                  </xsd:element>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
         <xsd:element name="Section_F" minOccurs="1">
            <xsd:annotation>
               <xsd:documentation source="FieldText">
                  <d:b>F. INSTALLATION DETAILS</d:b>
               </xsd:documentation>
               <xsd:documentation source="MinOccurs">This section is required.</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="F_BeginNote1" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"/>
                        <xsd:documentation source="AdditionalRequirements">
                           <d:i>The following tables indicate performance requirements as documented on the permitted Certificate of Compliance for all systems and components included in Table B. Installer Scope.  Also included are the as-built conditions documented by the installer/documentation author. </d:i>
                        </xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="F_RowLabelTopCell" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Per C of C</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">This is the text displayed at the left end of a row in the top cell for all tables in Section F.</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="F_RowLabelBottomCell" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">As-built Conditions</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">This is the text displayed at the left end of a row in the bottom cell for all tables in Section F</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="TableLum" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">
                           <d:b>Outdoor Lighting Fixture Schedule for Nonresidential Buildings, Parking Garages and Common Service Areas in Multifamily Buildings</d:b>
                        </xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">This table is required if B01_ProjectHasLuminaires  == true</xsd:documentation>
                     </xsd:annotation>
                     <xsd:complexType>
                        <xsd:sequence>
                           <xsd:element name="Row" minOccurs="1" maxOccurs="unbounded">
                              <xsd:complexType>
                                 <xsd:sequence>
                                    <xsd:element name="Lum01cc_LuminaireName" type="lit:LuminaireName">
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Name or Item Tag</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">If CCLTOE parent
If F01_LuminaireName IsPresent
result = F01_LuminaireName
Else result = BlankGrayedOut
End If</xsd:documentation>
                                          <xsd:appinfo source="CIsrc">
                                             <dtyp:srcSchema form="CCLTO01E" usage="copy">F01_LuminaireName</dtyp:srcSchema>
                                          </xsd:appinfo>
                                       </xsd:annotation>
                                       <xsd:simpleType>
                                          <xsd:union memberTypes="lit:LuminaireName comp:NotApplicableMessage"/>
                                       </xsd:simpleType>
                                    </xsd:element>
                                    <xsd:element name="Lum01_InstalledSameAsCC" type="comp:InstalledSameAsCC" minOccurs="0">
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">NOT REPORTED IN PDF</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">User selects true if installed is the same as CC value  Or false if installed is different than CC value</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="Lum02_ScopeIncludesFixture" type="comp:ScopeIncludesFixture">
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Fixture Within Scope?</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">User input
 If result == false  Lum03cc-Lum08cc and Lum03-Lum09 As-built cells are blank and grayed out
And D01_ExceptionalConditionMessageNRCILTOE += LumNotInScope  
End If</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="Lum03cc_LuminaireDescription">
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Luminaire Description</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">If CCLTOE parent
If F02_LuminaireDescription IsPresent
result = F02_LuminaireDescription
Else result = BlankGrayedOut
End If</xsd:documentation>
                                          <xsd:appinfo source="CIsrc">
                                             <dtyp:srcSchema form="CCLTO01E" usage="copy">F02_LuminaireDescription</dtyp:srcSchema>
                                          </xsd:appinfo>
                                       </xsd:annotation>
                                       <xsd:simpleType>
                                          <xsd:union memberTypes="lit:LuminaireDescription comp:NotApplicableMessage"/>
                                       </xsd:simpleType>
                                    </xsd:element>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="Lum03_LuminaireDescription" type="lit:LuminaireDescription">
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Luminaire Description</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">If Lum02_ScopeIncludesFixture == true 
If Lum01_InstalledSameasCC == true
If Lum03cc_LuminaireDescription == BlankGrayedOut
User Input
Else result = Lum03cc_LuminaireDescription
Else 
User input 
End If
End If</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="Lum03_NotApplicableMessage">
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Luminaire Description</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">If Lum02_ScopeIncludesFixture == false
 result = BlankGrayedOut
 End If</xsd:documentation>
                                          </xsd:annotation>
                                          <xsd:simpleType>
                                             <xsd:restriction base="comp:NotApplicableMessage">
                                                <xsd:enumeration value="BlankGrayedOut"/>
                                             </xsd:restriction>
                                          </xsd:simpleType>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:element name="Lum04cc_IsLuminaireLinear">
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Fixture Type</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">If CCLTOE parent
If F02a_IsLuminaireLinear IsPresent
result = F02a_IsLuminaireLinear
Else result = SeePlans
End If</xsd:documentation>
                                          <xsd:appinfo source="displayterm">
                                             <dtyp:displayterm value="true">Linear</dtyp:displayterm>
                                             <dtyp:displayterm value="false">Non-linear</dtyp:displayterm>
                                          </xsd:appinfo>
                                          <xsd:appinfo source="CIsrc">
                                             <dtyp:srcSchema form="CCLTO01E" usage="copy">F02a_IsLuminaireLinear</dtyp:srcSchema>
                                          </xsd:appinfo>
                                       </xsd:annotation>
                                       <xsd:simpleType>
                                          <xsd:union memberTypes="comp:IsLuminaireLinear comp:SeePlansMessage"/>
                                       </xsd:simpleType>
                                    </xsd:element>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="Lum04_IsLuminaireLinear" type="comp:IsLuminaireLinear">
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Fixture Type</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">If Lum02_ScopeIncludesFixture == true
If Lum01_InstalledSameasCC == true
If Lum04cc_IsLuminaireLinear == SeePlans
User Input
Else result = Lum04cc_IsLuminaireLinear
Else 
User input
End If
End If</xsd:documentation>
                                             <xsd:appinfo source="displayterm">
                                                <dtyp:displayterm value="true">Linear</dtyp:displayterm>
                                                <dtyp:displayterm value="false">Non-linear</dtyp:displayterm>
                                             </xsd:appinfo>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="Lum04_NotApplicableMessage">
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Fixture Type</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">If Lum02_ScopeIncludesFixture == false
 result = BlankGrayedOut
 End If</xsd:documentation>
                                          </xsd:annotation>
                                          <xsd:simpleType>
                                             <xsd:restriction base="comp:NotApplicableMessage">
                                                <xsd:enumeration value="BlankGrayedOut"/>
                                             </xsd:restriction>
                                          </xsd:simpleType>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="Lum05cc_LuminairePower">
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Rated Watts per Luminaire<d:sup>1</d:sup>
                                             </xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">If CCLTOE parent
If F03_LuminairePower IsPresent
result = F03_LuminairePower
Else result = SeePlans
End If</xsd:documentation>
                                             <xsd:appinfo source="CIsrc">
                                                <dtyp:srcSchema form="CCLTO01E" usage="copy">F03_LuminairePower</dtyp:srcSchema>
                                             </xsd:appinfo>
                                          </xsd:annotation>
                                          <xsd:simpleType>
                                             <xsd:union memberTypes="lit:LuminairePower comp:SeePlansMessage"/>
                                          </xsd:simpleType>
                                       </xsd:element>
                                       <xsd:element name="Lum05cc_LuminaireWattsPerLF">
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Rated Watts per Luminaire<d:sup>1</d:sup>
                                             </xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">If CCLTOE parent
If F03_LuminaireWattsPerLF IsPresent
result = F03_LuminaireWattsPerLF
Else result = SeePlans
End If</xsd:documentation>
                                             <xsd:appinfo source="CIsrc">
                                                <dtyp:srcSchema form="CCLTO01E" usage="copy">F03_LuminaireWattsPerLF</dtyp:srcSchema>
                                             </xsd:appinfo>
                                          </xsd:annotation>
                                          <xsd:simpleType>
                                             <xsd:union memberTypes="lit:LuminaireWattsPerLF comp:SeePlansMessage"/>
                                          </xsd:simpleType>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="Lum05_LuminairePower" type="lit:LuminairePower">
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Rated Watts per Luminaire<d:sup>1</d:sup>
                                             </xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">If Lum02_ScopeIncludesFixture == true and Lum04_IsLuminaireLinear == false
If Lum01_InstalledSameasCC == true
If Lum05cc_LuminairePower == SeePlans
User Input
Else result = Lum05cc_LuminairePower 
Else If Lum05cc_LuminairePower IsPresent
User input
End If
End If
End If</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="Lum05_LuminaireWattsPerLF" type="lit:LuminaireWattsPerLF">
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Rated Watts per Luminaire<d:sup>1</d:sup>
                                             </xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">If Lum02_ScopeIncludesFixture == true and Lum04_IsLuminaireLinear == true
If Lum01_InstalledSameasCC == true
If Lum05cc_LuminaireWattsPerLF == SeePlans
User Input
Else result = Lum05cc_LuminaireWattsPerLF
Else
User input
End If
End If
End If</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="Lum05_NotApplicableMessage">
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Rated Watts per Luminaire<d:sup>1</d:sup>
                                             </xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">If Lum02_ScopeIncludesFixture == false
 result = BlankGrayedOut
 End If</xsd:documentation>
                                          </xsd:annotation>
                                          <xsd:simpleType>
                                             <xsd:restriction base="comp:NotApplicableMessage">
                                                <xsd:enumeration value="BlankGrayedOut"/>
                                             </xsd:restriction>
                                          </xsd:simpleType>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="Lum06cc_LuminaireCountByType">
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Total Number of Luminaires</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">If CCLTOE parent
If F05_LuminaireCountByType IsPresent
result = F05_LuminaireCountByType
Else result == SeePlans
End If</xsd:documentation>
                                             <xsd:appinfo source="CIsrc">
                                                <dtyp:srcSchema form="CCLTO01E" usage="copy">F05_LuminaireCountByType</dtyp:srcSchema>
                                             </xsd:appinfo>
                                          </xsd:annotation>
                                          <xsd:simpleType>
                                             <xsd:union memberTypes="lit:LuminaireCountByType comp:SeePlansMessage"/>
                                          </xsd:simpleType>
                                       </xsd:element>
                                       <xsd:element name="Lum06cc_LuminairesTotalLF">
