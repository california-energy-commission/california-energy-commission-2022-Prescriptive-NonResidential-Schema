<?xml version="1.0" encoding="UTF-8"?>
<xsd:schema attributeFormDefault="unqualified" elementFormDefault="qualified" targetNamespace="http://www.lmonte.com/besm/NRCILTI01E" version="2022.0.000" xmlns="http://www.lmonte.com/besm/NRCILTI01E" xmlns:altova="http://www.altova.com/xml-schema-extensions" xmlns:bld="http://www.lmonte.com/besm/bld" xmlns:com="http://www.lmonte.com/besm/com" xmlns:comp="http://www.lmonte.com/besm/comp" xmlns:d="http://www.lmonte.com/besm/d" xmlns:dtyp="http://www.lmonte.com/besm/dtyp" xmlns:env="http://www.lmonte.com/besm/env" xmlns:hvac="http://www.lmonte.com/besm/hvac" xmlns:lit="http://www.lmonte.com/besm/lit" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
   <xsd:import namespace="http://www.lmonte.com/besm/bld" schemaLocation="../base/NrPresBuilding.xsd"/>
   <xsd:import namespace="http://www.lmonte.com/besm/com" schemaLocation="../base/NrPresCommon.xsd"/>
   <xsd:import namespace="http://www.lmonte.com/besm/comp" schemaLocation="../base/NrPresCompliance.xsd"/>
   <xsd:import namespace="http://www.lmonte.com/besm/dtyp" schemaLocation="../base/DataTypes.xsd"/>
   <xsd:import namespace="http://www.lmonte.com/besm/env" schemaLocation="../base/NrPresEnvelope.xsd"/>
   <xsd:import namespace="http://www.lmonte.com/besm/hvac" schemaLocation="../base/NrPresHvac.xsd"/>
   <xsd:import namespace="http://www.lmonte.com/besm/lit" schemaLocation="../base/NrPresLighting.xsd"/>
   <xsd:element name="ComplianceDocumentPackage">
      <xsd:complexType>
         <xsd:sequence>
            <xsd:element name="DocID" minOccurs="0">
               <xsd:complexType>
                  <xsd:attribute name="doc" type="comp:ComplianceDocumentTagNR" fixed="NRCILTI01E"/>
                  <xsd:attribute name="docType" type="comp:ComplianceDocumentTypeNR" fixed="CertificateInstallationNR"/>
                  <xsd:attribute name="docTitle" type="comp:ComplianceDocumentTitleNR" fixed="Indoor Lighting"/>
               </xsd:complexType>
            </xsd:element>
            <xsd:element name="Payload" type="comp:Payload"/>
            <xsd:element name="DocumentData">
               <xsd:complexType>
                  <xsd:sequence>
                     <xsd:element name="Header" type="comp:HeaderNRCI"/>
                     <xsd:element ref="nRCILTI01E"/>
                     <xsd:element name="DocAuthor" type="comp:DocumentAuthor" maxOccurs="unbounded"/>
                     <xsd:element name="RespPerson" type="comp:ResponsiblePersonNRCI" maxOccurs="unbounded"/>
                     <xsd:element name="Footer" type="comp:FooterNRCC"/>
                     <xsd:element name="ComplianceID" minOccurs="0"/>
                  </xsd:sequence>
               </xsd:complexType>
            </xsd:element>
            <xsd:element name="Report" type="xsd:base64Binary" minOccurs="0"/>
         </xsd:sequence>
         <xsd:attribute name="revision" use="required" type="xsd:string" fixed="rev 20220901"/>
         <xsd:attribute name="doc" use="required" type="comp:ComplianceDocumentTagNR" fixed="NRCILTI01E"/>
      </xsd:complexType>
   </xsd:element>
   <xsd:complexType name="NRCILTI01E">
      <xsd:annotation>
         <xsd:documentation>Indoor Lighting</xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="Section_A" minOccurs="1" maxOccurs="1">
            <xsd:annotation>
               <xsd:documentation source="FieldText">
                  <d:b>A. GENERAL INFORMATION</d:b>
               </xsd:documentation>
               <xsd:documentation source="MinOccurs">This section is required.</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="A_BeforeNote1" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"/>
                        <xsd:documentation source="AdditionalRequirements">
                           <d:i>This Certificate of Installation documents the installation of indoor lighting features, materials, components, and manufactured devices required to demonstrate compliance with Title 24, Part 6 per  
                              <d:t24refnolink/>10.103(a)3 for low-rise residential and low-rise mixed-use occupancies.</d:i>
                        </xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="A01_ProjectCity" type="comp:ProjectCity">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Project Location (city)</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">If CC-LTI parent, 
result = A01_ProjectCity
Else If  CC-PRF parent, 
result = Info01_ProjectCity
End If</xsd:documentation>
                        <xsd:appinfo source="CIsrc">
                           <dtyp:srcSchema form="CCLTI01E" usage="copy">A01_ProjectCity</dtyp:srcSchema>
                           <dtyp:srcSchema form="CCPRF01E" usage="copy">Info04_ProjectCity</dtyp:srcSchema>
                        </xsd:appinfo>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="A02_ProjectZipcode" type="comp:ProjectZipcode">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Zip Code</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">If CC-LTI parent, 
User input
Else If  CC-PRF parent, 
result = Info02_ProjectZipcode
End If</xsd:documentation>
                        <xsd:appinfo source="CIsrc">
                           <dtyp:srcSchema form="CCPRF01E" usage="copy">Info06_ProjectZipcode</dtyp:srcSchema>
                        </xsd:appinfo>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="A03_PermitSetDate" type="comp:PermitSetDate">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Date of Permit Set used for construction</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">User input</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="A04_AuthorityHavingJurisdiction" type="comp:AuthorityHavingJurisdiction">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Authority Having Jurisdiction</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">User input</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="A05_PermitNumber" type="comp:PermitNumber">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Building Permit #</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">User input</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="A06_AsBuiltSetDate" type="comp:AsBuiltSetDate">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Date of As-built Set</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">User input</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="A07_PermitSetName" type="comp:PermitSetName" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">NOT REPORTED IN PDF</xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">Name of Permit Set used for construction</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">This data is not currently required but may be needed in the future</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="A08_AsBuiltSetName" type="comp:AsBuiltSetName" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">NOT REPORTED IN PDF</xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">Name of As-built Set</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">This data is not currently required but may be needed in the future</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
         <xsd:element name="Section_B" minOccurs="1" maxOccurs="1">
            <xsd:annotation>
               <xsd:documentation source="FieldText">
                  <d:b>B. INSTALLER SCOPE</d:b>
               </xsd:documentation>
               <xsd:documentation source="MinOccurs">This section is required.</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="B_BeginNote1" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"/>
                        <xsd:documentation source="AdditionalRequirements">
                           <d:i>This table indicates the construction systems and materials documented on this Certificate of Installation. </d:i>
                        </xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="B01Subheading" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Luminaires</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="B01a_ProjectHasGenLight" type="comp:ProjectHasGenLight">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">General Lighting</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">User input</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="B01b_ProjectHasDecorLight" type="comp:ProjectHasDecorLight">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Decorative/ Accent</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">User input</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="B02Subheading" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Space Types</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="B02a_ProjectHasCondSpace" type="comp:ProjectHasCondSpace">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Conditioned</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">User input</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="B02b_ProjectHasUncondSpace" type="comp:ProjectHasUncondSpace">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Unconditioned</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">User input</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="B03Subheading" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Controls<d:sup>1</d:sup>
                        </xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="B03a_ProjectHasTimerControl" type="comp:ProjectHasTimerControl">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Time-switch</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">User input</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="B03b_ProjectHasDaylightControl" type="comp:ProjectHasDaylightControl">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Daylighting</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">User input</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="B03c_ProjectHasAreaControl" type="comp:ProjectHasAreaControl">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Area Controls</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">User input</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="B03d_ProjectHasMultilevelControl" type="comp:ProjectHasMultilevelControl">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Multi-Level</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">User input</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="B03e_ProjectHasSensorControl" type="comp:ProjectHasSensorControl">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Occupant Sensors</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">User input</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="B03f_ProjectHasInterlockControl" type="comp:ProjectHasInterlockControl">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Interlocked Systems</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">User input</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="B03g_ProjectHasDemandControl" type="comp:ProjectHasDemandControl">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Demand Response</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">User input</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="B03h_ProjectHasTuningControl" type="comp:ProjectHasTuningControl">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Institutional Tuning</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">User input</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="B03i_ProjectHasDimmerControl" type="comp:ProjectHasDimmerControl">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Dimmers</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">User input</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="B03j_ProjectHasShutOffControl" type="comp:ProjectHasShutOffControl">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Shut-off</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">User input</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="B03k_ProjectHasVideoControl" type="comp:ProjectHasVideoControl">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Video Conferencing Studio Controls</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">User input</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="BEndNote" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"/>
                        <xsd:documentation source="AdditionalRequirements">
                           <d:i><d:sup>1</d:sup>FOOTNOTES: If an Energy Managment Control System (EMCS) or Lighting Contol System is installed to function as another type of lighting control, the type of lighting control for which it functions is indicated.  Example: if the EMCS is acting as a time-switch control, "Time-switch" is checked here.   </d:i>
                        </xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
         <xsd:element name="Section_C" minOccurs="1">
            <xsd:annotation>
               <xsd:documentation source="FieldText">
                  <d:b>C. COMPLIANCE RESULTS</d:b>
               </xsd:documentation>
               <xsd:documentation source="MinOccurs">This section is required.</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="C_BeginNote1" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"/>
                        <xsd:documentation source="AdditionalRequirements">
                           <d:i>This table indicates whether the as-built conditions documented in this form are equal or better than what was documented on the permitted Certificate of Compliance.  If the installation is not equal or better, Section 10-103(a)2B requires the Certificate of Compliance form to be revised accordingly to demonstrate compliance.</d:i>
                        </xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="C01_ComplianceResultCI" type="comp:ComplianceResultCI">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"/>
                        <xsd:documentation source="CalculationsAndRules">Calculated result:  
 If in any required tables or subsections in Section F 
  ComplianceResultNRCI ==  TableIncomplete
 result = FormNotComplete
 Else If ComplianceResultNRCI == MayNotComply
 result = MayNotComply
  
Else If All required tables or subsections in Section F  have  a combination of ComplianceResultNRCI ==  ExactMatch or NotInScope
 result = ExactMatch
 Else If All required tables or subsections in Section F  have  a combination of ComplianceResultNRCI ==  EqualBetter or ExactMatch or NotInScope
 result = EqualBetter
 End If</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="C01Message_ComplianceResultMessageCI" type="comp:ComplianceResultMessageCI">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"/>
                        <xsd:documentation source="CalculationsAndRules">Calculated result:  
 If C01_ComplianceResultNRCI == ExactMatch Or EqualBetter
 result = ExactEqualBetterText
 Else If C01_ComplianceResultNRCI == MayNotComply
 result =  MayNotComplyText
 End If</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
         <xsd:element name="Section_D" minOccurs="1">
            <xsd:annotation>
               <xsd:documentation source="FieldText">
                  <d:b>D. EXCEPTIONAL CONDITIONS</d:b>
               </xsd:documentation>
               <xsd:documentation source="MinOccurs">This section is required.</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="D_BeginNote1" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"/>
                        <xsd:documentation source="AdditionalRequirements">
                           <d:i>This table is auto-filled with uneditable comments because of field conditions noted by the installer that may impact requirements documented on the Certificate of Compliance.</d:i>
                        </xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="D01_ExceptionalConditionMessageCILTIE" type="comp:ExceptionalConditionMessageCILTIE" minOccurs="0" maxOccurs="unbounded">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Exceptional Messages</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">Auto-filled with any system generated messages that explain exceptional conditions encountered in the the completed compliance document
If ExceptionalConditionMessageNRCILTIE == ""
result = NoExceptionNote
End If</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
         <xsd:element name="Section_E" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation source="FieldText">
                  <d:b>E. INSTALLER NOTES</d:b>
               </xsd:documentation>
               <xsd:documentation source="CalculationsAndRules">This section is required if there are  comments from the  installer to the Authority Having Jurisdiction</xsd:documentation>
               <xsd:documentation source="MinOccurs">If this section doesn't apply, do not display anything for this section</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="E_BeginNote1" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"/>
                        <xsd:documentation source="AdditionalRequirements">
                           <d:i>This table includes remarks made by the installer to the Authority Having Jurisdiction.</d:i>
                        </xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="E01_CommentsToPermitAuthority" type="comp:CommentsToPermitAuthority" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"/>
                     </xsd:annotation>
                  </xsd:element>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
         <xsd:element name="Section_F" minOccurs="1">
            <xsd:annotation>
               <xsd:documentation source="FieldText">
                  <d:b>F. INSTALLATION DETAILS</d:b>
               </xsd:documentation>
               <xsd:documentation source="MinOccurs">This section is required.</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="F_BeginNote1" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"/>
                        <xsd:documentation source="AdditionalRequirements">
                           <d:i>The following tables indicate performance requirements as documented on the permitted Certificate of Compliance for all systems and components included in Table B. Installer Scope.  Also included are the as-built conditions documented by the installer/documentation author. </d:i>
                        </xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="F_RowLabelTopCell" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Per C of C</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">This is the text displayed at the left end of a row in the top cell for all tables in Section F.</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="F_RowLabelBottomCell" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">As-built Conditions</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">This is the text displayed at the left end of a row in the bottom cell for all tables in Section F</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="TableLum" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">
                           <d:b>Indoor Lighting Fixture Schedule for Spaces Other Than Dwelling Units/ Hotel Rooms/ Motel Rooms</d:b>
                        </xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">This table is required if B01a_ProjectHasGenLight Or B01b_ProjectHasDecorLight  == true</xsd:documentation>
                     </xsd:annotation>
                     <xsd:complexType>
                        <xsd:sequence>
                           <xsd:element name="Row" minOccurs="1" maxOccurs="unbounded">
                              <xsd:complexType>
                                 <xsd:sequence>
                                    <xsd:element name="Lum01cc_LuminaireName" type="lit:LuminaireName">
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Name or Item Tag</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">If CCLTIE parent
 result loaded from Fc01_LuminaireName Or Fu01_LuminaireName Or Q09_LuminaireName
 Else If CCPRFE parent
 result loaded from LtgSched01_LuminaireName
End If</xsd:documentation>
                                          <xsd:appinfo source="CIsrc">
                                             <dtyp:srcSchema form="CCLTI01E" usage="copy">Fc01_LuminaireName</dtyp:srcSchema>
                                             <dtyp:srcSchema form="CCLTI01E" usage="copy">Fu01_LuminaireName</dtyp:srcSchema>
                                             <dtyp:srcSchema form="CCLTI01E" usage="copy">Q09_LuminaireName</dtyp:srcSchema>
                                             <dtyp:srcSchema form="CCPRF01E" usage="copy">LtgSched01_LuminaireName</dtyp:srcSchema>
                                          </xsd:appinfo>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="Lum01_ScopeIncludesFixture" type="comp:ScopeIncludesFixture">
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Fixture Within Scope?</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">User selects from true value display term Yes and false value display term No, by others
 If result == false  Lum03cc-Lum12cc and Lum03-Lum12 As-built cells are blank and grayed out  
And D01_ExceptionalConditionMessageNRCILTIE += LumNotInScope  
End If</xsd:documentation>
                                          <xsd:appinfo source="displayterm">
                                             <dtyp:displayterm value="false">No, by others</dtyp:displayterm>
                                          </xsd:appinfo>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="Lum01_InstalledSameAsCC" type="comp:InstalledSameAsCC" minOccurs="0">
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">NOT REPORTED IN PDF</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">User selects true if installed is the same as CC value  Or false if installed is different than CC value</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="Lum03cc_LuminaireDescription" type="lit:LuminaireDescription">
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Luminaire Description</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">If CCLTIE parent
 If Lum01cc_LuminaireName is loaded from  Fc01_LuminaireName
 result = Fc02_LuminaireDescription
 Else If Lum01cc_LuminaireName is loaded from  Fu01_LuminaireName
 result = Fu02_LuminaireDescription
 Else If Lum01cc_LuminaireName is loaded from  Q09_LuminaireName
 result = Q10_LuminaireDescription
 
 End If
 Else If CCPRFE parent
 result = LtgSched02_LuminaireDescription
 End If</xsd:documentation>
                                             <xsd:appinfo source="CIsrc">
                                                <dtyp:srcSchema form="CCLTI01E" usage="copy">Fc02_LuminaireDescription</dtyp:srcSchema>
                                                <dtyp:srcSchema form="CCLTI01E" usage="copy">Fu02_LuminaireDescription</dtyp:srcSchema>
                                                <dtyp:srcSchema form="CCLTI01E" usage="copy">Q10_LuminaireDescription</dtyp:srcSchema>
                                                <dtyp:srcSchema form="CCPRF01E" usage="copy">LtgSched02_LuminaireDescription</dtyp:srcSchema>
                                             </xsd:appinfo>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="Lum03cc_NotApplicableMessage">
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Luminaire Description</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">If CCPRFE parent And LtgSched02_LuminaireDescription NotPresent
 result = BlankGrayedOut
 End If</xsd:documentation>
                                             <xsd:appinfo source="CIsrc">
                                                <dtyp:srcSchema form="CCPRF01E" usage="value">BlankGrayedOut</dtyp:srcSchema>
                                             </xsd:appinfo>
                                          </xsd:annotation>
                                          <xsd:simpleType>
                                             <xsd:restriction base="comp:NotApplicableMessage">
                                                <xsd:enumeration value="BlankGrayedOut"/>
                                             </xsd:restriction>
                                          </xsd:simpleType>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="Lum03_LuminaireDescription" type="lit:LuminaireDescription">
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Luminaire Description</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">If Lum01_InstalledSameAsCC == true
 result = Lum03cc_LuminaireDescription but user can change value
 Else If Lum01_InstalledSameAsCC == false
 User input
 End If</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="Lum03_NotApplicableMessage">
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Luminaire Description</xsd:documentation>
