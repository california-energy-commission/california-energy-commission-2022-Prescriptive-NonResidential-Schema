                                                               <xsd:documentation source="CalculationsAndRules">If Doors03cc_NotApplicableMessage IsPresent
If Doors01_InstalledSameAsCC == true
result = Doors03cc_NotApplicableMessage
End If
End If</xsd:documentation>
                                                            </xsd:annotation>
                                                            <xsd:simpleType>
                                                               <xsd:restriction base="comp:NotApplicableMessage">
                                                                  <xsd:enumeration value="BlankGrayedOut"/>
                                                               </xsd:restriction>
                                                            </xsd:simpleType>
                                                         </xsd:element>
                                                      </xsd:choice>
