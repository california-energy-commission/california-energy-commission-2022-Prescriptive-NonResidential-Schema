<?xml version="1.0" encoding="UTF-8"?>
<xsd:schema attributeFormDefault="unqualified" elementFormDefault="qualified" targetNamespace="http://www.lmonte.com/besm/NRCIENV01E" version="2022.0.000" xmlns="http://www.lmonte.com/besm/NRCIENV01E" xmlns:altova="http://www.altova.com/xml-schema-extensions" xmlns:bld="http://www.lmonte.com/besm/bld" xmlns:com="http://www.lmonte.com/besm/com" xmlns:comp="http://www.lmonte.com/besm/comp" xmlns:d="http://www.lmonte.com/besm/d" xmlns:dtyp="http://www.lmonte.com/besm/dtyp" xmlns:env="http://www.lmonte.com/besm/env" xmlns:hvac="http://www.lmonte.com/besm/hvac" xmlns:lit="http://www.lmonte.com/besm/lit" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
   <xsd:import namespace="http://www.lmonte.com/besm/bld" schemaLocation="../base/NrPresBuilding.xsd"/>
   <xsd:import namespace="http://www.lmonte.com/besm/com" schemaLocation="../base/NrPresCommon.xsd"/>
   <xsd:import namespace="http://www.lmonte.com/besm/comp" schemaLocation="../base/NrPresCompliance.xsd"/>
   <xsd:import namespace="http://www.lmonte.com/besm/dtyp" schemaLocation="../base/DataTypes.xsd"/>
   <xsd:import namespace="http://www.lmonte.com/besm/env" schemaLocation="../base/NrPresEnvelope.xsd"/>
   <xsd:import namespace="http://www.lmonte.com/besm/hvac" schemaLocation="../base/NrPresHvac.xsd"/>
   <xsd:import namespace="http://www.lmonte.com/besm/lit" schemaLocation="../base/NrPresLighting.xsd"/>
   <xsd:element name="ComplianceDocumentPackage">
      <xsd:complexType>
         <xsd:sequence>
            <xsd:element name="DocID" minOccurs="0">
               <xsd:complexType>
                  <xsd:attribute name="doc" type="comp:ComplianceDocumentTagNR" fixed="NRCIENV01E"/>
                  <xsd:attribute name="docType" type="comp:ComplianceDocumentTypeNR" fixed="CertificateInstallationNR"/>
                  <xsd:attribute name="docTitle" type="comp:ComplianceDocumentTitleNR" fixed="Envelope Component Approach"/>
               </xsd:complexType>
            </xsd:element>
            <xsd:element name="Payload" type="comp:Payload"/>
            <xsd:element name="DocumentData">
               <xsd:complexType>
                  <xsd:sequence>
                     <xsd:element name="Header" type="comp:HeaderNRCI"/>
                     <xsd:element ref="nRCIENV01E"/>
                     <xsd:element name="DocAuthor" type="comp:DocumentAuthor" maxOccurs="unbounded"/>
                     <xsd:element name="RespPerson" type="comp:ResponsiblePersonNRCI" maxOccurs="unbounded"/>
                     <xsd:element name="Footer" type="comp:FooterNRCC"/>
                     <xsd:element name="ComplianceID" minOccurs="0"/>
                  </xsd:sequence>
               </xsd:complexType>
            </xsd:element>
            <xsd:element name="Report" type="xsd:base64Binary" minOccurs="0"/>
         </xsd:sequence>
         <xsd:attribute name="revision" use="required" type="xsd:string" fixed="rev 20220901"/>
         <xsd:attribute name="doc" use="required" type="comp:ComplianceDocumentTagNR" fixed="NRCIENV01E"/>
      </xsd:complexType>
   </xsd:element>
   <xsd:complexType name="NRCIENV01E">
      <xsd:annotation>
         <xsd:documentation>Envelope Component Approach</xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="Section_A" minOccurs="1" maxOccurs="1">
            <xsd:annotation>
               <xsd:documentation source="FieldText">
                  <d:b>A. GENERAL INFORMATION</d:b>
               </xsd:documentation>
               <xsd:documentation source="MinOccurs">This section is required.</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="A_BeforeNote1" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"/>
                        <xsd:documentation source="AdditionalRequirements">
                           <d:i>This Certificate of Installation documents the installation of envelope features, materials, components, and manufactured devices required to demonstrate compliance with Title 24, Part 6 per  
                              <d:t24refnolink/>10.103(a)3 for nonresidential, hotel/motel and high-rise residential occupancies, [ .</d:i>
                        </xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="A01_ProjectCity" type="comp:ProjectCity">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Project Location (city)</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">If CC-ENV parent, 
result = A01_ProjectCity
Else If  CC-PRF parent, 
result = Info01_ProjectCity
End If</xsd:documentation>
                        <xsd:appinfo source="CIsrc">
                           <dtyp:srcSchema form="CCENV01E" usage="copy">A01_ProjectCity</dtyp:srcSchema>
                           <dtyp:srcSchema form="CCPRF01E" usage="copy">Info04_ProjectCity</dtyp:srcSchema>
                        </xsd:appinfo>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="A02_ProjectZipcode" type="comp:ProjectZipcode">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Zip Code</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">If CC-ENV parent, 
result = A02_Zipcode
Else If  CC-PRF parent, 
result = Info02_ProjectZipcode
End If</xsd:documentation>
                        <xsd:appinfo source="CIsrc">
                           <dtyp:srcSchema form="CCENV01E" usage="copy">A02_Zipcode</dtyp:srcSchema>
                           <dtyp:srcSchema form="CCPRF01E" usage="copy">Info06_ProjectZipcode</dtyp:srcSchema>
                        </xsd:appinfo>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="A03_PermitSetDate" type="comp:PermitSetDate">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Date of Permit Set used for construction</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">User input</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="A04_AuthorityHavingJurisdiction" type="comp:AuthorityHavingJurisdiction">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Authority Having Jurisdiction</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">User input</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="A05_PermitNumber" type="comp:PermitNumber">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Building Permit #</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">User input</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="A06_AsBuiltSetDate" type="comp:AsBuiltSetDate">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Date of As-built Set</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">User input</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="A07_PermitSetName" type="comp:PermitSetName" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">NOT REPORTED IN PDF</xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">Name of Permit Set used for construction</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">This data is not currently required but may be needed in the future</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="A08_AsBuiltSetName" type="comp:AsBuiltSetName" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">NOT REPORTED IN PDF</xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">Name of As-built Set</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">This data is not currently required but may be needed in the future</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
         <xsd:element name="Section_B" minOccurs="1" maxOccurs="1">
            <xsd:annotation>
               <xsd:documentation source="FieldText">
                  <d:b>B. INSTALLER SCOPE</d:b>
               </xsd:documentation>
               <xsd:documentation source="MinOccurs">This section is required.</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="B_BeginNote1" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"/>
                        <xsd:documentation source="AdditionalRequirements">
                           <d:i>This table indicates the construction systems and materials documented on this Certificate of Installation. </d:i>
                        </xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="B01_Subheading" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Roofs</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="B02_Subheading" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Walls</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="B03_Subheading" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Fenestration</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="B04_Subheading" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Doors</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="B05_Subheading" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Floors</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="B01a_ProjectHasInsulationAboveDeck" type="comp:ProjectHasInsulationAboveDeck">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Above Deck Insulation</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">User input</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="B01b_ProjectHasInsulationBelowDeck" type="comp:ProjectHasInsulationBelowDeck">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Below Deck (Cavity) Insulation</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">User input</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="B01c_ProjectHasRoofSurfacing" type="comp:ProjectHasRoofSurfacing">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Surface Material</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">User input</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="B02a_ProjectHasWallAssembly" type="comp:ProjectHasWallAssembly">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Assembly type</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">User input</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="B02b_ProjectHasWallInsulation" type="comp:ProjectHasWallInsulation">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Insulation</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">User input</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="B03a_ProjectHasFenestrationVertical" type="comp:ProjectHasFenestrationVertical">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Vertical/ Glazed Doors</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">User input</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="B03b_ProjectHasFenestrationSkylight" type="comp:ProjectHasFenestrationSkylight">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Skylights</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">User input</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="B04a_ProjectHasNewSolidDoor" type="comp:ProjectHasNewSolidDoor">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">New solid doors</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">User input</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="B05a_ProjectHasFloorAssembly" type="comp:ProjectHasFloorAssembly">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Assembly type</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">User input</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="B05b_ProjectHasFloorInsulation" type="comp:ProjectHasFloorInsulation">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Insulation</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">User input</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
         <xsd:element name="Section_C" minOccurs="1">
            <xsd:annotation>
               <xsd:documentation source="FieldText">
                  <d:b>C. COMPLIANCE RESULTS</d:b>
               </xsd:documentation>
               <xsd:documentation source="MinOccurs">This section is required.</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="C_BeginNote1" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"/>
                        <xsd:documentation source="AdditionalRequirements">
                           <d:i>This table indicates whether the as-built conditions documented in this form are equal or better than what was documented on the permitted Certificate of Compliance.  If the installation is not equal or better, Section 10-103(a)2B requires the Certificate of Compliance form to be revised accordingly to demonstrate compliance.</d:i>
                        </xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="C01_ComplianceResultCI" type="comp:ComplianceResultCI">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"/>
                        <xsd:documentation source="CalculationsAndRules">Calculated result:  
 If in any required tables or subsections in Section F ComplianceResultCI == TableIncomplete
 result = FormNotComplete
 Else If ComplianceResultCI == MayNotComply
 result = MayNotComply
  
 Else If All require tables or subsections in Section F  have  ComplianceResultCI ==  ExactMatch
 result = ExactMatch
 Else If any have ComplianceResultCI ==  EqualBetter
 result = EqualBetter
 End If</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="C01Message_ComplianceResultMessageCI" type="comp:ComplianceResultMessageCI">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"/>
                        <xsd:documentation source="CalculationsAndRules">Calculated result:  
 If C01_ComplianceResultCI == ExactMatch Or EqualBetter
 result = ExactEqualBetterText
 Else If C01_ComplianceResultCI == MayNotComply
 result =  MayNotComplyText
 End If</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
         <xsd:element name="Section_D" minOccurs="1">
            <xsd:annotation>
               <xsd:documentation source="FieldText">
                  <d:b>D. EXCEPTIONAL CONDITIONS</d:b>
               </xsd:documentation>
               <xsd:documentation source="MinOccurs">This section is required.</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="D_BeginNote1" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"/>
                        <xsd:documentation source="AdditionalRequirements">
                           <d:i>This table is auto-filled with uneditable comments because of field conditions noted by the installer that may impact requirements documented on the Certificate of Compliance.</d:i>
                        </xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="D01_ExceptionalConditionMessageCIENVE" type="comp:ExceptionalConditionMessageCIENVE" minOccurs="0" maxOccurs="unbounded">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Exceptional Messages</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">Auto-filled with any system generated messages that explain exceptional conditions encountered in the the completed compliance document
If ExceptionalConditionMessageCIENVE == ""
result = NoExceptionNote
End If</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
         <xsd:element name="Section_E" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation source="FieldText">
                  <d:b>E. INSTALLER NOTES</d:b>
               </xsd:documentation>
               <xsd:documentation source="CalculationsAndRules">This section is required if there are  comments from the  installer to the Authority Having Jurisdiction</xsd:documentation>
               <xsd:documentation source="MinOccurs">If this section doesn't apply, do not display anything for this section</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="E_BeginNote1" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"/>
                        <xsd:documentation source="AdditionalRequirements">
                           <d:i>This table includes remarks made by the installer to the Authority Having Jurisdiction.</d:i>
                        </xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="E01_CommentsToPermitAuthority" type="comp:CommentsToPermitAuthority" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"/>
                     </xsd:annotation>
                  </xsd:element>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
         <xsd:element name="Section_F" minOccurs="1">
            <xsd:annotation>
               <xsd:documentation source="FieldText">
                  <d:b>F. INSTALLATION DETAILS</d:b>
               </xsd:documentation>
               <xsd:documentation source="MinOccurs">This section is required.</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="F_BeginNote1" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"/>
                        <xsd:documentation source="AdditionalRequirements">
                           <d:i>The following tables indicate performance requirements as documented on the permitted Certificate of Compliance (NRCC) for all systems and components included in Table B. Installer Scope.  Also included are the as-built conditions documented by the installer/documentation author. </d:i>
                        </xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="F_RowLabelTopCell" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Per C of C</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">This is the text displayed at the left end of a row in the top cell for all tables in Section F.</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="F_RowLabelBottomCell" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">As-built Conditions</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">This is the text displayed at the left end of a row in the bottom cell for all tables in Section F</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="TableRoof" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">
                           <d:b>Roof Insulation</d:b>
                        </xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">This subsection is required If B01a_ProjectHasInsulationAboveDeck == true Or  B01b_ProjectHasInsulationBelowDeck == true</xsd:documentation>
                     </xsd:annotation>
                     <xsd:complexType>
                        <xsd:sequence>
                           <xsd:element name="Row" minOccurs="1" maxOccurs="unbounded">
                              <xsd:complexType>
                                 <xsd:sequence>
                                    <xsd:element name="Roof01cc_RoofAssemblyName" type="env:RoofAssemblyName">
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Tag/Plan Detail ID</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">If CC-ENV parent
If F01Framed_RoofTypeApplies == true
result =  Fframed02_RoofAssemblyName
Else If F01SIPS_RoofTypeApplies == true
result = FSIPS02_RoofAssemblyName
Else If F01Span_RoofTypeApplies == true
result = Fspan02_RoofAssemblyName
Else If F01Metal_RoofTypeApplies == true
result = Fmetalp02_RoofAssemblyName
Else If F01MetalBldg_RoofTypeApplies == true
result = Fmetalb02_RoofAssemblyName
Else If F01FramedMF_RoofTypeApplies == true
result = FframedMF02_RoofAssemblyName
End If
Else If CC-PRF parent
If Opaq02_CompatibleSurfaceType == Roof
result = Opaq01_ConstructionAssemblyNameString
End If
 Note: All following fields in this table reference CC parent fields for the roof assigned to Roof01cc_RoofAssemblyName</xsd:documentation>
                                          <xsd:appinfo source="CIsrc">
                                             <dtyp:srcSchema form="CCENV01E" usage="reference">F01Framed_RoofTypeApplies</dtyp:srcSchema>
                                             <dtyp:srcSchema form="CCENV01E" usage="copy">Fframed02_RoofAssemblyName</dtyp:srcSchema>
                                             <dtyp:srcSchema form="CCENV01E" usage="reference">F01SIPS_RoofTypeApplies</dtyp:srcSchema>
                                             <dtyp:srcSchema form="CCENV01E" usage="copy">FSIPS02_RoofAssemblyName</dtyp:srcSchema>
                                             <dtyp:srcSchema form="CCENV01E" usage="reference">F01Span_RoofTypeApplies</dtyp:srcSchema>
                                             <dtyp:srcSchema form="CCENV01E" usage="copy">Fspan02_RoofAssemblyName</dtyp:srcSchema>
                                             <dtyp:srcSchema form="CCENV01E" usage="reference">F01Metal_RoofTypeApplies</dtyp:srcSchema>
                                             <dtyp:srcSchema form="CCENV01E" usage="copy">Fmetalp02_RoofAssemblyName</dtyp:srcSchema>
                                             <dtyp:srcSchema form="CCENV01E" usage="reference">F01MetalBldg_RoofTypeApplies</dtyp:srcSchema>
                                             <dtyp:srcSchema form="CCENV01E" usage="copy">Fmetalb02_RoofAssemblyName</dtyp:srcSchema>
                                             <dtyp:srcSchema form="CCENV01E" usage="copy">FframedMF02_RoofAssemblyName</dtyp:srcSchema>
                                             <dtyp:srcSchema form="CCPRF01E" usage="reference">Opaq02_CompatibleSurfaceType</dtyp:srcSchema>
                                             <dtyp:srcSchema form="CCPRF01E" usage="copy">Opaq01_ConstructionAssemblyNameString</dtyp:srcSchema>
                                          </xsd:appinfo>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="Roof01_InMyScope" type="comp:InMyScope">
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Is this in your scope?</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">User selects from true value display term Yes and false value display term No, by others
 If result == false Roof02cc-Roof04cc and Roof02-Roof04 As-built fields are blank and grayed out
And D01_ExceptionalConditionMessageCIENVE += RoofInsNotInScope  
End If</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="RoofScopeChoice">
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText"/>
                                       </xsd:annotation>
                                       <xsd:complexType>
                                          <xsd:choice>
                                             <xsd:element name="RoofScopeNA_NotApplicableMessage">
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText"/>
                                                   <xsd:documentation source="CalculationsAndRules">If Roof01_InMyScope == false
result = BlankGrayedOut for Roof02cc-Roof04cc and Roof02-Roof04 As-built fields are blank and grayed out 
End If</xsd:documentation>
                                                </xsd:annotation>
                                                <xsd:simpleType>
                                                   <xsd:restriction base="comp:NotApplicableMessage">
                                                      <xsd:enumeration value="BlankGrayedOut"/>
                                                   </xsd:restriction>
                                                </xsd:simpleType>
                                             </xsd:element>
                                             <xsd:element name="RoofInScope">
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText"/>
                                                </xsd:annotation>
                                                <xsd:complexType>
                                                   <xsd:sequence>
                                                      <xsd:element name="Roof01_InstalledSameAsCC" type="comp:InstalledSameAsCC" minOccurs="0">
                                                         <xsd:annotation>
                                                            <xsd:documentation source="FieldText">NOT REPORTED IN PDF</xsd:documentation>
                                                            <xsd:documentation source="CalculationsAndRules">User selects true if installed is the same as CC value  Or false if installed is different than CC value</xsd:documentation>
                                                         </xsd:annotation>
                                                      </xsd:element>
                                                      <xsd:choice maxOccurs="1">
                                                         <xsd:element name="Roof02cc_AssemblyType">
                                                            <xsd:annotation>
                                                               <xsd:documentation source="FieldText">Assembly/ Framing Type</xsd:documentation>
                                                               <xsd:documentation source="CalculationsAndRules">If CC-ENV parent
If Fframed02_RoofAssemblyName IsPresent Or FframedMF02_RoofAssemblyName IsPresent
If Fframed09_ConstructionAssemblyName == MetalFramedAttic
result = MetalFramedAttic
Else If Fframed09_ConstructionAssemblyName == MetalFramedRafter
result =  MetalFramedRafter
Else If Fframed09_ConstructionAssemblyName == WoodFramedAttic
result =  WoodFramedAttic
Else If Fframed09_ConstructionAssemblyName == MetalFramedRafter
result =  MetalFramedRafter
End If 
Else If FSIPS02_RoofAssemblyName IsPresent
result = SIPS
Else If Fspan02_RoofAssemblyName IsPresent
result = Span
Else If Fmetalp02_RoofAssemblyName IsPresent
result = MetalPanel
Else If Fmetalb02_RoofAssemblyName IsPresent
result = MetalBuilding
End If
End If</xsd:documentation>
                                                               <xsd:appinfo source="CIsrc">
                                                                  <dtyp:srcSchema form="CCENV01E" usage="reference">Fframed02_RoofAssemblyName</dtyp:srcSchema>
                                                                  <dtyp:srcSchema form="CCENV01E" usage="reference">FSIPS02_RoofAssemblyName</dtyp:srcSchema>
                                                                  <dtyp:srcSchema form="CCENV01E" usage="reference">Fspan02_RoofAssemblyName</dtyp:srcSchema>
                                                                  <dtyp:srcSchema form="CCENV01E" usage="reference">Fmetalp02_RoofAssemblyName</dtyp:srcSchema>
