                                                               <xsd:appinfo source="CIsrc">
                                                                  <dtyp:srcSchema form="CCENV01E" usage="copy">J05_DoorConstructionType</dtyp:srcSchema>
                                                               </xsd:appinfo>
                                                            </xsd:annotation>
                                                         </xsd:element>
                                                         <xsd:element name="Doors03cc_NotApplicableMessage">
