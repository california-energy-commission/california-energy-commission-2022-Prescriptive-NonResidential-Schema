                                                                     <xsd:appinfo source="CIsrc">
                                                                        <dtyp:srcSchema form="CCENV01E" usage="copy">Iframed06_FramingMaterialOther</dtyp:srcSchema>
                                                                     </xsd:appinfo>
                                                                  </xsd:annotation>
                                                               </xsd:element>
                                                               <xsd:element name="FloorsSIPS03cc_SplineType">
                                                                  <xsd:annotation>
                                                                     <xsd:documentation source="FieldText">Floor/ Spline Material</xsd:documentation>
                                                                     <xsd:documentation source="CalculationsAndRules">If CC-ENV parent
