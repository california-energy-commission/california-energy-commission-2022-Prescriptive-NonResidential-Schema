                  <xsd:element name="FurnaceReqs" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Gas/ Propane Furnaces Serving Individual Dwelling Units (Heat Pump Space Heater Ready)</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">This section is required and displayed 
If J01a_SingleUnitFurnace == true</xsd:documentation>
                     </xsd:annotation>
                     <xsd:complexType>
                        <xsd:sequence>
                           <xsd:element name="J02_Ready240V" type="comp:Ready240V" >
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText"></xsd:documentation>
                                 <xsd:documentation source="AdditionalRequirements">A dedicated 240 volt branch circuit shall be installed within 3 feet from the furnace and accessible to the furnace with no obstructions. The branch circuit shall be rated at 30 amps minimum. The blank cover shall be identified as "240V ready". All electrical components shall be installed in accordance with the California Electrical Code.</xsd:documentation>
                                 <xsd:documentation source="CalculationsAndRules">User input.  If checked result = true; else result = false</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                           <xsd:element name="J03_Future240V" type="comp:Future240V" >
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText"></xsd:documentation>
                                 <xsd:documentation source="AdditionalRequirements">The main electrical service panel shall have a reserved space to allow for the installation of a double pole circuit breaker for a future heat pump space heater installation. The reserved space shall be permanently marked as "For Future 240V use".</xsd:documentation>
                                 <xsd:documentation source="CalculationsAndRules">User input.  If checked result = true; else result = false</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                        </xsd:sequence>
                     </xsd:complexType>
                  </xsd:element>
