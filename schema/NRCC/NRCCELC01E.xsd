                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Gas/ Propane Cooktops Serving Individual Dwelling Units</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">This section is required and displayed 
If J01b_SingleUnitCooktop == true</xsd:documentation>
                     </xsd:annotation>
                     <xsd:complexType>
                        <xsd:sequence>
                           <xsd:element name="J04_Ready240V" type="comp:Ready240V" >
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText"></xsd:documentation>
                                 <xsd:documentation source="AdditionalRequirements">A dedicated 240 volt branch circuit shall be installed within 3 feet from the cooktop and accessible to the cooktop with no obstructions. The branch circuit shall be rated at 50 amps minimum. The blank cover shall be identified as "240V ready". All electrical components shall be installed in accordance with the California Electrical Code.</xsd:documentation>
                                 <xsd:documentation source="CalculationsAndRules">User input.  If checked result = true; else result = false</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                           <xsd:element name="J05_Future240V" type="comp:Future240V" >
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText"></xsd:documentation>
                                 <xsd:documentation source="AdditionalRequirements">The main electrical service panel shall have a reserved space to allow for the installation of a double pole circuit breaker for a future electric cooktop installation. The reserved space shall be permanently marked as "For Future 240V use".</xsd:documentation>
                                 <xsd:documentation source="CalculationsAndRules">User input.  If checked result = true; else result = false</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                        </xsd:sequence>
                     </xsd:complexType>
