                        <xsd:annotation>
                           <xsd:documentation source="FieldText">Ornamental [d:t24refnolink/]140.7(d)2 / [d:t24refnolink/]170.2(e)6 [d:line1/] (See Table L)</xsd:documentation>
                           <xsd:documentation source="CalculationsAndRules">If section I is required And I01d_UsingOrnamentalAllowance==true 
result = LTotals09_UseOrLoseOrnamentalTotalAllowance 
Else result = BlankGrayedOut stored in NotApplicableMessage and field is blank and grayed out
End If</xsd:documentation>
                        </xsd:annotation>
                     </xsd:element>
                     <xsd:element name="C04_NotApplicableMessage" >
                        <xsd:annotation>
                           <xsd:documentation source="FieldText">Ornamental [d:t24refnolink/]140.7(d)2 / [d:t24refnolink/]170.2(e)6 [d:line1/] (See Table L)</xsd:documentation>
                           <xsd:documentation source="CalculationsAndRules">If section I is required And I01d_UsingOrnamentalAllowance==true 
result = LTotals09_UseOrLoseOrnamentalTotalAllowance 
Else result = BlankGrayedOut stored in NotApplicableMessage and field is blank and grayed out
End If</xsd:documentation>
                        </xsd:annotation>
                        <xsd:simpleType>
                           <xsd:restriction base="comp:NotApplicableMessage">
                              <xsd:enumeration value="BlankGrayedOut"/>
                           </xsd:restriction>
                        </xsd:simpleType>
                     </xsd:element>
                  </xsd:choice>
                  <xsd:choice maxOccurs="1">
                     <xsd:element name="C05_UseOrLoseSpecificAreaTotalAllowance" type="comp:UseOrLoseSpecificAreaTotalAllowance" >
                        <xsd:annotation>
                           <xsd:documentation source="FieldText">Per Specific Area [d:t24refnolink/]140.7(d)2 / [d:t24refnolink/]170.2(e)6 [d:line1/] (See Table M)</xsd:documentation>
                           <xsd:documentation source="CalculationsAndRules">If section I is required And I01e_UsingSpecificAreaAllowance==true  
result = MTotals10_UseOrLoseSpecificAreaTotalAllowance 
Else result = BlankGrayedOut stored in NotApplicableMessage and field is blank and grayed out
End If</xsd:documentation>
                        </xsd:annotation>
                     </xsd:element>
                     <xsd:element name="C05_NotApplicableMessage" >
                        <xsd:annotation>
                           <xsd:documentation source="FieldText">Per Specific Area [d:t24refnolink/]140.7(d)2 / [d:t24refnolink/]170.2(e)6 [d:line1/](See Table M)</xsd:documentation>
                           <xsd:documentation source="CalculationsAndRules">If section I is required And I01e_UsingSpecificAreaAllowance==true  
result = MTotals10_UseOrLoseSpecificAreaTotalAllowance 
Else result = BlankGrayedOut stored in NotApplicableMessage and field is blank and grayed out
End If</xsd:documentation>
                        </xsd:annotation>
                        <xsd:simpleType>
                           <xsd:restriction base="comp:NotApplicableMessage">
                              <xsd:enumeration value="BlankGrayedOut"/>
                           </xsd:restriction>
                        </xsd:simpleType>
                     </xsd:element>
                  </xsd:choice>
                  <xsd:choice maxOccurs="1">
                     <xsd:element name="C06_ExistingConditionsTotalAllowance" type="comp:ExistingConditionsTotalAllowance" >
                        <xsd:annotation>
                           <xsd:documentation source="FieldText">Existing Power Allowance [d:t24refnolink/]141.0(b)2L / [d:t24refnolink/] 180.2(b)4Bv [d:line1/] (See Table N)</xsd:documentation>
                           <xsd:documentation source="CalculationsAndRules">If B05_OutdoorLightingCalculationMethod == ExistingPowerReduction Or ExistingPowerMaintained 
result =  NAllowance06_ExistingConditionsTotalAllowance  
Else result = BlankGrayedOut stored in NotApplicableMessage and field is blank and grayed out
End If</xsd:documentation>
                        </xsd:annotation>
                     </xsd:element>
                     <xsd:element name="C06_NotApplicableMessage" >
                        <xsd:annotation>
                           <xsd:documentation source="FieldText">Existing Power Allowance [d:t24refnolink/]141.0(b)2L / [d:t24refnolink/] 180.2(b)4Bv [d:line1/] (See Table N)</xsd:documentation>
                           <xsd:documentation source="CalculationsAndRules">If B05_OutdoorLightingCalculationMethod == ExistingPowerReduction Or ExistingPowerMaintained 
result =  NAllowance06_ExistingConditionsTotalAllowance  
Else result = BlankGrayedOut stored in NotApplicableMessage and field is blank and grayed out
End If</xsd:documentation>
                        </xsd:annotation>
                        <xsd:simpleType>
                           <xsd:restriction base="comp:NotApplicableMessage">
                              <xsd:enumeration value="BlankGrayedOut"/>
                           </xsd:restriction>
                        </xsd:simpleType>
                     </xsd:element>
                  </xsd:choice>
                  <xsd:element name="C07_OutdoorLightingTotalAllowance" type="comp:OutdoorLightingTotalAllowance" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">[d:b]Total Allowed[/d:b] (Watts)</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">Calculated Value: 
If B05_OutdoorLightingCalculationMethod ==  ExistingPowerMaintained Or  ExistingPowerReduction result = C06_ExistingConditionsTotalAllowance; 
 
Else result =  SUM (C01_HardscapeGeneralAllowance, C02_UseOrLoseApplicationTotalAllowance, C03_UseOrLoseSalesFrontageTotalAllowance, C04_UseOrLoseOrnamentalTotalAllowance,  C05_UseOrLoseSpecificAreaTotalAllowance)
End If</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="C08_LightingPowerInstalledTotal" type="comp:LightingPowerInstalledTotal" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">[d:b]Total Actual[/d:b] (Watts)</xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">(See Table F)</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">Auto-filled result = FTotals08_LightingPowerInstalledTotal</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="C09_DoesOutdoorLightingPowerComply" type="comp:DoesOutdoorLightingPowerComply" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">[d:b]07 must be  [d:gte/] 08 [/d:b]</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">Calculated value: 
If C07_OutdoorLightingTotalAllowance GTE C08_LightingPowerInstalledTotal 
result = true display term COMPLIES
Else 
result = false display term DOES NOT COMPLY
End If</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:choice maxOccurs="1">
                     <xsd:element name="C10_LightingPowerCutoffCompliance" type="comp:LightingPowerCutoffCompliance" >
                        <xsd:annotation>
                           <xsd:documentation source="FieldText">[d:b] Shielding Compliance (See Table G for Details)[/d:b]</xsd:documentation>
                           <xsd:documentation source="CalculationsAndRules">If no luminaire has F07_LightingPowerExcluded == false Then
If no luminaire has F09_LuminairePowerCutoffCompliance == CutoffGT150W then 
result = NotApplicable stored in C10_NotApplicableMessage with display term Not Applicable and field is grayed out; 
 
Else If in each row  
G05_BackLightRatingBUG LTE G04_BackLightRatingLimitBUG Or  G04_BackLightRatingLimitBUG == NoLimit 
And  G08_UpLightRatingBUG LTE G07_UpLightRatingLimitBUG 
And G11_GlareRatingBUG  LTE G10_GlareRatingLimitBUG  
result = true  display term COMPLIES, 
 
Else result =  false display term DOES NOT COMPLY.
End If
End If</xsd:documentation>
                        </xsd:annotation>
                     </xsd:element>
                     <xsd:element name="C10_NotApplicableMessage" >
                        <xsd:annotation>
                           <xsd:documentation source="FieldText">[d:b] Shielding Compliance (See Table G for Details)[/d:b]</xsd:documentation>
                           <xsd:documentation source="CalculationsAndRules">If any luminaire has F07_LightingPowerExcluded == false 
Or  no luminaire has F09_LuminairePowerCutoffCompliance == CutoffGT150W then 
result = NotApplicable stored in C10_NotApplicableMessage with display term Not Applicable and field is grayed out; 
End If</xsd:documentation>
                           <xsd:appinfo source="displayterm">
                              <dtyp:displayterm value="NotApplicable"> Not Applicable</dtyp:displayterm>
                           </xsd:appinfo>
                           
                        </xsd:annotation>
                        <xsd:simpleType>
                           <xsd:restriction base="comp:NotApplicableMessage">
                              <xsd:enumeration value="NotApplicable"/>
                           </xsd:restriction>
                        </xsd:simpleType>
                     </xsd:element>
                  </xsd:choice>
                  <xsd:choice maxOccurs="1">
                     <xsd:element name="C11_DoLightingControlsComply" type="comp:DoLightingControlsComply" >
                        <xsd:annotation>
                           <xsd:documentation source="FieldText">[d:b]Controls Compliance (See Table H for Details)[/d:b]</xsd:documentation>
                           <xsd:documentation source="CalculationsAndRules">If Section H NotPresent 
result = NotApplicable stored in C11_NotApplicableMessage; 
 
Else If  H02_ShutOffControlsOutdoors Or H03_AutoScheduleControlsOutdoors Or H04_MotionSensorCompliance  IsNull 
result = DoesNotComply display term DOES NOT COMPLY; 
 
Else If  TableControlsMF IsPresent And 
H01mf_SpaceDescription Or   H02mf_OutdoorCompliantLightSource  Or H03mf_OutdoorLightingMF IsNull
result = DoesNotComply display term DOES NOT COMPLY
Else
result = Complies display term COMPLIES
End If</xsd:documentation>
                        </xsd:annotation>
                     </xsd:element>
                     <xsd:element name="C11_NotApplicableMessage" type="comp:NotApplicableMessage" >
                        <xsd:annotation>
                           <xsd:documentation source="FieldText">[d:b]Controls Compliance (See Table H for Details)[/d:b]</xsd:documentation>
                           <xsd:documentation source="CalculationsAndRules">If Section H NotPresent 
result = NotApplicable stored in C11_NotApplicableMessage; 
End If</xsd:documentation>
                        </xsd:annotation>
                     </xsd:element>
                  </xsd:choice>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
         <xsd:element name="Section_D" minOccurs="1">
            <xsd:annotation>
               <xsd:documentation source="FieldText">[d:b]D. EXCEPTIONAL CONDITIONS[/d:b]</xsd:documentation>
               <xsd:documentation source="MinOccurs">This section is required.</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="D_BeginNote1" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">[d:i]This table is auto-filled with uneditable comments because of selections made or data entered in tables throughout the form.[/d:i]</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="TableAllExceptionalConditions" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">This table is required if there are any exceptional conditions in sections F, H or N</xsd:documentation>
                     </xsd:annotation>
                     <xsd:complexType>
                        <xsd:sequence>
                           <xsd:element name="Row" minOccurs="1" maxOccurs="unbounded">
                              <xsd:complexType>
                                 <xsd:sequence>
                                    <xsd:element name="D01_SectionTitle" type="comp:SectionTitle" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText"></xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">Auto-filled with the section title string  + Permit Applicant Notes. For section F result = F. Outdoor Lighting Fixture Schedule Permit Applicant Notes.   For section N result = N. Existing Conditions Power Allowance (alterations only) Permit Applicant Notes</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="D02_SectionExceptionalConditionNotes" type="comp:SectionExceptionalConditionNotes"  maxOccurs="unbounded">
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText"></xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">This combines all exceptional condition notes for the section defined in D01_SectionTitle.  For section F,  each entry = F_Exception01_LuminaireName + : + F_Exception02_SectionExceptionalConditionNote. .  For section N each entry = N_Exception01_LuminaireName + : + N_Exception02_SectionExceptionalConditionNote.</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                 </xsd:sequence>
                              </xsd:complexType>
                           </xsd:element>
                        </xsd:sequence>
                     </xsd:complexType>
                  </xsd:element>
                  <xsd:element name="D03_ExceptionalConditionMessageCCLTOE" type="comp:ExceptionalConditionMessageCCLTOE"  minOccurs="0" maxOccurs="unbounded">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Exceptional Messages</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">Auto-filled with any system generated messages that explain exceptional conditions encountered in the the completed compliance document</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
         <xsd:element name="Section_E" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation source="FieldText">[d:b]E. ADDITIONAL REMARKS[/d:b]</xsd:documentation>
               <xsd:documentation source="CalculationsAndRules">This section is required if there are  comments from the permit applicant to the Authority Having Jurisdiction</xsd:documentation>
               <xsd:documentation source="MinOccurs">If this section doesn't apply, do not display anything for this section</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="E_BeginNote1" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">[d:i]This table includes remarks made by the permit applicant to the Authority Having Jurisdiction.[/d:i]</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="E01_CommentsToPermitAuthority" type="comp:CommentsToPermitAuthority"  minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
         <xsd:element name="Section_F" minOccurs="1">
            <xsd:annotation>
               <xsd:documentation source="FieldText">[d:b]F. OUTDOOR LIGHTING FIXTURE SCHEDULE[/d:b]</xsd:documentation>
               <xsd:documentation source="CalculationsAndRules">This section is always required.</xsd:documentation>
               <xsd:documentation source="MinOccurs">This section is required.</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="F_BeginNote1" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">[d:i]For new or altered lighting systems demonstrating compliance with [d:t24refnolink/]140.7 / [d:t24refnolink/]170.2(e)6  all new luminaires being installed and any existing luminaires remaining or being moved within the spaces covered by the permit application are included in the Table below.  For altered lighting systems using the Existing Power method per  [d:t24refnolink/]141.0(b)2L only new luminaires being installed and replacement luminaires being installed as part of the project scope are included  (ie, existing luminaires remaining or existing luminaires being moved are not included). [d:line1/]Outdoor lighting attached to multifamily buildings and controlled from the inside of a dwelling unit are included in Table H. and are not included here. All other multifamily outdoor lighting is included here.[/d:i]</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="F_ColumnHeading01To09" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">[d:b]Designed Wattage:[/d:b]</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="TableAllLuminaires" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                     </xsd:annotation>
                     <xsd:complexType>
                        <xsd:sequence>
                           <xsd:element name="Row" minOccurs="1" maxOccurs="unbounded">
                              <xsd:complexType>
                                 <xsd:sequence>
                                    <xsd:element name="F01_LuminaireName" type="lit:LuminaireName" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Name or Item Tag</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">User input</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="F02_LuminaireDescription" type="lit:LuminaireDescription" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Complete Luminaire Description</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">User input</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="F02a_IsLuminaireLinear" type="comp:IsLuminaireLinear" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Linear</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">User input; If checked result = true Else result = false;  If result == true, D02_ExceptionalConditionMessageCCLTOE += LinearLightingInF</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="F03_LuminairePower" type="lit:LuminairePower" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Watts per luminaire[d:sup]1, 2[/d:sup]</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">User input as follows: If F02a_IsLuminaireLinear == false result is stored in F03_LuminairePower;  Else If F02a == true result is stored in F03_LuminaireWattsPerLF</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="F03_LuminaireWattsPerLF" type="lit:LuminaireWattsPerLF" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Watts per luminaire[d:sup]1, 2[/d:sup]</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">User input as follows: If F02a_IsLuminaireLinear == false result is stored in F03_LuminairePower;  Else If F02a == true result is stored in F03_LuminaireWattsPerLF</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:element name="F04_LuminaireSpecSource" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">How is Wattage determined</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">User input;  If user selects Other* user must enter note in Exception Conditions field for this luminaire;</xsd:documentation>
                                          <xsd:appinfo source="displayterm">
                                             <dtyp:displayterm value="ManufacturerSpec">Mfr. Spec[d:sup]1[/d:sup]</dtyp:displayterm>
                                             <dtyp:displayterm value="CEC_Default">NA8 Default</dtyp:displayterm>
                                             <dtyp:displayterm value="Other">Other*</dtyp:displayterm>
                                          </xsd:appinfo>
                                          
                                       </xsd:annotation>
                                       <xsd:simpleType>
                                          <xsd:restriction base="comp:LuminaireSpecSource">
                                             <xsd:enumeration value="CEC_Default"/>
                                             <xsd:enumeration value="ManufacturerSpec"/>
                                             <xsd:enumeration value="Other"/>
                                          </xsd:restriction>
                                       </xsd:simpleType>
                                    </xsd:element>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="F05_LuminaireCountByType" type="lit:LuminaireCountByType" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Total Number Luminaires [d:sup] 2[/d:sup]</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">User input  If F02a_IsLuminaireLinear == false result  is stored in F05_LuminaireCountByType; Else result is stored in F05_LuminairesTotalLF</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="F05_LuminairesTotalLF" type="lit:LuminairesTotalLF" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Total Number Luminaires [d:sup] 2[/d:sup]</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">User input  If F02a_IsLuminaireLinear == false result  is stored in F05_LuminaireCountByType; Else result is stored in F05_LuminairesTotalLF</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:element name="F06_LuminaireStatus" type="com:LuminaireStatus" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Luminaire Status[d:sup]3[/d:sup]</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">User input; 
                                             If B01a == NewLightingSystem  list: New;  
                                             Else If B05_OutdoorLightingCalculationMethod == CalculatedAllowance then list: New and Altered 
                                             Else If B05_OutdoorLightingCalculationMethod == ExistingPowerReduction or ExistingPowerMaintained then all options are listed: New, ExistingAlteration display term Altered, ExistingReinstalled display term Exist.Reinstalled, ExistingUnchanged display term Exist. Remain;</xsd:documentation>
                                          <xsd:appinfo source="displayterm">
                                             <dtyp:displayterm value="ExistingAlteration">Altered</dtyp:displayterm>
                                             <dtyp:displayterm value="ExistingUnchanged"> Exist. Remain</dtyp:displayterm>
                                          </xsd:appinfo>
                                          
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="F07_LightingPowerExcluded" type="comp:LightingPowerExcluded" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Excluded per [d:t24refnolink/]140.7(a) / [d:t24refnolink/]170.2(e)6A</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">User input;  If checked result = true Else result = false</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="F08_LuminairePowerByType" type="lit:LuminairePowerByType" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Design Watts</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">Calculated Value: If F07_LightingPowerExcluded == true then result = BlankGrayedOut stored in NotApplicableMessage and field is blank and grayed out;
 Else If F02a_IsLuminaireLinear == false result =  PRODUCT (F03_LuminairePower, F05_LuminaireCountByType); Else result = PRODUCT(F03_LuminaireWattsPerLF, F05_LuminairesTotalLF)</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="F08_NotApplicableMessage" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Design Watts</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">Calculated Value: If F07_LightingPowerExcluded == true then result = BlankGrayedOut stored in NotApplicableMessage and field is blank and grayed out;
 Else If F02a_IsLuminaireLinear == false result =  PRODUCT (F03_LuminairePower, F05_LuminaireCountByType); Else result = PRODUCT(F03_LuminaireWattsPerLF, F05_LuminairesTotalLF)</xsd:documentation>
                                          </xsd:annotation>
                                          <xsd:simpleType>
                                             <xsd:restriction base="comp:NotApplicableMessage">
                                                <xsd:enumeration value="BlankGrayedOut"/>
                                             </xsd:restriction>
                                          </xsd:simpleType>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="F09_LuminairePowerCutoffCompliance" type="comp:LuminairePowerCutoffCompliance" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Cutoff Req. [d:gt/] 6,200 initial lumen output [d:t24refnolink/]130.2(b) /[d:t24refnolink/]160.5(c)1 [d:sup]4[/d:sup]</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">If F06_LuminaireStatus == ExistingReinstalled Or ExistingUnchanged result = BlankGrayedOut stored in NotApplicableMessage and field is blank and grayed out;
 Else user selects from list.  
 
Mandatory Measure Triggers:
If result == CutoffGTE6200Lumens Then in MM Block
MM130_2b_MeasureApplies  = true and text is displayed and 
DUMM130_2b_MeasureApplies  = true and text is displayed 
End If</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="F09_NotApplicableMessage" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Cutoff Req. [d:gt/] 6,200 initial lumen output [d:t24refnolink/]130.2(b) /[d:t24refnolink/]160.5(c)1 [d:sup]4[/d:sup]</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">If F06_LuminaireStatus == ExistingReinstalled Or ExistingUnchanged result = BlankGrayedOut stored in NotApplicableMessage and field is blank and grayed out;
 Else user selects from list.</xsd:documentation>
                                          </xsd:annotation>
                                          <xsd:simpleType>
                                             <xsd:restriction base="comp:NotApplicableMessage">
                                                <xsd:enumeration value="BlankGrayedOut"/>
                                             </xsd:restriction>
                                          </xsd:simpleType>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:element name="F10_VerifedByInspector" type="comp:VerifedByInspector"  minOccurs="0">
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Field Inspector</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                 </xsd:sequence>
                              </xsd:complexType>
                           </xsd:element>
                        </xsd:sequence>
                     </xsd:complexType>
                  </xsd:element>
                  <xsd:element name="FTotals08_LightingPowerInstalledTotal" type="comp:LightingPowerInstalledTotal" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">[d:b]Total Design Watts:[/d:b]</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">Calculated Value: result = SUM All ( F08_ LuminairePowerByType)</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="FNote1" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">* NOTES: Selections with a * require a note in the space below explaining how compliance is achieved. [d:line1/]
  [d:i] EX: Luminaire is lighting a statue; EXCEPTION 2 to [d:t24refnolink/]130.2(b) [/d:i]</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="F_TableComplianceNotes" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">This table is required if any instances of  F04_LuminaireSpecSource ==  Other</xsd:documentation>
                     </xsd:annotation>
                     <xsd:complexType>
                        <xsd:sequence>
                           <xsd:element name="Row" minOccurs="1" maxOccurs="unbounded">
                              <xsd:complexType>
                                 <xsd:sequence>
                                    <xsd:element name="F_Exception01_LuminaireName" type="lit:LuminaireName" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText"></xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">Auto-filled with value in F01_LuminaireName for a row where exception occurred</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="F_Exception02_ExceptionalConditionNote" type="comp:ExceptionalConditionNote" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText"></xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">User input: If one or more luminaires in this table has F04_LuminaireSpecSource ==  Other, 
the user must enter an explanation on how wattage is determined for each luminaire.</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                 </xsd:sequence>
                              </xsd:complexType>
                           </xsd:element>
                        </xsd:sequence>
                     </xsd:complexType>
                  </xsd:element>
                  <xsd:element name="FErrorMessage_SectionErrorMessage" type="comp:SectionErrorMessage"  minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="FEndNote1" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">[d:i][d:sup]1[/d:sup]FOOTNOTES: Authority Having Jurisdiction may ask for Luminaire cut sheets to confirm wattage used for compliance per [d:t24refnolink/]130.0(c) / [d:t24refnolink/]160.5(b)[/d:i]</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="FEndNote2" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">[d:i][d:sup]2[/d:sup] For linear luminaires, wattage should be indicated as W/lf instead of Watts/luminaire.  Total linear feet should be indicated in column 05 instead of number of luminaires. [/d:i]</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="FEndNote3" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">[d:i][d:sup]3[/d:sup] Select "New" for new luminaires in a new outdoor lighting project, or for added luminaires in an alteration.  Select "Altered" for replacement luminaires in an alteration.  Select "Existing to Remain" for existing luminaires within the project scope that are not being altered and are remaining.  Select "Existing Reinstalled" for existing luminaires which are being removed and reinstalled as part of the project scope.[/d:i]</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="FEndNote4" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">[d:i][d:sup]4[/d:sup] Compliance with mandatory shielding requirements is required for luminaires with initial lumen output [d:gte/] 6,200 unless exempted by [d:t24refnolink/]130.2(b)/ [d:t24refnolink/]160.5(c) [/d:i]</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
         <xsd:element name="Section_G" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation source="FieldText">[d:b]G. SHIELDING REQUIREMENTS (BUG)[/d:b]</xsd:documentation>
               <xsd:documentation source="CalculationsAndRules">This section is required if any luminaire in section F has F09_LuminairePowerCutoffCompliance == CutoffGTE6200Lumens</xsd:documentation>
               <xsd:documentation source="MinOccurs">If this section doesn't apply, do not display anything for this section</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="G_BeginNote1" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">[d:i]This table includes fixtures of [d:gte/]6,200 initial lumens indicated on Table F as needing to comply with Shielding Requirements.  Maximum lumens can be found in Title 24, Part 11, Section 5.106.8.[/d:i]</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="G_HeadingColumns03To05" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">Backlight Rating[d:sup]2[/d:sup]</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="G_HeadingColumns06To08" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">Uplight Rating[d:sup]2[/d:sup]</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="G_HeadingColumns09To11" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">Glare Rating (Lumens)[d:sup]2[/d:sup]</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="TableBUG" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                     </xsd:annotation>
                     <xsd:complexType>
                        <xsd:sequence>
                           <xsd:element name="Row" minOccurs="1" maxOccurs="unbounded">
                              <xsd:complexType>
                                 <xsd:sequence>
                                    <xsd:element name="G01_LuminaireName" type="lit:LuminaireName" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Name or Item Tag</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">Auto-filled result = F01_LuminaireName for luminaire where  F09_LuminairePowerCutoffCompliance == CutoffGTE6200Lumens</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="G02_LuminaireDescription" type="lit:LuminaireDescription" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Complete Luminaire Description</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">Auto-filled result = F02_LuminaireDescription for luminaire in G01_LuminaireName</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="G03_MountingHeightRangeBUG" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Mounting Height[d:sup]1[/d:sup]</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">User selects from dropdown list: GT2MH display term GT 2 MH from property line, Back1To2MH display term Back hemisphere is 1 - 2 MH from prop line, BackHalfTo1MH display term Back hemisphere is 0.5 - 1 MH from prop line, BackLessThanHalfMH display term Back hemisphere LT 0.5 MH from prop line</xsd:documentation>
                                       </xsd:annotation>
                                       <xsd:simpleType>
                                          <xsd:restriction base="comp:MountingHeightRangeBUG">
                                             <xsd:enumeration value="Back1To2MH"/>
                                             <xsd:enumeration value="BackHalfTo1MH"/>
                                             <xsd:enumeration value="BackLessThanHalfMH"/>
                                             <xsd:enumeration value="GT2MH"/>
                                          </xsd:restriction>
                                       </xsd:simpleType>
                                    </xsd:element>
                                    <xsd:element name="G04_BackLightRatingLimitBUG" type="comp:BackLightRatingLimitBUG" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Max Allowable Backlight Rating [d:sup]3[/d:sup]</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">Calculated field:  If A03_OutdoorLightingZoneType == OutdoorLZ1 Switch (MountingHeightRangeBUG) case GT2MH: result = NoLimit; case Back1To2MH result = B2; case BackHalfTo1MH result = B1; case BackLT_HalfMH: result = B0; Else If A03_OutdoorLightingZoneType == OutdoorLZ2 Switch (MountingHeightRangeBUG) case GT2MH: result = NoLimit; case Back1To2MH result = B3; case BackHalfTo1MH result = B2; case BackLT_HalfMH: result = B0; Else If A03_OutdoorLightingZoneType == OutdoorLZ3 Switch (MountingHeightRangeBUG) case GT2MH: result = NoLimit; case Back1To2MH result = B4; case BackHalfTo1MH result = B3; case BackLT_HalfMH: result = B1;  Else If A03_OutdoorLightingZoneType == OutdoorLZ4 Switch (MountingHeightRangeBUG) case GT2MH: result = NoLimit; case Back1To2MH result = B4; case BackHalfTo1MH result = B3; case BackLT_HalfMH: result = B2;</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="G05_BackLightRatingBUG" type="comp:BackLightRatingBUG" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Backlight Rating Per Design</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">User selects from list: B0, B1, B2, B3, B4, B5</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="G06_UpLightRatingAreaBUG" type="comp:UpLightRatingAreaBUG" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Lighting type</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">User selects from dropdown list: OutdoorAreaLighting display term Area Lighting, OutdoorDecorativeLighting display term All other outdoor lighting, including decorative</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="G07_UpLightRatingLimitBUG" type="comp:UpLightRatingLimitBUG" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Max Allowable Uplight Rating [d:sup]3[/d:sup]</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">Calculated field:  If A03_OutdoorLightingZoneType == OutdoorLZ1 Switch (UpLightRatingAreaBUG) case OutdoorAreaLighting: result = U0; case OutdoorDecorativeLighting: result = U1;  Else If A03_OutdoorLightingZoneType == OutdoorLZ2 Switch (UpLightRatingAreaBUG) case OutdoorAreaLighting: result = U0; case OutdoorDecorativeLighting: result = U2; Else If A03_OutdoorLightingZoneType == OutdoorLZ3 Switch (UpLightRatingAreaBUG) case OutdoorAreaLighting: result = U0; case OutdoorDecorativeLighting: result = U3; Else If A03_OutdoorLightingZoneType == OutdoorLZ4 Switch (UpLightRatingAreaBUG) case OutdoorAreaLighting: result = U0; case OutdoorDecorativeLighting: result = U4;</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="G08_UpLightRatingBUG" type="comp:UpLightRatingBUG" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Uplight Rating Per Design</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">User selects from list: U0, U1, U2, U3, U4</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="G09_MountingHeightRangeBUG" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Mounting Height[d:sup]1[/d:sup]</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">User selects from dropdown list: GT2MH  display term GT 2 MH from property line, Front1To2MH display term Front hemisphere is 1 - 2 MH from prop line, FrontHalfTo1MH display term Front hemisphere is 0.5 - 1 MH from prop line, FrontLessThanHalfMH display term Front hemisphere LT 0.5 MH from prop line</xsd:documentation>
                                       </xsd:annotation>
                                       <xsd:simpleType>
                                          <xsd:restriction base="comp:MountingHeightRangeBUG">
                                             <xsd:enumeration value="FrontLessThanHalfMH"/>
                                             <xsd:enumeration value="Front1To2MH"/>
                                             <xsd:enumeration value="FrontHalfTo1MH"/>
                                             <xsd:enumeration value="GT2MH"/>
                                          </xsd:restriction>
                                       </xsd:simpleType>
                                    </xsd:element>
                                    <xsd:element name="G10_GlareRatingLimitBUG" type="comp:GlareRatingLimitBUG" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Max Allowable Glare Rating [d:sup]3[/d:sup]</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">Calculated field:  If A03_OutdoorLightingZoneType == OutdoorLZ1 Switch (MountingHeightRangeBUG) case GT2MH: result = G1; case Front1To2MH result = G0; case FrontHalfTo1MH result = G0; case FrontLessThanHalfMH: result = G0; Else If A03_OutdoorLightingZoneType == OutdoorLZ2 Switch (MountingHeightRangeBUG) case GT2MH: result = G2; case Front1To2MH result = G1; case FrontHalfTo1MH result = G0; case FrontLessThanHalfMH: result = G0; Else If A03_OutdoorLightingZoneType == OutdoorLZ3 Switch (MountingHeightRangeBUG) case GT2MH: result = G3; case Front1To2MH result = G1; case FrontHalfTo1MH result = G1; case FrontLessThanHalfMH:  result = G0;  Else If A03_OutdoorLightingZoneType == OutdoorLZ4 Switch (MountingHeightRangeBUG) case GT2MH: result = G4; case Front1To2MH result = G2; case FrontHalfTo1MH result = G1; case FrontLessThanHalfMH:  result = G1;</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="G11_GlareRatingBUG" type="comp:GlareRatingBUG" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Glare Rating Per Design</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">User selects from list: G0, G1, G2, G3, G4</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="G12_VerifedByInspector" type="comp:VerifedByInspector"  minOccurs="0">
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Field Inspector</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                 </xsd:sequence>
                              </xsd:complexType>
                           </xsd:element>
                        </xsd:sequence>
                     </xsd:complexType>
                  </xsd:element>
                  <xsd:element name="GEndNote1" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">[d:i][d:sup]1[/d:sup]FOOTNOTES: Mounting Height is labeled MH in this table.[/d:i]</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="GEndNote2" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">[d:i][d:sup]2[/d:sup]  Authority Having Jurisdiction may ask for Luminaire cut sheets or other documentation to confirm luminaire type, uplight ratings and glare ratings used for compliance per [d:t24refnolink/]130.2(b)/ [d:t24refnolink/]160.5(c)[/d:i]</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="GEndNote3" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">[d:i][d:sup]3[/d:sup] BUG ratings with a lower number than the 'Max Allowable' are compliant.  Ex. If Max Allowable is Bug Rating B4, then B0, B1, B2 and B3 are all compliant. [/d:i]</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="GErrorMessage_SectionErrorMessage" type="comp:SectionErrorMessage"  minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
         <xsd:element name="Section_H" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation source="FieldText">[d:b]H. OUTDOOR LIGHTING CONTROLS[/d:b]</xsd:documentation>
               <xsd:documentation source="CalculationsAndRules">This section is required if B01a_LightingProjectScopeNR == NewLightingSystem Or If B01b_LightingProjectScopeNR == AlteredLightingSystem AND B02_DoesAlterationIncreaseLightingLoad == true; Or If B01b_LightingProjectScopeNR == AlteredLightingSystem AND B02_DoesAlterationIncreaseLightingLoad == false And B03_LuminaireAlteredPercentRange == (GTE10PercentLT50Percent OR GTE50Percent) AND B04_LuminaireAddAlteredCount GTE 5</xsd:documentation>
               <xsd:documentation source="MinOccurs">If this section doesn't apply, do not display anything for this section</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="H_BeginNote1" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">[d:i]This table demonstrates compliance with controls requirements for all new or altered luminaires installed as part of the permit application.  For alteration projects, luminaires which are existing to remain (ie untouched) and luminaires which are removed and reinstalled (wiring only) do not need to be included in this table even if they are within the spaces covered by the permit application.   [d:line1/] Outdoor lighting for nonresidential buildings, parking garages and common service areas in multifamily buildings must be documented separately from outdoor lighting attached to multifamily buildings and controlled from the inside of a dwelling unit [/d:i]</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="H_ColumnHeading01To08" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">[d:b]Mandatory Controls for Nonresidential Occupancies, Parking Garages &amp; Common Areas in Multifamily Buildings[/d:b]</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="TableLightingControls" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                     </xsd:annotation>
                     <xsd:complexType>
                        <xsd:sequence>
                           <xsd:element name="Row" minOccurs="1" maxOccurs="unbounded">
                              <xsd:complexType>
                                 <xsd:sequence>
                                    <xsd:element name="H01_SpaceDescription" type="com:SpaceDescription" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Area Description</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">User input</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="H02_ShutOffControlsOutdoors" type="comp:ShutOffControlsOutdoors" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Shut-Off [d:line1/] [d:t24refnolink/]130.2(c)1 / [d:t24refnolink/]160.5(c)</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">User selects from dropdown list: 
AstronomicalTimer display term Astronomical Timer, , 
Photocontrol display term Photocontrol, MultifamilyLightingNA display term NA: MF Complying w 160.5(a), OtherTechnology display term Other Control Technology, NotPermittedNA display term NA: Not permitted by H&amp;LS, TunnelsNA display term NA: Lighting in Tunnels
   
 
Mandatory Measure Triggers:
If result == AstronomicalTimer Or Photocontrol 
MM130_2c1_MeasureApplies  = true and text is displayed 
End If</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="H03_AutoScheduleControlsOutdoors" type="comp:AutoScheduleControlsOutdoors" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Auto-Schedule [d:line1/] [d:t24refnolink/]130.2(c)2 / [d:t24refnolink/]160.5(c)</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules"> If BB01b_LightingProjectScopeNR IsPresent And  AND B03_LuminaireAlteredPercentRange = GTE10PercentLT50Percent Or GTE50Percent
User selects from full dropdown list: 
ParkingSalesNA display term NA: Parking or Sales Lots [d:gt/]24 ft/all other at any height, AutoSchedule display term Yes, EMCS display term Yes - EMCS, MultifamilyLightingNA display term NA: MF Complying w 160.5(a), NotPermittedNA display term NA: Not permitted by H&amp;LS, TunnelsNA display term NA: Lighting in Tunnels
 
Else User selects from the list excluding ParkingSalesNA
AutoSchedule display term Yes, EMCS display term Yes - EMCS, MultifamilyLightingNA display term NA: MF Complying w 160.5(a), NotPermittedNA display term NA: Not permitted by H&amp;LS, TunnelsNA display term NA: Lighting in Tunnels

Mandatory Measure Triggers:
If result == AutoSchedule Or EMCS 
MM130_2c2_MeasureApplies  = true and text is displayed 
End If</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="H04_MotionSensorCompliance" type="comp:MotionSensorCompliance" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Motion Sensor  [d:line1/] [d:t24refnolink/]130.2(c)3 / [d:t24refnolink/]160.5(c)</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">User selects from dropdown list: 
MotionSensorCompliant  display term Yes, 
FacadeGTE24Ft display term NA: Facade, etc. [d:gte/]24 ft, 
WallGTE24Ft display term NA: Wall [d:gte/]24 ft, MultifamilyLightingNA display term NA: MF Complying w 160.5(a), NotPermittedNA display term NA: Not permitted by H&amp;LS, TunnelsNA display term NA: Lighting in Tunnels, AthleticNA display term NA: Athletic Fields/Playgrounds Lighting, HistoricBuildingNA display term NA: Historical Building Lighting, IndustrialNA display term NA: Industrial Sites Lighting, LandscapeNA display term NA: Landscape Lighting, LuminaireLT40NA display term NA: Each Luminaire less than or equal to 40 Watts, MonumentNA display term NA: Public Monument Lighting, PublicRoadNA display term NA: Lighting for Public Roadways, RegulatedNA display term NA: Regulated by FAA or Coast Guard, StairsRampNA display term NA: Stairs Wheelchair Lift/Ramp Lighting, ThemeParkNA display term NA: Theme Park Special Effect Lighting, OutdoorTheaterNA display term NA: Outdoor Theater, ParkingLuminaireLT78NA display term NA: Parking Lot each Luminaire less than 48 Watts
 
Mandatory Measure Triggers:
If result == MotionSensorCompliant 
MM130_2c3_MeasureApplies  = true and text is displayed 
End If</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="H05_VerifedByInspector" type="comp:VerifedByInspector"  minOccurs="0">
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Field Inspector</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                 </xsd:sequence>
                              </xsd:complexType>
                           </xsd:element>
                        </xsd:sequence>
                     </xsd:complexType>
                  </xsd:element>
                  <xsd:element name="TableControlsMFApplies" type="comp:DwellingUnitOutdoorLighting" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">NOT REPORTED IN PDF</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">This field is required if A05_WholeBuildingOccupancy == MultiFamilyMixedUseGTE4
The field asks the user if they have outdoor lighting that is controlled from within a dwelling unit
If true
TableControlsMF is required 
Else
TableControlsMF is not required  </xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="TableControlsMF" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">[d:b] Outdoor Lighting Controlled from Inside Multifamily Dwelling Units [/d:b]</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">This table is required If TableControlsMFApplies == true</xsd:documentation>
                     </xsd:annotation>
                     <xsd:complexType>
                        <xsd:sequence>
                           <xsd:element name="Row" minOccurs="1" maxOccurs="unbounded">
                              <xsd:complexType>
                                 <xsd:sequence>
                                    <xsd:element name="HControlMF" minOccurs="0">
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText"></xsd:documentation>
                                          <xsd:documentation source="AdditionalRequirements">This table documents compliance with mandatory requirements for outdoor lighting attached to multifamily buildings and controlled from the inside of a dwelling unit per [d:t24refnolink/]160.5(a). Outdoor lighting systems in common use areas providing shared provisions for living, eating, cooking, or sanitation to dwelling units that would otherwise lack these provisions may document compliance with [d:t24refnolink/]160.5(a) in this table or may document compliance with outdoor lighting for nonresidential occupancies.</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="H01mf_SpaceDescription" type="com:SpaceDescription" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Space Name</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">User input</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="H02mf_OutdoorCompliantLightSource" type="comp:OutdoorCompliantLightSource"  maxOccurs="unbounded">
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Compliant Light Sources[d:sup]1,2[/d:sup]  [d:t24refnolink/]160.5(a)1</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">User selects all that apply</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="H03mf_OutdoorLightingMF" type="comp:OutdoorLightingMF" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Mandatory Controls [d:t24refnolink/]160.5(a) [d:line1/]  Shut-Off</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="H04mf" minOccurs="0">
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Recessed Downlights[d:sup]3[/d:sup] [d:line1/][d:t24refnolink/]160.5(a)1C</xsd:documentation>
                                          <xsd:documentation source="AdditionalRequirements">[d:i]i. Shall not contain screw base lamp sockets; and
ii. Have a label that certifies the luminaire is airtight with air leakage less than 2.0 cfm at 75 Pascals when tested in accordance with ASTM E283. An exhaust fan housing with integral light shall not be required to be certified airtight; and
iii. Be sealed with a gasket or caulk between the luminaire housing and ceiling, and have all air leak paths between conditioned and unconditioned spaces sealed with a gasket or caulk, or be installed per manufacturer's instructions to maintain airtightness between the luminaire housing and ceiling; and
iv. Meet the clearance and installation requirements of California Electrical Code Section 410.116 for recessed luminaires; and
v.  Lamps and other separable light sources that are not compliant with the JA8 elevated temperature requirements, including marking requirements, shall not be installed in enclosed or recessed luminaires.[/d:i]</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                 </xsd:sequence>
                              </xsd:complexType>
                           </xsd:element>
                        </xsd:sequence>
                     </xsd:complexType>
                  </xsd:element>
                  <xsd:element name="HmfEndNote1" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">[d:i][d:sup]1[/d:sup]FOOTNOTE: Text has been abbreviated, please refer to Table 160.5-A to confirm compliance with the specific light source technologies listed. [/d:i]</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="HmfEndNote2" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">[d:i][d:sup]2[/d:sup]Authority having jurisdiction may ask for cutsheets or other documentation to confirm compliance of light source. [/d:i]</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="HmfEndNote3" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">[d:i][d:sup]3[/d:sup]Recessed luminaires marked for use in fire-rated installations, and recessed luminaires installed in non-insulated ceilings are excepted from ii and iii.[/d:i]</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="HErrorMessage_SectionErrorMessage" type="comp:SectionErrorMessage"  minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
         <xsd:element name="Section_I" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation source="FieldText">[d:b]I. LIGHTING POWER ALLOWANCE (per [d:t24refnolink/]140.7 /[d:t24refnolink/]170.2(e)) [/d:b]</xsd:documentation>
               <xsd:documentation source="CalculationsAndRules">This section applies if B01a_LightingProjectScopeNR == NewLightingSystem Or B02_DoesAlterationIncreaseLightingLoad == true Or  B05_OutdoorLightingCalculationMethod == CalculatedAllowance</xsd:documentation>
               <xsd:documentation source="MinOccurs">If this section doesn't apply, do not display anything for this section</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="I_BeginNote1" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">[d:i]This table includes areas using allowance calculations per [d:t24refnolink/]140.7 /[d:t24refnolink/]170.2(e). General Hardscape Allowance is per Table 140.7-A/Table 170.2-R while "Use it or lose it" Allowances are per Table 140.7-B /Table 170.2-S.  Indicate which allowances are being used to expand sections for user input.  Luminaires  that qualify for one of the "Use it or lose it" allowances shall not qualify for another "Use it or lose it" allowance.  [d:line1/] Outdoor lighting attached to multifamily buildings and controlled from the inside of a dwelling unit are included in Table H. and are not included here. All other multifamily outdoor lighting is included here.  [/d:i]</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="I_ColumnHeading01" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">"Use it or lose it" Allowance (select all that apply)</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="I01a_UsingGeneralHardscapeAllowance" type="comp:UsingGeneralHardscapeAllowance" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">General Hardscape Allowance  [d:line1/] Table I (below)</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">User input.  If user checks field, result = true and user needs to complete the table below for calculating the general hardscape lighting power allowance; Else result = false.</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:choice maxOccurs="1">
                     <xsd:element name="I01b_UsingApplicationAllowance" type="comp:UsingApplicationAllowance" >
                        <xsd:annotation>
                           <xsd:documentation source="FieldText">Per Application  [d:line1/] Table J</xsd:documentation>
                           <xsd:documentation source="CalculationsAndRules">If A03_OutdoorLightingZoneType == OutdoorLZ0 then result = BlankGrayedOut  stored in I01b_NotApplicableMessage and field is grayed out; Else user input.  If user checks field, result = true and Section J is required, Else result = false.</xsd:documentation>
                        </xsd:annotation>
                     </xsd:element>
                     <xsd:element name="I01b_NotApplicableMessage" >
                        <xsd:annotation>
                           <xsd:documentation source="FieldText">Per Application  [d:line1/] Table J</xsd:documentation>
                           <xsd:documentation source="CalculationsAndRules">If A03_OutdoorLightingZoneType == OutdoorLZ0 then result = BlankGrayedOut  stored in I01b_NotApplicableMessage and field is grayed out; Else user input.  If user checks field, result = true and Section J is required, Else result = false.</xsd:documentation>
                        </xsd:annotation>
                        <xsd:simpleType>
                           <xsd:restriction base="comp:NotApplicableMessage">
                              <xsd:enumeration value="BlankGrayedOut"/>
                           </xsd:restriction>
                        </xsd:simpleType>
                     </xsd:element>
                  </xsd:choice>
                  <xsd:choice maxOccurs="1">
                     <xsd:element name="I01c_UsingSalesFrontageAllowance" type="comp:UsingSalesFrontageAllowance" >
                        <xsd:annotation>
                           <xsd:documentation source="FieldText">Sales Frontage   [d:line1/]  Table K</xsd:documentation>
                           <xsd:documentation source="CalculationsAndRules">If A03_OutdoorLightingZoneType == OutdoorLZ0 Or OutdoorLZ1 Or 
 If A05_WholeBuildingOccupancy == MultiFamilyMixedUseGTE4 
result = BlankGrayedOut stored in I01c_NotApplicableMessage and field is grayed out; 
Else user input.  If user checks field, result = true and Section K is required, Else result = false.</xsd:documentation>
                        </xsd:annotation>
                     </xsd:element>
                     <xsd:element name="I01c_NotApplicableMessage" >
                        <xsd:annotation>
                           <xsd:documentation source="FieldText">Sales Frontage   [d:line1/]  Table K</xsd:documentation>
                           <xsd:documentation source="CalculationsAndRules">If A03_OutdoorLightingZoneType == OutdoorLZ0 Or OutdoorLZ1 Or 
 If A05_WholeBuildingOccupancy == MultiFamilyMixedUseGTE4 
then result = BlankGrayedOut  stored in I01c_NotApplicableMessage and field is grayed out; Else user input.  If user checks field, result = true and Section K is required, Else result = false.</xsd:documentation>
                        </xsd:annotation>
                        <xsd:simpleType>
                           <xsd:restriction base="comp:NotApplicableMessage">
                              <xsd:enumeration value="BlankGrayedOut"/>
                           </xsd:restriction>
                        </xsd:simpleType>
                     </xsd:element>
                  </xsd:choice>
                  <xsd:choice maxOccurs="1">
                     <xsd:element name="I01d_UsingOrnamentalAllowance" type="comp:UsingOrnamentalAllowance" >
                        <xsd:annotation>
                           <xsd:documentation source="FieldText">Ornamental  [d:line1/]  Table L</xsd:documentation>
                           <xsd:documentation source="CalculationsAndRules">If A03_OutdoorLightingZoneType == OutdoorLZ0 Or OutdoorLZ1 then result = BlankGrayedOut  stored in I01d_NotApplicableMessage and field is grayed out; Else user input.  If user checks field, result = true and Section L is required, Else result = false.</xsd:documentation>
                        </xsd:annotation>
                     </xsd:element>
                     <xsd:element name="I01d_NotApplicableMessage" >
                        <xsd:annotation>
                           <xsd:documentation source="FieldText">Ornamental  [d:line1/]  Table L</xsd:documentation>
                           <xsd:documentation source="CalculationsAndRules">If A03_OutdoorLightingZoneType == OutdoorLZ0 Or OutdoorLZ1 then result = BlankGrayedOut  stored in I01d_NotApplicableMessage and field is grayed out; Else user input.  If user checks field, result = true and Section L is required, Else result = false.</xsd:documentation>
                        </xsd:annotation>
                        <xsd:simpleType>
                           <xsd:restriction base="comp:NotApplicableMessage">
                              <xsd:enumeration value="BlankGrayedOut"/>
                           </xsd:restriction>
                        </xsd:simpleType>
                     </xsd:element>
                  </xsd:choice>
                  <xsd:choice maxOccurs="1">
                     <xsd:element name="I01e_UsingSpecificAreaAllowance" type="comp:UsingSpecificAreaAllowance" >
                        <xsd:annotation>
                           <xsd:documentation source="FieldText">Per Specific Area  [d:line1/]  Table M</xsd:documentation>
                           <xsd:documentation source="CalculationsAndRules">If A03_OutdoorLightingZoneType == OutdoorLZ0 then result = BlankGrayedOut  stored in I01e_NotApplicableMessage and field is grayed out; Else user input.  If user checks field, result = true and Section M is required, Else result = false.</xsd:documentation>
                        </xsd:annotation>
                     </xsd:element>
                     <xsd:element name="I01e_NotApplicableMessage" >
                        <xsd:annotation>
                           <xsd:documentation source="FieldText">Per Specific Area  [d:line1/]  Table M</xsd:documentation>
                           <xsd:documentation source="CalculationsAndRules">If A03_OutdoorLightingZoneType == OutdoorLZ0 then result = BlankGrayedOut  stored in I01e_NotApplicableMessage and field is grayed out; Else user input.  If user checks field, result = true and Section M is required, Else result = false.</xsd:documentation>
                        </xsd:annotation>
                        <xsd:simpleType>
                           <xsd:restriction base="comp:NotApplicableMessage">
                              <xsd:enumeration value="BlankGrayedOut"/>
                           </xsd:restriction>
                        </xsd:simpleType>
                     </xsd:element>
                  </xsd:choice>
                  <xsd:element name="I_AllowanceChoice" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                     </xsd:annotation>
                     <xsd:complexType>
                        <xsd:choice >
                           <xsd:element name="GeneralAllowanceNR" >
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText"></xsd:documentation>
                                 <xsd:documentation source="CalculationsAndRules">This subsection choice is required if I01a_UsingGeneralHardscapeAllowance == true And
 A05_WholeBuildingOccupancy != MultiFamilyMixedUseGTE4</xsd:documentation>
                              </xsd:annotation>
                              <xsd:complexType>
                                 <xsd:sequence>
                                    <xsd:element name="I_ColumnSubheading03To05" minOccurs="0">
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText"></xsd:documentation>
                                          <xsd:documentation source="AdditionalRequirements">Area Wattage Allowance (AWA)</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="I_ColumnSubheading06To08" minOccurs="0">
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText"></xsd:documentation>
                                          <xsd:documentation source="AdditionalRequirements">Linear Wattage Allowance (LWA)</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="TableAllowanceNR" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Calculated General Hardscape Lighting Power Allowance per Table 140.7-A for Nonresidential &amp; Hotel/Motel</xsd:documentation>
                                       </xsd:annotation>
                                       <xsd:complexType>
                                          <xsd:sequence>
                                             <xsd:element name="Row" minOccurs="1" maxOccurs="unbounded">
                                                <xsd:complexType>
                                                   <xsd:sequence>
                                                      <xsd:element name="I02_SpaceDescription" type="com:SpaceDescription" >
                                                         <xsd:annotation>
                                                            <xsd:documentation source="FieldText">Area Description</xsd:documentation>
                                                            <xsd:documentation source="CalculationsAndRules">User input</xsd:documentation>
                                                         </xsd:annotation>
                                                      </xsd:element>
                                                      <xsd:element name="I03_LightedHardscapeArea" type="com:LightedHardscapeArea" >
                                                         <xsd:annotation>
                                                            <xsd:documentation source="FieldText">Illuminated Area (ft[d:sup]2[/d:sup])</xsd:documentation>
                                                            <xsd:documentation source="CalculationsAndRules">User input</xsd:documentation>
                                                         </xsd:annotation>
                                                      </xsd:element>
                                                      <xsd:element name="I04_HardscapeAllowedLPD" type="comp:HardscapeAllowedLPD" >
                                                         <xsd:annotation>
                                                            <xsd:documentation source="FieldText">Allowed Density (W/ft[d:sup]2[/d:sup])</xsd:documentation>
                                                            <xsd:documentation source="CalculationsAndRules">Calculated Value in W /ft2  from 2022 Table 140.7-A  
If A03_OutdoorLightingZoneType == OutdoorLZ0 result = 0;  
Else  If A03_OutdoorLightingZoneType == OutdoorLZ1 result = 0.016; 
Else  If A03_OutdoorLightingZoneType ==  OutdoorLZ2 result = 0.019
Else  If A03_OutdoorLightingZoneType ==  OutdoorLZ3 result = 0.021
Else  If A03_OutdoorLightingZoneType ==  OutdoorLZ4 result = 0.024
End If</xsd:documentation>
                                                         </xsd:annotation>
                                                      </xsd:element>
                                                      <xsd:element name="I05_HardscapeAreaAllowance" type="comp:HardscapeAreaAllowance" >
                                                         <xsd:annotation>
                                                            <xsd:documentation source="FieldText">Area Allowance (Watts)</xsd:documentation>
                                                            <xsd:documentation source="CalculationsAndRules">Calculated Value: result = I03_LightedHardscapeArea x I04_HardscapeAllowedLPD</xsd:documentation>
                                                         </xsd:annotation>
                                                      </xsd:element>
                                                      <xsd:element name="I06_Perimeter" type="com:Perimeter" >
                                                         <xsd:annotation>
                                                            <xsd:documentation source="FieldText">Perimeter Length (lf)</xsd:documentation>
                                                            <xsd:documentation source="CalculationsAndRules">User input</xsd:documentation>
                                                         </xsd:annotation>
                                                      </xsd:element>
                                                      <xsd:element name="I07_HardscapeAllowedLinearDensity" type="comp:HardscapeAllowedLinearDensity" >
                                                         <xsd:annotation>
                                                            <xsd:documentation source="FieldText">Allowed Density (W/lf)</xsd:documentation>
                                                            <xsd:documentation source="CalculationsAndRules">Calculated Value in W /lf  from 2022 Table 140.7-A  
If A03_OutdoorLightingZoneType == OutdoorLZ0 result = 0;
 Else If A03_OutdoorLightingZoneType == OutdoorLZ1 result = 0.13; 
Else If A03_OutdoorLightingZoneType ==  OutdoorLZ2 result = 0.15
Else If A03_OutdoorLightingZoneType ==  OutdoorLZ3 result = 0.20
Else If A03_OutdoorLightingZoneType ==  OutdoorLZ4 result = 0.29
End If</xsd:documentation>
                                                         </xsd:annotation>
                                                      </xsd:element>
                                                      <xsd:element name="I08_HardscapeLinearAllowance" type="comp:HardscapeLinearAllowance" >
                                                         <xsd:annotation>
                                                            <xsd:documentation source="FieldText">Linear Allowance (Watts)</xsd:documentation>
                                                            <xsd:documentation source="CalculationsAndRules">Calculated Value: result =  I06_Perimeter x I07_HardscapeAllowedLinearDensity</xsd:documentation>
                                                         </xsd:annotation>
                                                      </xsd:element>
                                                      <xsd:element name="I09_HardscapeAreaTotalAllowance" type="comp:HardscapeAreaTotalAllowance" >
                                                         <xsd:annotation>
                                                            <xsd:documentation source="FieldText">Total General AWA + LWA (Watts)</xsd:documentation>
                                                            <xsd:documentation source="CalculationsAndRules">Calculated Value: result = I05_HardscapeAreaAllowance +  I08_HardscapeLinearAllowance</xsd:documentation>
                                                         </xsd:annotation>
                                                      </xsd:element>
                                                   </xsd:sequence>
                                                </xsd:complexType>
                                             </xsd:element>
                                          </xsd:sequence>
                                       </xsd:complexType>
                                    </xsd:element>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="I10_HardscapeInitialAllowance" type="comp:HardscapeInitialAllowance" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">[d:b]Initial Wattage Allowance for Entire Site (Watts):[/d:b]</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">If B02_DoesAlterationIncreaseLightingLoad == true result = BlankGrayedOut stored in NotApplicableMessage and cell is blank and grayed out; 
Else calculated Value in W  from 2022 Table 140.7-A 
 If A03_OutdoorLightingZoneType == OutdoorLZ0 result = 15;
 Else If A03_OutdoorLightingZoneType ==   OutdoorLZ1 result = 150; 
Else If A03_OutdoorLightingZoneType ==   OutdoorLZ2
result = 200; 
Else If A03_OutdoorLightingZoneType ==   OutdoorLZ3 result = 250; 
Else If A03_OutdoorLightingZoneType == OutdoorLZ4 result = 320
End If
End If</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="I10_NotApplicableMessage" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">[d:b]Initial Wattage Allowance for Entire Site (Watts):[/d:b]</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">If if B01a_LightingProjectScopeNR is false OR if (B01a_LightingProjectScopeNR true and I01a_UsingGeneralHardscapeAllowance is false) 
result = BlankGrayedOut stored in NotApplicableMessage and cell is blank and grayed out;</xsd:documentation>
                                          </xsd:annotation>
                                          <xsd:simpleType>
                                             <xsd:restriction base="comp:NotApplicableMessage">
                                                <xsd:enumeration value="BlankGrayedOut"/>
                                             </xsd:restriction>
                                          </xsd:simpleType>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="I11_InitialAllowanceCount" type="comp:InitialAllowanceCount" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">[d:b]Instances of Initial Wattage Allowance (LZ 0 only)[d:sup]1[/d:sup][/d:b]</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">If A03_OutdoorLightingZoneType == OutdoorLZ0 
User input
Else
result = BlankGrayedOut stored in NotApplicableMessage
End If</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="I11_NotApplicableMessage" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">[d:b]Instances of Initial Wattage Allowance (LZ 0 only)[d:sup]1[/d:sup][/d:b]</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">If A03_OutdoorLightingZoneType != OutdoorLZ0 
result = BlankGrayedOut stored in NotApplicableMessage
End If</xsd:documentation>
                                          </xsd:annotation>
                                          <xsd:simpleType>
                                             <xsd:restriction base="comp:NotApplicableMessage">
                                                <xsd:enumeration value="BlankGrayedOut"/>
                                             </xsd:restriction>
                                          </xsd:simpleType>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:element name="ITotals09_HardscapeGeneralAllowance" type="comp:HardscapeGeneralAllowance" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">[d:b]Total General Hardscape Allowance  (Watts):[/d:b]</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">Calculated Value:  
If B01a_LightingProjectScopeNR is false OR if (B01a_LightingProjectScopeNR true and I01a_UsingGeneralHardscapeAllowance is false)
result = SUM All ( I09_HardscapeAreaTotalAllowance)
Else If I11_NotApplicableMessage IsPresent
result = SUM All ( I09_HardscapeAreaTotalAllowance) +  I10_HardscapeInitialAllowance
Else
result = SUM All ( I09_HardscapeAreaTotalAllowance) + ( I10_HardscapeInitialAllowance x I11_InitialAllowanceCount)
End If</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                 </xsd:sequence>
                              </xsd:complexType>
                           </xsd:element>
                           <xsd:element name="GeneralAllowanceMF" >
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText"></xsd:documentation>
                                 <xsd:documentation source="CalculationsAndRules">This subsection choice is required if I01a_UsingGeneralHardscapeAllowance == true And
 A05_WholeBuildingOccupancy == MultiFamilyMixedUseGTE4</xsd:documentation>
                              </xsd:annotation>
                              <xsd:complexType>
                                 <xsd:sequence>
                                    <xsd:element name="I_ColumnSubheading04To06" minOccurs="0">
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText"></xsd:documentation>
                                          <xsd:documentation source="AdditionalRequirements">Area Wattage Allowance (AWA)</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="TableAllowanceMF" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Calculated General Hardscape Lighting Power Allowance per Table 170.2-R for Multifamily &amp; Multifamily Mixed-Use</xsd:documentation>
                                       </xsd:annotation>
                                       <xsd:complexType>
                                          <xsd:sequence>
                                             <xsd:element name="Row" minOccurs="1" maxOccurs="unbounded">
                                                <xsd:complexType>
                                                   <xsd:sequence>
                                                      <xsd:element name="I02mf_SpaceDescription" type="com:SpaceDescription" >
                                                         <xsd:annotation>
                                                            <xsd:documentation source="FieldText">Area Description</xsd:documentation>
                                                            <xsd:documentation source="CalculationsAndRules">User input</xsd:documentation>
                                                         </xsd:annotation>
                                                      </xsd:element>
                                                      <xsd:element name="I03mf_LightedHardscapeArea" type="com:LightedHardscapeArea" >
                                                         <xsd:annotation>
                                                            <xsd:documentation source="FieldText">Illuminated Area (ft[d:sup]2[/d:sup])</xsd:documentation>
                                                            <xsd:documentation source="CalculationsAndRules">User input</xsd:documentation>
                                                         </xsd:annotation>
                                                      </xsd:element>
                                                      <xsd:element name="I04mf_HardscapeAllowedLPD" type="comp:HardscapeAllowedLPD" >
                                                         <xsd:annotation>
                                                            <xsd:documentation source="FieldText">Allowed Density  (W/ft[d:sup]2[/d:sup])</xsd:documentation>
                                                            <xsd:documentation source="CalculationsAndRules">Calculated Value in W /ft2  from 2022 Table 170.2-R   
If A03_OutdoorLightingZoneType == OutdoorLZ0 result = 0.00; 
Else If A03_OutdoorLightingZoneType == OutdoorLZ1 result = 0.026
Else If A03_OutdoorLightingZoneType == OutdoorLZ2 result = 0.03;   
Else If  A03_OutdoorLightingZoneType == OutdoorLZ3 result = 0.038; 
Else  If  A03_OutdoorLightingZoneType == OutdoorLZ4 result = 0.055
End If</xsd:documentation>
                                                         </xsd:annotation>
                                                      </xsd:element>
                                                      <xsd:element name="I05mf_HardscapeAreaAllowance" type="comp:HardscapeAreaAllowance" >
                                                         <xsd:annotation>
                                                            <xsd:documentation source="FieldText">Area Allowance (Watts)</xsd:documentation>
                                                            <xsd:documentation source="CalculationsAndRules">Calculated Value: result = I03mf_LightedHardscapeArea x  I04mf_HardscapeAllowedLPD</xsd:documentation>
                                                         </xsd:annotation>
                                                      </xsd:element>
                                                   </xsd:sequence>
                                                </xsd:complexType>
                                             </xsd:element>
                                          </xsd:sequence>
                                       </xsd:complexType>
                                    </xsd:element>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="I06mf_HardscapeInitialAllowance" type="comp:HardscapeInitialAllowance" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">[d:b]Initial Wattage Allowance for Entire Site (Watts):[/d:b]</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">If B02_DoesAlterationIncreaseLightingLoad == true 
result = BlankGrayedOut stored in NotApplicableMessage and cell is grayed out; 
Else calculated Value in W  from 2022 Table 170.2-R   
If A03_OutdoorLightingZoneType == OutdoorLZ0 result = 15 
Else If A03_OutdoorLightingZoneType == OutdoorLZ1 result = 300
Else If A03_OutdoorLightingZoneType == OutdoorLZ2 result = 350  
Else If  A03_OutdoorLightingZoneType == OutdoorLZ3 result = 400 
Else  If  A03_OutdoorLightingZoneType == OutdoorLZ4 result = 450
End If</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="I06mf_NotApplicableMessage" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">[d:b]Initial Wattage Allowance for Entire Site (Watts):[/d:b]</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">If if B01a_LightingProjectScopeNR is false OR if (B01a_LightingProjectScopeNR true and I01a_UsingGeneralHardscapeAllowance is false)
result =  BlankGrayedOut stored in NotApplicableMessage and cell is grayed out;   
End If</xsd:documentation>
                                          </xsd:annotation>
                                          <xsd:simpleType>
                                             <xsd:restriction base="comp:NotApplicableMessage">
                                                <xsd:enumeration value="BlankGrayedOut"/>
                                             </xsd:restriction>
                                          </xsd:simpleType>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="I07mf_InitialAllowanceCount" type="comp:InitialAllowanceCount" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">[d:b]Instances of Initial Wattage Allowance (LZ 0 only)[d:sup]1[/d:sup][/d:b]</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">If A03_OutdoorLightingZoneType == OutdoorLZ0 
User input
Else
result = BlankGrayedOut stored in NotApplicableMessage
End If</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="I07mf_NotApplicableMessage" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">[d:b]Instances of Initial Wattage Allowance (LZ 0 only)[d:sup]1[/d:sup][/d:b]</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">If A03_OutdoorLightingZoneType != OutdoorLZ0 
result = BlankGrayedOut stored in NotApplicableMessage
End If</xsd:documentation>
                                          </xsd:annotation>
                                          <xsd:simpleType>
                                             <xsd:restriction base="comp:NotApplicableMessage">
                                                <xsd:enumeration value="BlankGrayedOut"/>
                                             </xsd:restriction>
                                          </xsd:simpleType>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:element name="I08mf_HardscapeGeneralAllowance" type="comp:HardscapeGeneralAllowance" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">[d:b]Total General Hardscape Allowance  (Watts):[/d:b]</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">Calculated Value:  
If B01a_LightingProjectScopeNR is false OR if (B01a_LightingProjectScopeNR true and I01a_UsingGeneralHardscapeAllowance is false)
result = SUM All ( I05mf_HardscapeAreaTotalAllowance)
Else If I07mf_NotApplicableMessage IsPresent
result = SUM All ( I05mf_HardscapeAreaTotalAllowance) +   I06mf_HardscapeInitialAllowance
Else
result = SUM All ( I05mf_HardscapeAreaTotalAllowance) +  (I06mf_HardscapeInitialAllowance x I07mf_InitialAllowanceCount)
End If</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                 </xsd:sequence>
                              </xsd:complexType>
                           </xsd:element>
                        </xsd:choice>
                     </xsd:complexType>
                  </xsd:element>
                  <xsd:element name="IEndNote1" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">[d:i][d:sup]1[/d:sup]FOOTNOTES: Number of entrances to parking areas, trail heads, fee payment kiosks, outhouses and toilet facilities on site where lighting is within project scope. [/d:i]</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="IErrorMessage_SectionErrorMessage" type="comp:SectionErrorMessage"  minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
         <xsd:element name="Section_J" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation source="FieldText">[d:b]J. LIGHTING ALLOWANCE: PER APPLICATION[/d:b]</xsd:documentation>
               <xsd:documentation source="CalculationsAndRules">This section applies if in section I I01b_UsingApplicationAllowance == true.</xsd:documentation>
               <xsd:documentation source="MinOccurs">If this section doesn't apply, do not display anything for this section</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="J_BeginNote1" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">[d:i]This table includes areas using the wattage allowance per application from Table 140.7-B / Table 170.2-S.[/d:i]</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="J_ColumnSubheading03To05" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">[d:b]CALCULATED ALLOWANCE (Watts)[/d:b]</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="J_ColumnSubheading06To09" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">[d:b]DESIGN WATTS[/d:b]</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="TableApplicationAllowance" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                     </xsd:annotation>
                     <xsd:complexType>
                        <xsd:sequence>
                           <xsd:element name="Row" minOccurs="1" maxOccurs="unbounded">
                              <xsd:complexType>
                                 <xsd:sequence>
                                    <xsd:element name="J01_SpaceDescription" type="com:SpaceDescription" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Area Description</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">User input</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="J02_UseOrLoseApplicationType" type="comp:UseOrLoseApplicationType" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Application per Table 140.7-B [d:sup]1[/d:sup]</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">User selects from dropdown list of application types: ATM display term ATM Machine, DriveupWindow display term Drive up Window, EntranceExit display term Building Entrance/Exit, FuelDispenser display term Uncovered Fuel Dispenser, PrimaryEntrance display term Primary Entrance (Special)</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="J03_UseOrLoseApplicationLocationCount" type="comp:UseOrLoseApplicationLocationCount" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText"># of Locations</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">User input</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="J04_UseOrLoseApplicationLocationAllowance" type="comp:UseOrLoseApplicationLocationAllowance" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Allowance per Location[d:sup]2[/d:sup]</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">Calculated Value in W from Table 140.7-B  dependent on combination of values from J02_UseOrLoseApplicationType And A03_OutdoorLightingZoneType values: 
If J02_UseOrLoseApplicationType == EntranceExit 
Switch (A03_OutdoorLightingZoneType) case: OutdoorLZ0 result = 0;  case: OutdoorLZ1 result = 9; case: OutdoorLZ2 result = 15; case: OutdoorLZ3 result = 19; case: OutdoorLZ4 result = 21;  
 
Else If J02_UseOrLoseApplicationType == PrimaryEntrance 
Switch (A03_OutdoorLightingZoneType) case: OutdoorLZ0 result = 0;  case: OutdoorLZ1 result = 20; case: OutdoorLZ2 result = 40; case: OutdoorLZ3 result = 57; case: OutdoorLZ4 result = 60;  
 
Else If J02_UseOrLoseApplicationType = DriveupWindow 
Switch (A03_OutdoorLightingZoneType) case: OutdoorLZ0 result = 0; case: OutdoorLZ1 result = 16; case: OutdoorLZ2 result = 30; case: OutdoorLZ3 result = 50; case: OutdoorLZ4 result = 75; 
 
Else If J02_UseOrLoseApplicationType == FuelDispenser 
Switch (A03_OutdoorLightingZoneType) case: OutdoorLZ0 result = 0; case: OutdoorLZ1 result = 55; case: OutdoorLZ2 result = 77; case: OutdoorLZ3 result = 81; case: OutdoorLZ4 result = 135;     
 
Else If J02_UseOrLoseApplicationType == ATM  Switch (A03_OutdoorLightingZoneType) case: OutdoorLZ0 result = 0; case: OutdoorLZ1  OutdoorLZ2  OutdoorLZ3 Or OutdoorLZ4 result  = 100 for first ATM and  for each additional ATM result = 35.
End If</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="J05_ExtraPowerCalculatedForArea" type="comp:ExtraPowerCalculatedForArea" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Extra Allowance (Watts)</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">Calculated Value: If J02_UseOrLoseApplicationType != ATM result = J03_UseOrLoseApplicationLocationCount x  J04_UseOrLoseApplicationLocationAllowance 
Else If J02_UseOrLoseApplicationType == ATM  result = 100 + (( J03_UseOrLoseApplicationLocationCount - 1) x 35)</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="TableLuminaires" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText"></xsd:documentation>
                                       </xsd:annotation>
                                       <xsd:complexType>
                                          <xsd:sequence>
                                             <xsd:element name="Row" minOccurs="1" maxOccurs="unbounded">
                                                <xsd:complexType>
                                                   <xsd:sequence>
                                                      <xsd:element name="J06_LuminaireName" type="lit:LuminaireName" >
                                                         <xsd:annotation>
                                                            <xsd:documentation source="FieldText">Luminaire Name or Item Tag</xsd:documentation>
                                                            <xsd:documentation source="CalculationsAndRules">User selects from dropdown list loaded with values from F01_LuminaireName where  If F07_LightingPowerExcluded ==  false</xsd:documentation>
                                                         </xsd:annotation>
                                                      </xsd:element>
                                                      <xsd:choice maxOccurs="1">
                                                         <xsd:element name="J07_LuminairePower" type="lit:LuminairePower" >
                                                            <xsd:annotation>
                                                               <xsd:documentation source="FieldText">Watts  per Luminaire</xsd:documentation>
                                                               <xsd:documentation source="CalculationsAndRules">Auto-filled for  the luminaire selected in  J06_LuminaireName as follows: 
If F02a_IsLuminaireLinear == false result = F03_LuminairePower; 
Else result = F03_LuminaireWattsPerLF
End If</xsd:documentation>
                                                            </xsd:annotation>
                                                         </xsd:element>
                                                         <xsd:element name="J07_LuminaireWattsPerLF" type="lit:LuminaireWattsPerLF" >
                                                            <xsd:annotation>
                                                               <xsd:documentation source="FieldText">Watts  per Luminaire</xsd:documentation>
                                                               <xsd:documentation source="CalculationsAndRules">Auto-filled for  the luminaire selected in  J06_LuminaireName as follows: 
If F02a_IsLuminaireLinear == false result = F03_LuminairePower; 
Else result = F03_LuminaireWattsPerLF
End If</xsd:documentation>
                                                            </xsd:annotation>
                                                         </xsd:element>
                                                      </xsd:choice>
                                                      <xsd:choice maxOccurs="1">
                                                         <xsd:element name="J08_LuminaireCountByType" type="lit:LuminaireCountByType" >
                                                            <xsd:annotation>
                                                               <xsd:documentation source="FieldText"># of Luminaires</xsd:documentation>
                                                               <xsd:documentation source="CalculationsAndRules">User input as follows: 
If F02a_IsLuminaireLinear == false for this luminaire user enters count  stored in J08_LuminaireCountByType; 
Else user enters linear feet which is stored in J08_LuminairesTotalLF
End If</xsd:documentation>
                                                            </xsd:annotation>
                                                         </xsd:element>
                                                         <xsd:element name="J08_LuminairesTotalLF" type="lit:LuminairesTotalLF" >
                                                            <xsd:annotation>
                                                               <xsd:documentation source="FieldText"># of Luminaires</xsd:documentation>
                                                               <xsd:documentation source="CalculationsAndRules">User input as follows: 
If F02a_IsLuminaireLinear == false for this luminaire user enters count  stored in J08_LuminaireCountByType; 
Else user enters linear feet which is stored in J08_LuminairesTotalLF
End If</xsd:documentation>
                                                            </xsd:annotation>
                                                         </xsd:element>
                                                      </xsd:choice>
                                                      <xsd:element name="J09_LuminairePowerByType" type="lit:LuminairePowerByType" >
                                                         <xsd:annotation>
                                                            <xsd:documentation source="FieldText">Design Watts</xsd:documentation>
                                                            <xsd:documentation source="CalculationsAndRules">Calculated value: 
If F02a_IsLuminaireLinear == false result = J07_LuminairePower x J08_LuminaireCountByType 
Else result = J07_LuminaireWattsPerLF x J08_LuminairesTotalLF
End If</xsd:documentation>
                                                         </xsd:annotation>
                                                      </xsd:element>
                                                   </xsd:sequence>
                                                </xsd:complexType>
                                             </xsd:element>
                                          </xsd:sequence>
                                       </xsd:complexType>
                                    </xsd:element>
                                    <xsd:element name="JTotals09_LuminairePowerInArea" type="lit:LuminairePowerInArea" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Total Design Watts for this Area:</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">Calculated value: result = SUM All (J09_LuminairePowerByType)</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="J10_AllExtraPowerAllowedForArea" type="comp:AllExtraPowerAllowedForArea" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Additional Allowance (Watts)</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">Calculated value: result = Smallest( J05_ExtraPowerCalculatedForArea, JTotals09_LuminairePowerInArea)</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                 </xsd:sequence>
                              </xsd:complexType>
                           </xsd:element>
                        </xsd:sequence>
                     </xsd:complexType>
                  </xsd:element>
                  <xsd:element name="JTotals10_UseOrLoseApplicationTotalAllowance" type="comp:UseOrLoseApplicationTotalAllowance" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">[d:b]Total Allowance (Watts) All Areas:[/d:b]</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">Calculated value: result = SUM All (J10_AllExtraPowerAllowdForArea)</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="JEndNote1" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">[d:i][d:sup]1[/d:sup] FOOTNOTES: Primary entrance applications are only available for senior care facilities, healthcare facilities, police stations, hospitals, fire stations, and emergency vehicle facilities.[/d:i]</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="JEndNote2" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">[d:i][d:sup]2[/d:sup] The Allowance per Location for ATMs is 100W for the first ATM and 35W for each additional per  Table 140.7-B /Table 170.2-S.[/d:i]</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="JEndNote3" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">[d:i][d:sup]3[/d:sup] For luminaires indicated in Table F as linear, wattage in column 07 is W/lf instead of Watts/luminaire.  Total linear feet should be indicated in column 08 instead of number of luminaires. [/d:i]</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="JErrorMessage_SectionErrorMessage" type="comp:SectionErrorMessage"  minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
         <xsd:element name="Section_K" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation source="FieldText">[d:b]K. LIGHTING ALLOWANCE: SALES FRONTAGE[/d:b]</xsd:documentation>
               <xsd:documentation source="CalculationsAndRules">This section applies if I01c_UsingSalesFrontageAllowance == true.</xsd:documentation>
               <xsd:documentation source="MinOccurs">If this section doesn't apply, do not display anything for this section</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="K_BeginNote1" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">[d:i]The table includes areas using wattage allowance for Outdoor Sales Frontage from Table-140.7-B.  This allowance may be used for one or two frontage sides per site.[/d:i]</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="K_ColumnSubheading02To04" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">[d:b]CALCULATED ALLOWANCE(Watts)[/d:b]</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="K_ColumnSubheading05To08" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">[d:b]DESIGN WATTS[/d:b]</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="TableSalesFrontageAllowance" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                     </xsd:annotation>
                     <xsd:complexType>
                        <xsd:sequence>
                           <xsd:element name="Row" minOccurs="1" maxOccurs="unbounded">
                              <xsd:complexType>
                                 <xsd:sequence>
                                    <xsd:element name="K01_SpaceDescription" type="com:SpaceDescription" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Area Description</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">User input</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="K02_UseOrLoseSalesFrontageLinearFeet" type="comp:UseOrLoseSalesFrontageLinearFeet" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Linear ft. of Sales Frontage (lf)</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">User input</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="K03_UseOrLoseSalesFrontageLinearFeetAllowance" type="comp:UseOrLoseSalesFrontageLinearFeetAllowance" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Allowed Density (W/lf)</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">Calculated Value in W  from Table 140.7-B  If A03_OutdoorLightingZoneType == OutdoorLZ0 Or OutdoorLZ1  result = 0; Else If A03_OutdoorLightingZoneType == OutdoorLZ2 result = 11; Else If A03_OutdoorLightingZoneType == OutdoorLZ3 result = 19; Else If A03_OutdoorLightingZoneType == OutdoorLZ4 result = 25</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="K04_ExtraPowerCalculatedForArea" type="comp:ExtraPowerCalculatedForArea" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Extra Allowance (Watts)</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">Calculated value: result = PRODUCT (K02_UseOrLoseSalesFrontageLinearFeet, K03_UseOrLoseSalesFrontageLinearFeetAllowance)</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="TableLuminaires" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText"></xsd:documentation>
                                       </xsd:annotation>
                                       <xsd:complexType>
                                          <xsd:sequence>
                                             <xsd:element name="Row" minOccurs="1" maxOccurs="unbounded">
                                                <xsd:complexType>
                                                   <xsd:sequence>
                                                      <xsd:element name="K05_LuminaireName" type="lit:LuminaireName" >
                                                         <xsd:annotation>
                                                            <xsd:documentation source="FieldText">Luminaire Name or Item Tag</xsd:documentation>
                                                            <xsd:documentation source="CalculationsAndRules">User selects from dropdown list loaded with values from F01_LuminaireName where  If F07_LightingPowerExcluded ==  false</xsd:documentation>
                                                         </xsd:annotation>
                                                      </xsd:element>
                                                      <xsd:choice maxOccurs="1">
                                                         <xsd:element name="K06_LuminairePower" type="lit:LuminairePower" >
                                                            <xsd:annotation>
                                                               <xsd:documentation source="FieldText">Watts  per Luminaire</xsd:documentation>
                                                               <xsd:documentation source="CalculationsAndRules">Auto-filled for  the luminaire selected in  K05_LuminaireName as follows: If F02a_IsLuminaireLinear == false result = F03_LuminairePower; Else result = F03_LuminaireWattsPerLF</xsd:documentation>
                                                            </xsd:annotation>
                                                         </xsd:element>
                                                         <xsd:element name="K06_LuminaireWattsPerLF" type="lit:LuminaireWattsPerLF" >
                                                            <xsd:annotation>
                                                               <xsd:documentation source="FieldText">Watts  per Luminaire</xsd:documentation>
                                                               <xsd:documentation source="CalculationsAndRules">Auto-filled for  the luminaire selected in  K05_LuminaireName as follows: If F02a_IsLuminaireLinear == false result = F03_LuminairePower; Else result = F03_LuminaireWattsPerLF</xsd:documentation>
                                                            </xsd:annotation>
                                                         </xsd:element>
                                                      </xsd:choice>
                                                      <xsd:choice maxOccurs="1">
                                                         <xsd:element name="K07_LuminaireCountByType" type="lit:LuminaireCountByType" >
                                                            <xsd:annotation>
                                                               <xsd:documentation source="FieldText"># of Luminaires</xsd:documentation>
                                                               <xsd:documentation source="CalculationsAndRules">User input as follows: If F02a_IsLuminaireLinear == false for this luminaire user enters count  stored in K07_LuminaireCountByType; Else user enters linear feet which is stored in K07_LuminairesTotalLF</xsd:documentation>
                                                            </xsd:annotation>
                                                         </xsd:element>
                                                         <xsd:element name="K07_LuminairesTotalLF" type="lit:LuminairesTotalLF" >
                                                            <xsd:annotation>
                                                               <xsd:documentation source="FieldText"># of Luminaires</xsd:documentation>
                                                               <xsd:documentation source="CalculationsAndRules">User input as follows: If F02a_IsLuminaireLinear == false for this luminaire user enters count  stored in K07_LuminaireCountByType; Else user enters linear feet which is stored in K07_LuminairesTotalLF</xsd:documentation>
                                                            </xsd:annotation>
                                                         </xsd:element>
                                                      </xsd:choice>
                                                      <xsd:element name="K08_LuminairePowerByType" type="lit:LuminairePowerByType" >
                                                         <xsd:annotation>
                                                            <xsd:documentation source="FieldText">Design Watts</xsd:documentation>
                                                            <xsd:documentation source="CalculationsAndRules">Calculated value: If F02a_IsLuminaireLinear == false result = PRODUCT (K06_LuminairePower, K07_LuminaireCountByType); Else result = PRODUCT (K06_LuminaireWattsPerLF, K07_LuminairesTotalLF</xsd:documentation>
                                                         </xsd:annotation>
                                                      </xsd:element>
                                                   </xsd:sequence>
                                                </xsd:complexType>
                                             </xsd:element>
                                          </xsd:sequence>
                                       </xsd:complexType>
                                    </xsd:element>
                                    <xsd:element name="KTotals08_AllLuminairePowerInArea" type="lit:AllLuminairePowerInArea" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Total Design Watts for this Area:</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">Calculated value: result = SUM All ( K08_LuminairePowerByType)</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="K09_AllExtraPowerCalculatedForArea" type="comp:AllExtraPowerCalculatedForArea" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Additional Allowance (Watts)</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">Calculated value: result = Smallest (K04_ExtraPowerCalculatedForArea,  KTotals08_AllLuminairePowerInArea)</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                 </xsd:sequence>
                              </xsd:complexType>
                           </xsd:element>
                        </xsd:sequence>
                     </xsd:complexType>
                  </xsd:element>
                  <xsd:element name="KTotals09_UseOrLoseSalesFrontageTotalAllowance" type="comp:UseOrLoseSalesFrontageTotalAllowance" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">[d:b]Total Allowance (Watts) All Areas:[/d:b]</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">Calculated value: result =SUM All ( K09_AllExtraPowerCalculatedForArea)</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="KEndNote1" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">[d:i][d:sup]1[/d:sup]FOOTNOTES: For luminaires indicated in Table F as linear, wattage in column 06 is W/lf instead of Watts/luminaire.  Total linear feet should be indicated in column 07 instead of number of luminaires. [/d:i]</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="KErrorMessage_SectionErrorMessage" type="comp:SectionErrorMessage"  minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
         <xsd:element name="Section_L" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation source="FieldText">[d:b]L. LIGHTING ALLOWANCE: ORNAMENTAL[/d:b]</xsd:documentation>
               <xsd:documentation source="CalculationsAndRules">This section applies if I01d_UsingOrnamentalAllowance == true.</xsd:documentation>
               <xsd:documentation source="MinOccurs">If this section doesn't apply, do not display anything for this section</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="L_BeginNote1" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">[d:i]This table includes all ornamental luminaires within the scope of the permit application to calculate the allowance per  Table-140.7-B /Table 170.2-S. [/d:i]</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="L_ColumnSubheading02To04" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">[d:b]CALCULATED ALLOWANCE(Watts)[/d:b]</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="L_ColumnSubheading05To08" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">[d:b]DESIGN WATTS[/d:b]</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="L01" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Area Description</xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">Total Illuminated Hardscape Area for Site</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="L02_LightedHardscapeAreaTotal" type="com:LightedHardscapeAreaTotal" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Illuminated Area (ft[d:sup]2[/d:sup])</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">Auto-filled result = A04_LightedHardscapeAreaTotal</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="L03_UseOrLoseOrnamentalAllowedDensity" type="comp:UseOrLoseOrnamentalAllowedDensity" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Allowed Density (W/ft[d:sup]2[/d:sup])</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">Calculated Value in W/ft2  from Table 140.7-B  If A03_OutdoorLightingZoneType == OutdoorLZ0 Or OutdoorLZ1 result = 0;  Else If A03_OutdoorLightingZoneType == OutdoorLZ2 result = 0.007; Else If A03_OutdoorLightingZoneType == OutdoorLZ3 result = 0.013; Else If A03_OutdoorLightingZoneType == OutdoorLZ4 result = 0.019</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="L04_ExtraPowerCalculatedForArea" type="comp:ExtraPowerCalculatedForArea" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Extra Allowance (Watts)</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">Calculated Value: result = PRODUCT (L02_LightedHardscapeAreaTotal, L03_UseOrLoseOrnamentalAllowedDensity)</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="TableLuminaires" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                     </xsd:annotation>
                     <xsd:complexType>
                        <xsd:sequence>
                           <xsd:element name="Row" minOccurs="1" maxOccurs="unbounded">
                              <xsd:complexType>
                                 <xsd:sequence>
                                    <xsd:element name="L05_LuminaireName" type="lit:LuminaireName" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Luminaire Name or Item Tag</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">User selects from dropdown list loaded with values from F01_LuminaireName where  If F07_LightingPowerExcluded ==  false</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="L06_LuminairePower" type="lit:LuminairePower" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Watts  per Luminaire[d:sup]1[/d:sup]</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">Auto-filled for  the luminaire selected in  L05_LuminaireName as follows: If F02a_IsLuminaireLinear == false result = F03_LuminairePower; Else result = F03_LuminaireWattsPerLF</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="L06_LuminaireWattsPerLF" type="lit:LuminaireWattsPerLF" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Watts  per Luminaire</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">Auto-filled for  the luminaire selected in  L05_LuminaireName as follows: If F02a_IsLuminaireLinear == false result = F03_LuminairePower; Else result = F03_LuminaireWattsPerLF</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="L07_LuminaireCountByType" type="lit:LuminaireCountByType" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText"># of Luminaires</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">User input as follows: If F02a_IsLuminaireLinear == false for this luminaire user enters count  stored in L07_LuminaireCountByType; Else user enters linear feet which is stored in L07_LuminairesTotalLF</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="L07_LuminairesTotalLF" type="lit:LuminairesTotalLF" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText"># of Luminaires</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">User input as follows: If F02a_IsLuminaireLinear == false for this luminaire user enters count  stored in L07_LuminaireCountByType; Else user enters linear feet which is stored in L07_LuminairesTotalLF</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:element name="L08_LuminairePowerByType" type="lit:LuminairePowerByType" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Design Watts</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">Calculated value: If F02a_IsLuminaireLinear == false result = PRODUCT (L06_LuminairePower, L07_LuminaireCountByType); Else result = PRODUCT (L06_LuminaireWattsPerLF, L07_LuminairesTotalLF</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                 </xsd:sequence>
                              </xsd:complexType>
                           </xsd:element>
                        </xsd:sequence>
                     </xsd:complexType>
                  </xsd:element>
                  <xsd:element name="LTotals08_AllLuminairePowerInArea" type="lit:AllLuminairePowerInArea" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Total Design Watts for this Area:</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">Calculated value: result = SUM All( L08_LuminairePowerByType)</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="L09_AllExtraPowerCalculatedForArea" type="comp:AllExtraPowerCalculatedForArea" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Additional Allowance (Watts)</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">Calculated value: result = Smallest (L04_ExtraPowerCalculatedForArea,  L08Totals_AllLuminairePowerInArea)</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="LTotals09_UseOrLoseOrnamentalTotalAllowance" type="comp:UseOrLoseOrnamentalTotalAllowance" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">[d:b]Total Allowance (Watts) Ornamental:[/d:b]</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">Auto-filled result = L09_AllExtraPowerCalculatedForArea</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="LEndNote1" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">[d:i][d:sup]1[/d:sup]FOOTNOTES: Luminaires qualifying for this allowance shall be rated [d:lte/] 50 W and shall be post-top luminaires, lanterns, pendants or chandeliers.[/d:i]</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="LEndNote2" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">[d:i][d:sup]2[/d:sup] For luminaires indicated in Table F as linear, wattage in column 06 is W/lf instead of Watts/luminaire.  Total linear feet should be indicated in column 07 instead of number of luminaires. [/d:i]</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="LErrorMessage_SectionErrorMessage" type="comp:SectionErrorMessage"  minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
         <xsd:element name="Section_M" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation source="FieldText">[d:b]M. LIGHTING ALLOWANCE: PER SPECIFIC AREA[/d:b]</xsd:documentation>
               <xsd:documentation source="CalculationsAndRules">This section applies if I01e_UsingSpecificAreaAllowance == true.</xsd:documentation>
               <xsd:documentation source="MinOccurs">If this section doesn't apply, do not display anything for this section</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="M_BeginNote1" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">[d:i]This table includes areas using the wattage allowance per specific area from Table 140.7-B /Table 170.2-S.  More than one specific area allowance may be taken in a single project, if applicable.  However, multiple specific area allowances may not be taken for the exact same area on the site.[/d:i]</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="M_ColumnSubheading03To05" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">[d:b]CALCULATED ALLOWANCE (Watts)[/d:b]</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="M_ColumnSubheading06To09" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">[d:b]DESIGN WATTS[/d:b]</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="TableSpecificAreaAllowance" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                     </xsd:annotation>
                     <xsd:complexType>
                        <xsd:sequence>
                           <xsd:element name="Row" minOccurs="1" maxOccurs="unbounded">
                              <xsd:complexType>
                                 <xsd:sequence>
                                    <xsd:element name="M01_SpaceDescription" type="com:SpaceDescription" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Area Description</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">User input</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="M02_UseOrLoseSpecificAreaType" type="comp:UseOrLoseSpecificAreaType" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Specific Area Type per  Table 140.7-B</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">User selects from dropdown list options based on A03_OutdoorLightingZoneType values:  
If A03_OutdoorLightingZoneType == OutdoorLZ1, display GuardStation, NonSalesCanopy, SalesLot, ServiceStationCanopy, ServiceStationHardscape, OutdoorDining, ParkingPedestrianSecurity; 
 
Else f A03_OutdoorLightingZoneType == OutdoorLZ2Or  OutdoorLZ3  display all values for UseOrLoseSpecificAreaType: BuildingFacade, GuardStation, NonSalesCanopy, SalesCanopy, SalesLot, ServiceStationCanopy, ServiceStationHardscape, OutdoorDining, ParkingPedestrianSecurity, StudentDropOffPlace, Security CameraArea
 
Else If A03_OutdoorLightingZoneType == OutdoorLZ4, display BuildingFacade, GuardStation, NonSalesCanopy, SalesCanopy, SalesLot, ServiceStationCanopy, ServiceStationHardscape, OutdoorDining; Security CameraArea
End If</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="M03_UseOrLoseSpecificArea" type="comp:UseOrLoseSpecificArea" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Specific Area (ft[d:sup]2[/d:sup])[d:sup]1[/d:sup]</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">User input</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="M04_UseOrLoseSpecificAreaAllowedDensity" type="comp:UseOrLoseSpecificAreaAllowedDensity" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Allowed Density (W/ft[d:sup]2[/d:sup])</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">Calculated Value in W/ft2  from Table 140.7-B  dependent on combination of values from M02_UseOrLoseSpecificAreaType and A03_OutdoorLightingZoneType values: 
If A03_OutdoorLightingZoneType == LZ0 result = 0; 
Else If M02_UseOrLoseSpecificAreaType == BuildingFacade: Switch (A03_OutdoorLightingZoneType) 
case OutdoorLZ1: result = 0; case OutdoorLZ2: result = 0.1; case OutdoorLZ3: result = 0.17;  case OutdoorLZ4: result = 0.225;  
 
Else If M02_UseOrLoseSpecificAreaType == SalesLot   then Switch (A03_OutdoorLightingZoneType) 
case OutdoorLZ1: result = 0.06; case OutdoorLZ2: result = 0.21; case OutdoorLZ3: result = 0.28; case OutdoorLZ4: result = 0.485;  
 
Else If M02_UseOrLoseSpecificAreaType == ServiceStationHardscape  then Switch (A03_OutdoorLightingZoneType)  
case OutdoorLZ1: result = 0.006;  case OutdoorLZ2: result = 0.068; case OutdoorLZ3: result = 0.138; case OutdoorLZ4: result = 0.200; 
 
Else If M02_UseOrLoseSpecificAreaType == ServiceStationCanopy then Switch (A03_OutdoorLightingZoneType) 
case OutdoorLZ1: result = 0.220; case OutdoorLZ2: result = 0.430; case OutdoorLZ3: result = 0.580; case OutdoorLZ4: result = 1.010;  
 
Else If M02_UseOrLoseSpecificAreaType == SalesCanopy then Switch (A03_OutdoorLightingZoneType)  
case OutdoorLZ1: result = 0; case OutdoorLZ2: result = 0.47; case OutdoorLZ3: result = 0.622; case OutdoorLZ4: result = 0.74; 
 
Else If M02_UseOrLoseSpecificAreaType == NonSalesCanopy then Switch (A03_OutdoorLightingZoneType) 
case OutdoorLZ1: result = 0.057; case OutdoorLZ2: result = 0.137; case OutdoorLZ3: result = 0.270; case OutdoorLZ4: result = 0.370; 
 
Else If M02_UseOrLoseSpecificAreaType == GuardStation  then Switch (A03_OutdoorLightingZoneType) 
case OutdoorLZ1: result = 0.081; case OutdoorLZ2: result = 0.176; case OutdoorLZ3: result = 0.325; case OutdoorLZ4: result = 0.425; 
 
Else If M02_UseOrLoseSpecificAreaType == StudentDropOffPlace then Switch (A03_OutdoorLightingZoneType) 
case OutdoorLZ1: result = 0; case OutdoorLZ2: result = 0.056; case OutdoorLZ3: result = 0.200; case OutdoorLZ4: result = 0; 
 
Else If M02_UseOrLoseSpecificAreaType == OutdoorDining then Switch (A03_OutdoorLightingZoneType)  
case OutdoorLZ1: result = 0.004; case OutdoorLZ2: result = 0.030; case OutdoorLZ3: result = 0.050; case OutdoorLZ4: result = 0.075; 
 
Else If M02_UseOrLoseSpecificAreaType == SecurityCameraArea  then Switch (A03_OutdoorLightingZoneType) 
case OutdoorLZ1: result = 0.00; case OutdoorLZ2: result = 0.018; case OutdoorLZ3: result = 0.018; case OutdoorLZ4:  result = 0.018.
 
Else If M02_UseOrLoseSpecificAreaType == ParkingPedestrianSecurity then Switch (A03_OutdoorLightingZoneType) case OutdoorLZ1: result = 0.004; case OutdoorLZ2: result = 0.005; case OutdoorLZ3: result = 0.010; case OutdoorLZ4:  result = 0.
 
End If</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="M05_ExtraPowerCalculatedForArea" type="comp:ExtraPowerCalculatedForArea" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Extra Allowance (Watts)</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">Calculated value: result = M03_UseOrLoseSpecificArea x M04_UseOrLoseSpecificAreaAllowedDensity</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="TableLuminaires" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText"></xsd:documentation>
                                       </xsd:annotation>
                                       <xsd:complexType>
                                          <xsd:sequence>
                                             <xsd:element name="Row" minOccurs="1" maxOccurs="unbounded">
                                                <xsd:complexType>
                                                   <xsd:sequence>
                                                      <xsd:element name="M06_LuminaireName" type="lit:LuminaireName" >
                                                         <xsd:annotation>
                                                            <xsd:documentation source="FieldText">Luminaire Name or Item Tag</xsd:documentation>
                                                            <xsd:documentation source="CalculationsAndRules">User selects from dropdown list loaded with values from F01_LuminaireName where  If F07_LightingPowerExcluded ==  false</xsd:documentation>
                                                         </xsd:annotation>
                                                      </xsd:element>
                                                      <xsd:choice maxOccurs="1">
                                                         <xsd:element name="M07_LuminairePower" type="lit:LuminairePower" >
                                                            <xsd:annotation>
                                                               <xsd:documentation source="FieldText">Watts  per Luminaire</xsd:documentation>
                                                               <xsd:documentation source="CalculationsAndRules">Auto-filled for  the luminaire selected in  M06_LuminaireName as follows: If F02a_IsLuminaireLinear == false result = F03_LuminairePower; Else result = F03_LuminaireWattsPerLF</xsd:documentation>
                                                            </xsd:annotation>
                                                         </xsd:element>
                                                         <xsd:element name="M07_LuminaireWattsPerLF" type="lit:LuminaireWattsPerLF" >
                                                            <xsd:annotation>
                                                               <xsd:documentation source="FieldText">Watts  per Luminaire</xsd:documentation>
                                                               <xsd:documentation source="CalculationsAndRules">Auto-filled for  the luminaire selected in  M06_LuminaireName as follows: If F02a_IsLuminaireLinear == false result = F03_LuminairePower; Else result = F03_LuminaireWattsPerLF</xsd:documentation>
                                                            </xsd:annotation>
                                                         </xsd:element>
                                                      </xsd:choice>
                                                      <xsd:choice maxOccurs="1">
                                                         <xsd:element name="M08_LuminaireCountByType" type="lit:LuminaireCountByType" >
                                                            <xsd:annotation>
                                                               <xsd:documentation source="FieldText"># of Luminaires</xsd:documentation>
                                                               <xsd:documentation source="CalculationsAndRules">User input as follows: If F02a_IsLuminaireLinear == false for this luminaire user enters count  stored in M08_LuminaireCountByType; Else user enters linear feet which is stored in M08_LuminairesTotalLF</xsd:documentation>
                                                            </xsd:annotation>
                                                         </xsd:element>
                                                         <xsd:element name="M08_LuminairesTotalLF" type="lit:LuminairesTotalLF" >
                                                            <xsd:annotation>
                                                               <xsd:documentation source="FieldText"># of Luminaires</xsd:documentation>
                                                               <xsd:documentation source="CalculationsAndRules">User input as follows: If F02a_IsLuminaireLinear == false for this luminaire user enters count  stored in M08_LuminaireCountByType; Else user enters linear feet which is stored in M08_LuminairesTotalLF</xsd:documentation>
                                                            </xsd:annotation>
                                                         </xsd:element>
                                                      </xsd:choice>
                                                      <xsd:element name="M09_LuminairePowerByType" type="lit:LuminairePowerByType" >
                                                         <xsd:annotation>
                                                            <xsd:documentation source="FieldText">Design Watts</xsd:documentation>
                                                            <xsd:documentation source="CalculationsAndRules">Calculated value: If F02a_IsLuminaireLinear == false result = PRODUCT (M07_LuminairePower, M08_LuminaireCountByType); Else result = PRODUCT (M07_LuminaireWattsPerLF, M08_LuminairesTotalLF</xsd:documentation>
                                                         </xsd:annotation>
                                                      </xsd:element>
                                                   </xsd:sequence>
                                                </xsd:complexType>
                                             </xsd:element>
                                          </xsd:sequence>
                                       </xsd:complexType>
                                    </xsd:element>
                                    <xsd:element name="MTotals09_LuminairePowerInArea" type="lit:LuminairePowerInArea" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Total Design Watts for this Area:</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">Calculated value: result = SUM All ( M09_LuminairePowerByType)</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="M10_AllExtraPowerAllowedForArea" type="comp:AllExtraPowerAllowedForArea" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Additional Allowance (Watts)</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">Calculated value: result = Smallest (M05_ExtraPowerCalculatedForArea, MTotals09_LuminairePowerInArea)</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                 </xsd:sequence>
                              </xsd:complexType>
                           </xsd:element>
                        </xsd:sequence>
                     </xsd:complexType>
                  </xsd:element>
                  <xsd:element name="MTotals10_UseOrLoseSpecificAreaTotalAllowance" type="comp:UseOrLoseSpecificAreaTotalAllowance" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">[d:b]Total Allowance (Watts) All Areas:[/d:b]</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">Calculated value: result = SUM ALL (M10_AllExtraPowerAllowedForArea)</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="MEndNote1" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">[d:i][d:sup]1[/d:sup] FOOTNOTES: See Table 140.7-B /Table 170.2-S  for rules for calculating the specific areas (ft[d:sup]2[/d:sup] for these additional lighting allowances.[/d:i]</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="MEndNote2" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">[d:i][d:sup]2[/d:sup] For luminaires indicated in Table F as linear, wattage in column 07 is W/lf instead of Watts/luminaire.  Total linear feet should be indicated in column 08 instead of number of luminaires. [/d:i]</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="MErrorMessage_SectionErrorMessage" type="comp:SectionErrorMessage"  minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
         <xsd:element name="Section_N" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation source="FieldText">[d:b]N. EXISTING CONDITIONS POWER ALLOWANCE (alterations only)[/d:b]</xsd:documentation>
               <xsd:documentation source="CalculationsAndRules">This section is required if section B  B05_OutdoorLightingCalculationMethod == ExistingPowerReduction Or  ExistingPowerMaintained</xsd:documentation>
               <xsd:documentation source="MinOccurs">If this section doesn't apply, do not display anything for this section</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="N_BeginNote1" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">[d:i]This table includes all existing luminaires being replaced to establish the lighting power allowance per  [d:t24refnolink/]141.0(b)2Lii, [d:t24refnolink/]141.0(b)2Liii, [d:t24refnolink/]180.2(b)4Bvb and [d:t24refnolink/]180.2(b)4Bvc.  Existing luminaires that are remaining or being moved should NOT be included.[d:line1/]Outdoor lighting attached to multifamily buildings and controlled from the inside of a dwelling unit are included in Table H. and are not included here. All other multifamily outdoor lighting is included here.[/d:i]</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="N_ColumnHeading01To06" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">[d:b]Existing Conditions Wattage:[/d:b]</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="TableExistingPowerAllowance" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                     </xsd:annotation>
                     <xsd:complexType>
                        <xsd:sequence>
                           <xsd:element name="Row" minOccurs="1" maxOccurs="unbounded">
                              <xsd:complexType>
                                 <xsd:sequence>
                                    <xsd:element name="N01_LuminaireName" type="lit:LuminaireName" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Name or Item Tag</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">User input:</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="N02_LuminaireDescription" type="lit:LuminaireDescription" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Complete Luminaire Description</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">User input</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="N03_LuminairePower" type="lit:LuminairePower" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Watts per luminaire[d:sup]1,2[/d:sup]</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">User input as follows: If linear luminaire user enters W/lf stored in N03_LuminaireWattsperLF; Else user enters Watts/luminaire stored in N03_LuminairePower</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="N03_LuminaireWattsPerLF" type="lit:LuminaireWattsPerLF" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Watts  per Luminaire[d:sup]1,2[/d:sup]</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">User input as follows: If linear luminaire user enters W/lf stored in N03_LuminaireWattsperLF; Else user enters Watts/luminaire stored in N03_LuminairePower</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:element name="N04_LuminaireSpecSource" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">How is Wattage determined</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">User input;  If  Other* is selected, user must enter note explaining the source in N_Exception02</xsd:documentation>
                                          <xsd:appinfo source="displayterm">
                                             <dtyp:displayterm value="ManufacturerSpec">Mfr. Spec[d:sup]1[/d:sup]</dtyp:displayterm>
                                             <dtyp:displayterm value="CEC_Default">NA8 Default</dtyp:displayterm>
                                             <dtyp:displayterm value="Other">Other*</dtyp:displayterm>
                                          </xsd:appinfo>
                                          
                                       </xsd:annotation>
                                       <xsd:simpleType>
                                          <xsd:restriction base="comp:LuminaireSpecSource">
                                             <xsd:enumeration value="CEC_Default"/>
                                             <xsd:enumeration value="ManufacturerSpec"/>
                                             <xsd:enumeration value="Other"/>
                                          </xsd:restriction>
                                       </xsd:simpleType>
                                    </xsd:element>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="N05_LuminaireCountByType" type="lit:LuminaireCountByType" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText"># of Luminaires Being Replaced[d:sup]2[/d:sup]</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">User input as follows: If linear luminaire user enters the amount of  linear feet being replaced stored in N05_LuminairesTotalLF; Else user enters the number of luminaires being replaced stored in N05_LuminaireCountByType</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="N05_LuminairesTotalLF" type="lit:LuminairesTotalLF" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText"># of Luminaires Being Replaced[d:sup]2[/d:sup]</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">User input as follows: If linear luminaire user enters the amount of  linear feet being replaced stored in N05_LuminairesTotalLF; Else user enters the number of luminaires being replaced stored in N05_LuminaireCountByType</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:element name="N06_LuminairePowerByType" type="lit:LuminairePowerByType" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Existing Watts</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">Calculated value: If linear luminaire result = PRODUCT (N03_LuminaireWattsperLF, N05_LuminairesTotalLF); Else  result = PRODUCT (N03_LuminairePower, N05_LuminaireCountByType)</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="N07_VerifedByInspector" type="comp:VerifedByInspector"  minOccurs="0">
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Field Inspector</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                 </xsd:sequence>
                              </xsd:complexType>
                           </xsd:element>
                        </xsd:sequence>
                     </xsd:complexType>
                  </xsd:element>
                  <xsd:element name="NTotals06_AllLuminairePowerInArea" type="lit:AllLuminairePowerInArea" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">[d:b]Total Existing Watts[/d:b]</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">Calculated value: result = SUM All ( N06_LuminairePowerByType)</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="NAllowance06_ExistingConditionsTotalAllowance" type="comp:ExistingConditionsTotalAllowance" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">[d:b]Allowance per [d:t24refnolink/]141.0(b)2L /[d:t24refnolink/]180.2(b)4Bv:[/d:b]</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">Calculated value: If B03_LuminaireAlteredPercentRange == GTE50Percent Then result = PRODUCT (NTotals06_AllLuminairePowerInArea, 0.6)
                           Else result = NTotals06_AllLuminairePowerInArea</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="NNote1" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">[d:i]* NOTES: Selections with a * require a note in the space below explaining how compliance is achieved. [d:line1/]  EX: Luminaire is lighting a statue; EXCEPTION 2 to [d:t24refnolink/]130.2(b) [/d:i]</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="N_TableComplianceNotes" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">This table is required If N04_LuminaireSpecSource = Other</xsd:documentation>
                     </xsd:annotation>
                     <xsd:complexType>
                        <xsd:sequence>
                           <xsd:element name="Row" minOccurs="1" maxOccurs="unbounded">
                              <xsd:complexType>
                                 <xsd:sequence>
                                    <xsd:element name="N_Exception01_LuminaireName" type="lit:LuminaireName" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText"></xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">Auto-filled with value in N01_LuminaireName for a row where exception occurred</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="N_Exception02_ExceptionalConditionNote" type="comp:ExceptionalConditionNote" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText"></xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">User input: If one or more selected controls in an area has a *, the user must enter an explanation on how compliance is achieved for each control.</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                 </xsd:sequence>
                              </xsd:complexType>
                           </xsd:element>
                        </xsd:sequence>
                     </xsd:complexType>
                  </xsd:element>
                  <xsd:element name="NEndNote1" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">[d:i][d:sup]1[/d:sup]FOOTNOTES: Authority Having Jurisdiction may ask for Luminaire cut sheets to confirm wattage used for compliance per [d:t24refnolink/]130.0(c) /[d:t24refnolink/]160.5(b)[/d:i]</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="NEndNote2" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">[d:i][d:sup]2[/d:sup] For linear luminaires, wattage should be indicated as W/lf instead of Watts/luminaire.  Total linear feet should be indicated in column 05 instead of number of luminaires. [/d:i]</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="NErrorMessage_SectionErrorMessage" type="comp:SectionErrorMessage"  minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
         <xsd:element name="Section_Nrci" minOccurs="1">
            <xsd:annotation>
               <xsd:documentation source="FieldText">[d:b]O.  DECLARATION OF REQUIRED CERTIFICATES OF INSTALLATION[/d:b]</xsd:documentation>
               <xsd:documentation source="CalculationsAndRules">If any selection is changed by the user, 
Then
D03  ExceptionalConditionMessageCCLTOE += UserChangedSelectionInCI
End If</xsd:documentation>
               <xsd:documentation source="MinOccurs">This section is required.</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="Nrci_BeginNote1" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">[d:i] Selections have been made based on information provided in this document. If any selection has been changed by permit applicant, an explanation should be included in Table E. Additional Remarks.  These documents must be provided to the building inspector during construction and can be found online [/d:i]</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="Nrci01_RequireNRCILto01E" type="comp:RequireNRCILto01E" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Form/Title</xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">NRCI-LTO-E - Must be submitted for all buildings</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">Result = true and can be changed to false by user</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="Nrci01a_VerifedByInspector" type="comp:VerifedByInspector"  minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Field Inspector</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
         <xsd:element name="Section_Nrca" minOccurs="1">
            <xsd:annotation>
               <xsd:documentation source="FieldText">[d:b]P.  DECLARATION OF REQUIRED CERTIFICATES OF ACCEPTANCE[/d:b]</xsd:documentation>
               <xsd:documentation source="CalculationsAndRules">If any selection is changed by the user, 
Then
D03  ExceptionalConditionMessageCCLTOE += UserChangedSelectionInCA
End If</xsd:documentation>
               <xsd:documentation source="MinOccurs">This section is required.</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="Nrca_BeginNote1" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">[d:i] Selections have been made based on information provided in this document. If any selection has been changed by permit applicant, an explanation should be included in Table E. Additional Remarks.  These documents must be provided to the building inspector during construction and must be completed through an Acceptance Test Technician Certification Provider (ATTCP). For more information visit: http://www.energy.ca.gov/title24/attcp/providers.html [/d:i]</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="Nrca01_RequireNRCALto02A" type="comp:RequireNRCALto02A" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Form/Title</xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">NRCA-LTO-02-A - Must be submitted for all outdoor lighting controls except for alterations where controls are added to [d:lte/] 20 luminaires.</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">Calculated Boolean value based on user input in previous sections of this form. By default result = true display term Yes represented as a radio button.  If user changes this value to false display term No represented as a radio button, an explanation is required in Section E Additional Remarks.</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="Nrca01_SystemFieldVerified" type="comp:SystemFieldVerified"  minOccurs="0" maxOccurs="unbounded">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Systems/Spaces To Be Field Verified</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">If Nrca01_RequireNRCALto02A == true; this field is loaded with systems/spaces that satisfy this condition:
 
If Section H is required,  
load value in H01_SpaceDescription
End If</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="Nrca01_SystemsToFieldVerify" type="comp:SystemsToFieldVerify"  minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Systems/Spaces To Be Field Verified</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">If Nrca01_RequireNRCALto02A == true; this field is displayed and loaded with a string that concatenates systems/spaces from Nrca01_SystemFieldVerified</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="Nrca01a_VerifedByInspector" type="comp:VerifedByInspector"  minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Field Inspector</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
      </xsd:sequence>
   </xsd:complexType>
   <xsd:element name="nRCCLTO01E" type="NRCCLTO01E">
      <xsd:annotation>
         <xsd:documentation>This element contains all of the data and text required to generate the NRCCLTO01E compliance report.</xsd:documentation>
      </xsd:annotation>
   </xsd:element>
</xsd:schema>
