                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Sum Total of Luminaires Being Added or Altered</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">User input;</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="B05_OutdoorLightingCalculationMethod" type="comp:OutdoorLightingCalculationMethod"  minOccurs="0">
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Calculation Method</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">User selects from dropdown list loaded as follows: If B03_LuminaireAlteredPercentRange == GTE50Percent then list = CalculatedAllowance and ExistingPowerReduction, 
Else list = ExistingPowerMaintained</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                 </xsd:sequence>
                              </xsd:complexType>
                           </xsd:element>
                        </xsd:sequence>
                     </xsd:complexType>
                  </xsd:element>
                  <xsd:element name="BEndNote1" minOccurs="0">
