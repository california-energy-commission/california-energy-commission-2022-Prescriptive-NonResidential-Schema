                  <xsd:element name="C_BeginNote1" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">[d:i]Results in this table are automatically calculated from data input and calculations in Tables F through N.  Note: If any cell on this table says "COMPLIES with Exceptional Conditions"  refer to Table D. Exceptional Conditions for guidance or see applicable Table referenced below.[/d:i]</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="C00_ColumnHeading01To06" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">[d:b]Calculations of Total Allowed Lighting Power  (Watts)  [d:t24refnolink/]140.7 / [d:t24refnolink/]170.2(e)6 
 or [d:t24refnolink/]141.0(b)2L / [d:t24refnolink/] 180.2(b)4Bv [/d:b]</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="C00_ColumnHeading07To09" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">[d:b]Compliance Results[/d:b]</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:choice maxOccurs="1">
                     <xsd:element name="C01_HardscapeGeneralAllowance" type="comp:HardscapeGeneralAllowance" >
                        <xsd:annotation>
                           <xsd:documentation source="FieldText">General Hardscape Allowance [d:t24refnolink/]140.7(d)1 / [d:t24refnolink/]170.2(e)6 [d:line1/] (See Table I)</xsd:documentation>
                           <xsd:documentation source="CalculationsAndRules">If section I is not required, 
result = BlankGrayedOut stored in NotApplicableMessage and field is blank and grayed out. 
 
Else If A05_WholeBuildingOccupancy == MultiFamilyMixedUseGTE4 
result =  I08mf_HardscapeGeneralAllowance  
Else 
result = ITotals09_HardscapeGeneralAllowance 
End If</xsd:documentation>
                        </xsd:annotation>
                     </xsd:element>
                     <xsd:element name="C01_NotApplicableMessage" >
                        <xsd:annotation>
                           <xsd:documentation source="FieldText">General Hardscape Allowance [d:t24refnolink/]140.7(d)1 / [d:t24refnolink/]170.2(e)6 [d:line1/] (See Table I)</xsd:documentation>
                           <xsd:documentation source="CalculationsAndRules">If section I is not required, 
result = BlankGrayedOut stored in NotApplicableMessage and field is blank and grayed out. 
End If</xsd:documentation>
                        </xsd:annotation>
                        <xsd:simpleType>
                           <xsd:restriction base="comp:NotApplicableMessage">
                              <xsd:enumeration value="BlankGrayedOut"/>
                           </xsd:restriction>
                        </xsd:simpleType>
                     </xsd:element>
                  </xsd:choice>
                  <xsd:choice maxOccurs="1">
                     <xsd:element name="C02_UseOrLoseApplicationTotalAllowance" type="comp:UseOrLoseApplicationTotalAllowance" >
                        <xsd:annotation>
                           <xsd:documentation source="FieldText">Per Application [d:t24refnolink/]140.7(d)2 / [d:t24refnolink/]170.2(e)6 [d:line1/] (See Table J)</xsd:documentation>
                           <xsd:documentation source="CalculationsAndRules">If section I is required And I01b_UsingApplicationAllowance== true 
result =  JTotals10_UseOrLoseApplicationTotalAllowance
Else result = BlankGrayedOut stored in NotApplicableMessage and field is blank and grayed out
End If</xsd:documentation>
                        </xsd:annotation>
                     </xsd:element>
                     <xsd:element name="C02_NotApplicableMessage" >
                        <xsd:annotation>
                           <xsd:documentation source="FieldText">Per Application [d:t24refnolink/]140.7(d)2 / [d:t24refnolink/]170.2(e)6 [d:line1/] (See Table J)</xsd:documentation>
                           <xsd:documentation source="CalculationsAndRules">If section I is required And I01b_UsingApplicationAllowance== true 
result =  JTotals10_UseOrLoseApplicationTotalAllowance
Else result = BlankGrayedOut stored in NotApplicableMessage and field is blank and grayed out
End If</xsd:documentation>
                        </xsd:annotation>
                        <xsd:simpleType>
                           <xsd:restriction base="comp:NotApplicableMessage">
                              <xsd:enumeration value="BlankGrayedOut"/>
                           </xsd:restriction>
                        </xsd:simpleType>
                     </xsd:element>
                  </xsd:choice>
                  <xsd:choice maxOccurs="1">
                     <xsd:element name="C03_UseOrLoseSalesFrontageTotalAllowance" type="comp:UseOrLoseSalesFrontageTotalAllowance" >
                        <xsd:annotation>
                           <xsd:documentation source="FieldText">Sales Frontage [d:t24refnolink/]140.7(d)2 [d:line1/] (See Table K)</xsd:documentation>
                           <xsd:documentation source="CalculationsAndRules">If section I is required And I01c_UsingSalesFrontageAllowance== true 
result = KTotals09_UseOrLoseSalesFrontageTotalAllowance   
Else result = BlankGrayedOut stored in NotApplicableMessage and field is blank and grayed out;
End If</xsd:documentation>
                        </xsd:annotation>
                     </xsd:element>
                     <xsd:element name="C03_NotApplicableMessage" >
                        <xsd:annotation>
                           <xsd:documentation source="FieldText">Sales Frontage [d:t24refnolink/]140.7(d)2 [d:line1/] (See Table K)</xsd:documentation>
                           <xsd:documentation source="CalculationsAndRules">If section I is required And I01c_UsingSalesFrontageAllowance== true 
result = KTotals09_UseOrLoseSalesFrontageTotalAllowance   
Else result = BlankGrayedOut stored in NotApplicableMessage and field is blank and grayed out;
End If</xsd:documentation>
                        </xsd:annotation>
                        <xsd:simpleType>
