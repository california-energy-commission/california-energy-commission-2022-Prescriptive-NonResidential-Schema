                           <xsd:element name="Gpit05a_PipeNominalDiameter"  minOccurs="0">
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText">Nominal Pipe Diameter (in)</xsd:documentation>
                                 <xsd:documentation source="CalculationsAndRules">This is a static field not required in the XML data but if displayed should always display the restricted value display term 1 to [d:lt/]1.5</xsd:documentation>
