                              <xsd:annotation>
                                 <xsd:documentation source="FieldText"></xsd:documentation>
                                 <xsd:documentation source="AdditionalRequirements">[d:i][d:sup]2[/d:sup] For alterations using U-factor as the Thermal Performance Unit, at least R-10 insulation must be above deck.[/d:i]</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                           <xsd:element name="FSIPSFootnote3" minOccurs="0">
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText"></xsd:documentation>
                                 <xsd:documentation source="AdditionalRequirements">[d:i][d:sup]2[/d:sup] If "R-value" is shown in cell 13 as the Thermal Performance Unit, the R-value shown here is for continuous insulation per Table 141.0-C.[/d:i]</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                           <xsd:element name="FSIPSFootnote4" minOccurs="0">
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText"></xsd:documentation>
                                 <xsd:documentation source="AdditionalRequirements">[d:i][d:sup]3[/d:sup] Roof area minus any fenestration/ skylight area [/d:i]</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                        </xsd:sequence>
                     </xsd:complexType>
                  </xsd:element>
                  <xsd:element name="Fspan" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">[d:b]Span Deck And Concrete  Roof Assemblies [/d:b]</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">This subsection is required if F01Span_RoofTypeApplies == true</xsd:documentation>
                     </xsd:annotation>
                     <xsd:complexType>
                        <xsd:sequence>
                           <xsd:element name="Fspan01_UseInAreaWeightedAveUfactor" type="comp:UseInAreaWeightedAveUfactor" >
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText">Include Span Deck and Concrete Roof Assemblies in Area-Weighted Average U-factor Calculation [d:sup]1[/d:sup]</xsd:documentation>
                                 <xsd:documentation source="CalculationsAndRules">User input: If checked result = true; Else result = false.   
 
If result == true   D03_ExceptionalConditionMessageCCENVE += RoofAreaWeightedAveInF</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                           <xsd:element name="TableFspan" >
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText"></xsd:documentation>
                              </xsd:annotation>
                              <xsd:complexType>
                                 <xsd:sequence>
                                    <xsd:element name="Row" minOccurs="1" maxOccurs="unbounded">
                                       <xsd:complexType>
                                          <xsd:sequence>
                                             <xsd:element name="Fspan02_RoofAssemblyName" type="env:RoofAssemblyName" >
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">Tag/Plan Detail ID</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">User input</xsd:documentation>
                                                </xsd:annotation>
                                             </xsd:element>
                                             <xsd:element name="Fspan03_RoofDescription" type="env:RoofDescription" >
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">Name/Description</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">User input</xsd:documentation>
                                                </xsd:annotation>
                                             </xsd:element>
                                             <xsd:element name="Fspan04_Status" >
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">Status</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">User selects from list: New display term New, ExistingAlteration display term Altered</xsd:documentation>
                                                </xsd:annotation>
                                                <xsd:simpleType>
                                                   <xsd:restriction base="com:Status">
                                                      <xsd:enumeration value="ExistingAlteration"/>
                                                      <xsd:enumeration value="New"/>
                                                   </xsd:restriction>
                                                </xsd:simpleType>
                                             </xsd:element>
                                             <xsd:choice maxOccurs="1">
                                                <xsd:element name="Fspan05_RoofInsulationException" type="comp:RoofInsulationException" >
                                                   <xsd:annotation>
                                                      <xsd:documentation source="FieldText">Exception to Roof Insulation Requirements in [d:t24refnolink/]141.0(b)2Biii (Alts. Only)</xsd:documentation>
                                                      <xsd:documentation source="CalculationsAndRules">If Fspan04_Status == New result = BlankGrayedOut stored in NotApplicableMessage; 
 
Else default result = NoExceptionApplies but user can select from list: 
RecoverAboveDeckRvalue display term  Roof recovers with new R-10 insulation added abov deck
 
MechanicalEquipmentOnRoof display term  Existing mechanical equipment located on the roof will not be disconnected and lifted, insulation added is the greater of R-10 or the maximum installed thickness that will allow the distance between the height of the roof membrane surface to the top of the base flashing to remain in accordance with the manufacturer's instructions.
 
NoExceptionApplies display term None of these exceptions apply, 
 
RoofRecoat display term  The area of the roof recoat is not required to be insulated.
End If
 
If result !=  NoExceptionApplies    
D03_ExceptionalConditionMessageCCENVE += RoofAlterationExceptionInF
End If</xsd:documentation>
                                                   </xsd:annotation>
                                                </xsd:element>
                                                <xsd:element name="Fspan05_NotApplicableMessage" >
                                                   <xsd:annotation>
                                                      <xsd:documentation source="FieldText">Exception to Roof Insulation Requirements in [d:t24refnolink/]141.0(b)2Biii (Alts. Only)</xsd:documentation>
                                                      <xsd:documentation source="CalculationsAndRules">If Fspan04_Status == New result = BlankGrayedOut stored in NotApplicableMessage; Else default result = NoExceptionApplies</xsd:documentation>
                                                   </xsd:annotation>
                                                   <xsd:simpleType>
                                                      <xsd:restriction base="comp:NotApplicableMessage">
                                                         <xsd:enumeration value="BlankGrayedOut"/>
                                                      </xsd:restriction>
                                                   </xsd:simpleType>
                                                </xsd:element>
                                             </xsd:choice>
                                             <xsd:element name="Fspan06_TopLevelOccupancy" >
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">Occupancy Type</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">User selects from list: Nonresidential display term Nonresidential/ Relocatable 1 CZ, HotelMotel display term Hotel/ Motel, ResidentialMF display term Multifamily and     RelocableSchoolBuildingAllZones display term Relocatable all CZ</xsd:documentation>
                                                   <xsd:appinfo source="displayterm">
                                                      <dtyp:displayterm value="ResidentialMF">Multifamily</dtyp:displayterm>
                                                      <dtyp:displayterm value="HotelMotel">Hotel/ Motel</dtyp:displayterm>
                                                      <dtyp:displayterm value="Nonresidential">Nonresidential/ Relocatable 1 CZ</dtyp:displayterm>
                                                      <dtyp:displayterm value="RelocableSchoolBuildingAllZones">Relocatable all CZ</dtyp:displayterm>
                                                   </xsd:appinfo>
                                                   
                                                </xsd:annotation>
                                                <xsd:simpleType>
                                                   <xsd:restriction base="com:TopLevelOccupancy">
                                                      <xsd:enumeration value="HotelMotel"/>
                                                      <xsd:enumeration value="Nonresidential"/>
                                                      <xsd:enumeration value="RelocableSchoolBuildingAllZones"/>
                                                      <xsd:enumeration value="ResidentialMF"/>
                                                   </xsd:restriction>
                                                </xsd:simpleType>
                                             </xsd:element>
                                          </xsd:sequence>
                                       </xsd:complexType>
                                    </xsd:element>
                                 </xsd:sequence>
                              </xsd:complexType>
                           </xsd:element>
                           <xsd:element name="TableFspanperf" >
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText"></xsd:documentation>
                              </xsd:annotation>
                              <xsd:complexType>
                                 <xsd:sequence>
                                    <xsd:element name="Row" minOccurs="1" maxOccurs="unbounded">
                                       <xsd:complexType>
                                          <xsd:sequence>
                                             <xsd:element name="Fspan07_RoofAssemblyName" type="env:RoofAssemblyName" >
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">Tag/Plan Detail ID</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">Autofilled from Fspan02_RoofAssemblyName</xsd:documentation>
                                                </xsd:annotation>
                                             </xsd:element>
                                             <xsd:choice maxOccurs="1">
                                                <xsd:element name="Fspan08_UfactorSpecSource" type="comp:UfactorSpecSource" >
                                                   <xsd:annotation>
                                                      <xsd:documentation source="FieldText">How Design U-factor was determined</xsd:documentation>
                                                      <xsd:documentation source="CalculationsAndRules">If Fspan12_ConstructionSpecificationMethod == RValue result = BlankGrayedOut stored in NotApplicableMessage; 
 
Else User selects from list: 
JA4Tables display term JA4 Tables, 
JA4Other display term Other per JA4.1.2.1, 
ApprovedSoftware display term Approved Software. 
 
 If result == JA4Other Or ApprovedSoftware D03_ExceptionalConditionMessageCCENVE += AdditionalDocJA4OtherSoftware</xsd:documentation>
                                                   </xsd:annotation>
                                                </xsd:element>
                                                <xsd:element name="Fspan08_NotApplicableMessage" >
                                                   <xsd:annotation>
                                                      <xsd:documentation source="FieldText">How Design U-factor was determined</xsd:documentation>
                                                      <xsd:documentation source="CalculationsAndRules">If Fspan04_Status == ExistingAlteration And  Fspan05_RoofInsulationException  != NoExceptionApplies Or If Fspan12_ConstructionSpecificationMethod == RValue result = BlankGrayedOut stored in NotApplicableMessage</xsd:documentation>
                                                   </xsd:annotation>
                                                   <xsd:simpleType>
                                                      <xsd:restriction base="comp:NotApplicableMessage">
                                                         <xsd:enumeration value="BlankGrayedOut"/>
                                                      </xsd:restriction>
                                                   </xsd:simpleType>
                                                </xsd:element>
                                             </xsd:choice>
                                             <xsd:choice maxOccurs="1">
                                                <xsd:element name="Fspan09_FireProofing" type="env:FireProofing" >
                                                   <xsd:annotation>
                                                      <xsd:documentation source="FieldText">Fireproofing</xsd:documentation>
                                                      <xsd:documentation source="CalculationsAndRules">If Fspan08_UfactorSpecSource == JA4Other Or ApprovedSoftware this row is grayed out; 
 
Else If Fspan08_UfactorSpecSource == JA4Tables user selects from list: 
FireProofing display term Yes, 
NoFireProofing display term No</xsd:documentation>
                                                   </xsd:annotation>
                                                </xsd:element>
                                                <xsd:element name="Fspan09_FireProofingOther" type="env:FireProofingOther" >
                                                   <xsd:annotation>
                                                      <xsd:documentation source="FieldText">Fireproofing</xsd:documentation>
                                                      <xsd:documentation source="CalculationsAndRules">If Fspan08_UfactorSpecSource == JA4Tables this row is grayed out; 
 
Else If  Fspan08_UfactorSpecSource == JA4Other Or ApprovedSoftware user enters text</xsd:documentation>
                                                   </xsd:annotation>
                                                </xsd:element>
                                                <xsd:element name="Fspan09_NotApplicableMessage" >
                                                   <xsd:annotation>
                                                      <xsd:documentation source="FieldText">Fireproofing</xsd:documentation>
                                                      <xsd:documentation source="CalculationsAndRules">If Fspan04_Status == ExistingAlteration And  Fspan05_RoofInsulationException  != NoExceptionApplies Or If Fspan12_ConstructionSpecificationMethod == RValue result = BlankGrayedOut stored in NotApplicableMessage and both rows for F09 are grayed out;
 ;</xsd:documentation>
                                                   </xsd:annotation>
                                                   <xsd:simpleType>
                                                      <xsd:restriction base="comp:NotApplicableMessage">
                                                         <xsd:enumeration value="BlankGrayedOut"/>
                                                      </xsd:restriction>
                                                   </xsd:simpleType>
                                                </xsd:element>
                                             </xsd:choice>
                                             <xsd:choice maxOccurs="1">
                                                <xsd:element name="Fspan10_ConcreteThickness" type="env:ConcreteThickness" >
                                                   <xsd:annotation>
                                                      <xsd:documentation source="FieldText">Concrete Topping Thickness (in.)</xsd:documentation>
                                                      <xsd:documentation source="CalculationsAndRules">If Fspan08_UfactorSpecSource == JA4Tables user selects from list: 
None, 
2In display term 2 in.  
4In display term 4 in.  
6In display term 6 in.</xsd:documentation>
                                                   </xsd:annotation>
                                                </xsd:element>
                                                <xsd:element name="Fspan10_ConcreteThicknessOther" type="env:ConcreteThicknessOther" >
                                                   <xsd:annotation>
                                                      <xsd:documentation source="FieldText">Concrete Topping Thickness (in.)</xsd:documentation>
                                                      <xsd:documentation source="CalculationsAndRules">If Fspan08_UfactorSpecSource == JA4Tables, this row is grayed out; 
 
Else If Fspan08_UfactorSpecSource == JA4Other Or ApprovedSoftware user enters text</xsd:documentation>
                                                   </xsd:annotation>
                                                </xsd:element>
                                                <xsd:element name="Fspan10_NotApplicableMessage" >
                                                   <xsd:annotation>
                                                      <xsd:documentation source="FieldText">Concrete Topping Thickness (in.)</xsd:documentation>
                                                      <xsd:documentation source="CalculationsAndRules">If Fspan04_Status == ExistingAlteration And  Fspan05_RoofInsulationException  != NoExceptionApplies Or If Fspan12_ConstructionSpecificationMethod == RValue result = BlankGrayedOut stored in NotApplicableMessage and both rows for F10 are grayed out;</xsd:documentation>
                                                   </xsd:annotation>
                                                   <xsd:simpleType>
                                                      <xsd:restriction base="comp:NotApplicableMessage">
                                                         <xsd:enumeration value="BlankGrayedOut"/>
                                                      </xsd:restriction>
                                                   </xsd:simpleType>
                                                </xsd:element>
                                             </xsd:choice>
                                             <xsd:choice maxOccurs="1">
                                                <xsd:element name="Fspan11_ContinuousInsulationNR" >
                                                   <xsd:annotation>
                                                      <xsd:documentation source="FieldText">Continuous Insulation per Design[d:sup]2[/d:sup]</xsd:documentation>
                                                      <xsd:documentation source="CalculationsAndRules">If Fspan08_UfactorSpecSource == JA4Other Or ApprovedSoftware this row is grayed out; 
 
Else If Fspan08_UfactorSpecSource == JA4Tables user selects from list: 
R0ci display term None, 
R4ci display term R-4, 
R6ci display term R-6, 
R8ci display term R-8, 
R10ci display term R-10, 
R12ci display term R-12, 
R15ci display term R-15, 
R20ci display term R-20, 
R25ci display term R-25, 
R30ci display term R-30,</xsd:documentation>
                                                   </xsd:annotation>
                                                   <xsd:simpleType>
                                                      <xsd:restriction base="env:ContinuousInsulationNR">
                                                         <xsd:enumeration value="R0ci"/>
                                                         <xsd:enumeration value="R10ci"/>
                                                         <xsd:enumeration value="R12ci"/>
                                                         <xsd:enumeration value="R15ci"/>
                                                         <xsd:enumeration value="R20ci"/>
                                                         <xsd:enumeration value="R25ci"/>
                                                         <xsd:enumeration value="R30ci"/>
                                                         <xsd:enumeration value="R4ci"/>
                                                         <xsd:enumeration value="R6ci"/>
                                                         <xsd:enumeration value="R8ci"/>
                                                      </xsd:restriction>
                                                   </xsd:simpleType>
                                                </xsd:element>
                                                <xsd:element name="Fspan11_ContinuousInsulationOther" type="com:ContinuousInsulationOther" >
                                                   <xsd:annotation>
                                                      <xsd:documentation source="FieldText">Continuous Insulation per Design[d:sup]2[/d:sup]</xsd:documentation>
                                                      <xsd:documentation source="CalculationsAndRules">If Fspan08_UfactorSpecSource == JA4Tables, this row is grayed out; 
 
Else If Fspan08_UfactorSpecSource == JA4Other Or ApprovedSoftware user enters text</xsd:documentation>
                                                   </xsd:annotation>
                                                </xsd:element>
                                                <xsd:element name="Fspan11_NotApplicableMessage" >
                                                   <xsd:annotation>
                                                      <xsd:documentation source="FieldText"></xsd:documentation>
                                                      <xsd:documentation source="CalculationsAndRules">If Fspan04_Status == ExistingAlteration And  Fspan05_RoofInsulationException  != NoExceptionApplies result = BlankGrayedOut</xsd:documentation>
                                                   </xsd:annotation>
                                                   <xsd:simpleType>
                                                      <xsd:restriction base="comp:NotApplicableMessage">
                                                         <xsd:enumeration value="BlankGrayedOut"/>
                                                      </xsd:restriction>
                                                   </xsd:simpleType>
                                                </xsd:element>
                                             </xsd:choice>
                                             <xsd:choice maxOccurs="1">
                                                <xsd:element name="Fspan12_ConstructionSpecificationMethod" >
                                                   <xsd:annotation>
                                                      <xsd:documentation source="FieldText">Thermal Performance Unit</xsd:documentation>
                                                      <xsd:documentation source="CalculationsAndRules">If Fspan04_Status == ExistingAlteration And Fspan01_UseInAreaWeightedAveUfactor  == false list RValue display term R-value, UFactor display term U-factor ; 
 
Else result = UFactor and cannot be changed by user</xsd:documentation>
                                                   </xsd:annotation>
                                                   <xsd:simpleType>
                                                      <xsd:restriction base="env:ConstructionSpecificationMethod">
                                                         <xsd:enumeration value="RValue"/>
                                                         <xsd:enumeration value="UFactor"/>
                                                      </xsd:restriction>
                                                   </xsd:simpleType>
                                                </xsd:element>
                                                <xsd:element name="Fspan12_NotApplicableMessage" >
                                                   <xsd:annotation>
                                                      <xsd:documentation source="FieldText"></xsd:documentation>
                                                      <xsd:documentation source="CalculationsAndRules">If Fspan04_Status == ExistingAlteration And  Fspan05_RoofInsulationException  != NoExceptionApplies result = BlankGrayedOut</xsd:documentation>
                                                   </xsd:annotation>
                                                   <xsd:simpleType>
                                                      <xsd:restriction base="comp:NotApplicableMessage">
                                                         <xsd:enumeration value="BlankGrayedOut"/>
                                                      </xsd:restriction>
                                                   </xsd:simpleType>
                                                </xsd:element>
                                             </xsd:choice>
                                             <xsd:choice maxOccurs="1">
                                                <xsd:element name="Fspan13_InsulationUFactorLimit" type="env:InsulationUFactorLimit" >
                                                   <xsd:annotation>
                                                      <xsd:documentation source="FieldText">Required Thermal Performance[d:sup]3[/d:sup]</xsd:documentation>
                                                      <xsd:documentation source="CalculationsAndRules">If Fspan04_Status == New 
 
Switch (Fspan06_TopLevelOccupancy) 
case  Nonresidential: 
If A03_ClimateZoneNumber InRange (1, 5) Or InRange(9,16) result = 0.034; 
Else If A03_ClimateZoneNumber InRange (6, 8) ) result = 0.049; 
End If 
 
case HotelMotel: 
If A03_ClimateZoneNumber == 1, 2, 4 Or  InRange (8, 16) result = 0.028; 
Else If A03_ClimateZoneNumber == 3, 5 Or 6 result = 0.034; 
Else If A03_ClimateZoneNumber == 7 result = 0.039;
End If 
 
case ResidentialMF: 
If A03_ClimateZoneNumber == 1, 2, 4 Or  InRange (8, 16) result = 0.028; 
Else If A03_ClimateZoneNumber == 3, 5 Or 6 result = 0.034; 
Else If A03_ClimateZoneNumber == 7 result = 0.039;
End If 
 
case RelocableSchoolBuildingAllZones:  result = 0.034; 
End Switch 
 
Else If Fspan04_Status == ExistingAlteration And Fspan12_ConstructionSpecificationMethod == UFactor 
If Fspan06_TopLevelOccupancy != ResidentialMF
If A03_ClimateZoneNumber == InRange(1,5) Or InRange(9,16)  
result = 0.037
Else If A03_ClimateZoneNumber == 6, 7 Or 8
result = 0.047
End If;
 
Else If Fspan06_TopLevelOccupancy == ResidentialMF
 
Else If A03_ClimateZoneNumber == 1, 2, 4 Or InRange(8,16)  result =  0.039;
End If
End If
End if</xsd:documentation>
                                                   </xsd:annotation>
                                                </xsd:element>
                                                <xsd:element name="Fspan13_InsulationRValueLimit" type="env:InsulationRValueLimit" >
                                                   <xsd:annotation>
                                                      <xsd:documentation source="FieldText">Required Thermal Performance[d:sup]3[/d:sup]</xsd:documentation>
                                                      <xsd:documentation source="CalculationsAndRules">If Fspan04_Status == ExistingAlteration And Fspan12_ConstructionSpecificationMethod == RValue 
If Fspan06_TopLevelOccupancy != ResidentialMF
If A03_ClimateZoneNumber == InRange(1,5) Or InRange(9,16)  result = R23;
Else If A03_ClimateZoneNumber == InRange(6,8) result = R17; 
End If
Else If Fspan06_TopLevelOccupancy == ResidentialMF
If A03_ClimateZoneNumber == 1, 2, 4 Or InRange(8,16)  result =  R14
End If
End If</xsd:documentation>
                                                   </xsd:annotation>
                                                </xsd:element>
                                                <xsd:element name="Fspan13_NotApplicableMessage" >
                                                   <xsd:annotation>
                                                      <xsd:documentation source="FieldText"></xsd:documentation>
                                                      <xsd:documentation source="CalculationsAndRules">If Fspan04_Status == ExistingAlteration 
If Fspan05_RoofInsulationException  != NoExceptionApplies Or 
Fspan06_TopLevelOccupancy == ResidentialMF And A03_ClimateZoneNumber == 3, 5, 6 Or 7
 result = BlankGrayedOut
End If
End If</xsd:documentation>
                                                   </xsd:annotation>
                                                   <xsd:simpleType>
                                                      <xsd:restriction base="comp:NotApplicableMessage">
                                                         <xsd:enumeration value="BlankGrayedOut"/>
                                                      </xsd:restriction>
                                                   </xsd:simpleType>
                                                </xsd:element>
                                             </xsd:choice>
                                             <xsd:element name="Fspan14UF" >
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">U-factor per Design</xsd:documentation>
                                                </xsd:annotation>
                                                <xsd:complexType>
                                                   <xsd:choice >
                                                      <xsd:element name="Fspan14_NotApplicableMessage" >
                                                         <xsd:annotation>
                                                            <xsd:documentation source="FieldText"></xsd:documentation>
                                                            <xsd:documentation source="CalculationsAndRules">If Fspan04_Status == ExistingAlteration And  Fspan05_RoofInsulationException  != NoExceptionApplies Or If Fspan12_ConstructionSpecificationMethod == RValue result = BlankGrayedOut stored in NotApplicableMessage and all fields in Fspan14 are grayed out;</xsd:documentation>
                                                         </xsd:annotation>
                                                         <xsd:simpleType>
                                                            <xsd:restriction base="comp:NotApplicableMessage">
                                                               <xsd:enumeration value="BlankGrayedOut"/>
                                                            </xsd:restriction>
                                                         </xsd:simpleType>
                                                      </xsd:element>
                                                      <xsd:element name="Fspan14JA4" >
                                                         <xsd:annotation>
                                                            <xsd:documentation source="FieldText">per JA4</xsd:documentation>
                                                            <xsd:documentation source="CalculationsAndRules">If Fspan08_UfactorSpecSource == JA4Tables, result is stored in one of these choices from Table JA4.2.6</xsd:documentation>
                                                         </xsd:annotation>
                                                         <xsd:complexType>
                                                            <xsd:sequence>
                                                               <xsd:choice maxOccurs="1">
                                                                  <xsd:element name="Fspan14_UfactorJA426FireProof" type="comp:UfactorJA426FireProof" >
                                                                     <xsd:annotation>
                                                                        <xsd:documentation source="FieldText"></xsd:documentation>
                                                                        <xsd:documentation source="CalculationsAndRules">If Fspan09_FireProofing == FireProofing 
 
If Fspan10_ConcreteThickness == None 
Switch (Fspan11_ContinuousInsulationNR) 
case R0: result = 0.348;  
case R4:  result = 0.145;  
case R6: result = 0.113;   
case R8: result = 0.092;   
case R10: result = 0.078 ;  
case R12: result = 0.067 ;  
case R15: result = 0.056 ;  
case R20: result = 0.044 ;  
case R25: result = 0.036  ;  
case R30: result =  0.030; 
End Switch
 
Else If Fspan10_ConcreteThickness == 2In 
Switch (Fspan11_ContinuousInsulationNR) 
case R0: result = 0.324;  
case R4:  result = 0.141;  
case R6: result = 0.110;   
case R8: result = 0.090;   
case R10: result = 0.076 ;  
case R12: result = 0.066 ;  
case R15: result = 0.055 ;  
case R20: result = 0.043 ;  
case R25: result = 0.036  ;  
case R30: result =  0.030; 
End Switch
 
Else If Fspan10_ConcreteThickness == 4In 
Switch (Fspan11_ContinuousInsulationNR) 
case R0: result = 0.302;  
case R4:  result = 0.137;  
case R6: result = 0.107;   
case R8: result = 0.088;   
case R10: result = 0.075 ;  
case R12: result = 0.065;  
case R15: result = 0.055 ;  
case R20: result = 0.043 ;  
case R25: result = 0.035  ;  
case R30: result =  0.030; 
End Switch
 
 Else If Fspan10_ConcreteThickness == 6In 
Switch (Fspan11_ContinuousInsulationNR) 
case R0: result = 0.283;  
case R4:  result = 0.133;  
case R6: result = 0.105;   
case R8: result = 0.087;   
case R10: result = 0.074 ;  
case R12: result = 0.064;  
case R15: result = 0.054 ;  
case R20: result = 0.042 ;  
case R25: result = 0.035  ;  
case R30: result =  0.030;
End Switch
End If
End If</xsd:documentation>
                                                                     </xsd:annotation>
                                                                  </xsd:element>
                                                                  <xsd:element name="Fspan14_UfactorJA426NoFireProof" type="comp:UfactorJA426NoFireProof" >
                                                                     <xsd:annotation>
                                                                        <xsd:documentation source="FieldText"></xsd:documentation>
                                                                        <xsd:documentation source="CalculationsAndRules">If Fspan09_FireProofing == NoFireProofing 
 
If Fspan10_ConcreteThickness == None 
Switch (Fspan11_ContinuousInsulationNR) 
case R0: result = 0.503;  
case R4:  result = 0.167;  
case R6: result = 0.125;   
case R8: result = 0.100;   
case R10: result = 0.083 ;  
case R12: result = 0.071;  
case R15: result = 0.059;  
case R20: result = 0.045;  
case R25: result = 0.037;  
case R30: result =  0.031;
End Switch 
 
Else If Fspan10_ConcreteThickness == 2In 
Switch (Fspan11_ContinuousInsulationNR) 
case R0: result = 0.452;  
case R4:  result = 0.161;  
case R6: result = 0.122;   
case R8: result = 0.098;   
case R10: result = 0.082;  
case R12: result = 0.070;  
case R15: result = 0.058;  
case R20: result = 0.045;  
case R25: result = 0.037  ;  
case R30: result =  0.031;
End Switch  
 
Else If Fspan10_ConcreteThickness == 4In 
Switch (Fspan11_ContinuousInsulationNR) 
case R0: result = 0.412;  
case R4:  result = 0.156;  
case R6: result = 0.119;   
case R8: result = 0.096;   
case R10: result = 0.080;  
case R12: result = 0.069;  
case R15: result = 0.057;  
case R20: result = 0.045;  
case R25: result = 0.036;  
case R30: result =  0.031; 
End Switch  
 
Else If Fspan10_ConcreteThickness == 6In 
Switch (Fspan11_ContinuousInsulationNR) 
case R0: result = 0.377;  
case R4:  result = 0.150;  
case R6: result = 0.116;   
case R8: result = 0.094;   
case R10: result = 0.079;  
case R12: result = 0.068;  
case R15: result = 0.057;  
case R20: result = 0.044;  
case R25: result = 0.036;  
case R30: result =  0.031;
End Switch 
End If
End If</xsd:documentation>
                                                                     </xsd:annotation>
                                                                  </xsd:element>
                                                               </xsd:choice>
                                                            </xsd:sequence>
                                                         </xsd:complexType>
                                                      </xsd:element>
                                                      <xsd:element name="Fspan14_UfactorJA4Other" type="comp:UfactorJA4Other" >
                                                         <xsd:annotation>
                                                            <xsd:documentation source="FieldText">per Software/ Other</xsd:documentation>
                                                            <xsd:documentation source="CalculationsAndRules">If Fspan08_UfactorSpecSource == JA4Other Or  ApprovedSoftware, user input value</xsd:documentation>
                                                         </xsd:annotation>
                                                      </xsd:element>
                                                   </xsd:choice>
                                                </xsd:complexType>
                                             </xsd:element>
                                             <xsd:choice maxOccurs="1">
                                                <xsd:element name="Fspan15_RoofNetArea" type="comp:RoofNetArea" >
                                                   <xsd:annotation>
                                                      <xsd:documentation source="FieldText">Net Area [d:sup]4[/d:sup] ft[d:sup]2[/d:sup]</xsd:documentation>
                                                      <xsd:documentation source="CalculationsAndRules">If Fspan01_UseInAreaWeightedAveUfactor == true, user input</xsd:documentation>
