                                             <xsd:element name="Kskyperf10cLabel" minOccurs="0">
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">VT (min)</xsd:documentation>
                                                </xsd:annotation>
                                             </xsd:element>
                                             <xsd:choice maxOccurs="1">
                                                <xsd:element name="Kskyperf10a_UFactorLimit" type="com:UFactorLimit" >
                                                   <xsd:annotation>
                                                      <xsd:documentation source="FieldText">Required Product Performance</xsd:documentation>
                                                      <xsd:documentation source="CalculationsAndRules">If Kskyperf06_TopLevelOccupancy == Nonresidential  
Switch (Kskyperf05_FenestrationProductType) 
case SkylightGlassCurb:  result = 0.58; 
case SkylightGlassDeck:  result = 0.46;  
case SkylightPlasticCurb:  case SkylightTubular: result = 0.88; 
End Switch
 
Else If Kskyperf06_TopLevelOccupancy == HotelMotel 
Switch (Kskyperf05_FenestrationProductType) 
case SkylightGlassCurb:  result = 0.58; 
case SkylightGlassDeck:  result = 0.46;  
case SkylightPlasticCurb: result = 0.88; 
End Switch
 
Else If Kskyperf06_TopLevelOccupancy == RelocableSchoolBuildingAllZones  
Switch (Kskyperf05_FenestrationProductType) 
case SkylightGlassCurb:  result = 0.99; 
case SkylightGlassDeck:  result = 0.57;  
case SkylightPlasticCurb: result = 0.87;
End Switch
 
Else If Kskyperf06_TopLevelOccupancy == OtherOccupancy And Kskyperf06_FenestrationModification == NewSky16MF
result = 0.55
 
Else If Kskyperf06_TopLevelOccupancy == HighriseResidential
If Kskyperf06_FenestrationModification == AllNew Or Kskyperf09_ComplianceMethodSHGC_VT == Table170_2A
If A03_ClimateZoneNumber == InRange(1, 6) Or InRange (9, 16)
result = 0.30
Else If A03_ClimateZoneNumber == 7 Or 8
result = 0.34 
 End If
Else If Kskyperf09_ComplianceMethodSHGC_VT == Table180_2B
result 0.46
 
End If
End If</xsd:documentation>
                                                   </xsd:annotation>
                                                </xsd:element>
                                                <xsd:element name="Kskyperf10a_NotApplicableMessage" >
                                                   <xsd:annotation>
                                                      <xsd:documentation source="FieldText">Required Product Performance</xsd:documentation>
                                                      <xsd:documentation source="CalculationsAndRules">If  Kskyperf05_FenestrationProductType == ChromogenicFen Or
If Kskyperf06_TopLevelOccupancy == HotelMotel Or RelocableSchoolBuildingAllZones  And Kskyperf05_FenestrationProductType == SkylightTubular:
result = BlankGrayedOut
End If</xsd:documentation>
                                                   </xsd:annotation>
                                                   <xsd:simpleType>
                                                      <xsd:restriction base="comp:NotApplicableMessage">
                                                         <xsd:enumeration value="BlankGrayedOut"/>
                                                      </xsd:restriction>
                                                   </xsd:simpleType>
                                                </xsd:element>
                                             </xsd:choice>
                                             <xsd:choice maxOccurs="1">
                                                <xsd:element name="Kskyperf10b_SHGC_Limit" type="com:SHGC_Limit" >
                                                   <xsd:annotation>
                                                      <xsd:documentation source="FieldText">Required Product Performance</xsd:documentation>
                                                      <xsd:documentation source="CalculationsAndRules">If Kskyperf06_TopLevelOccupancy == Nonresidential  
Switch (Kskyperf05_FenestrationProductType) 
case SkylightGlassCurb: SkylightGlassDeck: result = 0.25; 
End Switch
 
Else If Kskyperf06_TopLevelOccupancy == HotelMotel
Switch (Kskyperf05_FenestrationProductType) 
case SkylightGlassCurb:  case SkylightGlassDeck:  result = 0.25;  
End Switch
 
Else If Kskyperf06_TopLevelOccupancy == RelocableSchoolBuildingAllZones  
Switch (Kskyperf05_FenestrationProductType)  
case SkylightGlassCurb:  case SkylightGlassDeck: 
If  Ksky04_TotalSkylightArea /  Ksky02_ExteriorSurfaceGrossArea LTE 0.02 result = 0.46; 
Else If Ksky04_TotalSkylightArea  /  Ksky02_ExteriorSurfaceGrossArea == InRange(0.021, 0.05) result = 0.36;
End If
case SkylightPlasticCurb
If  Ksky04_TotalSkylightArea /  Ksky02_ExteriorSurfaceGrossArea LTE 0.02 result = 0.69; 
Else If Ksky04_TotalSkylightArea  /  Ksky02_ExteriorSurfaceGrossArea == InRange(0.021, 0.05) result = 0.57;
End If
End Switch
 
Else If Kskyperf06_TopLevelOccupancy == OtherOccupancy And Kskyperf06_FenestrationModification == NewSky16MF
result= 0.30
 
Else If Kskyperf06_TopLevelOccupancy == HighriseResidential
If Kskyperf06_FenestrationModification == AllNew Or Kskyperf09_ComplianceMethodSHGC_VT == Table170_2A
If A03_ClimateZoneNuber == 1
result = 0.35
Else
result = 0.23
End If
End If
 
Else If Kskyperf09_ComplianceMethodSHGC_VT == Table180_2B
If A03_ClimateZoneNuber == 1
result = 0.35
Else
result = 0.25
End If
End If
 
End If</xsd:documentation>
                                                   </xsd:annotation>
                                                </xsd:element>
                                                <xsd:element name="Kskyperf10b_NotApplicableMessage" >
                                                   <xsd:annotation>
                                                      <xsd:documentation source="FieldText">Required Product Performance</xsd:documentation>
                                                      <xsd:documentation source="CalculationsAndRules">If  Kskyperf05_FenestrationProductType == ChromogenicFen Or  SkylightTubular Or 
If Kskyperf06_FenestrationModification == AddLTE50ft2  Or
 If  Kskyperf05_FenestrationProductType == SkylightPlasticCurb And  Kskyperf06_TopLevelOccupancy == Nonresidential Or HotelMotel 
result = BlankGrayedOut
End If</xsd:documentation>
                                                   </xsd:annotation>
