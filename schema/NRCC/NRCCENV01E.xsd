                  <xsd:element name="FframedMF" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">[d:b]Multifamily Framed Roof Assemblies[/d:b]</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">This subsection is required if F01FramedMF_RoofTypeApplies == true And A04_WholeBuildingOccupancy == HighRiseResidential</xsd:documentation>
                     </xsd:annotation>
                     <xsd:complexType>
                        <xsd:sequence>
                           <xsd:element name="FframedMFBeginNote" minOccurs="0">
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText"></xsd:documentation>
                                 <xsd:documentation source="AdditionalRequirements">[d:i]New construction multifamily occupancies with attic roofs may choose between U-factor or R-value to demonstrate compliance with Table 170.2-A or Section 180.2(b)1 for alterations. New construction non-attic roofs must comply with U-factors per Table 170.2-A. [/d:i]</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                           <xsd:element name="FframedMF01_UseInAreaWeightedAveUfactor" type="comp:UseInAreaWeightedAveUfactor" >
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText">Include Framed Roof Assemblies in Area-Weighted Average U-factor Calculation [d:sup]1[/d:sup]</xsd:documentation>
                                 <xsd:documentation source="CalculationsAndRules">User input: If checked result = true; Else result = false.  If result == true   D03_ExceptionalConditionMessageCCENVE += RoofAreaWeightedAveInF</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                           <xsd:element name="TableFframedMF" >
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText"></xsd:documentation>
                              </xsd:annotation>
                              <xsd:complexType>
                                 <xsd:sequence>
                                    <xsd:element name="Row" minOccurs="1" maxOccurs="unbounded">
                                       <xsd:complexType>
                                          <xsd:sequence>
                                             <xsd:element name="FframedMF02_RoofAssemblyName" type="env:RoofAssemblyName" >
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">Tag/Plan Detail ID</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">User input</xsd:documentation>
                                                </xsd:annotation>
                                             </xsd:element>
                                             <xsd:element name="FframedMF03_RoofDescription" type="env:RoofDescription" >
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">Name/Description</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">User input</xsd:documentation>
                                                </xsd:annotation>
                                             </xsd:element>
                                             <xsd:choice maxOccurs="1">
                                                <xsd:element name="FframedMF04_Status" >
                                                   <xsd:annotation>
                                                      <xsd:documentation source="FieldText">Status</xsd:documentation>
                                                      <xsd:documentation source="CalculationsAndRules">User selects one value from list that combines  FframedMF04_Status values and FframedMF04_RoofSlope values.  
   
Status values are  AddGT700sf display term New or Add greater than 700ft, and AddLTE700sf  display term Add less than or equal to 700sf, 
   
If user selects AddGT700sf or AddLTE700sf result is stored in this choice</xsd:documentation>
                                                   </xsd:annotation>
                                                   <xsd:simpleType>
                                                      <xsd:restriction base="com:Status">
                                                         <xsd:enumeration value="AddGT700sf"/>
                                                         <xsd:enumeration value="AddLTE700sf"/>
                                                      </xsd:restriction>
                                                   </xsd:simpleType>
                                                </xsd:element>
                                                <xsd:element name="FframedMF04_RoofSlope" >
                                                   <xsd:annotation>
                                                      <xsd:documentation source="FieldText">Status</xsd:documentation>
                                                      <xsd:documentation source="CalculationsAndRules">Roof values are LowSlope display term  Alt. Low Slope less than 2:12, and SteepSlope display term Alt. Steep Slope greater than or equal to 2:12
   
If user selects LowSlope Or SteepSlope result is stored in this choice</xsd:documentation>
                                                      <xsd:appinfo source="displayterm">
                                                         <dtyp:displayterm value="LowSlope"> Alt. Low Slope [d:lt/]2:12</dtyp:displayterm>
                                                         <dtyp:displayterm value="SteepSlope">Alt. Steep Slope[d:gte/] 2:12</dtyp:displayterm>
                                                      </xsd:appinfo>
                                                      
                                                   </xsd:annotation>
                                                   <xsd:simpleType>
                                                      <xsd:restriction base="env:RoofSlope">
                                                         <xsd:enumeration value="LowSlope"/>
                                                         <xsd:enumeration value="SteepSlope"/>
                                                      </xsd:restriction>
                                                   </xsd:simpleType>
                                                </xsd:element>
                                             </xsd:choice>
                                             <xsd:element name="FframedMF05_RoofType" >
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">Roof Type [d:sup]2[/d:sup]</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">User selects from list customized as follows: 
If FframedMF04_Status IsPresent and == AddGT700sf list AtticOptionB display term Attic-Opt. B, AtticOptionC display term Attic-Opt. C, and NonAtticOptionD display term Non-attic-Opt. D
 
Else If FframedMF04_Status IsPresent and == AddLTE700sf list VentedAttic display term Vented Attic, RafterRoof display term Enclosed Rafter Ceiling and RoofTypeOther display term Not vented attic or enclosed rafter
 
Else If FframedMF04_RoofSlope IsPresent and == LowSlope Or SteepSlope list VentedAttic display term Vented Attic and NotVentedAttic display term Other than Vented Attic
End If</xsd:documentation>
                                                   <xsd:appinfo source="displayterm">
                                                      <dtyp:displayterm value="RafterRoof">Enclosed Rafter Ceiling</dtyp:displayterm>
                                                      <dtyp:displayterm value="RoofTypeOther">Not vented attic or enclosed rafter</dtyp:displayterm>
                                                   </xsd:appinfo>
                                                   
                                                </xsd:annotation>
                                                <xsd:simpleType>
                                                   <xsd:restriction base="env:RoofType">
                                                      <xsd:enumeration value="AtticOptionB"/>
                                                      <xsd:enumeration value="AtticOptionC"/>
                                                      <xsd:enumeration value="NonAtticOptionD"/>
                                                      <xsd:enumeration value="NotVentedAttic"/>
                                                      <xsd:enumeration value="RafterRoof"/>
                                                      <xsd:enumeration value="RoofTypeOther"/>
                                                      <xsd:enumeration value="VentedAttic"/>
                                                   </xsd:restriction>
                                                </xsd:simpleType>
                                             </xsd:element>
                                             <xsd:choice maxOccurs="1">
                                                <xsd:element name="FframedMF06_RoofInsulationException" type="comp:RoofInsulationException" >
                                                   <xsd:annotation>
                                                      <xsd:documentation source="FieldText">Exception to Roof Insulation Requirements in [d:t24refnolink/]180.2(b)1 (Alts. Only)</xsd:documentation>
                                                      <xsd:documentation source="CalculationsAndRules">If FframedMF04_RoofSlope == LowSlope use this choice; 
End If
Default result = NoExceptionApplies but user can select from list: 
RecoverAboveDeckRvalue display term  Roof recovers with new R-10 insulation added abov deck
 
MechanicalEquipmentOnRoof display term  Existing mechanical equipment located on the roof will not be disconnected and lifted, insulation added is the greater of R-10 or the maximum installed thickness that will allow the distance between the height of the roof membrane surface to the top of the base flashing to remain in accordance with the manufacturer's instructions.
 
RoofRecoat display term  The area of the roof recoat is not required to be insulated.
 
NoExceptionApplies display term None of these exceptions apply, 
 
If result !=  NoExceptionApplies    
D03_ExceptionalConditionMessageCCENVE += RoofAlterationExceptionInF
End If</xsd:documentation>
                                                   </xsd:annotation>
                                                </xsd:element>
                                                <xsd:element name="FframedMF06_RoofInsulationExceptionMF" type="comp:RoofInsulationExceptionMF" >
                                                   <xsd:annotation>
                                                      <xsd:documentation source="FieldText">Exception to Roof Insulation Requirements in [d:t24refnolink/]180.2(b)1 (Alts. Only)</xsd:documentation>
                                                      <xsd:documentation source="CalculationsAndRules">If FframedMF04_RoofSlope == SteepSlope And FframedMF05_RoofType == VentedAttic use this choice  
End If
Default result = NoExceptionMF but user can select from list: 
CZ567 display term Project  located in Climate Zone 5, 6 or 7.
 
CZ1349 display term Dwelling units have at least R-19 existing insulation in climate zone 1, 3, 4 or 9.
 
CZ28and10to16 display term Dwelling units have at least R-38 existing insulation in climate zone 2, 8 or 10-16.
 
Asbestos display term The alteration would directly cause the distrubance of asbestos and the asbestos is not being abated.
 
VentedAtticWiring display term There is knob and tube wiring located in the vented attic.
 
LimitedSpace display term Accessible space in the attic is not large enough to accommodate the required R-value and the entire accessible space will be filled with insulation provided such installation does not violate Section 806.3 of Title 24, Part 2.5.
 
SharedAttic display term The attic space above the altered dwelling unit is shared with other dwelling units and the requirements of Section 180.2(b)1Bi are not triggered for the other dwelling units.
 
If result !=  NoExceptionMF    
D03_ExceptionalConditionMessageCCENVE += RoofAlterationExceptionInF
End If</xsd:documentation>
                                                   </xsd:annotation>
                                                </xsd:element>
                                                <xsd:element name="FframedMF06_NotApplicableMessage" >
                                                   <xsd:annotation>
                                                      <xsd:documentation source="FieldText">Exception to Roof Insulation Requirements in [d:t24refnolink/]180.2(b)1 (Alts. Only)</xsd:documentation>
                                                      <xsd:documentation source="CalculationsAndRules">If FframedMF04_Status == AddLTE700sf  Or  AddGT700sf Or
If FframedMF05_RoofType == RoofTypeOther Or 
If FframedMF04_RoofSlope == SteepSlope And  FframedMF05_RoofType == NotVentedAttic 
result = BlankGrayedOut stored in NotApplicableMessage; 
End If</xsd:documentation>
                                                   </xsd:annotation>
                                                   <xsd:simpleType>
                                                      <xsd:restriction base="comp:NotApplicableMessage">
                                                         <xsd:enumeration value="BlankGrayedOut"/>
                                                      </xsd:restriction>
                                                   </xsd:simpleType>
                                                </xsd:element>
                                             </xsd:choice>
                                             <xsd:choice maxOccurs="1">
                                                <xsd:element name="FframedMF07_ConstructionSpecificationMethod" >
                                                   <xsd:annotation>
                                                      <xsd:documentation source="FieldText">Compliance Unit</xsd:documentation>
                                                      <xsd:documentation source="CalculationsAndRules">If FframedMF01_UseInAreaWeightedAveUfactor  == false And 
FframedMF04_RoofSlope == LowSlope Or 
FframedMF05_RoofType  == VentedAttic And 
FframedMF04_Status == AddLTE700sf Or FframedMF04_RoofSlope == SteepSlope 
user selects from list: UFactor and RValue
 
Else If FframedMF04_Status == AddGT700sf 
If FframedMF05_RoofType == NonAtticOptionD
result = UFactor and cannot be changed by user
Else If FframedMF05_RoofType != NonAtticOptionD 
result = RValue and cannot be changed by user
End If
Else If FframedMF04_Status == AddLTE700sf  And FframedMF05_RoofType  == RafterRoof
result = RValue and cannot be changed by user
End If
End If</xsd:documentation>
                                                   </xsd:annotation>
                                                   <xsd:simpleType>
                                                      <xsd:restriction base="env:ConstructionSpecificationMethod">
                                                         <xsd:enumeration value="RValue"/>
                                                         <xsd:enumeration value="UFactor"/>
                                                      </xsd:restriction>
                                                   </xsd:simpleType>
                                                </xsd:element>
                                                <xsd:element name="FframedMF07_NotApplicableMessage" >
                                                   <xsd:annotation>
                                                      <xsd:documentation source="FieldText">Compliance Unit</xsd:documentation>
                                                      <xsd:documentation source="CalculationsAndRules">If  FframedMF05_RoofType == RoofTypeOther Or 
FframedMF04_RoofSlope == SteepSlope And  FframedMF05_RoofType == NotVentedAttic 
result = BlankGrayedOut stored in NotApplicableMessage; 
End If
Else If FframedMF06_RoofInsulationException != NoExceptionApplies Or
FframedMF06_RoofInsulationExceptionMF != NoExceptionMF
result = BlankGrayedOut stored in NotApplicableMessage;
End If</xsd:documentation>
                                                   </xsd:annotation>
                                                   <xsd:simpleType>
                                                      <xsd:restriction base="comp:NotApplicableMessage">
                                                         <xsd:enumeration value="BlankGrayedOut"/>
                                                      </xsd:restriction>
                                                   </xsd:simpleType>
                                                </xsd:element>
                                             </xsd:choice>
                                          </xsd:sequence>
                                       </xsd:complexType>
                                    </xsd:element>
                                 </xsd:sequence>
                              </xsd:complexType>
                           </xsd:element>
                           <xsd:element name="FframedMFFootnote1" minOccurs="0">
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText"></xsd:documentation>
                                 <xsd:documentation source="AdditionalRequirements">[d:i][d:sup]1[/d:sup]FOOTNOTE: If any individual assembly is non-compliant, assemblies may show compliance using an area-weighted calculation. The area-weighted compliance option is not available for assemblies demonstrating compliance with R-value requirements.    [/d:i]</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                           <xsd:element name="FframedMFFootnote2" minOccurs="0">
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText"></xsd:documentation>
                                 <xsd:documentation source="AdditionalRequirements">[d:i][d:sup]2[/d:sup] Option B: insulation installed between the roof rafters in contact with the roof deck and an additional layer of ceiling insulation located between the attic and the conditioned space; Option C: ceiling insulation located between the attic and the conditioned space.  [/d:i]</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                           <xsd:element name="TableFframedMFUfactor" minOccurs="0">
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText">[d:b]Assemblies Using U-factor as Compliance Unit [/d:b]</xsd:documentation>
                                 <xsd:documentation source="CalculationsAndRules">If FframedMF07_NotApplicableMessage IsPresent and FframedMF07_NotApplicableMessage == BlankGrayedOut  Or
If all rows  in TableFframedMF have FframedMF07_ConstructionSpecificationMethod != UFactor 
this table is not required 
 
Else there will be a row in this table for each row in TableFframedMF where FframedMF07_ConstructionSpecificationMethod == UFactor 
End If</xsd:documentation>
                              </xsd:annotation>
                              <xsd:complexType>
                                 <xsd:sequence>
                                    <xsd:element name="Row" minOccurs="1" maxOccurs="unbounded">
                                       <xsd:complexType>
                                          <xsd:sequence>
                                             <xsd:element name="FframedMF08_RoofAssemblyName" type="env:RoofAssemblyName" >
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">Tag/Plan Detail ID</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">Autofilled from FframedMF02_RoofAssemblyName for row where FframedMF07_ConstructionSpecificationMethod == UFactor</xsd:documentation>
                                                </xsd:annotation>
                                             </xsd:element>
                                             <xsd:element name="FframedMF09_UfactorSpecSource" type="comp:UfactorSpecSource" >
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">How Design U-factor was determined</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">User selects from list: 
JA4Tables display term JA4 Tables, 
JA4Other display term Other per JA4.1.2.1, 
ApprovedSoftware display term Approved Software.  
 
If result == JA4Other Or ApprovedSoftware D03_ExceptionalConditionMessageCCENVE += AdditionalDocJA4OtherSoftware</xsd:documentation>
                                                </xsd:annotation>
                                             </xsd:element>
                                             <xsd:choice maxOccurs="1">
                                                <xsd:element name="FframedMF10_ConstructionAssemblyName" >
                                                   <xsd:annotation>
                                                      <xsd:documentation source="FieldText">Frame Material</xsd:documentation>
                                                      <xsd:documentation source="CalculationsAndRules">If FframedMF09_UfactorSpecSource == JA4Other Or ApprovedSoftware this row is grayed out; 
 
Else If FframedMF09_UfactorSpecSource == JA4Tables user selects from list: WoodFramedRoof, MetalFramedRoof</xsd:documentation>
                                                   </xsd:annotation>
                                                   <xsd:simpleType>
                                                      <xsd:restriction base="env:ConstructionAssemblyName">
                                                         <xsd:enumeration value="MetalFramedRoof"/>
                                                         <xsd:enumeration value="WoodFramedRoof"/>
                                                      </xsd:restriction>
                                                   </xsd:simpleType>
                                                </xsd:element>
                                                <xsd:element name="FframedMF10_ConstructionAssemblyNameString" type="env:ConstructionAssemblyNameString" >
                                                   <xsd:annotation>
                                                      <xsd:documentation source="FieldText">Frame Material</xsd:documentation>
                                                      <xsd:documentation source="CalculationsAndRules">If FframedMF09_UfactorSpecSource == JA4Tables this row is grayed out; 
 
Else If FframedMF09_UfactorSpecSource == JA4Other Or ApprovedSoftware user enters text</xsd:documentation>
                                                   </xsd:annotation>
                                                </xsd:element>
                                             </xsd:choice>
                                             <xsd:element name="FframedMF11" >
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">Frame Spacing  &amp; Depth</xsd:documentation>
                                                </xsd:annotation>
                                                <xsd:complexType>
                                                   <xsd:choice >
                                                      <xsd:element name="FframedMF11" >
                                                         <xsd:annotation>
                                                            <xsd:documentation source="FieldText"></xsd:documentation>
                                                            <xsd:documentation source="CalculationsAndRules">If FframedMF09_UfactorSpecSource == JA4Tables user selects from dropdown list composed of items that combine display terms for FramingSpacing values followed by acceptable FramingSize values for the roof type; 
 
Else If FframedMF10_UfactorSpecSource == JA4Other Or ApprovedSoftware user enters text</xsd:documentation>
                                                         </xsd:annotation>
                                                         <xsd:complexType>
                                                            <xsd:sequence>
                                                               <xsd:element name="FframedMF11_FramingSpacing" >
                                                                  <xsd:annotation>
                                                                     <xsd:documentation source="FieldText"></xsd:documentation>
                                                                     <xsd:documentation source="CalculationsAndRules">If FframedMF10_UfactorSpecSource == JA4Tables user selects from list: 16InchOC, 24InchOC; 16InchOC display term is prepended to every FramingSize in the list And 24InchOC display term is prepended to every FramingSize in the lis</xsd:documentation>
                                                                     <xsd:appinfo source="displayterm">
                                                                        <dtyp:displayterm value="16InchOC">16" OC</dtyp:displayterm>
                                                                        <dtyp:displayterm value="24InchOC">24" OC</dtyp:displayterm>
                                                                     </xsd:appinfo>
                                                                     
                                                                  </xsd:annotation>
                                                                  <xsd:simpleType>
                                                                     <xsd:restriction base="env:FramingSpacing">
                                                                        <xsd:enumeration value="16InchOC"/>
                                                                        <xsd:enumeration value="24InchOC"/>
                                                                     </xsd:restriction>
                                                                  </xsd:simpleType>
                                                               </xsd:element>
                                                               <xsd:element name="FframedMF11_FramingSize" >
                                                                  <xsd:annotation>
                                                                     <xsd:documentation source="FieldText"></xsd:documentation>
                                                                     <xsd:documentation source="CalculationsAndRules">If FframedMF10_UfactorSpecSource == JA4Tables user selects from list customized as follows with FramingSize terms appended to the FramingSpacing display terms
End If
   
If FframedMF05_RoofType != NonAtticOptionD Or RafterRoof (for attics): list 2x4 once for FramingSpacing 16InchOC and once for FramingSpacing 24InchOC
 
Else (for rafters)  list 2x4, 2x6, 2x8, 2x10, 2x12, 2x14; each one is listed once for FramingSpacing 16InchOC and once for FramingSpacing 24InchOC
End If</xsd:documentation>
                                                                  </xsd:annotation>
                                                                  <xsd:simpleType>
                                                                     <xsd:restriction base="env:FramingSize">
                                                                        <xsd:enumeration value="2x10"/>
                                                                        <xsd:enumeration value="2x12"/>
                                                                        <xsd:enumeration value="2x14"/>
                                                                        <xsd:enumeration value="2x4"/>
                                                                        <xsd:enumeration value="2x6"/>
                                                                        <xsd:enumeration value="2x8"/>
                                                                     </xsd:restriction>
                                                                  </xsd:simpleType>
                                                               </xsd:element>
                                                            </xsd:sequence>
                                                         </xsd:complexType>
                                                      </xsd:element>
                                                      <xsd:element name="FframedMF11_FramingMaterialOther" type="env:FramingMaterialOther" >
                                                         <xsd:annotation>
                                                            <xsd:documentation source="FieldText">Frame Spacing  &amp; Depth</xsd:documentation>
                                                            <xsd:documentation source="CalculationsAndRules">If FframedMF09_UfactorSpecSource == JA4Tables, this row is grayed out; 
 
Else If FframedMF09_UfactorSpecSource == JA4Other Or ApprovedSoftware user enters text
End If</xsd:documentation>
                                                         </xsd:annotation>
                                                      </xsd:element>
                                                   </xsd:choice>
                                                </xsd:complexType>
                                             </xsd:element>
                                             <xsd:choice maxOccurs="1">
                                                <xsd:element name="FframedMF12_CavityInsulationNR" >
                                                   <xsd:annotation>
                                                      <xsd:documentation source="FieldText">Cavity Insulation per Design</xsd:documentation>
                                                      <xsd:documentation source="CalculationsAndRules">If FframedMF09_UfactorSpecSource == JA4Other Or ApprovedSoftware this row is grayed out; 
 
Else If FframedMF09_UfactorSpecSource == JA4Tables user selects from list as follows:  
If FframedMF05_RoofType != NonAtticOptionD Or RafterRoof (for attics):  list R0 (display term None), R11, R13, R19,  R21, R22, R25, R30, R38, R44, R49, R60;   
 
Else (for rafters)
Switch (FframedMF12_FramingSize) 
case 2x4:  list R0 (display term None), R11, R13, R15, R19Batt, R19SprayRigid; 
 
case 2x6: list R0 (display term None), R11, R13, R15, R19,  
  If FframedMF11_ConstructionAssemblyName ==  WoodFramedRoof also list   R21;  
  End If
case 2x8: list R0 (display term None), R19, R21; 
case 2x10:  list R0 (display term None),  R25, R30;
  If FframedMF11_ConstructionAssemblyName ==  WoodFramedRoof also list  R22,
  End If 
case 2x12: list R0 (display term None), R30, R38; 
case 2x14:  list R0 (display term None), R38;
End Switch
End If
End If</xsd:documentation>
                                                   </xsd:annotation>
                                                   <xsd:simpleType>
                                                      <xsd:restriction base="env:CavityInsulationNR">
                                                         <xsd:enumeration value="R0"/>
                                                         <xsd:enumeration value="R11"/>
                                                         <xsd:enumeration value="R13"/>
                                                         <xsd:enumeration value="R15"/>
                                                         <xsd:enumeration value="R19"/>
                                                         <xsd:enumeration value="R19Batt"/>
                                                         <xsd:enumeration value="R19SprayRigid"/>
                                                         <xsd:enumeration value="R21"/>
                                                         <xsd:enumeration value="R22"/>
                                                         <xsd:enumeration value="R25"/>
                                                         <xsd:enumeration value="R30"/>
                                                         <xsd:enumeration value="R38"/>
                                                         <xsd:enumeration value="R44"/>
                                                         <xsd:enumeration value="R49"/>
                                                         <xsd:enumeration value="R60"/>
                                                      </xsd:restriction>
                                                   </xsd:simpleType>
                                                </xsd:element>
                                                <xsd:element name="FframedMF12_CavityInsulationOther" type="com:CavityInsulationOther" >
                                                   <xsd:annotation>
                                                      <xsd:documentation source="FieldText">Cavity Insulation per Design</xsd:documentation>
                                                      <xsd:documentation source="CalculationsAndRules">If FframedMF09_UfactorSpecSource == JA4Tables, this row is grayed out; 
 
Else If FframedMF09_UfactorSpecSource == JA4Other Or ApprovedSoftware user enters number for R-value</xsd:documentation>
                                                   </xsd:annotation>
                                                </xsd:element>
                                             </xsd:choice>
                                             <xsd:choice maxOccurs="1">
                                                <xsd:element name="FframedMF13_ContinuousInsulationNR" >
                                                   <xsd:annotation>
                                                      <xsd:documentation source="FieldText">Continuous Insulation per Design</xsd:documentation>
                                                      <xsd:documentation source="CalculationsAndRules">If FframedMF09_UfactorSpecSource == JA4Other Or ApprovedSoftware this row is grayed out; 
 
Else If FframedMF09_UfactorSpecSource == JA4Tables user selects from list customized as follows:
 
If FframedMF05_RoofType == NonAtticOptionD Or RafterRoof And 
FframedMF11_ConstructionAssemblyName ==  WoodFramedRoof list: None, R-2, R-4, R-6, R-7, R-8, R-10, R-14, R-17, R-20, and R-23
 
Else list: None, R-2, R-4, R-6, R-7, R-8, R-10 and R-14
End If
End If</xsd:documentation>
                                                   </xsd:annotation>
                                                   <xsd:simpleType>
                                                      <xsd:restriction base="env:ContinuousInsulationNR">
                                                         <xsd:enumeration value="R0ci"/>
                                                         <xsd:enumeration value="R10ci"/>
                                                         <xsd:enumeration value="R14ci"/>
                                                         <xsd:enumeration value="R17ci"/>
                                                         <xsd:enumeration value="R20ci"/>
                                                         <xsd:enumeration value="R23ci"/>
                                                         <xsd:enumeration value="R2ci"/>
                                                         <xsd:enumeration value="R4ci"/>
                                                         <xsd:enumeration value="R6ci"/>
                                                         <xsd:enumeration value="R7ci"/>
                                                         <xsd:enumeration value="R8ci"/>
                                                      </xsd:restriction>
                                                   </xsd:simpleType>
                                                </xsd:element>
                                                <xsd:element name="FframedMF13_ContinuousInsulationOther" type="com:ContinuousInsulationOther" >
                                                   <xsd:annotation>
                                                      <xsd:documentation source="FieldText">Continuous Insulation per Design</xsd:documentation>
                                                      <xsd:documentation source="CalculationsAndRules">If  FframedMF09_UfactorSpecSource == JA4Tables, this row is grayed out; 
 
Else If  FframedMF09_UfactorSpecSource == JA4Other Or ApprovedSoftware user enters R-value number</xsd:documentation>
                                                   </xsd:annotation>
                                                </xsd:element>
                                             </xsd:choice>
                                             <xsd:element name="FframedMF14_ConstructionSpecificationMethod" >
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">Thermal Performance Unit</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">Result = UFactor and cannot be changed by user</xsd:documentation>
                                                </xsd:annotation>
                                                <xsd:simpleType>
                                                   <xsd:restriction base="env:ConstructionSpecificationMethod">
                                                      <xsd:enumeration value="UFactor"/>
                                                   </xsd:restriction>
                                                </xsd:simpleType>
                                             </xsd:element>
                                             <xsd:choice maxOccurs="1">
                                                <xsd:element name="FframedMF15_InsulationUFactorLimit" type="env:InsulationUFactorLimit" >
                                                   <xsd:annotation>
                                                      <xsd:documentation source="FieldText">Required Thermal Performance</xsd:documentation>
                                                      <xsd:documentation source="CalculationsAndRules">If FframedMF04_Status IsPresent 
If  FframedMF04_Status == AddGT700sf 
If  A03_ClimateZoneNumber == 1 Or 2 Or 4 Or InRange(8,16) result = 0.028; 
Else If  A03_ClimateZoneNumber == 3 Or 5 Or 6 result = 0.034; 
Else If  A03_ClimateZoneNumber == 7 result = 0.039; 
End If
Else If FframedMF04_Status == AddLTE700sf 
If FframedMF05_RoofType == VentedAttic Then
If A03_ClimateZoneNumber == 1 Or 2 Or 4 Or InRange(8,16) result = 0.025; 
Else If  A03_ClimateZoneNumber == 3 Or 5 Or 6 Or 7 result = 0.031; 
End If
Else If FframedMF05_RoofType == RafterRoof result = 0.075
End If
End If
 
Else If FframedMF04_RoofSlope IsPresent
 If A03_ClimateZoneNumber == 3 Or 5 Or 6 Or 7 result = BlankGrayedOut stored in NotApplicableMessage
End If;
If FframedMF04_RoofSlope == LowSlope 
If  A03_ClimateZoneNumber == 1 Or 2 Or 4 Or InRange(8,16) result = 0.039
End If
Else If FframedMF04_RoofSlope == SteepSlope And FframedMF05_RoofType == VentedAttic Then
If A03_ClimateZoneNumber == 1 Or 2 Or 4 Or InRange(8,16) result = 0.020
End If
End If
End If</xsd:documentation>
                                                   </xsd:annotation>
                                                </xsd:element>
                                                <xsd:element name="FframedMF15_NotApplicableMessage" >
                                                   <xsd:annotation>
                                                      <xsd:documentation source="FieldText">Required Thermal Performance</xsd:documentation>
                                                      <xsd:documentation source="CalculationsAndRules">If FframedMF04_Status == AddLTE700sf
If FframedMF05_RoofType == RoofTypeOther result = BlankGrayedOut
End if
End If
If FframedMF04_RoofSlope IsPresent Then
If  A03_ClimateZoneNumber == 3 Or 5 Or 6 Or 7
result = BlankGrayedOut
End If
End If</xsd:documentation>
                                                   </xsd:annotation>
                                                   <xsd:simpleType>
                                                      <xsd:restriction base="comp:NotApplicableMessage">
                                                         <xsd:enumeration value="BlankGrayedOut"/>
