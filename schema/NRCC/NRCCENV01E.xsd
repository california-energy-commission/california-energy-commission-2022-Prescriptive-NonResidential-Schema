End If
End If
If result == true continue evaluation: 
 
If I01SIPS_FloorTypeApplies == true, 
If fields beginning with ISIPS02, ISIPS03, ISIPS04, ISIPS05, ISIPS06, ISIPS06, ISIPS07, ISIPS08, ISIPS09, ISIPS10, ISIPS11, ISIPS12 == BlankGrayedOut or have another value result = true, Else result = false.  
End If
 If result == true continue evaluation: 
If ISIPS01_UseInAreaWeightedAveUfactor == false 
If field beginning with ISIPS11_UfactorJA4 that IsPresent is LTE ISIPS10_InsulationUFactorLimit result = true; Else result = false;   
End If
Else If ISIPS01_UseInAreaWeightedAveUfactor == true  
If ICalc2UF05_DoesAreaWeightAvgComplyUfactor == true result = true; Else result = false; 
End If
End If
End If
 If result == true continue evaluation: 
 
If I01Mass_FloorTypeApplies == true, 
If fields beginning with Imass02, Imass03, Imass04a, Imass04b, Imass05, Imass06, Imass07, Imass08, Imass09, Imass10, Imass11 == BlankGrayedOut or have another value result = true, Else result = false.   
End If
If result == true continue evaluation: 
If Imass01_UseInAreaWeightedAveUfactor == false 
If Imass08_ConstructionSpecificationMethod == RValue 
If Imass07_ContinuousInsulationNR Or Imass07_ContinuousInsulationOther GTE Imass09_ InsulationRValueLimit  result = true; Else result = false;   
End If 
Else If Imass08_ConstructionSpecificationMethod == UFactor 
If field beginning with Imass10_UfactorJA4 that IsPresent is LTE Imass09_InsulationUFactorLimit result = true; Else result = false;  
End If 
End If 
Else If Imass01_UseInAreaWeightedAveUfactor == true  
If ICalc1UF05_DoesAreaWeightAvgComplyUfactor == true result = true; Else result = false;  
End If
End If
End If
If result == true continue evaluation:  
 
If I01Heated_FloorTypeApplies == true, 
If fields beginning with Iheated01, Iheated02, Iheated03, Iheated05a, Iheated05b, Iheated06a, Iheated06b, Iheated08 == BlankGrayedOut or have another value result = true, Else result = false.   
End If
If result == true continue evaluation: 
If Iheated06a_ InsulationInteriorRValue GTE Iheated05a_ SlabInsulationRValueRequired result = true; Else result = false;
End If
 If result == true continue evaluation: 
If Iheated06b_ InsulationInteriorRValue IsPresent If Iheated06b_ InsulationInteriorRValue GTE Iheated05b_ SlabInsulationRValueRequired result = true; Else result = false;
End If
End If
If result == true 
final result = true
Else 
result = False
End If</xsd:documentation>
