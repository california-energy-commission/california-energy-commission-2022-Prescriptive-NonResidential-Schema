                                             <xsd:element name="ISIPS10_InsulationUFactorLimit" type="env:InsulationUFactorLimit" >
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">Maximum U-factor Allowed</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">Calculated field: 
Switch (  ISIPS03_TopLevelOccupancy) 
case  Nonresidential: 
If A03_ClimateZoneNumber == 1 result = 0.048; 
Else  If A03_ClimateZoneNumber == 2, 11, 14, 15 Or 16 result = 0.039; 
Else If A03_ClimateZoneNumber == 12 Or 13 Or InRange (3, 10) result = 0.071;   
End If
 
case HotelMotel: 
If A03_ClimateZoneNumber ==  1, 2, 14 Or 16 result = 0.034; 
Else If A03_ClimateZoneNumber == InRange(3, 6) Or InRange(8, 13) Or 15  result = 0.039; 
Else If A03_ClimateZoneNumber == 7 result = 0.071; 
End If
 
case RelocableSchoolBuildingAllZones: result = 0.048;
 
case ResidentialMF: 
If A03_ClimateZoneNumber == 1 result = 0.048; 
Else  If A03_ClimateZoneNumber == 2, 11, 14, 15 Or 16 result = 0.039; 
Else If A03_ClimateZoneNumber == 12 Or 13 Or InRange (3, 10) result = 0.071;   
End If
 
End Switch</xsd:documentation>
