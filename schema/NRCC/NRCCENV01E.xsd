Else If Hlog06_FramingDepth == 1_0In 
Switch (Hlog07_CavityInsulationNR) 
case R0: Rvalue = 1.0; case R1: Rvalue = 1.1;  case R2: Rvalue = 1.4;  case R3: Rvalue = 1.6; case R4: Rvalue = 1.7;  case R5: Rvalue = 1.8;  case R6: Rvalue = 1.8; case R7: Rvalue = 1.9; case R8: Rvalue = 1.9; 
