                                             <xsd:element name="Hconcrete10_UFactorLimit" type="com:UFactorLimit" >
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">Maximum U-factor Allowed[d:sup]2[/d:sup]</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">Calculated field: Values are for Mass Heavy because Hconcrete10a_HeatCapacityRange ==  GT15 for all cases in Table JA4.3.7  and therefore all assigned results are for  Mass Heavy
 
If Hconcrete03_TopLevelOccupancy == Nonresidential 
If A03_ClimateZoneNumber == 1 Or 12 result = 0.253; 
Else If A03_ClimateZoneNumber == InRange(2, 5) Or 10 result = 0.650; 
Else If A03_ClimateZoneNumber == 6, 7, 8 Or 9 result = 0.690; 
Else If A03_ClimateZoneNumber == 11, 14 Or 15 result = 0.184; 
Else If A03_ClimateZoneNumber == 13 result = 0.211; 
Else If A03_ClimateZoneNumber == 16 result = 0.160;  
End If
 
Else If Hconcrete03_TopLevelOccupancy == HotelMotel
If A03_ClimateZoneNumber == 1, 2, 3 Or 16 result = 0.160; 
Else If A03_ClimateZoneNumber == 4, 11, 14 Or 15 result = 0.184; 
Else If A03_ClimateZoneNumber == 5 Or 13 result = 0.211; 
Else If A03_ClimateZoneNumber == InRange( 6, 10) result = 0.690; 
Else If A03_ClimateZoneNumber == 12 result = 0.253;  
End If
 
Else If Hconcrete03_TopLevelOccupancy == RelocableSchoolBuildingAllZones  result = 0.170;
Else If A03_ClimateZoneNumber == InRange( 6, 10) result = 0.690; 
Else If A03_ClimateZoneNumber == 12 result = 0.253; 
End If
 
Else If Hconcrete03_TopLevelOccupancy == ResidentialMF Or OtherOccupancy 
 
Switch (Hconcrete10a_HeatCapacityRange) 
case LT7: 
If Hconcrete03a_StructureFireRating == GT1hr
If A03_ClimateZoneNumber == InRange(1,5) Or 8 Or 9 Or 10 Or 12 Or 13  result = 0.059
Else If A03_ClimateZoneNumber == 6 Or 7 result = 0.065
Else If A03_ClimateZoneNumber == 11 Or 14 Or 15 Or 16  result = 0.051
End If
Else If Hconcrete03a_StructureFireRating == LTE1hr
If A03_ClimateZoneNumber == InRange(1,5) Or InRange(8,16)  result = 0.051
Else If A03_ClimateZoneNumber == 6 Or 7 result = 0.065
End If
End If
 
case GTE7LT15:
If A03_ClimateZoneNumber == InRange(1,15)  result = 0.077
Else If A03_ClimateZoneNumber == 16  result = 0.059
End If
 
case GTE15:
If A03_ClimateZoneNumber == 1 Or 12 result = 0.253
Else If A03_ClimateZoneNumber == 2 Or 3 Or 4 Or 5 Or  Or 10  result = 0.650
Else If A03_ClimateZoneNumber == 6 Or 7 Or 8 Or 9   result = 0.690
Else  If A03_ClimateZoneNumber == 11 Or 14 Or 15 result = 0.184
Else If A03_ClimateZoneNumber == 13 result = 0.211
Else If A03_ClimateZoneNumber == 16  result = 0.160 
End If
End Switch
 
End If
 
 
 
 
 
 
If A03_ClimateZoneNumber == 1 Or 12 result = 0.253
Else If A03_ClimateZoneNumber == 2 Or 3 Or 4 Or 5 Or  Or 10  result = 0.650
Else If A03_ClimateZoneNumber == 6 Or 7 Or 8 Or 9   result = 0.690
Else  If A03_ClimateZoneNumber == 11 Or 14 Or 15 result = 0.184
Else If A03_ClimateZoneNumber == 13 result = 0.211
Else If A03_ClimateZoneNumber == 16  result = 0.160 
End If
End If</xsd:documentation>
                                                </xsd:annotation>
