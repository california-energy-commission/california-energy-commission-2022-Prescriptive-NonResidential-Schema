                                                            <xsd:annotation>
                                                               <xsd:documentation source="FieldText">Product Performance per  NA6</xsd:documentation>
                                                               <xsd:documentation source="CalculationsAndRules">Calculated field:
If Kskyperf11a_NotApplicableMessage IsPresent
result = BlankGrayedOut
End If</xsd:documentation>
                                                            </xsd:annotation>
                                                            <xsd:simpleType>
