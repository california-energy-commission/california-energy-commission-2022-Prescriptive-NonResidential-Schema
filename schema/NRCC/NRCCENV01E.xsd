Else If Hlog06_FramingDepth == 1_5In 
Switch (Hlog07_CavityInsulationNR) 
case R0: Rvalue = 1.1; case R1: Rvalue = 1.2;  case R2: Rvalue = 1.6;  case R3: Rvalue = 1.9; case R4: Rvalue = 2.1;  case R5: Rvalue = 2.2;  case R6: Rvalue = 2.3; case R7: Rvalue = 2.4; case R8: Rvalue = 2.5;  case R9:  Rvalue = 2.5; case R10: Rvalue = 2.6; case  R11: Rvalue = 2.6; case R12: Rvalue = 2.7;  
End Switch
Else If Hlog06_FramingDepth == 2In 
Switch (Hlog07_CavityInsulationNR) 
case R0: Rvalue = 1.1; case R1: Rvalue = 1.2;  case R2: Rvalue = 1.7;  case R3: Rvalue = 2.1; case R4: Rvalue = 2.3;  case R5: Rvalue = 2.5;  case R6: Rvalue = 2.7; case R7: Rvalue = 2.8; case R8: Rvalue = 2.9;  case R9:  Rvalue = 3.0; case R10: Rvalue =3.1; case  R11: Rvalue = 3.2; case R12: Rvalue = 3.2;  case R13: Rvalue = 3.3; case R14: Rvalue = 3.3;  case R15: Rvalue = 3.4; case R16: Rvalue = 3.4;  
End Switch
Else If Hlog06_FramingDepth == 2_5In  
Switch (Hlog07_CavityInsulationNR) 
case R0: Rvalue = 1.2; case R1: Rvalue = 1.3;  case R2: Rvalue = 1.8;  case R3: Rvalue = 2.3; case R4: Rvalue = 2.6;  case R5: Rvalue = 2.8;  case R6: Rvalue = 3.0; case R7: Rvalue = 3.2; case R8: Rvalue = 3.3;  case R9:  Rvalue = 3.5; case R10: Rvalue =3.6; case  R11: Rvalue = 3.6; case R12: Rvalue = 3.7;  case R13: Rvalue = 3.8; case R14: Rvalue = 3.9;  case R15: Rvalue = 3.9; case R16: Rvalue = 4.0;  case R17: Rvalue = 4.0; case R18: Rvalue = 4.1; case R19:  Rvalue = 4.1; case R20: Rvalue = 4.1;  
End Switch
Else If Hlog06_FramingDepth == 3In  
Switch (Hlog07_CavityInsulationNR) 
case R0: Rvalue = 1.2; case R1: Rvalue = 1.3;  case R2: Rvalue = 1.9;  case R3: Rvalue = 2.4; case R4: Rvalue = 2.8;  case R5: Rvalue = 3.1;  case R6: Rvalue = 3.3; case R7: Rvalue = 3.5; case R8: Rvalue = 3.7;  case R9:  Rvalue = 3.8; case R10: Rvalue =4.0; case  R11: Rvalue = 4.1; case R12: Rvalue = 4.2;  case R13: Rvalue = 4.3; case R14: Rvalue = 4.4;  case R15: Rvalue = 4.4; case R16: Rvalue = 4.5;  case R17: Rvalue = 4.6; case R18: Rvalue = 4.6; case R19:  Rvalue = 4.7; case R20: Rvalue = 4.7;  case R21: Rvalue = 4.8;  
End Switch
Else If Hlog06_FramingDepth == 3_5In  
Switch (Hlog07_CavityInsulationNR) 
case R0: Rvalue = 1.2; case R1: Rvalue = 1.3;  case R2: Rvalue = 2.0;  case R3: Rvalue = 2.5; case R4: Rvalue = 2.9;  case R5: Rvalue = 3.2;  case R6: Rvalue = 3.5; case R7: Rvalue = 3.8; case R8: Rvalue = 4.0;  case R9:  Rvalue = 4.2; case R10: Rvalue =4.3; case  R11: Rvalue = 4.5; case R12: Rvalue = 4.6;  case R13: Rvalue = 4.7; case R14: Rvalue = 4.8;  case R15: Rvalue = 4.9; case R16: Rvalue = 5.0;  case R17: Rvalue = 5.1; case R18: Rvalue = 5.1; case R19:  Rvalue = 5.2; case R20: Rvalue = 5.2;  case R21: Rvalue = 5.3;  
End Switch
Else If Hlog06_FramingDepth == 4In  
Switch (Hlog07_CavityInsulationNR) 
case R0: Rvalue = 1.2; case R1: Rvalue = 1.3;  case R2: Rvalue = 2.0;  case R3: Rvalue = 2.6; case R4: Rvalue = 3.0;  case R5: Rvalue = 3.4;  case R6: Rvalue = 3.7; case R7: Rvalue = 4.0; case R8: Rvalue = 4.2;  case R9:  Rvalue = 4.5; case R10: Rvalue =4.6; case  R11: Rvalue = 4.8; case R12: Rvalue = 5.0;  case R13: Rvalue = 5.1; case R14: Rvalue = 5.2;  case R15: Rvalue = 5.3; case R16: Rvalue = 5.4;  case R17: Rvalue = 5.5; case R18: Rvalue = 5.6; case R19:  Rvalue = 5.7; case R20: Rvalue = 5.8;  case R21: Rvalue = 5.8;  
End Switch
Else If Hlog06_FramingDepth == 4_5In  
Switch (Hlog07_CavityInsulationNR) 
case R0: Rvalue = 1.2; case R1: Rvalue = 1.3;  case R2: Rvalue = 2.1;  case R3: Rvalue = 2.6; case R4: Rvalue = 3.1;  case R5: Rvalue = 3.5;  case R6: Rvalue = 3.9; case R7: Rvalue = 4.2; case R8: Rvalue = 4.5;  case R9:  Rvalue = 4.7; case R10: Rvalue =4.9; case  R11: Rvalue = 5.1; case R12: Rvalue = 5.3;  case R13: Rvalue = 5.4; case R14: Rvalue = 5.6;  case R15: Rvalue = 5.7; case R16: Rvalue = 5.8;  case R17: Rvalue = 5.9; case R18: Rvalue = 6.0; case R19:  Rvalue = 6.1; case R20: Rvalue = 6.2;  case R21: Rvalue = 6.3;  
End Switch
If Hlog06_FramingDepth == 5In  
Switch (Hlog07_CavityInsulationNR) 
case R0: Rvalue = 1.2; case R1: Rvalue = 1.4;  case R2: Rvalue = 2.1;  case R3: Rvalue = 2.7; case R4: Rvalue = 3.2;  case R5: Rvalue = 3.7;  case R6: Rvalue = 4.1; case R7: Rvalue = 4.4; case R8: Rvalue = 4.7;  case R9:  Rvalue = 5.0; case R10: Rvalue =5.2; case  R11: Rvalue = 5.4; case R12: Rvalue = 5.6;  case R13: Rvalue = 5.8; case R14: Rvalue = 5.9;  case R15: Rvalue = 6.1; case R16: Rvalue = 6.2;  case R17: Rvalue = 6.3; case R18: Rvalue = 6.5; case R19:  Rvalue = 6.6; case R20: Rvalue = 6.7;  case R21: Rvalue = 6.8;  
End Switch
Else If Hlog06_FramingDepth == 5_5In  
Switch (Hlog07_CavityInsulationNR) 
case R0: Rvalue = 1.3; case R1: Rvalue = 1.4;  case R2: Rvalue = 2.1;  case R3: Rvalue = 2.8; case R4: Rvalue = 3.3;  case R5: Rvalue = 3.8;  case R6: Rvalue = 4.2; case R7: Rvalue = 4.6; case R8: Rvalue = 4.9;  case R9:  Rvalue = 5.2; case R10: Rvalue =5.4; case  R11: Rvalue = 5.7; case R12: Rvalue = 5.9;  case R13: Rvalue =6.1; case R14: Rvalue = 6.3;  case R15: Rvalue = 6.4; case R16: Rvalue = 6.6;  case R17: Rvalue = 6.7; case R18: Rvalue = 6.8; case R19:  Rvalue = 7.0; case R20: Rvalue = 7.1;  case R21: Rvalue = 7.2;
End Switch
End If</xsd:documentation>
                                                                     </xsd:annotation>
                                                                  </xsd:element>
                                                                  <xsd:element name="Hlog09_UfactorJA4314Other" type="comp:UfactorJA4314Other" >
                                                                     <xsd:annotation>
                                                                        <xsd:documentation source="FieldText"></xsd:documentation>
                                                                        <xsd:documentation source="CalculationsAndRules">If Hlog06_NotApplicableMessage != FramingNA And Hlog06_PartitionFramingMaterial == NoFraming Or EIFS, use this choice to adjust data to be reported.  Adjusted result for JA4 U-factor = 1/ ((1/result from Hlog09_ UfactorJA4311) + Rvalue) where Rvalue is assigned as follows using JA4 table 4.3.14  
 
If Hlog06_PartitionFramingMaterial == NoFraming 
Switch (Hlog07_CavityInsulationNR) 
case R0: Rvalue = 0.5; case R1: Rvalue = 1.5;  case R2: Rvalue = 2.5;  case R3: Rvalue = 3.5; case R4: Rvalue = 4.5;  case R5: Rvalue = 5.5;  case R6: Rvalue = 6.5; case R7: Rvalue = 7.5; case R8: Rvalue = 8.5; case R9:  Rvalue = 9.5; case R10: Rvalue = 10.5; case  R11: Rvalue = 11.5; case R12: Rvalue = 12.5; case R13: Rvalue = 13.5; case R14: Rvalue = 14.5;  case R15: Rvalue = 15.5; case R16: Rvalue = 16.5;  case R17: Rvalue = 17.5; case R18: Rvalue = 18.5; case R19:  Rvalue = 19.5; case R20: Rvalue = 20.5;  case R21: Rvalue = 21.5; 
End Switch
 
Else If Hlog06_PartitionFramingMaterial == EIFS 
Switch (Hlog07_CavityInsulationNR) 
case R0: Rvalue = 0.0; case R1: Rvalue = 1.0;  case R2: Rvalue = 2.0;  case R3: Rvalue = 3.0; case R4: Rvalue = 4.0;  case R5: Rvalue = 5.0;  case R6: Rvalue = 6.0; case R7: Rvalue = 7.0; case R8: Rvalue = 8.0; case R9:  Rvalue = 9.0; case R10: Rvalue = 10.0; case  R11: Rvalue = 11.0; case R12: Rvalue = 12.0; case R13: Rvalue = 13.0; case R14: Rvalue = 14.0;  case R15: Rvalue = 15.0; case R16: Rvalue = 16.0;  case R17: Rvalue = 17.0; case R18: Rvalue = 18.0; case R19:  Rvalue = 19.0; case R20: Rvalue = 20.0;  case R21: Rvalue = 21.0;
End Switch
End If</xsd:documentation>
                                                                     </xsd:annotation>
                                                                  </xsd:element>
                                                               </xsd:choice>
                                                            </xsd:sequence>
                                                         </xsd:complexType>
                                                      </xsd:element>
                                                      <xsd:element name="Hlog09_UfactorJA4Other" type="comp:UfactorJA4Other" >
                                                         <xsd:annotation>
                                                            <xsd:documentation source="FieldText">per Software/ Other</xsd:documentation>
                                                            <xsd:documentation source="CalculationsAndRules">If  Hlog04_UfactorSpecSource == JA4Other Or ApprovedSoftware user enters text; 
Else this row is grayed out
End If</xsd:documentation>
                                                         </xsd:annotation>
                                                      </xsd:element>
                                                   </xsd:choice>
                                                </xsd:complexType>
                                             </xsd:element>
                                             <xsd:choice maxOccurs="1">
                                                <xsd:element name="Hlog10_WallNetArea" type="comp:WallNetArea" >
                                                   <xsd:annotation>
                                                      <xsd:documentation source="FieldText">Net Area [d:sup]3[/d:sup] ft[d:sup]2[/d:sup]</xsd:documentation>
                                                      <xsd:documentation source="CalculationsAndRules">If Hlog01_UseInAreaWeightedAveUfactor == true, user input
End If</xsd:documentation>
                                                   </xsd:annotation>
                                                </xsd:element>
                                                <xsd:element name="Hlog10_NotApplicableMessage" >
                                                   <xsd:annotation>
                                                      <xsd:documentation source="FieldText">Net Area [d:sup]3[/d:sup] ft[d:sup]2[/d:sup]</xsd:documentation>
                                                      <xsd:documentation source="CalculationsAndRules">If Hlog01_UseInAreaWeightedAveUfactor == false  result = BlankGrayedOut stored in NotApplicableMessage;
End If</xsd:documentation>
                                                   </xsd:annotation>
                                                   <xsd:simpleType>
                                                      <xsd:restriction base="comp:NotApplicableMessage">
                                                         <xsd:enumeration value="BlankGrayedOut"/>
                                                      </xsd:restriction>
                                                   </xsd:simpleType>
                                                </xsd:element>
                                             </xsd:choice>
                                          </xsd:sequence>
                                       </xsd:complexType>
                                    </xsd:element>
                                 </xsd:sequence>
                              </xsd:complexType>
                           </xsd:element>
                           <xsd:element name="HlogFootnote1" minOccurs="0">
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText"></xsd:documentation>
                                 <xsd:documentation source="AdditionalRequirements">[d:i][d:sup]1[/d:sup]FOOTNOTES: If any individual assembly is non-compliant, assemblies may show compliance using an area-weighted calculation.  Log Home walls may be combined with mass, concrete sandwich panel and ICF wall types.  Log Home walls must meet mandatory insulation requirements in [d:t24refnolink/]120.7(b), but may area-weight to comply with prescriptive requirements in Table 140.3 for new construction. [/d:i]</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                           <xsd:element name="HlogFootnote2" minOccurs="0">
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText"></xsd:documentation>
                                 <xsd:documentation source="AdditionalRequirements">[d:i][d:sup]2[/d:sup] Log Home walls are defined as "wood framed and other" or "light " depending on their Heat Capacity.  Heat Capacity is determined in Table 4.3.11 in Joint Appendix 4.  Walls with Heat Capacity from 7 to less than 15 are "light".  Walls with heat capacity less than 7 would be categorized as "Wood framed and Other" for compliance purposes.[/d:i]</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                           <xsd:element name="HlogFootnote3" minOccurs="0">
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText"></xsd:documentation>
                                 <xsd:documentation source="AdditionalRequirements">[d:i][d:sup]3[/d:sup] Wall area minus any fenestration area [/d:i]</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                        </xsd:sequence>
                     </xsd:complexType>
                  </xsd:element>
                  <xsd:element name="Hstraw" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">[d:b]Straw Bale Walls [/d:b]</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">This subsection is required If H01Straw_WallTypeApplies == true</xsd:documentation>
                     </xsd:annotation>
                     <xsd:complexType>
                        <xsd:sequence>
                           <xsd:element name="Hstraw01_UseInAreaWeightedAveUfactor" type="comp:UseInAreaWeightedAveUfactor" >
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText">Include Straw Bale Walls in Area-Weighted Average U-factor Calculation[d:sup]1[/d:sup]</xsd:documentation>
                                 <xsd:documentation source="CalculationsAndRules">User input: If checked result = true; Else result = false.  If result == true   D03_ExceptionalConditionMessageCCENVE += WallAreaWeightedAveInH</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                           <xsd:element name="TableHstraw" >
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText"></xsd:documentation>
                              </xsd:annotation>
                              <xsd:complexType>
                                 <xsd:sequence>
                                    <xsd:element name="Row" minOccurs="1" maxOccurs="unbounded">
                                       <xsd:complexType>
                                          <xsd:sequence>
                                             <xsd:element name="Hstraw02_WallAssemblyName" type="env:WallAssemblyName" >
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">Tag/Plan Detail ID</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">User input</xsd:documentation>
                                                </xsd:annotation>
                                             </xsd:element>
                                             <xsd:element name="Hstraw03_WallDescription" type="env:WallDescription" >
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">Name/Description</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">User input</xsd:documentation>
                                                </xsd:annotation>
                                             </xsd:element>
                                             <xsd:element name="Hstraw04a_TopLevelOccupancy" >
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">Occupancy &amp; Status</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">User selects from list: Nonresidential display term Nonresidential/ Relocatable 1 CZ: New, HotelMotel display term Hotel/ Motel: New, RelocableSchoolBuildingAllZones display term Relocatable all CZ: New,  AlteredOccupancy display term Any occupancy: Altered.</xsd:documentation>
                                                   <xsd:appinfo source="displayterm">
                                                      <dtyp:displayterm value="HotelMotel">Hotel/ Motel: New</dtyp:displayterm>
                                                      <dtyp:displayterm value="Nonresidential">Nonresidential/ Relocatable 1 CZ: New</dtyp:displayterm>
                                                      <dtyp:displayterm value="RelocableSchoolBuildingAllZones">Relocatable all CZ: New</dtyp:displayterm>
                                                      <dtyp:displayterm value="AlteredOccupancy">Any occupancy: Altered</dtyp:displayterm>
                                                   </xsd:appinfo>
                                                   
                                                </xsd:annotation>
                                                <xsd:simpleType>
                                                   <xsd:restriction base="com:TopLevelOccupancy">
                                                      <xsd:enumeration value="AlteredOccupancy"/>
                                                      <xsd:enumeration value="HotelMotel"/>
                                                      <xsd:enumeration value="Nonresidential"/>
                                                      <xsd:enumeration value="RelocableSchoolBuildingAllZones"/>
                                                   </xsd:restriction>
                                                </xsd:simpleType>
                                             </xsd:element>
                                             <xsd:element name="Hstraw04b_Status" >
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">Occupancy &amp; Status</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">Calculatedf field: If Hstraw04a_TopLevelOccupancy == AlteredOccupancy result = ExistingAlteration; 
Else result = New
End If</xsd:documentation>
                                                </xsd:annotation>
                                                <xsd:simpleType>
                                                   <xsd:restriction base="com:Status">
                                                      <xsd:enumeration value="ExistingAlteration"/>
                                                      <xsd:enumeration value="New"/>
                                                   </xsd:restriction>
                                                </xsd:simpleType>
                                             </xsd:element>
                                             <xsd:choice maxOccurs="1">
                                                <xsd:element name="Hstraw05_UfactorSpecSource" type="comp:UfactorSpecSource" >
                                                   <xsd:annotation>
                                                      <xsd:documentation source="FieldText">How Design U-factor was determined</xsd:documentation>
                                                      <xsd:documentation source="CalculationsAndRules">User selects from list: JA4Tables display term JA4 Tables, JA4Other display term Other per JA4.1.2.1, ApprovedSoftware display term Approved Software.  
 
If result == JA4Other  Or ApprovedSoftware  D03_ExceptionalConditionMessageCCENVE += AdditionalDocJA4OtherSoftware</xsd:documentation>
                                                   </xsd:annotation>
                                                </xsd:element>
                                                <xsd:element name="Hstraw05_NotApplicableMessage" >
                                                   <xsd:annotation>
                                                      <xsd:documentation source="FieldText">How Design U-factor was determined</xsd:documentation>
                                                      <xsd:documentation source="CalculationsAndRules">If Hstraw07_ConstructionSpecificationMethod ==  RValue result = BlankGrayedOut stored in NotApplicableMessage
End If</xsd:documentation>
                                                   </xsd:annotation>
                                                   <xsd:simpleType>
                                                      <xsd:restriction base="comp:NotApplicableMessage">
                                                         <xsd:enumeration value="BlankGrayedOut"/>
                                                      </xsd:restriction>
                                                   </xsd:simpleType>
                                                </xsd:element>
                                             </xsd:choice>
                                             <xsd:choice maxOccurs="1">
                                                <xsd:element name="Hstraw06_StrawBaleRvalue" type="env:StrawBaleRvalue" >
                                                   <xsd:annotation>
                                                      <xsd:documentation source="FieldText">Insulation per Design</xsd:documentation>
                                                      <xsd:documentation source="CalculationsAndRules">If Hstraw05_UfactorSpecSource == JA4Tables user selects from list: R30straw display term R-30; Else  this row is grayed out
End If</xsd:documentation>
                                                   </xsd:annotation>
                                                </xsd:element>
                                                <xsd:element name="Hstraw06_InsulationOther" type="env:InsulationOther" >
                                                   <xsd:annotation>
                                                      <xsd:documentation source="FieldText">Insulation per Design</xsd:documentation>
                                                      <xsd:documentation source="CalculationsAndRules">If Hstraw05_UfactorSpecSource == JA4Tables this row is grayed out; 
Else If Hstraw05_UfactorSpecSource == JA4Other Or ApprovedSoftware 
Or If Hstraw07_ConstructionSpecificationMethod == RValue
user enters text
End if</xsd:documentation>
                                                   </xsd:annotation>
                                                </xsd:element>
                                             </xsd:choice>
                                             <xsd:element name="Hstraw07_ConstructionSpecificationMethod" >
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">Thermal Performance Unit</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">If Hstraw04b_Status == ExistingAlteration And Hstraw01_UseInAreaWeightedAveUfactor  == false list RValue display term R-value, UFactor display term U-factor ; 
Else result = UFactor and cannot be changed by user
End If</xsd:documentation>
                                                </xsd:annotation>
                                                <xsd:simpleType>
                                                   <xsd:restriction base="env:ConstructionSpecificationMethod">
                                                      <xsd:enumeration value="RValue"/>
                                                      <xsd:enumeration value="UFactor"/>
                                                   </xsd:restriction>
                                                </xsd:simpleType>
                                             </xsd:element>
                                             <xsd:choice maxOccurs="1">
                                                <xsd:element name="Hstraw08_InsulationUFactorLimit" type="env:InsulationUFactorLimit" >
                                                   <xsd:annotation>
                                                      <xsd:documentation source="FieldText">Required Thermal Performance [d:sup]2[/d:sup]</xsd:documentation>
                                                      <xsd:documentation source="CalculationsAndRules">If Hstraw04b_Status == New 
Switch (Hstraw04a_TopLevelOccupancy) 
case  Nonresidential:  
If A03_ClimateZoneNumber == 1 result = 0.095; 
Else  If A03_ClimateZoneNumber == 2, 4, 9, 10, 16 Or InRange (12 14)  result = 0.059; 
Else If A03_ClimateZoneNumber == 3, 6, Or 7  result = 0.110;  
Else If A03_ClimateZoneNumber == 5 Or 8 result = 0.102;  
Else If A03_ClimateZoneNumber == 11 result = 0.045; 
Else If A03_ClimateZoneNumber == 15 result = 0.042;  
End If
 
case HotelMotel: 
If A03_ClimateZoneNumber == 12, 13 Or  InRange (1, 10) result = 0.059; 
Else If A03_ClimateZoneNumber == 11, Or  InRange (14, 16) result = 0.042; 
End If
 
case RelocableSchoolBuildingAllZones:  result = 0.059; 
End Switch
 
Else If Hstraw04b_Status == ExistingAlteration And Hstraw07_ConstructionSpecificationMethod  == UFactor  result = 0.110;
End if</xsd:documentation>
                                                   </xsd:annotation>
                                                </xsd:element>
                                                <xsd:element name="Hstraw08_InsulationRValueLimit" >
                                                   <xsd:annotation>
                                                      <xsd:documentation source="FieldText">Required Thermal Performance [d:sup]2[/d:sup]</xsd:documentation>
                                                      <xsd:documentation source="CalculationsAndRules">If Hstraw04b_Status == ExistingAlteration And Hstraw07_ConstructionSpecificationMethod  == RValue result = R11;
End If</xsd:documentation>
                                                   </xsd:annotation>
                                                   <xsd:simpleType>
                                                      <xsd:restriction base="env:InsulationRValueLimit">
                                                         <xsd:enumeration value="R11"/>
                                                      </xsd:restriction>
                                                   </xsd:simpleType>
                                                </xsd:element>
                                             </xsd:choice>
                                             <xsd:element name="Hstraw09Label" minOccurs="0">
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">U-factor per Design</xsd:documentation>
                                                </xsd:annotation>
                                             </xsd:element>
                                             <xsd:choice maxOccurs="1">
                                                <xsd:element name="Hstraw09_UfactorJA4312" type="comp:UfactorJA4312" >
                                                   <xsd:annotation>
                                                      <xsd:documentation source="FieldText">per JA4</xsd:documentation>
                                                      <xsd:documentation source="CalculationsAndRules">If Hstraw05_UfactorSpecSource == JA4Tables calculated field: 
If Hstraw06_StrawBaleRvalue == R30straw result = 0.033
End If</xsd:documentation>
                                                   </xsd:annotation>
                                                </xsd:element>
                                                <xsd:element name="Hstraw09_UfactorJA4Other" type="comp:UfactorJA4Other" >
                                                   <xsd:annotation>
                                                      <xsd:documentation source="FieldText">per Software/ Other</xsd:documentation>
                                                      <xsd:documentation source="CalculationsAndRules">If Hstraw05_UfactorSpecSource == JA4Tables this row is grayed out; 
Else user enters text
End If</xsd:documentation>
                                                   </xsd:annotation>
                                                </xsd:element>
                                                <xsd:element name="Hstraw09_NotApplicableMessage" >
                                                   <xsd:annotation>
                                                      <xsd:documentation source="FieldText">U-factor per Design</xsd:documentation>
                                                      <xsd:documentation source="CalculationsAndRules">If Hstraw07_ConstructionSpecificationMethod ==  RValue result = BlankGrayedOut stored in NotApplicableMessage
End If</xsd:documentation>
                                                   </xsd:annotation>
                                                   <xsd:simpleType>
                                                      <xsd:restriction base="comp:NotApplicableMessage">
                                                         <xsd:enumeration value="BlankGrayedOut"/>
                                                      </xsd:restriction>
                                                   </xsd:simpleType>
                                                </xsd:element>
                                             </xsd:choice>
                                             <xsd:choice maxOccurs="1">
                                                <xsd:element name="Hstraw10_WallNetArea" type="comp:WallNetArea" >
                                                   <xsd:annotation>
                                                      <xsd:documentation source="FieldText">Net Area [d:sup]3[/d:sup] ft[d:sup]2[/d:sup]</xsd:documentation>
                                                      <xsd:documentation source="CalculationsAndRules">if Hstraw01_UseInAreaWeightedAveUfactor == true user enters value
End If</xsd:documentation>
                                                   </xsd:annotation>
                                                </xsd:element>
                                                <xsd:element name="Hstraw10_NotApplicableMessage" >
                                                   <xsd:annotation>
                                                      <xsd:documentation source="FieldText">Net Area [d:sup]3[/d:sup] ft[d:sup]2[/d:sup]</xsd:documentation>
                                                      <xsd:documentation source="CalculationsAndRules">If Hstraw07_ConstructionSpecificationMethod == RValue  Or Hstraw01_UseInAreaWeightedAveUfactor == false result = BlankGrayedOut stored in NotApplicableMessage;
End If</xsd:documentation>
                                                   </xsd:annotation>
                                                   <xsd:simpleType>
                                                      <xsd:restriction base="comp:NotApplicableMessage">
                                                         <xsd:enumeration value="BlankGrayedOut"/>
                                                      </xsd:restriction>
                                                   </xsd:simpleType>
                                                </xsd:element>
                                             </xsd:choice>
                                          </xsd:sequence>
                                       </xsd:complexType>
                                    </xsd:element>
                                 </xsd:sequence>
                              </xsd:complexType>
                           </xsd:element>
                           <xsd:element name="HstrawFootnote1" minOccurs="0">
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText"></xsd:documentation>
                                 <xsd:documentation source="AdditionalRequirements">[d:i][d:sup]1[/d:sup]FOOTNOTES: If any individual assembly is non-compliant, assemblies may show compliance using an area-weighted calculation.  Straw bale walls are combined with wood framed, spandrel &amp; curtain, metal panel and SIPs wall types.  The area-weighted compliance option is not available for alterations demonstrating compliance with R-values in [d:t24refnolink/]141.0(b)1B3.[/d:i]</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                           <xsd:element name="HstrawFootnote2" minOccurs="0">
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText"></xsd:documentation>
                                 <xsd:documentation source="AdditionalRequirements">[d:i][d:sup]2[/d:sup] If "R-value" is shown in cell 07 as the Thermal Performance Unit, the R-value shown here is for cavity insulation per [d:t24refnolink/]141.0(b)1B3.[/d:i]</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                           <xsd:element name="HstrawFootnote3" minOccurs="0">
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText"></xsd:documentation>
                                 <xsd:documentation source="AdditionalRequirements">[d:i][d:sup]3[/d:sup] Wall area minus any fenestration area [/d:i]</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                        </xsd:sequence>
                     </xsd:complexType>
                  </xsd:element>
                  <xsd:element name="HICF" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">[d:b]Insulated Concrete Form Walls (new walls only)[/d:b]</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">This subsection is required If H01ICF_WallTypeApplies == true</xsd:documentation>
                     </xsd:annotation>
                     <xsd:complexType>
                        <xsd:sequence>
                           <xsd:element name="HICF01_UseInAreaWeightedAveUfactor" type="comp:UseInAreaWeightedAveUfactor" >
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText">Calculate Area-Weighted Average U-factor for ICF Walls[d:sup]1[/d:sup]</xsd:documentation>
                                 <xsd:documentation source="CalculationsAndRules">User input: If checked result = true; Else result = false.  If result == true   D03_ExceptionalConditionMessageCCENVE += WallAreaWeightedAveInH</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                           <xsd:element name="TableHICF" >
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText"></xsd:documentation>
                              </xsd:annotation>
                              <xsd:complexType>
                                 <xsd:sequence>
                                    <xsd:element name="Row" minOccurs="1" maxOccurs="unbounded">
                                       <xsd:complexType>
                                          <xsd:sequence>
                                             <xsd:element name="HICF02_WallAssemblyName" type="env:WallAssemblyName" >
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">Tag/Plan Detail ID</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">User input</xsd:documentation>
                                                </xsd:annotation>
                                             </xsd:element>
                                             <xsd:element name="HICF03_TopLevelOccupancy" >
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">Occupancy Type</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">User selects from list: Nonresidential display term Nonresidential/ Relocatable 1 CZ, HotelMotel display term Hotel/ Motel, RelocableSchoolBuildingAllZones display term Relocatable all CZ,  ResidentialMF  display term Multifamily greater than 1 hr fire rating  and OtherOccupancy display term Multifamily less than or equal to 1 hr fire rating.</xsd:documentation>
                                                   <xsd:appinfo source="displayterm">
                                                      <dtyp:displayterm value="ResidentialMF">Multifamily [d:gt/] 1 hr fire rating</dtyp:displayterm>
                                                      <dtyp:displayterm value="OtherOccupancy">Multifamily [d:lte/] 1 hr fire rating</dtyp:displayterm>
                                                      <dtyp:displayterm value="HotelMotel">Hotel/ Motel</dtyp:displayterm>
                                                      <dtyp:displayterm value="Nonresidential">Nonresidential/ Relocatable 1 CZ</dtyp:displayterm>
                                                      <dtyp:displayterm value="RelocableSchoolBuildingAllZones">Relocatable all CZ</dtyp:displayterm>
                                                   </xsd:appinfo>
                                                   
                                                </xsd:annotation>
                                                <xsd:simpleType>
                                                   <xsd:restriction base="com:TopLevelOccupancy">
                                                      <xsd:enumeration value="HotelMotel"/>
                                                      <xsd:enumeration value="Nonresidential"/>
                                                      <xsd:enumeration value="OtherOccupancy"/>
                                                      <xsd:enumeration value="RelocableSchoolBuildingAllZones"/>
                                                      <xsd:enumeration value="ResidentialMF"/>
                                                   </xsd:restriction>
                                                </xsd:simpleType>
                                             </xsd:element>
                                             <xsd:element name="HICF03b_StructureFireRating" type="env:StructureFireRating" >
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">NOT REPORTED IN PDF</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">If HICF03_TopLevelOccupancy == ResidentialMF
result = GT1hr
Else If HICF03_TopLevelOccupancy == OtherOccupancy
result = LTE1hr
Else
result = RatingNA
End If</xsd:documentation>
                                                </xsd:annotation>
                                             </xsd:element>
                                             <xsd:element name="HICF04_UfactorSpecSource" type="comp:UfactorSpecSource" >
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">How Design U-factor was determined</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">User selects from list: JA4Tables display term JA4 Tables, JA4Other display term Other per JA4.1.2.1, ApprovedSoftware display term Approved Software. 
 If result == JA4Other  Or ApprovedSoftware  D03_ExceptionalConditionMessageCCENVE += AdditionalDocJA4OtherSoftware</xsd:documentation>
                                                </xsd:annotation>
                                             </xsd:element>
                                             <xsd:element name="HICF05_TypeICF" type="env:TypeICF" >
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">ICF Type</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">User selects from list: Flat, GridWaffle GridScreen</xsd:documentation>
                                                </xsd:annotation>
                                             </xsd:element>
                                             <xsd:element name="HICF06_CoreICF" type="env:CoreICF" >
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">Concrete Core Thickness (in)</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">User selects from customized list:  
If HICF05_TypeICF == Flat list: 4core display term 4 in, 6core display term 6 in, 8core display term 8 in, 10core display term 10 in, 12core display term 12 in; 
 
Else If HICF05_TypeICF == GridWaffle list: 6core display term 6 in, 8core display term 8 in; Else  If HICF05_TypeICF == GridScreen list: 6core display term 6 in;
End If</xsd:documentation>
                                                </xsd:annotation>
                                             </xsd:element>
                                             <xsd:element name="HICF07_InsulationType" >
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">Insulation Type</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">User selects from customized list:  
If HICF05_TypeICF == Flat 
list: ExpandedPolystyrene display term EPS,  ExtrudedPolystyrene display term XPS,  Polyurethane 
 
Else   If HICF05_TypeICF == GridWaffle Or GridScreen 
list: ExpandedPolystyrene display term EPS, CementEPScompound display term Cement EPS compound
End If</xsd:documentation>
                                                   <xsd:appinfo source="displayterm">
                                                      <dtyp:displayterm value="ExpandedPolystyrene">EPS</dtyp:displayterm>
                                                      <dtyp:displayterm value="ExtrudedPolystyrene"> XPS</dtyp:displayterm>
                                                   </xsd:appinfo>
                                                   
                                                </xsd:annotation>
                                                <xsd:simpleType>
                                                   <xsd:restriction base="env:InsulationType">
                                                      <xsd:enumeration value="CementEPScompound"/>
                                                      <xsd:enumeration value="ExpandedPolystyrene"/>
                                                      <xsd:enumeration value="ExtrudedPolystyrene"/>
                                                      <xsd:enumeration value="Polyurethane"/>
                                                   </xsd:restriction>
                                                </xsd:simpleType>
                                             </xsd:element>
                                             <xsd:element name="HICF08_InsulationICF" type="env:InsulationICF" >
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">Insulation Thickness (in)</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">User selects from list customized as follows: 
If HICF07_InsulationType == ExpandedPolystyrene list: 2icf display term 2 in., 2_25icf display term 2.25 in., 2_5icf display term 2.5 in., 2_625icf display term 2.625 in., 2_75icf display term 2.75 in., 3icf display term 3 in., 3icf display term 3 in., 4icf display term 4 in.; 
 
Else If HICF07_InsulationType == ExtrudedPolystyrene list: 2icf display term 2 in., 2_5icf display term 2.5 in., 2_625icf display term 2.625 in., 2_75icf display term 2.75 in., 3icf display term 3 in., 3icf display term 3 in., 4icf display term 4 in.; 
 
Else If HICF07_InsulationType == Polyurethane list: 1_5icf display term 1.5 in., 2icf display term 2 in., 4_5icf display term 4.5 in.;  
Else If HICF07_InsulationType == CementEPScompound list: 2icf display term 2 in., 3icf display term 3 in., 4icf display term 4 in.;
End If
End If</xsd:documentation>
                                                </xsd:annotation>
                                             </xsd:element>
                                             <xsd:element name="HICF09_UFactorLimit" type="com:UFactorLimit" >
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">Maximum U-factor Allowed[d:sup]2[/d:sup]</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">Calculated field: 
If HICF03_TopLevelOccupancy == Nonresidential 
Switch (HICF09a_HeatCapacityRange) 
case GTE7LT15: 
If A03_ClimateZoneNumber == 1 result = 0.196; 
Else If A03_ClimateZoneNumber == 2 Or InRange(10, 16) result = 0.170; 
Else If A03_ClimateZoneNumber == 3 result = 0.278; 
Else If A03_ClimateZoneNumber == 4 result = 0.227; 
Else If A03_ClimateZoneNumber == InRange (5, 9) result = 0.440;  
End If
 
case GTE15: 
If A03_ClimateZoneNumber == 1 Or 12 result = 0.253; 
Else If A03_ClimateZoneNumber == InRange(2, 5) Or 10 result = 0.650; 
Else If A03_ClimateZoneNumber == 6, 7, 8 Or 9 result = 0.690; 
Else If A03_ClimateZoneNumber == 11, 14 Or 15 result = 0.184; 
Else If A03_ClimateZoneNumber == 13 result = 0.211; 
Else If A03_ClimateZoneNumber == 16 result = 0.160;  
End If
End Switch
 
If HICF03_TopLevelOccupancy == HotelMotel  
Switch (HICF09a_HeatCapacityRange) 
case GTE7LT15: 
If A03_ClimateZoneNumber == InRange(1,5) Or InRange(10, 16) result =0.170; 
Else If A03_ClimateZoneNumber == 6, 7 Or 8 result = 0.227; 
Else If A03_ClimateZoneNumber == 9 result = 0.196;  
End If
 
case GTE15: 
If A03_ClimateZoneNumber == 1, 2, 3 Or 16 result = 0.160; 
Else If A03_ClimateZoneNumber == 4, 11, 14 Or 15 result = 0.184; 
Else If A03_ClimateZoneNumber == 5 Or 13 result = 0.211; 
Else If A03_ClimateZoneNumber == InRange( 6, 10) result = 0.690; 
Else If A03_ClimateZoneNumber == 12 result = 0.253;  
End If
End Switch
 
If HICF03_TopLevelOccupancy == RelocableSchoolBuildingAllZones  
If HICF09a_HeatCapacityRange == GTE7LT15 Or GTE15 result = 0.170;
 
Else If HICF03_TopLevelOccupancy == ResidentialMF Or OtherOccupancy
Switch (HICF09a_HeatCapacityRange) 
case GTE7LT15: 
If A03_ClimateZoneNumber == InRange(1, 15) result = 0.077; 
Else If A03_ClimateZoneNumber == 16 result = 0.059; 
End If
 
case GTE15: 
If A03_ClimateZoneNumber == 1 Or 12 result = 0.253; 
Else If A03_ClimateZoneNumber == InRange(2, 5) Or 10 result = 0.650; 
Else If A03_ClimateZoneNumber == 6, 7, 8 Or 9 result = 0.690; 
Else If A03_ClimateZoneNumber == 11, 14 Or 15 result = 0.184; 
Else If A03_ClimateZoneNumber == 13 result = 0.211; 
Else If A03_ClimateZoneNumber == 16 result = 0.160;  
End If
End Switch
 
End If</xsd:documentation>
                                                </xsd:annotation>
                                             </xsd:element>
                                             <xsd:element name="HICF09a_HeatCapacityRange" type="com:HeatCapacityRange" >
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">NOT REPORTED IN PDF</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">Calculated value:  values from JA Table 4.3.13 as follows:  
If HICF06_CoreICF == 4core Or 
If HICF06_CoreICF == 6core And HICF05_TypeICF != Flat And HICF07_InsulationType != CementEPScompound Or 
HICF06_CoreICF == 6core And HICF07_InsulationType == CementEPScompound And HICF05_TypeICF == GridScreen And  HICF08_InsulationICF == 2icf  
result = GTE7LT15;  
 
Else If HICF05_TypeICF == Flat And HICF06_CoreICF != 4core And HICF07_InsulationType != CementEPScompound Or 
If HICF05_TypeICF ==GridWaffle And HICF06_CoreICF == 8core And HICF07_InsulationType == ExpandedPolystyrene   Or 
If HICF07_InsulationType == CementEPScompound And HICF05_TypeICF == GridWaffle Or 
HICF07_InsulationType == CementEPScompound And HICF05_TypeICF ==GridScreen And  HICF08_InsulationICF != 2icf  
result = GTE15;
 
End If</xsd:documentation>
                                                </xsd:annotation>
                                             </xsd:element>
                                             <xsd:element name="HICF10UF" >
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">U-factor per Design</xsd:documentation>
                                                </xsd:annotation>
                                                <xsd:complexType>
                                                   <xsd:choice >
                                                      <xsd:element name="HICF10JA4" >
                                                         <xsd:annotation>
                                                            <xsd:documentation source="FieldText">per JA4</xsd:documentation>
                                                         </xsd:annotation>
                                                         <xsd:complexType>
                                                            <xsd:choice >
                                                               <xsd:element name="HICF10_UfactorJA4313EPS" type="comp:UfactorJA4313EPS" >
                                                                  <xsd:annotation>
                                                                     <xsd:documentation source="FieldText"></xsd:documentation>
                                                                     <xsd:documentation source="CalculationsAndRules">Calculated field: 
If HICF07_InsulationType == ExpandedPolystyrene  
If HICF05_TypeICF == Flat 
If HICF08_InsulationICF == 2icf 
Switch (HICF06_CoreICF) 
case 4core: result = 0.058; case 6core: result = 0.057; case 8core: result = 0.056; case 10core: result = 0.055; case 12core: result = 0.055; 
End Switch
Else If HICF08_InsulationICF == 2_25icf 
Switch (HICF06_CoreICF) 
case 4core: result = 0.052; case 6core: result = 0.051; case 8core: result = 0.051; case 10core: result = 0.050; case 12core: result = 0.050;  
End Switch
Else If HICF08_InsulationICF == 2_5icf 
Switch (HICF06_CoreICF) 
case 4core: result = 0.047; case 6core: result = 0.047; case 8core: result = 0.046; case 10core: result = 0.046; case 12core: result = 0.045;  
End Switch
Else If HICF08_InsulationICF == 2_625icf 
Switch (HICF06_CoreICF) 
case 4core: result = 0.045; case 6core: result = 0.045; case 8core: result = 0.044; case 10core: result = 0.044; case 12core: result = 0.043;  
End Switch
Else If HICF08_InsulationICF == 2_75icf 
Switch (HICF06_CoreICF) 
case 4core: result = 0.043; case 6core: result = 0.043; case 8core: result = 0.042; case 10core: result = 0.042; case 12core: result = 0.042;  
End Switch
Else If HICF08_InsulationICF == 3icf 
Switch (HICF06_CoreICF) 
case 4core: result = 0.040; case 6core: result = 0.040; case 8core: result = 0.039; case 10core: result = 0.039; case 12core: result = 0.039;  
End Switch
Else If HICF08_InsulationICF == 3_5icf 
Switch (HICF06_CoreICF) 
case 4core: result = 0.035; case 6core: result = 0.034; case 8core: result = 0.034; case 10core: result = 0.034; case 12core: result = 0.034;  
End Switch
Else If HICF08_InsulationICF == 4icf 
Switch (HICF06_CoreICF) 
case 4core: result = 0.031; case 6core: result = 0.030; case 8core: result = 0.030; case 10core: result = 0.030; case 12core: result = 0.030;  
End Switch
End If
 
Else If HICF05_TypeICF == GridWaffle 
Switch (HICF08_InsulationICF) 
case 2icf:  
If HICF06_CoreICF == 6core result = 0.047; 
Else If HICF06_CoreICF == 8core result = 0.039; 
End If
case 2_25icf: 
If HICF06_CoreICF == 6core result = 0.043; 
Else If HICF06_CoreICF == 8core result = 0.036;  
End If
case 2_5icf: 
If HICF06_CoreICF == 6core result = 0.040; 
Else If HICF06_CoreICF == 8core result = 0.034;  
end If
case 2_625icf: 
If HICF06_CoreICF == 6core result = 0.038; 
Else If HICF06_CoreICF == 8core result = 0.033;  
End If
case 2_75icf: 
If HICF06_CoreICF == 6core result = 0.037; 
Else If HICF06_CoreICF == 8core result = 0.032;  
End If
case 3icf: 
If HICF06_CoreICF == 6core result = 0.0334; 
Else If HICF06_CoreICF == 8core result = 0.030;  
End If
case 3_5icf: 
If HICF06_CoreICF == 6core result = 0.030; 
Else If HICF06_CoreICF == 8core result = 0.027;  
End If
case 4icf: 
If HICF06_CoreICF == 6core result = 0.027; 
Else If HICF06_CoreICF == 8core result = 0.024;  
End If
End Switch
 
Else If HICF05_TypeICF == GridScreen 
Switch (HICF08_InsulationICF) 
case 2icf: result = 0.041; case 2_25icf: result = 0.038; case 2_5icf: result = 0.036; case 2_625icf: result = 0.035;  case 2_75icf: result 0.0323; case 3icf: result = 0.031;  case 3_5icf: result = 0.028;  case 4icf: result = 0.025;
End Switch
End If</xsd:documentation>
                                                                  </xsd:annotation>
                                                               </xsd:element>
                                                               <xsd:element name="HICF10_UfactorJA4313Other" type="comp:UfactorJA4313Other" >
                                                                  <xsd:annotation>
                                                                     <xsd:documentation source="FieldText"></xsd:documentation>
                                                                     <xsd:documentation source="CalculationsAndRules">Calculated field: 
If HICF07_InsulationType == ExtrudedPolystyrene  
If HICF05_TypeICF == Flat 
If HICF08_InsulationICF == 2icf 
Switch (HICF06_CoreICF) 
case 4core: result = 0.045; case 6core: result = 0.045; case 8core: result = 0.045; case 10core: result = 0.044; case 12core: result = 0.044; 
End Switch
Else If HICF08_InsulationICF == 2_5icf 
Switch (HICF06_CoreICF) 
case 4core: result = 0.037; case 6core: result = 0.037; case 8core: result = 0.036; case 10core: result = 0.036; case 12core: result = 0.036;  
End Swittch
Else If HICF08_InsulationICF == 2_625icf 
Switch (HICF06_CoreICF) 
case 4core: result = 0.035; case 6core: result = 0.035; case 8core: result = 0.035; case 10core: result = 0.035; case 12core: result = 0.034;  
End Switch
Else If HICF08_InsulationICF == 2_75icf 
Switch (HICF06_CoreICF) 
case 4core: result = 0.034; case 6core: result = 0.034; case 8core: result = 0.033; case 10core: result = 0.033; case 12core: result = 0.033;  
End Switch
Else If HICF08_InsulationICF == 3icf 
Switch (HICF06_CoreICF) 
case 4core: result = 0.031; case 6core: result = 0.031; case 8core: result = 0.031; case 10core: result = 0.031; case 12core: result = 0.030;  
End Switch
Else If HICF08_InsulationICF == 3_5icf 
Switch (HICF06_CoreICF) 
case 4core: result = 0.027; case 6core: result = 0.027; case 8core: result = 0.027; case 10core: result = 0.027; case 12core: result = 0.026;  
End Switch
Else If HICF08_InsulationICF == 4icf 
Switch (HICF06_CoreICF) 
case 4core: result = 0.024; case 6core: result = 0.024; case 8core: result = 0.024; case 10core: result = 0.023; case 12core: result = 0.023;  
End Switch
End If
End If
 
Else If HICF07_InsulationType == Polyurethane   
If HICF05_TypeICF == Flat 
If HICF08_InsulationICF == 1_5icf  
Switch (HICF06_CoreICF) 
case 4core: result = 0.050; case 6core: result = 0.049; case 8core: result = 0.049; case 10core: result = 0.048; case 12core: result = 0.048; 
End Switch
Else If HICF08_InsulationICF == 2icf 
Switch (HICF06_CoreICF) 
case 4core: result = 0.042; case 6core: result = 0.042; case 8core: result = 0.041; case 10core: result = 0.041; case 12core: result = 0.041; 
End Switch
If HICF08_InsulationICF == 4_5icf 
Switch (HICF06_CoreICF) 
case 4core: result = 0.023; case 6core: result = 0.023; case 8core: result = 0.023; case 10core: result = 0.022; case 12core: result = 0.022;  
End Switch
End If
End If
 
Else If HICF07_InsulationType == CementEPScompound 
If HICF05_TypeICF == GridWaffle 
If HICF08_InsulationICF == 2icf 
If HICF06_CoreICF == 6core result = 0.059; 
Else If HICF06_CoreICF == 8core result = 0.048;  
End If
Else If HICF08_InsulationICF == 3icf 
If HICF06_CoreICF == 6core result = 0.043; 
Else If HICF06_CoreICF == 8core result = 0.037;  
End If
Else If HICF08_InsulationICF == 4icf 
If HICF06_CoreICF == 6core result = 0.034; 
Else If HICF06_CoreICF == 8core result = 0.031;  
End if
End If
Else If If HICF05_TypeICF == GridScreen 
If HICF08_InsulationICF == 2icf  result = 0.052; 
Else If HICF08_InsulationICF == 3icf  result = 0.040; 
Else If HICF08_InsulationICF == 4icf  result = 0.032;
End If
End If</xsd:documentation>
                                                                  </xsd:annotation>
                                                               </xsd:element>
                                                            </xsd:choice>
                                                         </xsd:complexType>
                                                      </xsd:element>
                                                      <xsd:element name="HICF10_UfactorJA4Other" type="comp:UfactorJA4Other" >
                                                         <xsd:annotation>
                                                            <xsd:documentation source="FieldText">per Software/ Other</xsd:documentation>
                                                            <xsd:documentation source="CalculationsAndRules">If HICF04_UfactorSpecSource == JAOther user enters value; 
Else row is grayed out.
End if</xsd:documentation>
                                                         </xsd:annotation>
                                                      </xsd:element>
                                                   </xsd:choice>
                                                </xsd:complexType>
                                             </xsd:element>
                                             <xsd:choice maxOccurs="1">
                                                <xsd:element name="HICF11_WallNetArea" type="comp:WallNetArea" >
                                                   <xsd:annotation>
                                                      <xsd:documentation source="FieldText">Net Area [d:sup]3[/d:sup] ft[d:sup]2[/d:sup]</xsd:documentation>
                                                      <xsd:documentation source="CalculationsAndRules">if HICF01_UseInAreaWeightedAveUfactor == true user enters value
End If</xsd:documentation>
                                                   </xsd:annotation>
                                                </xsd:element>
                                                <xsd:element name="HICF11_NotApplicableMessage" >
                                                   <xsd:annotation>
                                                      <xsd:documentation source="FieldText">Net Area [d:sup]3[/d:sup] ft[d:sup]2[/d:sup]</xsd:documentation>
                                                      <xsd:documentation source="CalculationsAndRules">if HICF01_UseInAreaWeightedAveUfactor == false result = BlankGrayedOut stored in NotApplicableMessage;
End If</xsd:documentation>
                                                   </xsd:annotation>
                                                   <xsd:simpleType>
                                                      <xsd:restriction base="comp:NotApplicableMessage">
                                                         <xsd:enumeration value="BlankGrayedOut"/>
                                                      </xsd:restriction>
                                                   </xsd:simpleType>
                                                </xsd:element>
                                             </xsd:choice>
                                          </xsd:sequence>
                                       </xsd:complexType>
                                    </xsd:element>
                                 </xsd:sequence>
                              </xsd:complexType>
                           </xsd:element>
                           <xsd:element name="HICFFootnote1" minOccurs="0">
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText"></xsd:documentation>
                                 <xsd:documentation source="AdditionalRequirements">[d:i][d:sup]1[/d:sup]FOOTNOTES: If any individual assembly is non-compliant, assemblies may show compliance using an area-weighted calculation.  ICF walls may be combined with mass, concrete sandwich panel and log wall types.  ICF walls must meet mandatory insulation requirements in [d:t24refnolink/]120.7(b), but may area-weight to comply with prescriptive requirements in Table 140.3 for new construction. [/d:i]</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                           <xsd:element name="HICFFootnote2" minOccurs="0">
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText"></xsd:documentation>
                                 <xsd:documentation source="AdditionalRequirements">[d:i][d:sup]2[/d:sup]ICF walls are defined as "light" or "heavy" depending on their Heat Capacity.  Heat Capacity is determined in Table 4.3.13 in Joint Appendix 4.  Walls with Heat Capacity of 15 or greater are "heavy" while walls with Heat Capacity from 7 to less than 15 are "light". [/d:i]</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                           <xsd:element name="HICFFootnote3" minOccurs="0">
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText"></xsd:documentation>
                                 <xsd:documentation source="AdditionalRequirements">[d:i][d:sup]3[/d:sup] Wall area minus any fenestration area [/d:i]</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                        </xsd:sequence>
                     </xsd:complexType>
                  </xsd:element>
                  <xsd:element name="HCalc1UF" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">[d:b]Area-Weighted Average U-factor Compliance Calculation for Metal Framed Walls[/d:b]</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">This subsection is required If  Hframed01_UseInAreaWeightedAveUfactor == true</xsd:documentation>
                     </xsd:annotation>
                     <xsd:complexType>
                        <xsd:sequence>
                           <xsd:element name="HCalc1UF01_AreaWeightAvgType" >
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText">Wall Type</xsd:documentation>
                                 <xsd:documentation source="CalculationsAndRules">Calculated field: result = MetalFramed display term Metal Framed</xsd:documentation>
                              </xsd:annotation>
                              <xsd:simpleType>
                                 <xsd:restriction base="comp:AreaWeightAvgType">
                                    <xsd:enumeration value="MetalFramed"/>
                                 </xsd:restriction>
                              </xsd:simpleType>
                           </xsd:element>
                           <xsd:element name="HCalc1UF02_WallTypeTotalArea" type="comp:WallTypeTotalArea" >
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText">Total Area of Wall Type (ft[d:sup]2[/d:sup])</xsd:documentation>
                                 <xsd:documentation source="CalculationsAndRules">Calculated field: result = For all rows where Hframed07_PartitionFramingMaterial == Metal SUM ALL (Hframed13_WallNetArea);</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                           <xsd:element name="HCalc1UF03to04Label" minOccurs="0">
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText">Area-weighted U-factor for Wall Type</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                           <xsd:element name="HCalc1UF03_AreaWeightAvgRequiredUfactor" type="comp:AreaWeightAvgRequiredUfactor" >
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText">Required</xsd:documentation>
                                 <xsd:documentation source="CalculationsAndRules">Calculated field: For all rows where Hframed07_PartitionFramingMaterial == Metal: result = (SUM ALL (Hframed13_WallNetArea x Hframed11 _InsulationUFactorLimit)) / HCalc1UF02_WallTypeTotalArea;</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                           <xsd:element name="HCalc1UF04_AreaWeightAvgUfactor" type="comp:AreaWeightAvgUfactor" >
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText">Designed</xsd:documentation>
                                 <xsd:documentation source="CalculationsAndRules">Calculated field: For all rows where Hframed07_PartitionFramingMaterial == Metal:  result= (SUM ALL (Hframed13_WallNetArea x Ufactor) ) / HCalc1UF02_WallTypeTotalArea.  
 
Ufactor for row is the property that IsPresent:  Hframed12_UfactorJA4Other Or  Hframed12_UfactorJA433_16OC or Hframed12_UfactorJA433_24OC</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                           <xsd:element name="HCalc1UF05_DoesAreaWeightAvgComplyUfactor" type="comp:DoesAreaWeightAvgComplyUfactor" >
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText">Compliance Results Using Area-Weighted Calculation Option</xsd:documentation>
                                 <xsd:documentation source="CalculationsAndRules">Calculated field: If HCalc1UF03_AreaWeightAvgUfactorRequired GTE HCalc1UF04_AreaWeightAvgUfactor result = true display term COMPLIES; 
Else result = false display term DOES NOT COMPLY
End If</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                        </xsd:sequence>
                     </xsd:complexType>
                  </xsd:element>
                  <xsd:element name="HCalc2UF" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">[d:b]Area-Weighted Average U-factor Compliance Calculation for Mass/ Concrete Sandwich Panel/ Log/ ICF Walls[/d:b]</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">This subsection is required If  Hmass01_UseInAreaWeightedAveUfactor Or Hconcrete01_UseInAreaWeightedAveUfactor Or Hlog01_UseInAreaWeightedAveUfactor Or HICF01_UseInAreaWeightedAveUfactor  == true</xsd:documentation>
                     </xsd:annotation>
                     <xsd:complexType>
                        <xsd:sequence>
                           <xsd:element name="TableHCalc2UF" minOccurs="0">
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText">NOT REPORTED IN PDF</xsd:documentation>
                                 <xsd:documentation source="CalculationsAndRules">This table is NOT REPORTED IN PDF, but data is required to aggregate the different mass wall types into light and heavy totals for reporting</xsd:documentation>
                              </xsd:annotation>
                              <xsd:complexType>
                                 <xsd:sequence>
                                    <xsd:element name="Row" minOccurs="1" maxOccurs="unbounded">
                                       <xsd:complexType>
                                          <xsd:sequence>
                                             <xsd:element name="HCalc2UF01_AreaWeightAvgType" >
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">Wall Type</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">Calculated field: 
If  Hmass01_UseInAreaWeightedAveUfactor == true 
If any row has  Hmass10a_HeatCapacityRange == LT7 Or GTE7LT15 add row with result = MassLight; 
And  If any row has Hmass10a_HeatCapacityRange == GTE15 add row with result = MassHeavy;  
End If
End If
 
 If Hlog01_UseInAreaWeightedAveUfactor == true add row  with result = Log; 
End If
 
If HICF01_UseInAreaWeightedAveUfactor == true 
If any row has HICF09a_HeatCapacityRange == LT7 Or GTE7LT15 add row with result = MassLightICF; 
End If
If any row has  HICF09a_HeatCapacityRange == GTE15 add row with result = MassHeavyICF;  
End If
End If
 
If Hconcrete01_UseInAreaWeightedAveUfactor == true add row with result = ConcreteSandwich;
End If</xsd:documentation>
                                                </xsd:annotation>
                                                <xsd:simpleType>
                                                   <xsd:restriction base="comp:AreaWeightAvgType">
                                                      <xsd:enumeration value="ConcreteSandwich"/>
                                                      <xsd:enumeration value="Log"/>
                                                      <xsd:enumeration value="MassHeavy"/>
                                                      <xsd:enumeration value="MassHeavyICF"/>
                                                      <xsd:enumeration value="MassLight"/>
                                                      <xsd:enumeration value="MassLightICF"/>
                                                   </xsd:restriction>
                                                </xsd:simpleType>
                                             </xsd:element>
                                             <xsd:element name="HCalc2UF01a_WallMass" type="env:WallMass" >
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">Light or Heavy Mass</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">Calculated field:  
Switch (HCalc2UF01_AreaWeightAvgType) 
case MassLight: result = Light;  
case MassHeavy: result = Heavy; 
case Log: result = Light; 
case MassLightICF: result = Light; 
case MassHeavyICF: result = Heavy; 
case ConcreteSandwich: result = Heavy;
End Switch</xsd:documentation>
                                                </xsd:annotation>
                                             </xsd:element>
                                             <xsd:element name="HCalc2UF02_WallTypeTotalArea" type="comp:WallTypeTotalArea" >
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">Total Area of Wall Type (ft[d:sup]2[/d:sup])</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">Calculated field: 
Switch  (HCalc2UF01_AreaWeightAvgType) 
case MassLight: for rows with Hmass06_ MasonryGrout != NoGrout  result =  SUM  (Hmass12_WallNetArea ); 
case MassHeavy: for rows with Hmass06_ MasonryGrout == NoGrout  result =  SUM  (Hmass12_WallNetArea );  
case Log: result = SUM  (Hlog10_WallNetArea); 
case MassLightICF for rows with HICF09a_HeatCapacityRange == LT7 Or GTE7LT15 result =  SUM (HICF11_WallNetArea); 
case MassHeavyICF for rows with HICF09a_HeatCapacityRange == GTE15 result =  SUM (HICF11_WallNetArea); 
case ConcreteSandwich: SUM (Hconcrete12_WallNetArea);
End Switch</xsd:documentation>
                                                </xsd:annotation>
                                             </xsd:element>
                                             <xsd:choice maxOccurs="1">
                                                <xsd:element name="HCalc2UF03_UfactorMandatory" type="comp:UfactorMandatory" >
                                                   <xsd:annotation>
                                                      <xsd:documentation source="FieldText">Mandatory U-factor Required</xsd:documentation>
                                                      <xsd:documentation source="CalculationsAndRules">Calculated field: 
If HCalc2UF01_AreaWeightAvgType == MassLight Or MassHeavy  
If Hmass05_MasonryType == CMULightWeight, CMUMediumWeight Or CMUNormalWeight Then 
If Hmass06_ MasonryGrout != NoGrout And Hmass07_MassThickness == 6mass,  8mass, 10mass  Or 12mass  result = 0.440; 
Else If Hmass06_ MasonryGrout == NoGrout And Hmass07_MassThickness == 6mass, 7mass, 8mass, 9mass, 10mass, 11mass, Or 12mass  result = 0.440;
End if
End if
End if</xsd:documentation>
                                                   </xsd:annotation>
                                                </xsd:element>
                                                <xsd:element name="HCalc2UF03_NotApplicableMessage" >
                                                   <xsd:annotation>
                                                      <xsd:documentation source="FieldText">Mandatory U-factor Required</xsd:documentation>
                                                      <xsd:documentation source="CalculationsAndRules">Calculated field: 
If HCalc2UF01_AreaWeightAvgType == MassLight  Or MassHeavy Then  
If Hmass05_MasonryType != CMULightWeight, CMUMediumWeight Or CMUNormalWeight result = NotApplicable display term N/A stored in NotApplicableMessage; 
Else If Hmass06_ MasonryGrout == NoGrout And Hmass07_MassThickness == 3mass, 4mass or 5mass result = NotApplicable display term N/A stored in NotApplicableMessage; 
Else  If HCalc2UF01_AreaWeightAvgType == Log, MassLightICF, MassHeavyICF Or ConcreteSandwich  result = NotApplicable display term N/A stored in NotApplicableMessage;
End If
End If</xsd:documentation>
                                                   </xsd:annotation>
                                                   <xsd:simpleType>
                                                      <xsd:restriction base="comp:NotApplicableMessage">
                                                         <xsd:enumeration value="NotApplicable"/>
                                                      </xsd:restriction>
                                                   </xsd:simpleType>
                                                </xsd:element>
                                             </xsd:choice>
                                             <xsd:element name="HCalc2UF04_AreaWeightAvgRequiredUfactor" type="comp:AreaWeightAvgRequiredUfactor" >
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">Required</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">Calculated field: 
Switch (HCalc2UF01_AreaWeightAveType) 
case MassLight:  for rows with Hmass06_ MasonryGrout != NoGrout  result = (SUM (Hmass12_WallNetArea x Hmass10_UFactorLimit)) / HCalc2UF02_WallTypeTotalArea;  
 
case MassHeavy: for rows with Hmass06_ MasonryGrout == NoGrout  result =  (SUM (Hmass12_WallNetArea x Hmass10_UFactorLimit)) / HCalc2UF02_WallTypeTotalArea;  
 
case Log: result = (SUM (Hlog10_WallNetArea x Hlog08_UFactorLimit)) / HCalc2UF02_WallTypeTotalArea;  
 
case MassLightICF: for rows with HICF09a_HeatCapacityRange == LT7 Or GTE7LT15   result = (SUM (HICF11_WallNetArea x HICF09_UFactorLimit))/ HCalc2UF02_WallTypeTotalArea; 
 
case MassHeavyICF: for rows with HICF09a_HeatCapacityRange == GTE15   result = (SUM (HICF11_WallNetArea x HICF09_UFactorLimit)) / HCalc2UF02_WallTypeTotalArea; 
 
case ConcreteSandwich: (SUM (Hconcrete12_WallNetArea x Hconcrete10_UFactorLimit)) / HCalc2UF02_WallTypeTotalArea;
 
End Switch</xsd:documentation>
                                                </xsd:annotation>
                                             </xsd:element>
                                             <xsd:element name="HCalc2UF05_AreaWeightAvgUfactor" type="comp:AreaWeightAvgUfactor" >
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">Designed</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">Calculated field: 
Switch (HCalc2UF01_AreaWeightAveType) 
case MassLight:  for rows with Hmass06_ MasonryGrout != NoGrout  result = (SUM (Hmass12_WallNetArea x UFactor)) / HCalc2UF02_WallTypeTotalArea where Ufactor is the property that IsPresent for the row: Hmass11_UfactorJA4314Wood, Hmass11_UfactorJA4314Metal, Hmass11_UfactorJA4314Other Or Hmass11_UfactorJA4Other;   
 
case MassHeavy: for rows with Hmass06_ MasonryGrout == NoGrout  result = (SUM (Hmass12_WallNetArea x UFactor)) / HCalc2UF02_WallTypeTotalArea where Ufactor is the property that IsPresent for the row: Hmass11_UfactorJA4314Wood, Hmass11_UfactorJA4314Metal, Hmass11_UfactorJA4314Other Or Hmass11_UfactorJA4Other;  
 
case Log: result = (SUM (Hlog10_WallNetArea x Hlog08_UFactor)) / HCalc2UF02_WallTypeTotalArea where Ufactor for row is the property that IsPresent:  Hlog09_UfactorJA4314Wood,  Hlog09_UfactorJA4314Metal,  Hlog09_UfactorJA4314Other Or  Hlog09_UfactorJA4Other;  
 
case MassLightICF: for rows with HICF09a_HeatCapacityRange == LT7 Or GTE7LT15   result = (SUM (HICF11_WallNetArea x HICF09_UFactor))/ HCalc2UF02_WallTypeTotalArea where Ufactor for row is the property that IsPresent: HICF10_UfactorJA4313EPS, HICF10_UfactorJA4313Other Or HICF10_UfactorJA4Other;  
 
case MassHeavyICF: for rows with HICF09a_HeatCapacityRange == GTE15   result = (SUM (HICF11_WallNetArea x HICF09_UFactor)) / HCalc2UF02_WallTypeTotalArea where Ufactor for row is the property that IsPresent: HICF10_UfactorJA4313EPS, HICF10_UfactorJA4313Other Or HICF10_UfactorJA4Other;  
 
case ConcreteSandwich: (SUM (Hconcrete12_WallNetArea x Hconcrete10_UFactor)) / HCalc2UF02_WallTypeTotalArea where Ufactor for row is the property that IsPresent:  Hconcrete11_UfactorJA4314Wood,  Hconcrete11_UfactorJA4314Metal,  Hconcrete11_UfactorJA4314Other Or  Hconcrete11_UfactorJA4Other;
 
End Switch</xsd:documentation>
                                                </xsd:annotation>
                                             </xsd:element>
                                          </xsd:sequence>
                                       </xsd:complexType>
                                    </xsd:element>
                                 </xsd:sequence>
                              </xsd:complexType>
                           </xsd:element>
                           <xsd:element name="HCalc2UF_TotalsLight" minOccurs="0">
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText"></xsd:documentation>
                                 <xsd:documentation source="CalculationsAndRules">This subsection is required if any rows have  HCalc2UF01a_WallMass == Light  
If required this subsection is displayed in the pdf as the first row.  
.</xsd:documentation>
                              </xsd:annotation>
                              <xsd:complexType>
                                 <xsd:sequence>
                                    <xsd:element name="HCalc2UF01Light_WallMass" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Wall Type</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">result = Light display term Light Mass</xsd:documentation>
                                       </xsd:annotation>
                                       <xsd:simpleType>
                                          <xsd:restriction base="env:WallMass">
                                             <xsd:enumeration value="Light"/>
                                          </xsd:restriction>
                                       </xsd:simpleType>
                                    </xsd:element>
                                    <xsd:element name="HCalc2UF02Light_AreaWeightAvgTotalArea" type="comp:AreaWeightAvgTotalArea" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Total Area of Wall Type (ft[d:sup]2[/d:sup])</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">Calculated field:  result = Sum (HCalc2UF02_WallTypeTotalArea) for all rows with HCalc2UF01a_WallMass == Light ;</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="HCalc2UF03Light_UfactorMandatory" type="comp:UfactorMandatory" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Mandatory U-factor Required</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">For any row with HCalc2UF01a_WallMass == Light;
If HCalc2UF03_UfactorMandatory is present   
result = HCalc2UF03_UfactorMandatory  from that row
End If
As all cases have the same value,  0.440 only need the first instance for result and there in no need to average. If the values in HCalc2UF03_UfactorMandatory were different would need the average of all of them as the result.</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="HCalc2UF03Light_NotApplicableMessage" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Mandatory U-factor Required</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">For all row with HCalc2UF01a_WallMass == Light 
If all rows have HCalc2UF03_NotApplicableMessage present (NO rows have HCalc2UF03_UfactorMandatory  present )  
result = HCalc2UF03_NotApplicableMessage  
End If</xsd:documentation>
                                          </xsd:annotation>
                                          <xsd:simpleType>
                                             <xsd:restriction base="comp:NotApplicableMessage">
                                                <xsd:enumeration value="NotApplicable"/>
                                             </xsd:restriction>
                                          </xsd:simpleType>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:element name="HCalc2UF04to05Label" minOccurs="0">
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Area-weighted U-factor for Wall Type</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="HCalc2UF04Light_AreaWeightAvgUfactorRequiredAll" type="comp:AreaWeightAvgUfactorRequiredAll" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Required</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">Calculated field: result = For all rows with HCalc2UF01a_WallMass == Light  
(Sum all  (HCalc2UF02_WallTypeTotalArea x HCalc2UF04_AreaWeightAvgRequiredUfactor)) / HCalc2UF02Light _AreaWeightAvgTotalArea</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="HCalc2UF05Light_AreaWeightAvgUfactorAll" type="comp:AreaWeightAvgUfactorAll" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Designed</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">Calculated field: result = For all rows with HCalc2UF01a_WallMass == Light 
(Sum  (HCalc2UF02_WallTypeTotalArea x HCalc2UF05_AreaWeightAvgUfactor)) / HCalc2UF02Light
 _AreaWeightAvgTotalArea</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="HCalc2UF06Light_DoesAreaWeightAvgComplyUfactor" type="comp:DoesAreaWeightAvgComplyUfactor" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Compliance Results Using Area-Weighted Calculation Option</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">Calculated field: 
If HCalc2UF03Light_NotApplicableMessage == NotApplicable Or 
If HCalc2UF03Light_UfactorMandatory GTE HCalc2UF05Light_AreaWeightAvgUfactorAll 
And  
If HCalc2UF04Light _ AreaWeightAvgUfactorRequiredAll GTE HCalc2UF05Light_AreaWeightAvgUfactorAll  
 
result = true display term COMPLIES;
 
Else result = false display term DOES NOT COMPLY
End If</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                 </xsd:sequence>
                              </xsd:complexType>
                           </xsd:element>
                           <xsd:element name="HCalc2UF_TotalsHeavy" minOccurs="0">
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText"></xsd:documentation>
                                 <xsd:documentation source="CalculationsAndRules">This subsection is required if any rows have  HCalc2UF01a_WallMass == Heavy  
If required this subsection is displayed in the pdf as the second row.</xsd:documentation>
                              </xsd:annotation>
                              <xsd:complexType>
                                 <xsd:sequence>
                                    <xsd:element name="HCalc2UF01Heavy_WallMass" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Wall Type</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">result = Heavy display term Heavy Mass</xsd:documentation>
                                       </xsd:annotation>
                                       <xsd:simpleType>
                                          <xsd:restriction base="env:WallMass">
                                             <xsd:enumeration value="Heavy"/>
                                          </xsd:restriction>
                                       </xsd:simpleType>
                                    </xsd:element>
                                    <xsd:element name="HCalc2UF02Heavy_AreaWeightAvgTotalArea" type="comp:AreaWeightAvgTotalArea" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Total Area of Wall Type (ft[d:sup]2[/d:sup])</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">Calculated field: result = Sum all rows with HCalc2UF01a_WallMass == Heavy (HCalc2UF02_WallTypeTotalArea);</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="HCalc2UF03Heavy_UfactorMandatory" type="comp:UfactorMandatory" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Mandatory U-factor Required</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">For any row with HCalc2UF01a_WallMass == Heavy
If HCalc2UF03_UfactorMandatory is present   
result = HCalc2UF03_UfactorMandatory  from that row
End If
As all cases have the same value,  0.440 only need the first instance for result and there in no need to average. If the values in HCalc2UF03_UfactorMandatory were different would need the average of all of them as the result.</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="HCalc2UF03Heavy_NotApplicableMessage" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Mandatory U-factor Required</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">For all row with HCalc2UF01a_WallMass == Heavy 
If all rows have HCalc2UF03_NotApplicableMessage present (NO rows have HCalc2UF03_UfactorMandatory  present )  
result = HCalc2UF03_NotApplicableMessage  
End If</xsd:documentation>
                                          </xsd:annotation>
                                          <xsd:simpleType>
                                             <xsd:restriction base="comp:NotApplicableMessage">
                                                <xsd:enumeration value="NotApplicable"/>
                                             </xsd:restriction>
                                          </xsd:simpleType>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:element name="HCalc2UF04to05Label" minOccurs="0">
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Area-weighted U-factor for Wall Type</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="HCalc2UF04Heavy_AreaWeightAvgUfactorRequiredAll" type="comp:AreaWeightAvgUfactorRequiredAll" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Required</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">Calculated field: result = For all rows with HCalc2UF01a_WallMass == Heavy  
(Sum all  (HCalc2UF02_WallTypeTotalArea x HCalc2UF04_AreaWeightAvgRequiredUfactor)) / HCalc2UF02Heavy_AreaWeightAvgTotalArea</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="HCalc2UF05Heavy_AreaWeightAvgUfactorAll" type="comp:AreaWeightAvgUfactorAll" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Designed</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">Calculated field: result = For all rows with HCalc2UF01a_WallMass == Heavy
(Sum  (HCalc2UF02_WallTypeTotalArea x HCalc2UF05_AreaWeightAvgUfactor)) / HCalc2UF02Heavy
 _AreaWeightAvgTotalArea</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="HCalc2UF06Heavy_DoesAreaWeightAvgComplyUfactor" type="comp:DoesAreaWeightAvgComplyUfactor" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Compliance Results Using Area-Weighted Calculation Option</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">Calculated field: 
If HCalc2UF03Heavy_NotApplicableMessage == NotApplicable Or 
If HCalc2UF03Heavy_UfactorMandatory GTE HCalc2UF05Heavy_AreaWeightAvgUfactorAll 
And  
If HCalc2UF04Heavy _ AreaWeightAvgUfactorRequiredAll GTE HCalc2UF05Heavy_AreaWeightAvgUfactorAll  
 
result = true display term COMPLIES;
 
Else result = false display term DOES NOT COMPLY
End If</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                 </xsd:sequence>
                              </xsd:complexType>
                           </xsd:element>
                        </xsd:sequence>
                     </xsd:complexType>
                  </xsd:element>
                  <xsd:element name="HCalc3UF" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">[d:b]Area-Weighted Average U-factor Compliance Calculation for Wood Framed/ SIPs/ Spandrel/ Curtain/ Metal Panel/ Straw Bale Wall Types[/d:b]</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">This subsection is required If Hframed02_UseInAreaWeightedAveUfactor,  HSIPS01_UseInAreaWeightedAveUfactor, Hspandrel01_UseInAreaWeightedAveUfactor, Hmetalp01_UseInAreaWeightedAveUfactor Or Hstraw01_UseInAreaWeightedAveUfactor  == true</xsd:documentation>
                     </xsd:annotation>
                     <xsd:complexType>
                        <xsd:sequence>
                           <xsd:element name="TableHCalc3UF" >
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText"></xsd:documentation>
                              </xsd:annotation>
                              <xsd:complexType>
                                 <xsd:sequence>
                                    <xsd:element name="Row" minOccurs="1" maxOccurs="unbounded">
                                       <xsd:complexType>
                                          <xsd:sequence>
                                             <xsd:element name="HCalc3UF01_AreaWeightAvgType" >
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">Wall Type</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">Calculated field: If any of the following conditions are true, a row is added and the result loaded into this field: 
 If Hframed02_UseInAreaWeightedAveUfactor == true  result Framed;  
End if
If  HSIPS01_UseInAreaWeightedAveUfactor == true, result = SIPS; 
End If
If Hspandrel01_UseInAreaWeightedAveUfactor ==  true result = Spandrel;  
End if
If Hmetalp01_UseInAreaWeightedAveUfactor  == true; result = MetalPanel; 
End If
If Hstraw01_UseInAreaWeightedAveUfactor  == true result = StrawBale
End If</xsd:documentation>
                                                </xsd:annotation>
                                                <xsd:simpleType>
                                                   <xsd:restriction base="comp:AreaWeightAvgType">
                                                      <xsd:enumeration value="Framed"/>
                                                      <xsd:enumeration value="MetalPanel"/>
                                                      <xsd:enumeration value="SIPS"/>
                                                      <xsd:enumeration value="Spandrel"/>
                                                      <xsd:enumeration value="StrawBale"/>
                                                   </xsd:restriction>
                                                </xsd:simpleType>
                                             </xsd:element>
                                             <xsd:element name="HCalc3UF02_WallTypeTotalArea" type="comp:WallTypeTotalArea" >
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">Total Area of Wall Type (ft[d:sup]2[/d:sup])</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">Calculated field: For current row, 
Switch (HCalc3UF01_AreaWeightAvgType) 
case Framed: result = SUM ALL (Hframed13_WallNetArea);  
case SIPS:  result = SUM ALL (HSIPS12_WallNetArea); 
case Spandrel:  result = SUM ALL (Hspandrel11_WallNetArea); 
case MetalPanel:  result = SUM ALL (Hmetalp09_WallNetArea);  
case StrawBale:  result = SUM ALL (Hstraw10_WallNetArea).
End Switch</xsd:documentation>
                                                </xsd:annotation>
                                             </xsd:element>
                                             <xsd:element name="HCalc3UF03to04Label" minOccurs="0">
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">Area-weighted U-factor for Wall Type</xsd:documentation>
                                                </xsd:annotation>
                                             </xsd:element>
                                             <xsd:element name="HCalc3UF03_AreaWeightAvgRequiredUfactor" type="comp:AreaWeightAvgRequiredUfactor" >
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">Required</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">Calculated field: 
Switch (HCalc3UF01_AreaWeightAvgType) 
case Framed: result = (SUM ALL (Hframed13_WallNetArea x Hframed11 _InsulationUFactorLimit)) / HCalc3UF02_WallTypeTotalArea; 
 
case SIPS: (SUM ALL (HSIPS12_WallNetArea x HSIPS10_InsulationUFactorLimit)) / HCalc3UF02_WallTypeTotalArea;  
 
case Spandrel: (SUM ALL (Hspandrel11_WallNetArea x Hspandrel09_InsulationUFactorLimit)) / HCalc3UF02_WallTypeTotalArea; 
 
case MetalPanel: (SUM ALL (Hmetalp09_WallNetArea x Hmetalp07_UFactorLimit)) / HCalc3UF02_WallTypeTotalArea; 
 
case Strawbale: (SUM ALL (Hstraw10_WallNetArea x Hstraw08_InsulationUFactorLimit)) / HCalc3UF02_WallTypeTotalArea;
 
End Switch</xsd:documentation>
                                                </xsd:annotation>
                                             </xsd:element>
                                             <xsd:choice maxOccurs="1">
                                                <xsd:element name="HCalc3UF04Framed_AreaWeightAvgUfactor" type="comp:AreaWeightAvgUfactor" >
                                                   <xsd:annotation>
                                                      <xsd:documentation source="FieldText">Designed</xsd:documentation>
                                                      <xsd:documentation source="CalculationsAndRules">Calculated field: 
If HCalc3UF01_AreaWeightAvgType == Framed result= (SUM ALL (Hframed13_WallNetArea x Ufactor) ) / HCalc3UF02_WallTypeTotalArea.  
End If
 
Ufactor for row is the property that IsPresent: Hframed12_UfactorJA4Other Or  Hframed12_UfactorJA431_16OC Or  Hframed12_UfactorJA431_24OC or Hframed12_UfactorJA431a_16OC Or Hframed12_UfactorJA431a_24OC</xsd:documentation>
                                                   </xsd:annotation>
                                                </xsd:element>
                                                <xsd:element name="HCalc3UF04SIPS_AreaWeightAvgUfactor" type="comp:AreaWeightAvgUfactor" >
                                                   <xsd:annotation>
                                                      <xsd:documentation source="FieldText">Designed</xsd:documentation>
                                                      <xsd:documentation source="CalculationsAndRules">Calculated field: 
If HCalc3UF01_AreaWeightAvgType == SIPS result= (SUM ALL (HSIPS12_WallNetArea x Ufactor) ) / HCalc3UF02_WallTypeTotalArea.  
End If
 
Ufactor for row is the property that IsPresent: HSIPS11_UfactorJA4Other Or HSIPS11_UfactorJA432OSB Or HSIPS11_UfactorJA432Single or HSIPS11_UfactorJA432Double Or HSIPS11_UfactorJA432Ijoist</xsd:documentation>
                                                   </xsd:annotation>
                                                </xsd:element>
                                                <xsd:element name="HCalc3UF04Spandrel_AreaWeightAvgUfactor" type="comp:AreaWeightAvgUfactor" >
                                                   <xsd:annotation>
                                                      <xsd:documentation source="FieldText">Designed</xsd:documentation>
                                                      <xsd:documentation source="CalculationsAndRules">Calculated field: 
If HCalc3UF01_AreaWeightAvgType == Spandrel result= (SUM ALL (Hspandrel11_WallNetArea x Ufactor) ) / HCalc3UF02_WallTypeTotalArea.
End If  
 
Ufactor for row is the property that IsPresent: Hspandrel10_UfactorJA4Other Or Hspandrel10_UfactorJA438Spandrel Or  Hspandrel10_UfactorJA438Curtain</xsd:documentation>
                                                   </xsd:annotation>
                                                </xsd:element>
                                                <xsd:element name="HCalc3UF04Straw_AreaWeightAvgUfactor" type="comp:AreaWeightAvgUfactor" >
                                                   <xsd:annotation>
                                                      <xsd:documentation source="FieldText">Designed</xsd:documentation>
                                                      <xsd:documentation source="CalculationsAndRules">Calculated field: 
If HCalc3UF01_AreaWeightAvgType == Strawbale result= (SUM ALL (Hstraw10_WallNetArea x Ufactor) ) / HCalc3UF02_WallTypeTotalArea.  
End If
 
Ufactor for row is the property that IsPresent: Hstraw09_ UfactorJA4Other Or Hstraw09_UfactorJA4312</xsd:documentation>
                                                   </xsd:annotation>
                                                </xsd:element>
                                                <xsd:element name="HCalc3UF04Metalp_AreaWeightAvgUfactor" type="comp:AreaWeightAvgUfactor" >
                                                   <xsd:annotation>
                                                      <xsd:documentation source="FieldText">Designed</xsd:documentation>
                                                      <xsd:documentation source="CalculationsAndRules">Calculated field: 
If HCalc3UF01_AreaWeightAvgType == MetalPanel result= (SUM ALL (Hmetalp09_WallNetArea x Ufactor) ) / HCalc3UF02_WallTypeTotalArea.  
End If
 
Ufactor for row is the property that IsPresent: Hmetalp08_ UfactorJA4Other Or Hmetalp08_UfactorJA4310</xsd:documentation>
                                                   </xsd:annotation>
                                                </xsd:element>
                                             </xsd:choice>
                                          </xsd:sequence>
                                       </xsd:complexType>
                                    </xsd:element>
                                 </xsd:sequence>
                              </xsd:complexType>
                           </xsd:element>
                           <xsd:element name="HCalc3UF02to04TotalsLabel" minOccurs="0">
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText">[d:b]Total for all Wall Types:[/d:b]</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                           <xsd:element name="HCalc3UF02Totals_AreaWeightAvgTotalArea" type="comp:AreaWeightAvgTotalArea" >
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText"></xsd:documentation>
                                 <xsd:documentation source="CalculationsAndRules">Calculated field: result = SUM ALL (HCalc3UF02_WallTypeTotalArea)</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                           <xsd:element name="HCalc3UF03Totals_AreaWeightAvgUfactorRequiredAll" type="comp:AreaWeightAvgUfactorRequiredAll" >
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText"></xsd:documentation>
                                 <xsd:documentation source="CalculationsAndRules">Calculated field: result = (SUM ALL ( HCalc3UF02_WallTypeTotalArea x HCalc3UF03_AreaWeightAvgUfactorRequired)) / HCalc3UF02Totals_AreaWeightAvgTotalArea</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                           <xsd:element name="HCalc3UF04Totals_AreaWeightAvgUfactorAll" type="comp:AreaWeightAvgUfactorAll" >
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText"></xsd:documentation>
                                 <xsd:documentation source="CalculationsAndRules">Calculated field: result = (SUM ALL (HCalc3UF02_WallTypeTotalArea x AreaWeightAvgUfactor)) / HCalc1UF02Totals_AreaWeightAvgTotalArea where AreaWeightAvgUfactor for row is the property that is Not Null: HCalc3UF04Framed_AreaWeightAvgUfactor Or HCalc3UF04SIPS_AreaWeightAvgUfactor Or HCalc3UF04Spandrel_AreaWeightAvgUfactor Or HCalc3UF04Straw_AreaWeightAvgUfactor Or HCalc3UF04Metalp_AreaWeightAvgUfactor</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                           <xsd:element name="HCalc3UF05_DoesAreaWeightAvgComplyUfactor" type="comp:DoesAreaWeightAvgComplyUfactor" >
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText">Compliance Results Using Area-Weighted Calculation Option</xsd:documentation>
                                 <xsd:documentation source="CalculationsAndRules">Calculated field: 
If HCalc3UF03Totals_AreaWeightAvgUfactorRequiredAll GTE HCalc3UF04Totals_AreaWeightAvgUfactorAll result = true display term COMPLIES; 
Else result = false display term DOES NOT COMPLY
End If</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                        </xsd:sequence>
                     </xsd:complexType>
                  </xsd:element>
                  <xsd:element name="HCalc4UF" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">[d:b]Area-Weighted Average U-factor Compliance Calculation for Metal Building Walls[/d:b]</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">This subsection is required If  Hmetalb01_UseInAreaWeightedAveUfactor == true</xsd:documentation>
                     </xsd:annotation>
                     <xsd:complexType>
                        <xsd:sequence>
                           <xsd:element name="HCalc4UF01_AreaWeightAvgType" >
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText">Wall Type</xsd:documentation>
                                 <xsd:documentation source="CalculationsAndRules">Calculated field: result = MetalBuilding display term Metal Building</xsd:documentation>
                              </xsd:annotation>
                              <xsd:simpleType>
                                 <xsd:restriction base="comp:AreaWeightAvgType">
                                    <xsd:enumeration value="MetalBuilding"/>
                                 </xsd:restriction>
                              </xsd:simpleType>
                           </xsd:element>
                           <xsd:element name="HCalc4UF02_WallTypeTotalArea" type="comp:WallTypeTotalArea" >
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText">Total Area of Wall Type (ft[d:sup]2[/d:sup])</xsd:documentation>
                                 <xsd:documentation source="CalculationsAndRules">Calculated field: result = SUM ALL (Hmetalb11_WallNetArea);</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                           <xsd:element name="HCalc4UF03to04Label" minOccurs="0">
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText">Area-weighted U-factor for Wall Type</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                           <xsd:element name="HCalc4UF03_AreaWeightAvgRequiredUfactor" type="comp:AreaWeightAvgRequiredUfactor" >
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText">Required</xsd:documentation>
                                 <xsd:documentation source="CalculationsAndRules">Calculated field: result = (SUM ALL (Hmetalb11_WallNetArea x Hmetalb09_InsulationUFactorLimit)) / HCalc4UF02_WallTypeTotalArea;</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                           <xsd:element name="HCalc4UF04_AreaWeightAvgUfactor" type="comp:AreaWeightAvgUfactor" >
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText">Designed</xsd:documentation>
                                 <xsd:documentation source="CalculationsAndRules">Calculated field:SUM ALL (Hmetalb11_WallNetArea x Ufactor) ) / HCalc4UF02_WallTypeTotalArea.  Ufactor for row is the property that IsPresent: Hmetalb10_UfactorJA4Other Or  Hmetalb10_UfactorJA439</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                           <xsd:element name="HCalc4UF05_DoesAreaWeightAvgComplyUfactor" type="comp:DoesAreaWeightAvgComplyUfactor" >
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText">Compliance Results Using Area-Weighted Calculation Option</xsd:documentation>
                                 <xsd:documentation source="CalculationsAndRules">Calculated field: 
If HCalc4UF03_AreaWeightAvgRequiredUfactor GTE HCalc4UF04_AreaWeightAvgUfactor result = true display term COMPLIES; 
Else result = false display term DOES NOT COMPLY
End If</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                        </xsd:sequence>
                     </xsd:complexType>
                  </xsd:element>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
         <xsd:element name="Section_I" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation source="FieldText">[d:b]I. FLOOR ASSEMBLY SCHEDULE[/d:b]</xsd:documentation>
               <xsd:documentation source="CalculationsAndRules">This section is required If B02NewFloor_ComponentApplies, B02AddFloor_ComponentApplies Or B02AltFloor_ComponentApplies == true
 
Mandatory Measure Triggers:
If this section is required 
MM120_7c_MeasureApplies = true and is displayed</xsd:documentation>
               <xsd:documentation source="MinOccurs">If this section doesn't apply, do not display anything for this section</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="I_BeginNote1" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">[d:i]This table demonstrates compliance with prescriptive floor assembly requirements in [d:t24refnolink/]140.3(a)4/ [d:t24refnolink/]170.2(a)5  for new construction, [d:t24refnolink/]141.0(a)/  [d:t24refnolink/]180.1 for additions, or mandatory floor assembly requirements in [d:t24refnolink/]141.0(b)1C/  [d:t24refnolink/]180.2 for alterations. [/d:i]</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="I01Label" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Indicate floor  types included in the project: [d:sup]1[/d:sup]</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="I01Framed_FloorTypeApplies" type="comp:FloorTypeApplies" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Framed</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">User input: If checked result = true; Else result = false
 
Mandatory Measure Block Triggers:
If result == true  
MM120_7c2_MeasureApplies = true and is displayed
End If</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="I01SIPS_FloorTypeApplies" type="comp:FloorTypeApplies" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">SIPS (new only)</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">User input: If checked result = true; Else result = false
 
Mandatory Measure Block Triggers:
If result == true  
MM120_7c2_MeasureApplies = true and is displayed
End If</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="I01Mass_FloorTypeApplies" type="comp:FloorTypeApplies" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Raised Mass</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">User input: If checked result = true; Else result = false
 
Mandatory Measure Block Triggers:
If result == true 
MM120_7c1_MeasureApplies = true and is displayed
End If</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="I01Heated_FloorTypeApplies" type="comp:FloorTypeApplies" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Heated Slab-on-grade (new only)</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">User input: If checked result = true; Else result = false
 
Mandatory Measure Block Triggers:
If result == true  in MM Block 
MM110_8g_MeasureApplies  = true and is displayed And 
MM120_7c3_MeasureApplies = true and is displayed
End If</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="IFootnote1" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">[d:i][d:sup]1[/d:sup] FOOTNOTES: Floor types indicated above as "(new only)" do not have Title 24, Part 6 requirements for alterations.  New construction and additions do have requirements and should be clicked above and compliance demonstrated within this table.  [/d:i]</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="Iframed" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">[d:b]Framed Floors[/d:b]</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">This subsection is required If I01Framed_FloorTypeApplies == true</xsd:documentation>
                     </xsd:annotation>
                     <xsd:complexType>
                        <xsd:sequence>
                           <xsd:element name="Iframed01_UseInAreaWeightedAveUfactor" type="comp:UseInAreaWeightedAveUfactor" >
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText">Include Framed Floors in Area-Weighted Average U-factor Calculation [d:sup]1[/d:sup]</xsd:documentation>
                                 <xsd:documentation source="CalculationsAndRules">User input: If checked result = true; Else result = false.  If result == true   D03_ExceptionalConditionMessageCCENVE += FloorAreaWeightedAveInI</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                           <xsd:element name="TableIframed" >
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText"></xsd:documentation>
                              </xsd:annotation>
                              <xsd:complexType>
                                 <xsd:sequence>
                                    <xsd:element name="Row" minOccurs="1" maxOccurs="unbounded">
                                       <xsd:complexType>
                                          <xsd:sequence>
                                             <xsd:element name="Iframed02_FloorAssemblyName" type="env:FloorAssemblyName" >
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">Tag/Plan Detail ID</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">User input</xsd:documentation>
                                                </xsd:annotation>
                                             </xsd:element>
                                             <xsd:element name="Iframed03a_TopLevelOccupancy" >
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">Occupancy &amp; Status</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">User selects from list: Nonresidential display term Nonresidential/ Relocatable 1 CZ: New, HotelMotel display term Hotel/ Motel: New, RelocableSchoolBuildingAllZones display term Relocatable all CZ: New,  AlteredOccupancy display term Any occupancy: Altered,  ResidentialMF display term Multifamily: New</xsd:documentation>
                                                   <xsd:appinfo source="displayterm">
                                                      <dtyp:displayterm value="HotelMotel">Hotel/ Motel: New</dtyp:displayterm>
                                                      <dtyp:displayterm value="Nonresidential">Nonresidential/ Relocatable 1 CZ: New</dtyp:displayterm>
                                                      <dtyp:displayterm value="RelocableSchoolBuildingAllZones">Relocatable all CZ: New</dtyp:displayterm>
                                                      <dtyp:displayterm value="AlteredOccupancy">Any occupancy: Altered</dtyp:displayterm>
                                                      <dtyp:displayterm value="ResidentialMF">Multifamily: New</dtyp:displayterm>
                                                   </xsd:appinfo>
                                                   
                                                </xsd:annotation>
                                                <xsd:simpleType>
                                                   <xsd:restriction base="com:TopLevelOccupancy">
                                                      <xsd:enumeration value="AlteredOccupancy"/>
                                                      <xsd:enumeration value="HotelMotel"/>
                                                      <xsd:enumeration value="Nonresidential"/>
                                                      <xsd:enumeration value="RelocableSchoolBuildingAllZones"/>
                                                      <xsd:enumeration value="ResidentialMF"/>
                                                   </xsd:restriction>
                                                </xsd:simpleType>
                                             </xsd:element>
                                             <xsd:element name="Iframed03b_Status" >
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">NOT REPORTED IN PDF</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">Calculated field: If Iframed03a_TopLevelOccupancy == AlteredOccupancy result = ExistingAlteration; 
Else result = New
End If</xsd:documentation>
                                                </xsd:annotation>
                                                <xsd:simpleType>
                                                   <xsd:restriction base="com:Status">
                                                      <xsd:enumeration value="ExistingAlteration"/>
                                                      <xsd:enumeration value="New"/>
                                                   </xsd:restriction>
                                                </xsd:simpleType>
                                             </xsd:element>
                                             <xsd:choice maxOccurs="1">
                                                <xsd:element name="Iframed04_UfactorSpecSource" type="comp:UfactorSpecSource" >
                                                   <xsd:annotation>
                                                      <xsd:documentation source="FieldText">How Design U-factor was determined</xsd:documentation>
                                                      <xsd:documentation source="CalculationsAndRules">If Iframed09_ConstructionSpecificationMethod == RValue result = BlankGrayedOut stored in NotApplicableMessage; 
Else User selects from list: JA4Tables display term JA4 Tables, JA4Other display term Other per JA4.1.2.1, ApprovedSoftware display term Approved Software. 
End If 
 
If result == JA4Other  Or ApprovedSoftware  D03_ExceptionalConditionMessageCCENVE += AdditionalDocJA4OtherSoftware</xsd:documentation>
                                                   </xsd:annotation>
                                                </xsd:element>
                                                <xsd:element name="Iframed04_NotApplicableMessage" >
                                                   <xsd:annotation>
                                                      <xsd:documentation source="FieldText">How Design U-factor was determined</xsd:documentation>
                                                      <xsd:documentation source="CalculationsAndRules">If  Iframed09_ConstructionSpecificationMethod == RValue result = BlankGrayedOut stored in NotApplicableMessage;
End If</xsd:documentation>
                                                   </xsd:annotation>
                                                   <xsd:simpleType>
                                                      <xsd:restriction base="comp:NotApplicableMessage">
                                                         <xsd:enumeration value="BlankGrayedOut"/>
                                                      </xsd:restriction>
                                                   </xsd:simpleType>
                                                </xsd:element>
                                             </xsd:choice>
                                             <xsd:choice maxOccurs="1">
                                                <xsd:element name="Iframed05_HasCrawlspace" type="env:HasCrawlspace" >
                                                   <xsd:annotation>
