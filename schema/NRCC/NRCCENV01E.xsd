If result == true continue evaluation: 
 
If Hframed01_UseInAreaWeightedAveUfactor == false Or Hframed02_UseInAreaWeightedAveUfactor == false 
If Hframed10_ConstructionSpecificationMethod == RValue 
If Hframed08_CavityInsulationNR Or Hframed08_CavityInsulationOther GTE Hframed11_ InsulationRValueLimit  result = true; Else result = false;  
End If 
Else If Hframed10_ConstructionSpecificationMethod == UFactor
If Hframed12_UfactorJA4Other IsPresent And Hframed12_UfactorJA4Other LTE Hframed11_InsulationUFactorLimit
Or field beginning with Hframed12_UfactorJA4 that IsPresent is LTE Hframed11_InsulationUFactorLimit 
result = true; Else result = false;  
End If 
End If
Else If Hframed01_UseInAreaWeightedAveUfactor == true  And HCalc1UF05_DoesAreaWeightAvgComplyUfactor == true 
Or Hframed02_UseInAreaWeightedAveUfactor == true  If HCalc3UF05_DoesAreaWeightAvgComplyUfactor == true
result = true; Else result = false;   
End If
End If 
If result == true continue evaluation:  
 
If H01Mass_WallTypeApplies == true, 
If fields beginning with Hmass02, Hmass03, Hmass04, Hmass05, Hmass06, Hmass07, Hmass08, Hmass09, Hmass10, Hmass11, Hmass12 == BlankGrayedOut, or have another value result = true, Else result = false.   
End If
If result == true continue evaluation: 
 
If Hmass01_UseInAreaWeightedAveUfactor == false And field beginning with Hmass11_UfactorJA4314 Or Hmass11_UfactorJA4Other that IsPresent is  LTE Hmass10_UFactorLimit  
 
result = true   Else result = false;  
 
Else If Hmass01_UseInAreaWeightedAveUfactor == true And HCalc2UF06_DoesAreaWeightAvgComplyUfactor == true result = true; Else result = false;  
End If
End If
 If result == true continue evaluation:  
 
 
If H01Concrete_WallTypeApplies == true, 
If fields beginning with Hconcrete02, Hconcrete03, Hconcrete04, Hconcrete05, Hconcrete06, Hconcrete07, Hconcrete08, Hconcrete09, Hconcrete10, Hconcrete11, Hconcrete12 == BlankGrayedOut or have another value result = true, Else result = false.  
End If
 If result == true continue evaluation: 
 
If Hconcrete01_UseInAreaWeightedAveUfactor == false And field beginning with Hconcrete11_UfactorJA4314 or Hconcrete11_UfactorJA4Other that IsPresent is LTE Hconcrete10_UFactorLimit result = true; Else result = false;  
Else If Hconcrete01_UseInAreaWeightedAveUfactor == true  And HCalc2UF06_DoesAreaWeightAvgComplyUfactor == true result = true; Else result = false;  
End If
End If
If result == true continue evaluation:  
 
If H01SIPS_WallTypeApplies == true, 
If fields beginning with HSIPS02, HSIPS03a, HSIPS03b, HSIPS04, HSIPS05, HSIPS06, HSIPS07, HSIPS08, HSIPS09, HSIPS10, HSIPS11, HSIPS12 == BlankGrayedOut or have another value result = true, Else result = false.   
End If
If result == true continue evaluation: 
 
If HSIPS01_UseInAreaWeightedAveUfactor == false 
If HSIPS09_ConstructionSpecificationMethod == RValue Then 
If HSIPS07_CoreInsulationSIPS Or HSIPS07_CoreInsulationOtherSIPS GTE HSIPS10_InsulationRValueLimit result = true; Else result = false;  
End If
 
Else If HSIPS09_ConstructionSpecificationMethod == UFactor
If HSIPS11_UfactorJA4Other IsPresent and HSIPS11_UfactorJA4Other LTE HSIPS10_InsulationUFactorLimit 
Or field beginning with HSIPS11_UfactorJA4 that IsPresent is LTE HSIPS10_InsulationUFactorLimit 
result = true; Else result = false;  
End If
End If
Else If HSIPS01_UseInAreaWeightedAveUfactor == true  And HCalc3UF05_DoesAreaWeightAvgComplyUfactor == true result = true; Else result = false; 
End If
End If
 If result == true continue evaluation:  
 
If H01Spandrel_WallTypeApplies == true, 
If fields beginning with Hspandrel02, Hspandrel03a, Hspandrel03b, Hspandrel04, Hspandrel05, Hspandrel06, Hspandrel07, Hspandrel08, Hspandrel09, Hspandrel10, Hspandrel11 == BlankGrayedOut or have another value result = true, Else result = false.  
End If
 If result == true continue evaluation: 
 
If Hspandrel01_UseInAreaWeightedAveUfactor == false 
If Hspandrel08_ConstructionSpecificationMethod == RValue 
