End Switch
Else If Hlog06_FramingDepth == 3_5In 
Switch (Hlog07_CavityInsulationNR) 
case R0: Rvalue = 1.4; case R1: Rvalue = 1.5;  case R2: Rvalue = 2.6;  case R3: Rvalue = 3.5; case R4: Rvalue = 4.4;  case R5: Rvalue = 5.2;  case R6: Rvalue = 6.0; case R7: Rvalue = 6.7; case R8: Rvalue = 7.4;  case R9:  Rvalue = 8.1; case R10: Rvalue =8.7; case  R11: Rvalue = 9.3; case R12: Rvalue = 9.8;  case R13: Rvalue = 10.4; case R14: Rvalue = 10.9;  case R15: Rvalue = 11.3; case R16: Rvalue = 11.8;  case R17: Rvalue = 12.2; case R18: Rvalue = 12.6; case R19:  Rvalue = 13.0; case R20: Rvalue = 13.4;  case R21: Rvalue = 13.8;  
