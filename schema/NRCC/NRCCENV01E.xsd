                                             </xsd:element>
                                             <xsd:element name="Kskyperf10aLabel" minOccurs="0">
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">U-factor (max)</xsd:documentation>
                                                </xsd:annotation>
