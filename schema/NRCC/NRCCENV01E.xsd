Else If HSIPS06_PanelThickness == 10_25In 
 If HSIPS07_CoreInsulation == R36 
Switch (HSIPS08_ContinuousInsulationNR) 
case R0: result = 0.035; case R2: result = 0.032; case R4: result = 0.030; case R5: result = 0.029; case R6: result = 0.028; case R8: result = 0.026; 
End Switch
Else If HSIPS07_CoreInsulation == R55 
Switch (HSIPS08_ContinuousInsulationNR) 
case R0: result = 0.028; case R2: result = 0.025; case R4: result = 0.023; case R5: result = 0.023; case R6: result = 0.022; case R8: result = 0.021; 
End Switch
End If
 
Else If HSIPS06_PanelThickness == 12_25In  
If HSIPS07_CoreInsulation == R44 
Switch (HSIPS08_ContinuousInsulationNR) 
case R0: result = 0.028; case R2: result = 0.027; case R4: result = 0.025; case R5: result = 0.025; case R6: result = 0.024; case R8: result = 0.023;
End Switch
End If
End If
End If</xsd:documentation>
                                                                  </xsd:annotation>
                                                               </xsd:element>
                                                               <xsd:element name="HSIPS11_UfactorJA432Ijoist" type="comp:UfactorJA432Ijoist" >
                                                                  <xsd:annotation>
                                                                     <xsd:documentation source="FieldText"></xsd:documentation>
                                                                     <xsd:documentation source="CalculationsAndRules">If HSIPS05_SplineType == I_Joist  
If HSIPS06_PanelThickness == 4_5In  
If HSIPS07_CoreInsulation == R14 
Switch (HSIPS08_ContinuousInsulationNR) 
case R0: result = 0.070; case R2: result = 0.060; case R4: result = 0.053; case R5: result = 0.051; case R6: result = 0.048; case R8: result = 0.044;  
End Switch
Else If HSIPS07_CoreInsulation == R18 
Switch (HSIPS08_ContinuousInsulationNR) 
case R0: result = 0.059; case R2: result = 0.051; case R4: result = 0.046; case R5: result = 0.044; case R6: result = 0.042; case R8: result = 0.038;  
End Switch
End if
 
Else If HSIPS06_PanelThickness == 6_5In  
If HSIPS07_CoreInsulation == R22 
Switch (HSIPS08_ContinuousInsulationNR) 
case R0: result = 0.048; case R2: result = 0.043; case R4: result = 0.039; case R5: result = 0.038; case R6: result = 0.036; case R8: result = 0.033;  
End Switch
Else If HSIPS07_CoreInsulation == R33 
Switch (HSIPS08_ContinuousInsulationNR) 
case R0: result = 0.036; case R2: result = 0.033; case R4: result = 0.030; case R5: result = 0.029; case R6: result = 0.028; case R8: result = 0.026;  
End Switch
End If
 
Else If HSIPS06_PanelThickness == 8_25In 
If HSIPS07_CoreInsulation == R28 
Switch (HSIPS08_ContinuousInsulationNR) 
case R0: result = 0.037; case R2: result = 0.034; case R4: result = 0.032; case R5: result = 0.031; case R6: result = 0.030; case R8: result = 0.028;  
End Switch
End If
 
Else If HSIPS06_PanelThickness == 10_25In  
If HSIPS07_CoreInsulation == R36 
Switch (HSIPS08_ContinuousInsulationNR) 
case R0: result = 0.030; case R2: result = 0.028; case R4: result = 0.026; case R5: result = 0.026; case R6: result = 0.025; case R8: result = 0.023; 
End Switch
Else If HSIPS07_CoreInsulation == R55 
Switch (HSIPS08_ContinuousInsulationNR) 
case R0: result = 0.022; case R2: result = 0.021; case R4: result = 0.019; case R5: result = 0.019; case R6: result = 0.018; case R8: result = 0.018; 
End Switch
End If
 
Else If HSIPS06_PanelThickness == 12_25In  
If HSIPS07_CoreInsulation == R44 
Switch (HSIPS08_ContinuousInsulationNR) 
case R0: result = 0.025; case R2: result = 0.024; case R4: result = 0.022; case R5: result = 0.022; case R6: result = 0.021; case R8: result = 0.020;
End Switch
End If
End If
End If</xsd:documentation>
                                                                  </xsd:annotation>
                                                               </xsd:element>
                                                            </xsd:choice>
                                                         </xsd:complexType>
                                                      </xsd:element>
                                                      <xsd:element name="HSIPS11_UfactorJA4Other" type="comp:UfactorJA4Other" >
                                                         <xsd:annotation>
                                                            <xsd:documentation source="FieldText">per Software/ Other</xsd:documentation>
                                                            <xsd:documentation source="CalculationsAndRules">If  HSIPS04__UfactorSpecSource == JA4Other Or ApprovedSoftware user enters text; 
Else this row is grayed out
 End If</xsd:documentation>
                                                         </xsd:annotation>
                                                      </xsd:element>
                                                   </xsd:choice>
                                                </xsd:complexType>
                                             </xsd:element>
                                             <xsd:choice maxOccurs="1">
                                                <xsd:element name="HSIPS12_WallNetArea" type="comp:WallNetArea" >
                                                   <xsd:annotation>
                                                      <xsd:documentation source="FieldText">Net Area [d:sup]3[/d:sup] ft[d:sup]2[/d:sup]</xsd:documentation>
                                                      <xsd:documentation source="CalculationsAndRules">If HSIPS01_UseInAreaWeightedAveUfactor == true, user input
End If</xsd:documentation>
                                                   </xsd:annotation>
                                                </xsd:element>
                                                <xsd:element name="HSIPS12_NotApplicableMessage" >
                                                   <xsd:annotation>
                                                      <xsd:documentation source="FieldText">Net Area [d:sup]3[/d:sup] ft[d:sup]2[/d:sup]</xsd:documentation>
                                                      <xsd:documentation source="CalculationsAndRules">If HSIPS09_ConstructionSpecificationMethod == RValue  Or HSIPS01_UseInAreaWeightedAveUfactor == false result = BlankGrayedOut stored in NotApplicableMessage;
End If</xsd:documentation>
                                                   </xsd:annotation>
                                                   <xsd:simpleType>
                                                      <xsd:restriction base="comp:NotApplicableMessage">
                                                         <xsd:enumeration value="BlankGrayedOut"/>
                                                      </xsd:restriction>
                                                   </xsd:simpleType>
                                                </xsd:element>
                                             </xsd:choice>
                                          </xsd:sequence>
                                       </xsd:complexType>
                                    </xsd:element>
                                 </xsd:sequence>
                              </xsd:complexType>
                           </xsd:element>
                           <xsd:element name="HSIPSFootnote1" minOccurs="0">
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText"></xsd:documentation>
                                 <xsd:documentation source="AdditionalRequirements">[d:i][d:sup]1[/d:sup]FOOTNOTES: If any individual assembly is non-compliant, assemblies may show compliance using an area-weighted calculation.  SIP walls are combined with wood framed, spandrel &amp; curtain, metal panel and straw bale wall types.  The area-weighted compliance option is not available for alterations demonstrating compliance with R-values in [d:t24refnolink/]141.0(b)1B3.[/d:i]</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                           <xsd:element name="HSIPSFootnote2" minOccurs="0">
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText"></xsd:documentation>
                                 <xsd:documentation source="AdditionalRequirements">[d:i][d:sup]2[/d:sup] If "R-value" is shown in cell 09 as the Thermal Performance Unit, the R-value shown here is for core insulation per [d:t24refnolink/]141.0(b)1B3.[/d:i]</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                           <xsd:element name="HSIPSFootnote3" minOccurs="0">
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText"></xsd:documentation>
                                 <xsd:documentation source="AdditionalRequirements">[d:i][d:sup]3[/d:sup] Wall area minus any fenestration area [/d:i]</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                        </xsd:sequence>
                     </xsd:complexType>
                  </xsd:element>
                  <xsd:element name="Hspandrel" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">[d:b]Spandrel &amp; Curtain Walls [/d:b]</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">This subsection is required If H01Spandrel_WallTypeApplies == true</xsd:documentation>
                     </xsd:annotation>
                     <xsd:complexType>
                        <xsd:sequence>
                           <xsd:element name="Hspandrel01_UseInAreaWeightedAveUfactor" type="comp:UseInAreaWeightedAveUfactor" >
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText">Include Spandrel/ Curtain Walls in Area-Weighted Average U-factor Calculation[d:sup]1[/d:sup]</xsd:documentation>
                                 <xsd:documentation source="CalculationsAndRules">User input: If checked result = true; Else result = false.  If result == true   D03_ExceptionalConditionMessageCCENVE += WallAreaWeightedAveInH</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                           <xsd:element name="TableHspandrel" >
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText"></xsd:documentation>
                              </xsd:annotation>
                              <xsd:complexType>
                                 <xsd:sequence>
                                    <xsd:element name="Row" minOccurs="1" maxOccurs="unbounded">
                                       <xsd:complexType>
                                          <xsd:sequence>
                                             <xsd:element name="Hspandrel02_WallAssemblyName" type="env:WallAssemblyName" >
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">Tag/Plan Detail ID</xsd:documentation>
                                                </xsd:annotation>
                                             </xsd:element>
                                             <xsd:element name="Hspandrel03a_TopLevelOccupancy" >
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">Occupancy &amp; Status</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">User selects from list: Nonresidential display term Nonresidential/ Relocatable 1 CZ: New, HotelMotel display term Hotel/ Motel: New, RelocableSchoolBuildingAllZones display term Relocatable all CZ: New,  AlteredOccupancy display term Any occupancy: Altered, 
ResidentialMF  display term Multifamily greater than 1 hr fire rating  and OtherOccupancy display term Multifamily less than or equal to 1 hr fire rating.</xsd:documentation>
                                                   <xsd:appinfo source="displayterm">
                                                      <dtyp:displayterm value="AlteredOccupancy">Any Occupancy: Altered</dtyp:displayterm>
                                                      <dtyp:displayterm value="ResidentialMF">Multifamily [d:gt/] 1 hr fire rating</dtyp:displayterm>
                                                      <dtyp:displayterm value="OtherOccupancy">Multifamily [d:lte/] 1 hr fire rating</dtyp:displayterm>
                                                      <dtyp:displayterm value="HotelMotel">Hotel/ Motel: New</dtyp:displayterm>
                                                      <dtyp:displayterm value="Nonresidential">Nonresidential/ Relocatable 1 CZ: New</dtyp:displayterm>
                                                      <dtyp:displayterm value="RelocableSchoolBuildingAllZones">Relocatable all CZ: New</dtyp:displayterm>
                                                   </xsd:appinfo>
                                                   
                                                </xsd:annotation>
                                                <xsd:simpleType>
                                                   <xsd:restriction base="com:TopLevelOccupancy">
                                                      <xsd:enumeration value="AlteredOccupancy"/>
                                                      <xsd:enumeration value="HotelMotel"/>
                                                      <xsd:enumeration value="Nonresidential"/>
                                                      <xsd:enumeration value="OtherOccupancy"/>
                                                      <xsd:enumeration value="RelocableSchoolBuildingAllZones"/>
                                                      <xsd:enumeration value="ResidentialMF"/>
                                                   </xsd:restriction>
                                                </xsd:simpleType>
                                             </xsd:element>
                                             <xsd:element name="Hspandrel03b_Status" >
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">NOT REPORTED IN PDF</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">Calculatedf field: If HSpandrel03a_TopLevelOccupancy == AlteredOccupancy result = ExistingAlteration; 
Else result = New
End If</xsd:documentation>
                                                </xsd:annotation>
                                                <xsd:simpleType>
                                                   <xsd:restriction base="com:Status">
                                                      <xsd:enumeration value="ExistingAlteration"/>
                                                      <xsd:enumeration value="New"/>
                                                   </xsd:restriction>
                                                </xsd:simpleType>
                                             </xsd:element>
                                             <xsd:element name="Hspandrel03c_StructureFireRating" type="env:StructureFireRating" >
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">NOT REPORTED IN PDF</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">If Hspandrel03a_TopLevelOccupancy == ResidentialMF
result = GT1hr
Else If Hspandrel03a_TopLevelOccupancy == OtherOccupancy
result = LTE1hr
Else
result = RatingNA
End If</xsd:documentation>
                                                </xsd:annotation>
                                             </xsd:element>
                                             <xsd:choice maxOccurs="1">
                                                <xsd:element name="Hspandrel04_UfactorSpecSource" type="comp:UfactorSpecSource" >
                                                   <xsd:annotation>
                                                      <xsd:documentation source="FieldText">How Design U-factor was determined</xsd:documentation>
                                                      <xsd:documentation source="CalculationsAndRules">User selects from list: JA4Tables display term JA4 Tables, JA4Other display term Other per JA4.1.2.1, ApprovedSoftware display term Approved Software. 
 
 If result == JA4Other  Or ApprovedSoftware D03_ExceptionalConditionMessageCCENVE += AdditionalDocJA4OtherSoftware</xsd:documentation>
                                                   </xsd:annotation>
                                                </xsd:element>
                                                <xsd:element name="Hspandrel04_NotApplicableMessage" >
                                                   <xsd:annotation>
                                                      <xsd:documentation source="FieldText">How Design U-factor was determined</xsd:documentation>
                                                      <xsd:documentation source="CalculationsAndRules">If Hspandrel08_ConstructionSpecificationMethod == RValue result = BlankGrayedOut stored in NotApplicableMessage
End If</xsd:documentation>
                                                   </xsd:annotation>
                                                   <xsd:simpleType>
                                                      <xsd:restriction base="comp:NotApplicableMessage">
                                                         <xsd:enumeration value="BlankGrayedOut"/>
                                                      </xsd:restriction>
                                                   </xsd:simpleType>
                                                </xsd:element>
                                             </xsd:choice>
                                             <xsd:choice maxOccurs="1">
                                                <xsd:element name="Hspandrel05_SpandrelCurtainType" type="env:SpandrelCurtainType" >
                                                   <xsd:annotation>
                                                      <xsd:documentation source="FieldText">Type</xsd:documentation>
                                                      <xsd:documentation source="CalculationsAndRules">User selects from list: 
AluminumNoThermalBreak display term Spandrel: Aluminum, no thermal break, 
AluminumThermalBreak display term Spandrel: Aluminum, with thermal break, 
NoFrameContinuousInsul display term Spandrel: No framing/ Continuous insulation, 
StructuralGlazing display term Spandrel: Structural Glazing, 
AluminumNoThermalBreakCW display term CW: Aluminum, no thermal break, 
AluminumThermalBreakCW display term CW: Aluminum, with thermal break, 
NoFrameContinuousInsulCW display term CW: No framing/ Continuous insulation, 
StructuralGlazingCW display term CW: Structural Glazing</xsd:documentation>
                                                   </xsd:annotation>
                                                </xsd:element>
                                                <xsd:element name="Hspandrel05_SpandrelCW_TypeOther" type="env:SpandrelCW_TypeOther" >
                                                   <xsd:annotation>
                                                      <xsd:documentation source="FieldText">Type</xsd:documentation>
                                                      <xsd:documentation source="CalculationsAndRules">If  Hspandrel04__UfactorSpecSource == JA4Other Or ApprovedSoftware user enters text; 
Else this row is grayed out
End If</xsd:documentation>
                                                   </xsd:annotation>
                                                </xsd:element>
                                                <xsd:element name="Hspandrel05_NotApplicableMessage" >
                                                   <xsd:annotation>
                                                      <xsd:documentation source="FieldText">Type</xsd:documentation>
                                                      <xsd:documentation source="CalculationsAndRules">If HSpandrel08_ConstructionSpecificationMethod == RValue result = BlankGrayedOut stored in NotApplicableMessage and both rows for H05 are grayed out;
End If</xsd:documentation>
                                                   </xsd:annotation>
                                                   <xsd:simpleType>
                                                      <xsd:restriction base="comp:NotApplicableMessage">
                                                         <xsd:enumeration value="BlankGrayedOut"/>
                                                      </xsd:restriction>
                                                   </xsd:simpleType>
                                                </xsd:element>
                                             </xsd:choice>
                                             <xsd:element name="Hspandrel05a_GlassWallType" type="env:GlassWallType"  minOccurs="0">
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">NOT REPORTED IN PDF</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">Calculated value used in assigning U-factors from JA4.3.8 table: 
If Hspandrel05_NotApplicableMessage IsPresent this field is not required
Else
 
If Hspandrel05_SpandrelCurtainType IsPresent Then If Hspandrel05_SpandrelCurtainType == AluminumNoThermalBreak, AluminumThermalBreak, NoFrameContinuousInsul Or StructuralGlazing  result = Spandrel; 
Else result = Curtain
End If</xsd:documentation>
                                                </xsd:annotation>
                                             </xsd:element>
                                             <xsd:choice maxOccurs="1">
                                                <xsd:element name="Hspandrel06_SpandrelCurtainFinish" type="env:SpandrelCurtainFinish" >
                                                   <xsd:annotation>
                                                      <xsd:documentation source="FieldText">Finish</xsd:documentation>
                                                      <xsd:documentation source="CalculationsAndRules">User selects from list:
SinglePaneGlass display term Single Pane Glass, 
DoubleLowE display term Dual Pane Glass with Low-E, 
DoubleNoLowE display term Dual Pane Glass, no Low-E, 
TriplePaneGlass display term Triple Pane Glass, 
StoneMetalConcrete display term Stone Veneer, Metal, or Concrete Panel</xsd:documentation>
                                                   </xsd:annotation>
                                                </xsd:element>
                                                <xsd:element name="Hspandrel06_SpandrelCW_FinishOther" type="env:SpandrelCW_FinishOther" >
                                                   <xsd:annotation>
                                                      <xsd:documentation source="FieldText">Finish</xsd:documentation>
                                                      <xsd:documentation source="CalculationsAndRules">If  Hspandrel04__UfactorSpecSource == JA4Other Or ApprovedSoftware user enters text; 
Else this row is grayed out
End If</xsd:documentation>
                                                   </xsd:annotation>
                                                </xsd:element>
                                                <xsd:element name="Hspandrel06_NotApplicableMessage" >
                                                   <xsd:annotation>
                                                      <xsd:documentation source="FieldText">Finish</xsd:documentation>
                                                      <xsd:documentation source="CalculationsAndRules">If HSpandrel08_ConstructionSpecificationMethod == RValue result = BlankGrayedOut stored in NotApplicableMessage and both rows for H06 are grayed out;
End If</xsd:documentation>
                                                   </xsd:annotation>
                                                   <xsd:simpleType>
                                                      <xsd:restriction base="comp:NotApplicableMessage">
                                                         <xsd:enumeration value="BlankGrayedOut"/>
                                                      </xsd:restriction>
                                                   </xsd:simpleType>
                                                </xsd:element>
                                             </xsd:choice>
                                             <xsd:choice maxOccurs="1">
                                                <xsd:element name="Hspandrel07_CavityInsulationNR" >
                                                   <xsd:annotation>
                                                      <xsd:documentation source="FieldText">Insulation R-value</xsd:documentation>
                                                      <xsd:documentation source="CalculationsAndRules">If Hspandrel04__UfactorSpecSource == JA4Tables, user selects from list: R0 display term none, R4 display term R-4, R7 display term R-7, R10 display term R-10, R15 display term R-15, R20 display term R-20, R25 display term R-25, R30 display term R-30
End If</xsd:documentation>
                                                   </xsd:annotation>
                                                   <xsd:simpleType>
                                                      <xsd:restriction base="env:CavityInsulationNR">
                                                         <xsd:enumeration value="R0"/>
                                                         <xsd:enumeration value="R10"/>
                                                         <xsd:enumeration value="R15"/>
                                                         <xsd:enumeration value="R20"/>
                                                         <xsd:enumeration value="R25"/>
                                                         <xsd:enumeration value="R30"/>
                                                         <xsd:enumeration value="R4"/>
                                                         <xsd:enumeration value="R7"/>
                                                      </xsd:restriction>
                                                   </xsd:simpleType>
                                                </xsd:element>
                                                <xsd:element name="Hspandrel07_CavityInsulationOther" type="com:CavityInsulationOther" >
                                                   <xsd:annotation>
                                                      <xsd:documentation source="FieldText">Insulation R-value</xsd:documentation>
                                                      <xsd:documentation source="CalculationsAndRules">If  Hspandrel04_UfactorSpecSource == JA4Other Or ApprovedSoftware user enters text; 
Else this row is grayed out
End If</xsd:documentation>
                                                   </xsd:annotation>
                                                </xsd:element>
                                             </xsd:choice>
                                             <xsd:element name="Hspandrel08_ConstructionSpecificationMethod" >
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">Thermal Performance Unit</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">If Hspandrel03b_Status == ExistingAlteration And Hspandrel01_UseInAreaWeightedAveUfactor  == false list RValue display term R-value, UFactor display term U-factor ; 
Else result = UFactor and cannot be changed by user
End If</xsd:documentation>
                                                </xsd:annotation>
                                                <xsd:simpleType>
                                                   <xsd:restriction base="env:ConstructionSpecificationMethod">
                                                      <xsd:enumeration value="RValue"/>
                                                      <xsd:enumeration value="UFactor"/>
                                                   </xsd:restriction>
                                                </xsd:simpleType>
                                             </xsd:element>
                                             <xsd:choice maxOccurs="1">
                                                <xsd:element name="Hspandrel09_InsulationUFactorLimit" type="env:InsulationUFactorLimit" >
                                                   <xsd:annotation>
                                                      <xsd:documentation source="FieldText">Required Thermal Performance [d:sup]2[/d:sup]</xsd:documentation>
                                                      <xsd:documentation source="CalculationsAndRules">If Hspandrel3b_Status == New 
Switch (Hspandrel03a_TopLevelOccupancy) 
case  Nonresidential: 
 If A03_ClimateZoneNumber == 1 result = 0.095; 
Else  If A03_ClimateZoneNumber == 2, 4, 9, 10, 16 Or InRange (12, 14)  result = 0.059; 
Else If A03_ClimateZoneNumber == 3, 6, Or 7  result = 0.110;  
Else If A03_ClimateZoneNumber == 5 Or 8 result = 0.102;  
Else If A03_ClimateZoneNumber == 11 result = 0.045; 
Else If A03_ClimateZoneNumber == 15 result = 0.042;  
End If
 
case HotelMotel: 
If A03_ClimateZoneNumber == 12, 13 Or  InRange (1, 10) result = 0.059; 
Else If A03_ClimateZoneNumber == 11, Or  InRange (14, 16) result = 0.042; 
End If
 
case RelocableSchoolBuildingAllZones:  result = 0.059; 
 
case ResidentialMF: case OtherOccupancy:
If Hspandrel03c_StructureFireRating == GT1hr
If A03_ClimateZoneNumber == InRange(1,5) Or 8 Or 9 Or 10 Or 12 Or 13   result = 0.059
Else If A03_ClimateZoneNumber == 11 Or 14 Or 15 Or 16   result = 0.051
Else If A03_ClimateZoneNumber == 6 Or 7   result = 0.065
End If
Else If Hspandrel03c_StructureFireRating == LTE1hr
If A03_ClimateZoneNumber == InRange(1,5) Or InRange(8,16)  result = 0.051
Else If A03_ClimateZoneNumber == 6 Or 7   result = 0.065
End If
End If
End Switch
 
Else If Hspandrel3b_Status == ExistingAlteration And Hspandrel08_ConstructionSpecificationMethod  == UFactor  result = 0.280;
End If</xsd:documentation>
                                                   </xsd:annotation>
                                                </xsd:element>
                                                <xsd:element name="Hspandrel09_InsulationRValueLimit" >
                                                   <xsd:annotation>
                                                      <xsd:documentation source="FieldText">Required Thermal Performance [d:sup]2[/d:sup]</xsd:documentation>
                                                      <xsd:documentation source="CalculationsAndRules">If Hspandre03b_Status == ExistingAlteration And Hspandrel08_ConstructionSpecificationMethod  == RValue result = R4;
End If</xsd:documentation>
                                                   </xsd:annotation>
                                                   <xsd:simpleType>
                                                      <xsd:restriction base="env:InsulationRValueLimit">
                                                         <xsd:enumeration value="R4"/>
                                                      </xsd:restriction>
                                                   </xsd:simpleType>
                                                </xsd:element>
                                             </xsd:choice>
                                             <xsd:element name="Hspandrel10UF" >
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">U-factor per Design</xsd:documentation>
                                                </xsd:annotation>
                                                <xsd:complexType>
                                                   <xsd:choice >
                                                      <xsd:element name="Hspandrel10_NotApplicableMessage" >
                                                         <xsd:annotation>
                                                            <xsd:documentation source="FieldText">U-factor per Design</xsd:documentation>
                                                            <xsd:documentation source="CalculationsAndRules">If HSpandrel08_ConstructionSpecificationMethod == RValue result = BlankGrayedOut stored in NotApplicableMessage and both rows for H10 are grayed out;
End If</xsd:documentation>
                                                         </xsd:annotation>
                                                         <xsd:simpleType>
                                                            <xsd:restriction base="comp:NotApplicableMessage">
                                                               <xsd:enumeration value="BlankGrayedOut"/>
                                                            </xsd:restriction>
                                                         </xsd:simpleType>
                                                      </xsd:element>
                                                      <xsd:element name="Hspandrel10JA4" >
                                                         <xsd:annotation>
                                                            <xsd:documentation source="FieldText">per JA4</xsd:documentation>
                                                         </xsd:annotation>
                                                         <xsd:complexType>
                                                            <xsd:choice >
                                                               <xsd:element name="Hspandrel10_UfactorJA438Spandrel" type="comp:UfactorJA438Spandrel" >
                                                                  <xsd:annotation>
                                                                     <xsd:documentation source="FieldText">per JA4</xsd:documentation>
                                                                     <xsd:documentation source="CalculationsAndRules">Use this choice If Hspandrel05a_GlassWallType == Spandrel:   
If Hspandrel05_SpandrelCurtainType == AluminumNoThermalBreak 
If Hspandrel06_SpandrelCurtainFinish  == SinglePaneGlass Or StoneMetalConcrete 
Switch (Hspandrel07_CavityInsulationNR) 
case R0:  0.445;  case R4: 0.285;  case R7:  0.259; case R10: 0.247;   case R15: 0.236;   case R20: 0.230;    case R25: 0.226;   case R30: 0.224;  
End Switch
Else Hspandrel06_SpandrelCurtainFinish  == DoubleNoLowE 
Switch (Hspandrel07_CavityInsulationNR) 
case R0:  0.356;  case R4: 0.273;  case R7:  0.254; case R10: 0.244;   case R15: 0.234;   case R20: 0.229;    case R25: 0.226;   case R30: 0.223;  
End Switch
Else Hspandrel06_SpandrelCurtainFinish  == DoubleLowE Or TriplePaneGlass  
