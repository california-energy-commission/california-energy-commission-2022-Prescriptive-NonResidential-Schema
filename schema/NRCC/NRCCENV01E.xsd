                                                         <xsd:annotation>
                                                            <xsd:documentation source="FieldText">Glazing Tint</xsd:documentation>
                                                            <xsd:documentation source="CalculationsAndRules">User selects from customized list as follows: 
If Kvertperf23_FenestrationPanes == GlassBlock list: 
ClearGlazing display term No; 
 
Else list:  TintedGlazing display term Yes, ClearGlazing display term No
End If</xsd:documentation>
                                                            <xsd:appinfo source="displayterm">
                                                               <dtyp:displayterm value="TintedGlazing"> Yes</dtyp:displayterm>
                                                               <dtyp:displayterm value="ClearGlazing"> No
</dtyp:displayterm>
                                                            </xsd:appinfo>
                                                            
                                                         </xsd:annotation>
                                                      </xsd:element>
                                                      <xsd:element name="Kvertperf25Label" minOccurs="0">
                                                         <xsd:annotation>
                                                            <xsd:documentation source="FieldText">Product Performance per   Default 110.6 Tables</xsd:documentation>
                                                         </xsd:annotation>
                                                      </xsd:element>
                                                      <xsd:choice maxOccurs="1">
                                                         <xsd:element name="Kvertperf25a_Ufactor110Default" type="env:Ufactor110Default" >
                                                            <xsd:annotation>
                                                               <xsd:documentation source="FieldText">U-factor</xsd:documentation>
                                                               <xsd:documentation source="CalculationsAndRules">Calculated field: 
 If Kvertperf05_FenestrationProductType == GlazedDoor
Switch (Kvertperf22_FenestrationFramingNR) 
case MetalFraming:  
If Kvertperf23_FenestrationPanes == SinglePane result = 1.25; 
Else If Kvertperf23_FenestrationPanes == DoublePane result = 0.77; 
Else If Kvertperf23_FenestrationPanes == DoubleWithSpacers Or DoublePaneDivided result = 0.82
End If
 
case MetalFramingWithThermalBreak: 
If Kvertperf23_FenestrationPanes == DoublePane result = 0.59;  
Else If Kvertperf23_FenestrationPanes == DoubleWithSpacers Or DoublePaneDivided result = 0.64;
End If
 
case NonMetalFraming:  
If Kvertperf23_FenestrationPanes == SinglePane result =0.99; 
Else If Kvertperf23_FenestrationPanes == DoublePane result = 0.53;
Else If Kvertperf23_FenestrationPanes == DoubleWithSpacers Or DoublePaneDivided result = 0.58
End If
End Switch
 
Else
 
Switch (Kvertperf22_FenestrationFramingNR) 
case MetalFraming: 
 
If Kvertperf20_GreenhouseGardenWindow == true Then 
If Kvertperf23_FenestrationPanes == SinglePane result = 2.26; 
Else If Kvertperf23_FenestrationPanes == DoublePane result = 1.40; 
Else If Kvertperf23_FenestrationPanes == DoubleWithSpacers Or DoublePaneDivided result =1.45;
End If
 
Else If Kvertperf05_FenestrationProductType == OperableWindow  Then 
If Kvertperf23_FenestrationPanes == SinglePane result = 1.28; 
Else If Kvertperf23_FenestrationPanes == DoublePane result = 0.79; 
Else If Kvertperf23_FenestrationPanes == DoubleWithSpacers Or DoublePaneDivided result = 0.84;
Else If Kvertperf23_FenestrationPanes == GlassBlock result = 0.87; 
End If
 
Else If Kvertperf05_FenestrationProductType == FixedWindow Or StoreFront Or DemisingWallFen Or CurtainWall  Then 
If Kvertperf23_FenestrationPanes == SinglePane result = 1.19; 
Else If Kvertperf23_FenestrationPanes == DoublePane result = 0.71; 
Else If Kvertperf23_FenestrationPanes == DoubleWithSpacers Or DoublePaneDivided result = 0.76;
Else If Kvertperf23_FenestrationPanes == GlassBlock result = 0.72; 
End If
End If
 
case MetalFramingWithThermalBreak: 
If Kvertperf20_GreenhouseGardenWindow == true  
 If Kvertperf23_FenestrationPanes == DoublePane  result = 1.12; 
Else If Kvertperf23_FenestrationPanes == DoubleWithSpacers Or DoublePaneDivided result = 1.17;
End If
 
Else If Kvertperf23_FenestrationPanes == DoublePane 
If Kvertperf05_FenestrationProductType == OperableWindow result = 0.66; 
Else If Kvertperf05_FenestrationProductType == FixedWindow Or StoreFront Or DemisingWallFen Or CurtainWall result = 0.55;  
End If
Else If Kvertperf23_FenestrationPanes == DoubleWithSpacers Or DoublePaneDivided
If Kvertperf05_FenestrationProductType == OperableWindow result = 0.71; 
Else If Kvertperf05_FenestrationProductType == FixedWindow Or StoreFront Or DemisingWallFen Or CurtainWall result = 0.60;  
End If
End If
 
case NonMetalFraming: 
If Kvertperf20_GreenhouseGardenWindow == true Then 
If Kvertperf23_FenestrationPanes == SinglePane result = 1.94; 
Else If Kvertperf23_FenestrationPanes == DoublePane result = 1.06;  
Else If Kvertperf23_FenestrationPanes == DoubleWithSpacers Or DoublePaneDivided result = 1.11;
End If
 
Else If Kvertperf05_FenestrationProductType == OperableWindow  Then 
If Kvertperf23_FenestrationPanes == SinglePane result =0.99; 
Else If Kvertperf23_FenestrationPanes == DoublePane result = 0.58;
Else If Kvertperf23_FenestrationPanes == DoubleWithSpacers Or DoublePaneDivided result = 0.63;
Else If Kvertperf23_FenestrationPanes == GlassBlock result = 0.60; 
End If
 
Else If Kvertperf05_FenestrationProductType == FixedWindow Or StoreFront Or DemisingWallFen Or CurtainWall  Then 
If Kvertperf23_FenestrationPanes == SinglePane result = 1.04; 
Else If Kvertperf23_FenestrationPanes == DoublePane result = 0.55;
Else If Kvertperf23_FenestrationPanes == DoubleWithSpacers Or DoublePaneDivided result = 60;
Else If Kvertperf23_FenestrationPanes == GlassBlock result = 0.57;
End If
End If
End Switch</xsd:documentation>
                                                            </xsd:annotation>
                                                         </xsd:element>
                                                         <xsd:element name="Kvertperf25a_NotApplicableMessage" >
                                                            <xsd:annotation>
                                                               <xsd:documentation source="FieldText">U-factor</xsd:documentation>
                                                               <xsd:documentation source="CalculationsAndRules">Calculated field: 
 
If Kvertperf05_FenestrationProductType == ChromogenicFen Or GlazedDoorMF
result = BlankGrayedOut
 
End If</xsd:documentation>
                                                            </xsd:annotation>
                                                            <xsd:simpleType>
                                                               <xsd:restriction base="comp:NotApplicableMessage">
                                                                  <xsd:enumeration value="BlankGrayedOut"/>
                                                               </xsd:restriction>
                                                            </xsd:simpleType>
                                                         </xsd:element>
                                                      </xsd:choice>
                                                      <xsd:choice maxOccurs="1">
                                                         <xsd:element name="Kvertperf25b_SHGC_110Default" type="com:SHGC_110Default" >
                                                            <xsd:annotation>
                                                               <xsd:documentation source="FieldText">SHGC</xsd:documentation>
                                                               <xsd:documentation source="CalculationsAndRules">Calculated field: 
 
Switch (Kvertperf22_FenestrationFramingNR) 
case MetalFraming:  
If Kvertperf05_FenestrationProductType == OperableWindow  Then 
 
If Kvertperf24_GlazingTint == ClearGlazing 
If Kvertperf23_FenestrationPanes == SinglePane result = 0.80; 
Else If Kvertperf23_FenestrationPanes == DoublePane,  DoubleWithSpacers Or DoublePaneDivided
 result = 0.70; 
Else If Kvertperf23_FenestrationPanes == GlassBlock result = 0.70 
End If
 
Else If Kvertperf24_GlazingTint == TintedGlazing 
If Kvertperf23_FenestrationPanes == SinglePane result = 0.67; 
Else If Kvertperf23_FenestrationPanes == DoublePane,  DoubleWithSpacers Or DoublePaneDivided
 result = 0.59; 
End If
End If
 
Else If Kvertperf05_FenestrationProductType == FixedWindow, GlazedDoor, CurtainWall, Or StoreFront Then 
If Kvertperf24_GlazingTint == ClearGlazing 
If Kvertperf23_FenestrationPanes == SinglePane result = 0.83; 
Else If Kvertperf23_FenestrationPanes == DoublePane,  DoubleWithSpacers Or DoublePaneDivided
result = 0.73; 
Else If Kvertperf23_FenestrationPanes == GlassBlock result = 0.73; 
End If
 
Else If Kvertperf24_GlazingTint == TintedGlazing 
If Kvertperf23_FenestrationPanes == SinglePane result = 0.68; 
Else If Kvertperf23_FenestrationPanes == DoublePane,  DoubleWithSpacers Or DoublePaneDivided
 result = 0.60;  
End If
End If
End If
 
case MetalFramingWithThermalBreak: 
If Kvertperf23_FenestrationPanes == DoublePane,  DoubleWithSpacers Or DoublePaneDivided
 Then 
If Kvertperf05_FenestrationProductType == OperableWindow  
If Kvertperf24_GlazingTint == ClearGlazing result = 0.63; 
Else If Kvertperf24_GlazingTint == TintedGlazing result = 0.53; 
End If
 
Else If Kvertperf05_FenestrationProductType ==  FixedWindow, GlazedDoor, CurtainWall, Or StoreFront 
If Kvertperf24_GlazingTint == ClearGlazing result = 0.69; 
Else If Kvertperf24_GlazingTint == TintedGlazing result = 0.57;  
End If
End If
End If
 
case NonMetalFraming:  
If Kvertperf05_FenestrationProductType == OperableWindow  Then 
If Kvertperf24_GlazingTint == ClearGlazing 
If Kvertperf23_FenestrationPanes == SinglePane result =0.74; 
Else If Kvertperf23_FenestrationPanes == DoublePane,  DoubleWithSpacers Or DoublePaneDivided
 result = 0.65; 
Else If Kvertperf23_FenestrationPanes == GlassBlock result = 0.70; 
End If
 
Else If Kvertperf24_GlazingTint == TintedGlazing 
If Kvertperf23_FenestrationPanes == SinglePane result =0.60; 
Else If Kvertperf23_FenestrationPanes == DoublePane,  DoubleWithSpacers Or DoublePaneDivided
 result = 0.53; 
End If
 
Else If Kvertperf05_FenestrationProductType ==  FixedWindow, GlazedDoor, CurtainWall, Or StoreFront  Then 
If Kvertperf24_GlazingTint == ClearGlazing 
If Kvertperf23_FenestrationPanes == SinglePane result = 0.76; 
Else If Kvertperf23_FenestrationPanes == DoublePane,  DoubleWithSpacers Or DoublePaneDivided
 result = 0.67; 
Else If Kvertperf23_FenestrationPanes == GlassBlock result = 0.67; 
End If
 
Else If Kvertperf24_GlazingTint == TintedGlazing 
If Kvertperf23_FenestrationPanes == SinglePane result =0.63; 
Else If Kvertperf23_FenestrationPanes ==DoublePane,  DoubleWithSpacers Or DoublePaneDivided
 result = 0.55;
End If
End If
End If
End Switch</xsd:documentation>
                                                            </xsd:annotation>
                                                         </xsd:element>
                                                         <xsd:element name="Kvertperf25b_NotApplicableMessage" >
                                                            <xsd:annotation>
                                                               <xsd:documentation source="FieldText">SHGC</xsd:documentation>
                                                               <xsd:documentation source="CalculationsAndRules">Calculated field: 
If Kvertperf05_FenestrationProductType == ChromogenicFen Or DemisingWallFen Or GlazedDoorMF
Or If Kvertperf06_FenestrationModification == ReplaceLTE150ft2 Or AddLTE50ft2  
 
result = BlankGrayedOut
 
Else
If Kvertperf22_FenestrationFramingNR ==  MetalFramingWithThermalBreak: 
And  Kvertperf23_FenestrationPanes == SinglePane  Or GlassBlock
result = NotApplicable display term N/A stored in NotApplicableMessage
Else
If Kvertperf22_FenestrationFramingNR ==  MetalFraming Or NonMetalFraming 
And  If Kvertperf24_GlazingTint == TintedGlazing
And If Kvertperf23_FenestrationPanes == GlassBlock
result = NotApplicable display term N/A stored in NotApplicableMessage
End If</xsd:documentation>
                                                            </xsd:annotation>
                                                            <xsd:simpleType>
                                                               <xsd:restriction base="comp:NotApplicableMessage">
                                                                  <xsd:enumeration value="BlankGrayedOut"/>
                                                                  <xsd:enumeration value="NotApplicable"/>
                                                               </xsd:restriction>
                                                            </xsd:simpleType>
                                                         </xsd:element>
                                                      </xsd:choice>
                                                      <xsd:choice maxOccurs="1">
                                                         <xsd:element name="Kvertperf25c_VT_110Default" type="env:VT_110Default" >
                                                            <xsd:annotation>
                                                               <xsd:documentation source="FieldText">VT</xsd:documentation>
                                                               <xsd:documentation source="CalculationsAndRules">Calculated field: 
If Kvertperf21_WindowProjecting == true result = 0.53; 
Else If Kvertperf05_FenestrationProductType == OperableWindow result = 0.67; 
Else If Kvertperf05_FenestrationProductType == FixedWindow Or GlazedDoor
 result = 0.77; 
Else If Kvertperf05_FenestrationProductType == Storefront Or  CurtainWall  result = 0.88;
End If</xsd:documentation>
                                                            </xsd:annotation>
                                                         </xsd:element>
                                                         <xsd:element name="Kvertperf25c_NotApplicableMessage" >
                                                            <xsd:annotation>
                                                               <xsd:documentation source="FieldText">VT</xsd:documentation>
                                                               <xsd:documentation source="CalculationsAndRules">Calculated field: 
If Kvertperf05_FenestrationProductType == ChromogenicFen Or DemisingWallFen Or GlazedDoorMF 
Or
If Kvertperf06_FenestrationModification == ReplaceLTE150ft2 Or AddLTE50ft2  Or
 
Or If Kvertperf06_TopLevelOccupancy == RelocableSchoolBuildingAllZones and  Kvertperf06_FenestrationModification == AllNew Or AddGT50ft2  Or
 
If Kvertperf06_TopLevelOccupancy == RelocableSchoolBuildingAllZones And  
Kvertperf05_FenestrationProductType == GlazedDoor  Or
 
If Kvertperf08_ComplianceMethodSHGC_VT == NA_SkylitOverlap
 
 result = BlankGrayedOutresult = BlankGrayedOut
End If</xsd:documentation>
                                                            </xsd:annotation>
                                                            <xsd:simpleType>
                                                               <xsd:restriction base="comp:NotApplicableMessage">
                                                                  <xsd:enumeration value="BlankGrayedOut"/>
                                                               </xsd:restriction>
                                                            </xsd:simpleType>
                                                         </xsd:element>
                                                      </xsd:choice>
                                                   </xsd:sequence>
                                                </xsd:complexType>
                                             </xsd:element>
                                             <xsd:element name="KvertperfOverhang" minOccurs="0">
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">[d:b]Overhang Details for RSHGC per [d:t24refnolink/]140.3/ [d:t24refnolink/]170.2[/d:b]</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">This section is required If  Kvertperf09a_OverhangForRSHGC == true</xsd:documentation>
                                                </xsd:annotation>
                                                <xsd:complexType>
                                                   <xsd:sequence>
                                                      <xsd:element name="Kvertperf26_SHGC" type="com:SHGC" >
                                                         <xsd:annotation>
                                                            <xsd:documentation source="FieldText">SHGC of Window</xsd:documentation>
                                                            <xsd:documentation source="CalculationsAndRules">User input value</xsd:documentation>
                                                         </xsd:annotation>
                                                      </xsd:element>
                                                      <xsd:element name="Kvertperf27_ConstructedShadingType" type="env:ConstructedShadingType" >
                                                         <xsd:annotation>
                                                            <xsd:documentation source="FieldText">Overhang or Horizontal Slats</xsd:documentation>
                                                            <xsd:documentation source="CalculationsAndRules">User selects from list: Overhang or HorizontalSlats</xsd:documentation>
                                                         </xsd:annotation>
                                                      </xsd:element>
                                                      <xsd:element name="Kvertperf28_Azimuth" type="dtyp:Azimuth" >
                                                         <xsd:annotation>
                                                            <xsd:documentation source="FieldText">Azimuth of Fenestration (deg)</xsd:documentation>
                                                            <xsd:documentation source="CalculationsAndRules">User input value</xsd:documentation>
                                                         </xsd:annotation>
                                                      </xsd:element>
                                                      <xsd:element name="Kvertperf29_OverhangProjection" type="env:OverhangProjection" >
                                                         <xsd:annotation>
                                                            <xsd:documentation source="FieldText">Projection per Eq. 140.3-C/ 170.2-A</xsd:documentation>
                                                            <xsd:documentation source="CalculationsAndRules">User input value</xsd:documentation>
                                                         </xsd:annotation>
                                                      </xsd:element>
                                                      <xsd:element name="Kvertperf30_OverhangSpacing" type="env:OverhangSpacing" >
                                                         <xsd:annotation>
                                                            <xsd:documentation source="FieldText">Spacing per Eq. 140.3-C/ 170.2-A</xsd:documentation>
                                                            <xsd:documentation source="CalculationsAndRules">User input value</xsd:documentation>
                                                         </xsd:annotation>
                                                      </xsd:element>
                                                      <xsd:element name="Kvertperf31_ProjectionFactor" type="com:ProjectionFactor" >
                                                         <xsd:annotation>
                                                            <xsd:documentation source="FieldText">Projection Factor</xsd:documentation>
                                                            <xsd:documentation source="CalculationsAndRules">Calculated Field: 
result = Kvertperf29_OverhangProjection / Kvertperf30_OverhangSpacing</xsd:documentation>
                                                         </xsd:annotation>
                                                      </xsd:element>
                                                      <xsd:element name="Kvertperf32_RSHGC" type="com:RSHGC" >
                                                         <xsd:annotation>
                                                            <xsd:documentation source="FieldText">RSHGC</xsd:documentation>
                                                            <xsd:documentation source="CalculationsAndRules">Calculated Field: Assign of equation variables as follows: 
 If Kvertperf27_ConstructedShadingType == Overhang  a = 0.150 and  b = 0.130 and c=5.67; 
Else If Kvertperf27_ConstructedShadingType == HorizontalSlats  a = 0.144 and  b = 0.133 and c = 5.13;     
End If
Az = Kvertperf28_Azimuth
PF =Kvertperf31_ProjectionFactor
 
Equation for the result = Kvertperf26_SHGC  x  (1 + a x  (2.72^-PF  - 1) x (sin(b x Az)) - c)</xsd:documentation>
                                                         </xsd:annotation>
                                                      </xsd:element>
                                                   </xsd:sequence>
                                                </xsd:complexType>
                                             </xsd:element>
                                          </xsd:sequence>
                                       </xsd:complexType>
                                    </xsd:element>
                                 </xsd:sequence>
                              </xsd:complexType>
                           </xsd:element>
                           <xsd:element name="KvertperfFootNote1" minOccurs="0">
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText"></xsd:documentation>
                                 <xsd:documentation source="AdditionalRequirements">[d:i][d:sup]1[/d:sup]FOOTNOTES: If any individual fenestration product is non-compliant, products may show compliance using an area-weighted calculation. Chromogenic glazing is not included in area-weighted calculations. Area-weighted calculation shown in separate area-weighted table below.[/d:i]</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                           <xsd:element name="KvertperfFootNote2" minOccurs="0">
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText"></xsd:documentation>
                                 <xsd:documentation source="AdditionalRequirements">[d:i][d:sup]2[/d:sup]The NA6 Default Calculation can only be used for alterations or dwelling units in buildings with [d:lte/] 3 habitable stories.  Alterations are limited to 200ft[d:sup]2[/d:sup] of site built glazing and dwelling units are limited to 250ft[d:sup]2[/d:sup] or 5% of conditioned floor area.  If the fenestration does not meet these conditions, the only options for determining fenestration values are NFRC Certification or the Default Tables in [d:t24refnolink/]110.6.[/d:i]</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                           <xsd:element name="KvertperfFootNote3" minOccurs="0">
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText"></xsd:documentation>
                                 <xsd:documentation source="AdditionalRequirements">[d:i][d:sup]3[/d:sup] Overhangs must extend past the left and right window the same distance as the depth of the overhang or greater to show an affect on the RSHGC.  If an overhang does not meet this requirement, the affect of the overhang will be ignored.[/d:i]</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                           <xsd:element name="KvertperfFootNote4" minOccurs="0">
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText"></xsd:documentation>
                                 <xsd:documentation source="AdditionalRequirements">[d:i][d:sup]4[/d:sup]Projecting includes casement and awning windows.[/d:i]</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                        </xsd:sequence>
                     </xsd:complexType>
                  </xsd:element>
                  <xsd:element name="Ksky" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">[d:b] Skylights- Total Area[/d:b]</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">This subsection is required If  K01Skylights_FenestrationTypeApplies  ==  true</xsd:documentation>
                     </xsd:annotation>
                     <xsd:complexType>
                        <xsd:sequence>
                           <xsd:element name="Ksky00_Adding16SkyMF" type="env:Adding16SkyMF" >
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText">Multifamily adding up to 16ft[d:sup]2[/d:sup] of skylight per dwelling unit?</xsd:documentation>
                                 <xsd:documentation source="CalculationsAndRules">User selects Yes or checks a box (result = true) or selects No or leaves box unchecked (result = false)
If result = true
Ksky01-Ksky04 are Blank and grayed out
End if</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                           <xsd:choice maxOccurs="1">
                              <xsd:element name="Ksky01_IsAtriumGT55Ft" type="com:IsAtriumGT55Ft" >
                                 <xsd:annotation>
                                    <xsd:documentation source="FieldText">Building has Atria [d:gt/] 55ft?</xsd:documentation>
                                    <xsd:documentation source="CalculationsAndRules">User selects from list: Yes (result = true) No (result = false)</xsd:documentation>
                                 </xsd:annotation>
                              </xsd:element>
                              <xsd:element name="Ksky01_NotApplicableMessage" >
                                 <xsd:annotation>
                                    <xsd:documentation source="FieldText">Building has Atria [d:gt/] 55ft?</xsd:documentation>
                                    <xsd:documentation source="CalculationsAndRules">If KSky00_Adding16SkyMF == true
result = BlankGrayedOut
End If</xsd:documentation>
                                 </xsd:annotation>
                                 <xsd:simpleType>
                                    <xsd:restriction base="comp:NotApplicableMessage">
                                       <xsd:enumeration value="BlankGrayedOut"/>
                                    </xsd:restriction>
                                 </xsd:simpleType>
                              </xsd:element>
                           </xsd:choice>
                           <xsd:choice maxOccurs="1">
                              <xsd:element name="Ksky02_ExteriorSurfaceGrossArea" type="env:ExteriorSurfaceGrossArea" >
                                 <xsd:annotation>
                                    <xsd:documentation source="FieldText">Gross Exterior Roof Area (ft[d:sup]2[/d:sup])</xsd:documentation>
                                    <xsd:documentation source="CalculationsAndRules">User inputs value</xsd:documentation>
                                 </xsd:annotation>
                              </xsd:element>
                              <xsd:element name="Ksky02_NotApplicableMessage" >
                                 <xsd:annotation>
                                    <xsd:documentation source="FieldText">Gross Exterior Roof Area (ft[d:sup]2[/d:sup])</xsd:documentation>
                                    <xsd:documentation source="CalculationsAndRules">If KSky00_Adding16SkyMF == true
result = BlankGrayedOut
End If</xsd:documentation>
                                 </xsd:annotation>
                                 <xsd:simpleType>
                                    <xsd:restriction base="comp:NotApplicableMessage">
                                       <xsd:enumeration value="BlankGrayedOut"/>
                                    </xsd:restriction>
                                 </xsd:simpleType>
                              </xsd:element>
                           </xsd:choice>
                           <xsd:choice maxOccurs="1">
                              <xsd:element name="Ksky03_SkylightAreaLimit" type="env:SkylightAreaLimit" >
                                 <xsd:annotation>
                                    <xsd:documentation source="FieldText">Maximum Allowed Skylight Area[d:sup]1[/d:sup] (ft[d:sup]2[/d:sup])</xsd:documentation>
                                    <xsd:documentation source="CalculationsAndRules">Calculated field: 
If Ksky01_IsAtriumGT55Ft == true result = 0.10 x Ksky02_ExteriorSurfaceGrossArea; 
Else result = 0.05 x Ksky02_ExteriorSurfaceGrossArea</xsd:documentation>
                                 </xsd:annotation>
                              </xsd:element>
                              <xsd:element name="Ksky03_NotApplicableMessage" >
                                 <xsd:annotation>
                                    <xsd:documentation source="FieldText">Maximum Allowed Skylight Area[d:sup]1[/d:sup] (ft[d:sup]2[/d:sup])</xsd:documentation>
                                    <xsd:documentation source="CalculationsAndRules">If KSky00_Adding16SkyMF == true
result = BlankGrayedOut
End If</xsd:documentation>
                                 </xsd:annotation>
                                 <xsd:simpleType>
                                    <xsd:restriction base="comp:NotApplicableMessage">
                                       <xsd:enumeration value="BlankGrayedOut"/>
                                    </xsd:restriction>
                                 </xsd:simpleType>
                              </xsd:element>
                           </xsd:choice>
                           <xsd:choice maxOccurs="1">
                              <xsd:element name="Ksky04_TotalSkylightArea" type="env:TotalSkylightArea" >
                                 <xsd:annotation>
                                    <xsd:documentation source="FieldText">Total Skylight Area per Design (ft[d:sup]2[/d:sup])</xsd:documentation>
                                    <xsd:documentation source="CalculationsAndRules">User inputs value</xsd:documentation>
                                 </xsd:annotation>
                              </xsd:element>
                              <xsd:element name="Ksky04_NotApplicableMessage" >
                                 <xsd:annotation>
                                    <xsd:documentation source="FieldText">Total Skylight Area per Design (ft[d:sup]2[/d:sup])</xsd:documentation>
                                    <xsd:documentation source="CalculationsAndRules">If KSky00_Adding16SkyMF == true
result = BlankGrayedOut
End If</xsd:documentation>
                                 </xsd:annotation>
                                 <xsd:simpleType>
                                    <xsd:restriction base="comp:NotApplicableMessage">
                                       <xsd:enumeration value="BlankGrayedOut"/>
                                    </xsd:restriction>
                                 </xsd:simpleType>
                              </xsd:element>
                           </xsd:choice>
                           <xsd:element name="KskyFootNote1" minOccurs="0">
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText"></xsd:documentation>
                                 <xsd:documentation source="AdditionalRequirements">[d:i][d:sup]1[/d:sup]FOOTNOTES: 5% of total roof area allowed for areas other than atria [d:gt/] 55 ft.  10% allowed for atria [d:gt/] 55 ft.[/d:i]</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                        </xsd:sequence>
                     </xsd:complexType>
                  </xsd:element>
                  <xsd:element name="Kskyperf" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">[d:b]Skylights- U-factor, Solar Heat Gain Coefficient (SHGC), Visible Transmittance (VT)[/d:b]</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">This subsection is required If  K01Skylights_FenestrationTypeApplies  ==  true</xsd:documentation>
                     </xsd:annotation>
                     <xsd:complexType>
                        <xsd:sequence>
                           <xsd:element name="Kskyperf01_UseInAreaWeightedAveUfactor" type="comp:UseInAreaWeightedAveUfactor" >
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText">Calculate Area-Weighted Average U-factor for Skylights[d:sup]1[/d:sup]</xsd:documentation>
                                 <xsd:documentation source="CalculationsAndRules">User input: If checked result = true, Else result = false</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                           <xsd:element name="Kskyperf02_UseInAreaWeightedAveSHGC" type="comp:UseInAreaWeightedAveSHGC" >
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText">Calculate Area-Weighted Average SHGC for Skylights[d:sup]1[/d:sup]</xsd:documentation>
                                 <xsd:documentation source="CalculationsAndRules">User input: If checked result = true, Else result = false</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                           <xsd:element name="Kskyperf03_UseInAreaWeightedAveVT" type="comp:UseInAreaWeightedAveVT" >
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText">Calculate Area-Weighted Average VT for Skylights[d:sup]1[/d:sup]</xsd:documentation>
                                 <xsd:documentation source="CalculationsAndRules">User input: If checked result = true, Else result = false</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                           <xsd:element name="TableKskyperf" >
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText"></xsd:documentation>
                              </xsd:annotation>
                              <xsd:complexType>
                                 <xsd:sequence>
                                    <xsd:element name="Row" minOccurs="1" maxOccurs="unbounded">
                                       <xsd:complexType>
                                          <xsd:sequence>
                                             <xsd:element name="Kskyperf04_SkylightName" type="env:SkylightName" >
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">Tag/Plan Detail ID</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">User input</xsd:documentation>
                                                </xsd:annotation>
                                             </xsd:element>
                                             <xsd:element name="Kskyperf05_FenestrationProductType" >
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">Fenestration Type</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">User selects from list: SkylightGlassCurb display term Glass, curb mounted, SkylightGlassDeck display term Glass, deck mounted, SkylightPlasticCurb display term Plastic, curb mounted, ChromogenicFen display term Chromogenic Glazing, SkylightTubular display term Tubular Daylight Devices</xsd:documentation>
                                                </xsd:annotation>
                                                <xsd:simpleType>
                                                   <xsd:restriction base="env:FenestrationProductType">
                                                      <xsd:enumeration value="ChromogenicFen"/>
                                                      <xsd:enumeration value="SkylightGlassCurb"/>
                                                      <xsd:enumeration value="SkylightGlassDeck"/>
                                                      <xsd:enumeration value="SkylightPlasticCurb"/>
                                                      <xsd:enumeration value="SkylightTubular"/>
                                                   </xsd:restriction>
                                                </xsd:simpleType>
                                             </xsd:element>
                                             <xsd:element name="Kskyperf06Occup" >
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText"></xsd:documentation>
                                                </xsd:annotation>
                                                <xsd:complexType>
                                                   <xsd:choice >
                                                      <xsd:element name="Kskyperf06" >
                                                         <xsd:annotation>
                                                            <xsd:documentation source="FieldText">Occupancy &amp; Status</xsd:documentation>
                                                            <xsd:documentation source="CalculationsAndRules">If  Kskyperf05_FenestrationProductType == ChromogenicFen result = BlankGrayedOut stored in NotApplicableMessage;  
 
Else User selects from customized dropdown list generated from the display terms for the two properties Kskyperf06_TopLevelOccupancy and  Kskyperf06_FenestrationModification</xsd:documentation>
                                                         </xsd:annotation>
                                                         <xsd:complexType>
                                                            <xsd:sequence>
                                                               <xsd:element name="Kskyperf06_TopLevelOccupancy" >
                                                                  <xsd:annotation>
                                                                     <xsd:documentation source="FieldText"></xsd:documentation>
                                                                     <xsd:documentation source="CalculationsAndRules">User selects occupancy type with the appropriate status value from list customized as follows:
 
If Kskyperf05_FenestrationProductType == SkylightTubular 
list Nonresidential display term Nonresidential/ Relocatable 1 CZ:  3 times, once for each of these Kskyperf06_FenestrationModification values: AllNew,, AddGT50ft2 and AddLTE50ft2
 
Else 
list 3 times, once for each of these Kskyperf06_FenestrationModification values: AllNew,, AddGT50ft2 and AddLTE50ft2:
Nonresidential display term Nonresidential/ Relocatable 1 CZ,  HotelMotel display term Hotel/ Motel, RelocableSchoolBuildingAllZones display term Relocatable all CZ, 
And
list  HighriseResidential display term Dwelling Units [d:lte/]3 stories:
3 times,once with each of these Kskyperf06_FenestrationModification value display terms appended: AllNew, ReplaceSkyMF and AddSky16PlusMF
And
list only 1 time OtherOccupancy with no display term  with Kskyperf06_FenestrationModification value NewSky16MF
End If</xsd:documentation>
                                                                     <xsd:appinfo source="displayterm">
                                                                        <dtyp:displayterm value="HotelMotel">High-rise Hotel/ Motel: </dtyp:displayterm>
                                                                        <dtyp:displayterm value="Nonresidential">Nonresidential/ Relocatable 1 CZ: </dtyp:displayterm>
                                                                        <dtyp:displayterm value="RelocableSchoolBuildingAllZones">Relocatable all CZ: </dtyp:displayterm>
                                                                        <dtyp:displayterm value="HighriseResidential">Dwelling Units [d:gt/]3 stories: </dtyp:displayterm>
                                                                        <dtyp:displayterm value="OtherOccupancy"></dtyp:displayterm>
                                                                     </xsd:appinfo>
                                                                     
                                                                  </xsd:annotation>
                                                                  <xsd:simpleType>
                                                                     <xsd:restriction base="com:TopLevelOccupancy">
                                                                        <xsd:enumeration value="HighRiseResidential"/>
                                                                        <xsd:enumeration value="HotelMotel"/>
                                                                        <xsd:enumeration value="Nonresidential"/>
                                                                        <xsd:enumeration value="OtherOccupancy"/>
                                                                        <xsd:enumeration value="RelocableSchoolBuildingAllZones"/>
                                                                     </xsd:restriction>
                                                                  </xsd:simpleType>
                                                               </xsd:element>
                                                               <xsd:element name="Kskyperf06_FenestrationModification" >
                                                                  <xsd:annotation>
                                                                     <xsd:documentation source="FieldText"></xsd:documentation>
                                                                     <xsd:documentation source="CalculationsAndRules">User selects status value with the appropriate occupancy type from list:  AllNew display term New, AddGT50ft2 display term  Alt. (Add/Replacement [d:gt/] 50ft[d:sup]2[/d:sup]), AddLTE50ft2 display term  Alt. (Add/Replacement  [d:lte/] 50ft[d:sup]2[/d:sup]), NewSky16MF display term [d:lte/] 16 ft[d:sup]2[/d:sup] new skylight per dwelling unit, ReplaceSkyMF display term Alt. (Replacement) and AddSky16PlusMF display term Alt. (Adding [d:gt/] 16 ft[d:sup]2[/d:sup])</xsd:documentation>
                                                                     <xsd:appinfo source="displayterm">
                                                                        <dtyp:displayterm value="AddGT50ft2">Alt. (Add/Replacement [d:gt/] 50ft[d:sup]2[/d:sup]) </dtyp:displayterm>
                                                                        <dtyp:displayterm value="AddLTE50ft2">Alt. (Add/Replacement [d:lte/] 50ft[d:sup]2[/d:sup])</dtyp:displayterm>
                                                                        <dtyp:displayterm value="AllNew">New </dtyp:displayterm>
                                                                        <dtyp:displayterm value="ReplaceSkyMF">Alt. (Replacement) </dtyp:displayterm>
                                                                        <dtyp:displayterm value="NewSky16MF">[d:lte/] 16 ft[d:sup]2[/d:sup] new skylight per dwelling unit</dtyp:displayterm>
                                                                     </xsd:appinfo>
                                                                     
                                                                  </xsd:annotation>
                                                                  <xsd:simpleType>
                                                                     <xsd:restriction base="env:FenestrationModification">
                                                                        <xsd:enumeration value="AddGT50ft2"/>
                                                                        <xsd:enumeration value="AddLTE50ft2"/>
                                                                        <xsd:enumeration value="AddSky16PlusMF"/>
                                                                        <xsd:enumeration value="AllNew"/>
                                                                        <xsd:enumeration value="NewSky16MF"/>
                                                                        <xsd:enumeration value="ReplaceSkyMF"/>
                                                                     </xsd:restriction>
                                                                  </xsd:simpleType>
                                                               </xsd:element>
                                                            </xsd:sequence>
                                                         </xsd:complexType>
                                                      </xsd:element>
                                                      <xsd:element name="Kskyperf06_NotApplicableMessage" >
                                                         <xsd:annotation>
                                                            <xsd:documentation source="FieldText">Occupancy &amp; Status</xsd:documentation>
                                                            <xsd:documentation source="CalculationsAndRules">If  Kskyperf05_FenestrationProductType == ChromogenicFen result = BlankGrayedOut stored in NotApplicableMessage
End If</xsd:documentation>
                                                         </xsd:annotation>
                                                         <xsd:simpleType>
                                                            <xsd:restriction base="comp:NotApplicableMessage">
                                                               <xsd:enumeration value="BlankGrayedOut"/>
                                                            </xsd:restriction>
                                                         </xsd:simpleType>
                                                      </xsd:element>
                                                   </xsd:choice>
                                                </xsd:complexType>
                                             </xsd:element>
                                             <xsd:choice maxOccurs="1">
                                                <xsd:element name="Kskyperf07_FenestrationComplianceCalc" type="comp:FenestrationComplianceCalc" >
                                                   <xsd:annotation>
                                                      <xsd:documentation source="FieldText">Calculation Method for Performance Values per Design[d:sup]2[/d:sup]</xsd:documentation>
                                                      <xsd:documentation source="CalculationsAndRules">User selects from list: NFRC_Certified display term NFRC Certified, NA6DefaultCOG display term NA6 Default (COG)[d:sup]2[/d:sup], Table110_6Defaults display term [d:t24ref/]110.6 Defaults
 
Mandatory Measure Triggers:
If result == NFRC_Certified 
MM110_6a5_MeasureApplies  = true and is displayed
Elsel If result == Table110_6Defaults 
MM110_6b_MeasureApplies  = true and is displayed
End If</xsd:documentation>
                                                      <xsd:appinfo source="displayterm">
                                                         <dtyp:displayterm value="NA6DefaultCOG"> NA6 Default (COG)[d:sup]2[/d:sup]
</dtyp:displayterm>
                                                      </xsd:appinfo>
                                                      
                                                   </xsd:annotation>
                                                </xsd:element>
                                                <xsd:element name="Kskyperf07_NotApplicableMessage" >
                                                   <xsd:annotation>
                                                      <xsd:documentation source="FieldText">Calculation Method for Performance Values per Design[d:sup]2[/d:sup]</xsd:documentation>
                                                      <xsd:documentation source="CalculationsAndRules">If  Kskyperf05_FenestrationProductType == ChromogenicFen result = BlankGrayedOut
End If</xsd:documentation>
                                                   </xsd:annotation>
                                                   <xsd:simpleType>
                                                      <xsd:restriction base="comp:NotApplicableMessage">
                                                         <xsd:enumeration value="BlankGrayedOut"/>
                                                      </xsd:restriction>
                                                   </xsd:simpleType>
                                                </xsd:element>
                                             </xsd:choice>
                                             <xsd:choice maxOccurs="1">
                                                <xsd:element name="Kskyperf08_Diffusing" type="com:Diffusing" >
                                                   <xsd:annotation>
                                                      <xsd:documentation source="FieldText">Glaze/ Diffuser with Haze Value [d:gt/] 90%?</xsd:documentation>
                                                      <xsd:documentation source="CalculationsAndRules">User selects from list: Yes (result = true) and NA: No direct sun on skylight (result = false)</xsd:documentation>
