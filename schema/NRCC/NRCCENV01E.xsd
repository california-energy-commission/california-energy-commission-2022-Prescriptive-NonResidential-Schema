                                                      <xsd:documentation source="CalculationsAndRules">If Kvertperf05_FenestrationProductType == GlazedDoor
result = 0.45 
Else If Kvertperf06_FenestrationModification == AllNew, AddGT50ft2 Or AddLTE50ft2  
If Kvertperf06_TopLevelOccupancy == Nonresidential
Switch (Kvertperf05_FenestrationProductType) 
case FixedWindow: 
If A03_ClimateZoneNumber == InRange (1,8) Or 10 Or 16
 result = 0.36; 
Else If A03_ClimateZoneNumber == 9 Or InRange (11, 15)
 result = 0.34; 
End If
 
case OperableWindow:  
result = 0.46;  
case StoreFront:  case CurtainWall: case DemisingWallFen:
If A03_ClimateZoneNumber == 1 Or 7
resut = 0.38
Else
result = 0.41; 
End If
End Switch
 
Else If Kvertperf06_TopLevelOccupancy == HotelMotel 
Switch (Kvertperf05_FenestrationProductType) 
case FixedWindow: 
result = 0.36
case OperableWindow:  
result = 0.46;  
case StoreFront:  case CurtainWall: case DemisingWallFen:
result = 0.41
End Switch
 
Else If Kvertperf06_TopLevelOccupancy == RelocableSchoolBuildingAllZones result = 0.47; 
 
Else  If Kvertperf06_TopLevelOccupancy == HighriseResidential
If Kvertperf05_FenestrationProductType == StoreFront Or CurtainWall Or DemisingWallFen
If A03_ClimateZoneNumber == 1 Or 16
resut = 0.38
Else 
result = 0.41; 
End If
Else  If Kvertperf05_FenestrationProductType != StoreFront And != CurtainWall And != DemisingWallFen
If A03_ClimateZoneNumber == InRange (1,6) Or InRange(9, 16)
 result = 0.30; 
Else If A03_ClimateZoneNumber == 7 Or 8
 result = 0.34; 
End If
End if
 
Else If Kvertperf06_FenestrationModification == ReplaceGT150ft2 Or ReplaceLTE150ft2 
If Kvertperf06_TopLevelOccupancy != HighriseResidential
If A03_ClimateZoneNumber == 1, 2, 4 Or InRange (6, 16) result = 0.47; 
Else If A03_ClimateZoneNumber == 3 Or 5 result = 0.58;
End If
Else If Kvertperf06_TopLevelOccupancy == HighriseResidential
If Kvertperf05_FenestrationProductType == StoreFront Or CurtainWall Or DemisingWallFen
If Kvertperf07_ComplianceMethodSHGC_VT == Table170_2A Or Table180_2B
If A03_ClimateZoneNumber == 1 Or 16
resut = 0.38
Else
result = 0.41; 
End If
End If
 
Else Kvertperf05_FenestrationProductType != StoreFront Or CurtainWall Or DemisingWallFen If Kvertperf07_ComplianceMethodSHGC_VT == Table170_2A
If A03_ClimateZoneNumber == InRange (1,6) Or InRange(9, 16)
 result = 0.30; 
Else If A03_ClimateZoneNumber == 7 Or 8
result = 0.34; 
End If
End If
Else If Kvertperf07_ComplianceMethodSHGC_VT == Table180_2B
If A03_ClimateZoneNumber == InRange (1,6) Or InRange(8, 16)
 result = 0.30; 
Else If A03_ClimateZoneNumber == 7
result = 0.34; 
End If
 
End If
End If</xsd:documentation>
