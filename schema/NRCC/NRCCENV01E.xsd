                  <xsd:element name="Hframed" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">[d:b]Framed Walls [/d:b]</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">This subsection is required If H01Framed_WallTypeApplies == true</xsd:documentation>
                     </xsd:annotation>
                     <xsd:complexType>
                        <xsd:sequence>
                           <xsd:element name="Hframed01_UseInAreaWeightedAveUfactor" type="comp:UseInAreaWeightedAveUfactor" >
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText">Calculate Area-Weighted Average U-factor for Metal Framed Walls [d:sup]1[/d:sup]</xsd:documentation>
                                 <xsd:documentation source="CalculationsAndRules">User input: If checked result = true; Else result = false.  If result == true   D03_ExceptionalConditionMessageCCENVE += WallAreaWeightedAveInH</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                           <xsd:element name="Hframed02_UseInAreaWeightedAveUfactor" type="comp:UseInAreaWeightedAveUfactor" >
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText">Include Wood Framed Walls in Area-Weighted Average U-factor Calculation [d:sup]1[/d:sup]</xsd:documentation>
                                 <xsd:documentation source="CalculationsAndRules">User input: If checked result = true; Else result = false.  If result == true   D03_ExceptionalConditionMessageCCENVE += WallAreaWeightedAveInH</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                           <xsd:element name="TableHframed" >
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText"></xsd:documentation>
                              </xsd:annotation>
                              <xsd:complexType>
                                 <xsd:sequence>
                                    <xsd:element name="Row" minOccurs="1" maxOccurs="unbounded">
                                       <xsd:complexType>
                                          <xsd:sequence>
                                             <xsd:element name="Hframed03_WallAssemblyName" type="env:WallAssemblyName" >
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">Tag/Plan Detail ID</xsd:documentation>
                                                </xsd:annotation>
                                             </xsd:element>
                                             <xsd:element name="Hframed04a_TopLevelOccupancy" >
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">Occupancy &amp; Status</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">User selects from list: Nonresidential display term Nonresidential/ Relocatable 1 CZ: New, HotelMotel  display term Hotel/ Motel: New, RelocableSchoolBuildingAllZones display term Relocatable all CZ: New,  AlteredOccupancy display term Any occupancy: Altered, and ResidentialMF  display term Multifamily: New</xsd:documentation>
                                                   <xsd:appinfo source="displayterm">
                                                      <dtyp:displayterm value="Nonresidential">Nonresidential/ Relocatable 1 CZ: New</dtyp:displayterm>
                                                      <dtyp:displayterm value="HotelMotel">Hotel/Motel: New</dtyp:displayterm>
                                                      <dtyp:displayterm value="RelocableSchoolBuildingAllZones">Relocatable all CZ: New</dtyp:displayterm>
                                                      <dtyp:displayterm value="ResidentialMF">Multifamily: New</dtyp:displayterm>
                                                      <dtyp:displayterm value="AlteredOccupancy">Any occupancy: Altered</dtyp:displayterm>
                                                   </xsd:appinfo>
                                                   
                                                </xsd:annotation>
                                                <xsd:simpleType>
                                                   <xsd:restriction base="com:TopLevelOccupancy">
                                                      <xsd:enumeration value="AlteredOccupancy"/>
                                                      <xsd:enumeration value="HotelMotel"/>
                                                      <xsd:enumeration value="Nonresidential"/>
                                                      <xsd:enumeration value="RelocableSchoolBuildingAllZones"/>
                                                      <xsd:enumeration value="ResidentialMF"/>
                                                   </xsd:restriction>
                                                </xsd:simpleType>
                                             </xsd:element>
                                             <xsd:element name="Hframed04b_Status" >
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">NOT REPORTED IN PDF</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">Calculatedf field: If Hframed04a_TopLevelOccupancy == AlteredOccupancy  result = ExistingAlteration; 
Else result = New
End If</xsd:documentation>
                                                </xsd:annotation>
                                                <xsd:simpleType>
                                                   <xsd:restriction base="com:Status">
                                                      <xsd:enumeration value="ExistingAlteration"/>
                                                      <xsd:enumeration value="New"/>
                                                   </xsd:restriction>
                                                </xsd:simpleType>
                                             </xsd:element>
                                             <xsd:choice maxOccurs="1">
                                                <xsd:element name="Hframed05_UfactorSpecSource" type="comp:UfactorSpecSource" >
                                                   <xsd:annotation>
                                                      <xsd:documentation source="FieldText">How Design U-factor was determined</xsd:documentation>
                                                      <xsd:documentation source="CalculationsAndRules">If Hframed10_ConstructionSpecificationMethod == RValue result = BlankGrayedOut stored in NotApplicableMessage; 
Else User selects from list: JA4Tables display term JA4 Tables, JA4Other display term Other per JA4.1.2.1, ApprovedSoftware display term Approved Software. 
End If
 
 If result == JA4Other   Or ApprovedSoftware D03_ExceptionalConditionMessageCCENVE += AdditionalDocJA4OtherSoftware</xsd:documentation>
                                                   </xsd:annotation>
                                                </xsd:element>
                                                <xsd:element name="Hframed05_NotApplicableMessage" >
                                                   <xsd:annotation>
                                                      <xsd:documentation source="FieldText">How Design U-factor was determined</xsd:documentation>
                                                      <xsd:documentation source="CalculationsAndRules">If Hframed10_ConstructionSpecificationMethod == RValue result = BlankGrayedOut stored in NotApplicableMessage;
End If</xsd:documentation>
                                                   </xsd:annotation>
                                                   <xsd:simpleType>
                                                      <xsd:restriction base="comp:NotApplicableMessage">
                                                         <xsd:enumeration value="BlankGrayedOut"/>
                                                      </xsd:restriction>
                                                   </xsd:simpleType>
                                                </xsd:element>
                                             </xsd:choice>
                                             <xsd:element name="Hframed06_WallType" >
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">Location/ Fire Rating</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">User selects from list customized as follows: 
If Hframed04a_TopLevelOccupancy != ResidentialMF list
ExteriorWall display term Exterior, DemisingWall display term Demising
Else If Hframed04a_TopLevelOccupancy == ResidentialMF list
ExteriorWall display term Exterior greater than 1hr fire rating,  WallTypeOther display term Exterior less than or equal to 1hr fire rating, DemisingWall display term Demising
End If
 
Mandatory Measure Triggers:
If result == DemisingWall and HFramed07_PartitionFramingMaterial == Metal 
MM120_7b7B_MeasureApplies = true and is displayed
Else If result == DemisingWall and HFramed07_PartitionFramingMaterial == WoodFraming Or Wood0_625Gypsum 
MM120_7b7A_MeasureApplies = true and is displayed
End If
 
If result == DemisingWall and HFramed07_PartitionFramingMaterial == Metal Then 
If HFramed07_FramingSize == 2x4 
MM120_7b7B1_MeasureApplies = true and is displayed
Else If HFramed07_FramingSize == 2x6 
MM120_7b7B2_MeasureApplies = true and is displayed
Else If HFramed07_FramingSize == 2x8 
MM120_7b7B3_MeasureApplies = true and is displayed
End If
End If
 
If result == DemisingWall and HFramed07_PartitionFramingMaterial == WoodFraming Or Wood0_625Gypsum Then 
If HFramed07_FramingSize == 2x4 
 
If HFramed07_FramingSpacing == 24InchOC
MM120_7b7A1_MeasureApplies = true and is displayed
Else If HFramed07_FramingSpacing == 16InchOC
MM120_7b7A2_MeasureApplies = true and is displayed
End If
 
Else If HFramed07_FramingSize == 2x6 Or 2x8 
MM120_7b7A3_MeasureApplies = true and is displayed
End If
End If</xsd:documentation>
                                                   <xsd:appinfo source="displayterm">
                                                      <dtyp:displayterm value="ExteriorWall">Exterior [d:gt/] 1hr fire rating</dtyp:displayterm>
                                                      <dtyp:displayterm value="WallTypeOther">Exterior [d:lte/] 1hr fire rating</dtyp:displayterm>
                                                   </xsd:appinfo>
                                                   
                                                </xsd:annotation>
                                                <xsd:simpleType>
                                                   <xsd:restriction base="env:WallType">
                                                      <xsd:enumeration value="DemisingWall"/>
                                                      <xsd:enumeration value="ExteriorWall"/>
                                                      <xsd:enumeration value="WallTypeOther"/>
                                                   </xsd:restriction>
                                                </xsd:simpleType>
                                             </xsd:element>
                                             <xsd:element name="Hframed06a_StructureFireRating" type="env:StructureFireRating" >
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">NOT REPORTED IN PDF</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">If Hframed04a_TopLevelOccupancy != ResidentialMF
result = RatingNA
Else If Hframed04a_TopLevelOccupancy == ResidentialMF
If Hframed06_WallType == ExteriorWall
result = GT1hr
Else If Hframed06_WallType  == WallTypeOther
result = LTE1hr
End If
End If</xsd:documentation>
                                                </xsd:annotation>
                                             </xsd:element>
                                             <xsd:element name="Hframed07Frame" >
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">Frame Material, Spacing  &amp; Depth</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">Dropdown list for 07 concatenates in this order the display terms from  Hframed07_PartitionFramingMaterial + Hframed07_FramingSpacing + Hframed07_FramingSize  e.g. Metal 16" OC 2x4, to list 18 combinations of these three properties</xsd:documentation>
                                                </xsd:annotation>
                                                <xsd:complexType>
                                                   <xsd:sequence>
                                                      <xsd:element name="Hframed07_PartitionFramingMaterial" >
                                                         <xsd:annotation>
                                                            <xsd:documentation source="FieldText"></xsd:documentation>
                                                            <xsd:documentation source="CalculationsAndRules">User selects from list: Metal display term Metal, WoodFraming display term Wood 1/2" gyp, Wood0_625Gypsum display term Wood 5/8" gyp that has the correct spacing and depth
 
Mandatory Measure Triggers:  
Multi-field triggers: See HFramed06_WallType  CalculationsAndRules for trigger values from this field</xsd:documentation>
                                                            <xsd:appinfo source="displayterm">
                                                               <dtyp:displayterm value="Metal"> Metal</dtyp:displayterm>
                                                               <dtyp:displayterm value="WoodFraming">Wood 1/2" gyp</dtyp:displayterm>
                                                               <dtyp:displayterm value="Wood0_625Gypsum"> Wood 5/8" gyp</dtyp:displayterm>
                                                            </xsd:appinfo>
                                                            
                                                         </xsd:annotation>
                                                         <xsd:simpleType>
                                                            <xsd:restriction base="env:PartitionFramingMaterial">
                                                               <xsd:enumeration value="Metal"/>
                                                               <xsd:enumeration value="Wood0_625Gypsum"/>
                                                               <xsd:enumeration value="WoodFraming"/>
                                                            </xsd:restriction>
                                                         </xsd:simpleType>
                                                      </xsd:element>
                                                      <xsd:element name="Hframed07_FramingSpacing" >
                                                         <xsd:annotation>
                                                            <xsd:documentation source="FieldText"></xsd:documentation>
                                                            <xsd:documentation source="CalculationsAndRules">User selects from list: 16InchOC display term 16" OC, 24InchOC display term 24" OC that has the correct material and depth
 
Mandatory Measure Triggers:  
Multi-field triggers:  See HFramed06_WallType CalculationsAndRules for trigger values from this field</xsd:documentation>
                                                            <xsd:appinfo source="displayterm">
                                                               <dtyp:displayterm value="16InchOC">16" OC</dtyp:displayterm>
                                                               <dtyp:displayterm value="24InchOC">24" OC</dtyp:displayterm>
                                                            </xsd:appinfo>
                                                            
                                                         </xsd:annotation>
                                                         <xsd:simpleType>
                                                            <xsd:restriction base="env:FramingSpacing">
                                                               <xsd:enumeration value="16InchOC"/>
                                                               <xsd:enumeration value="24InchOC"/>
                                                            </xsd:restriction>
                                                         </xsd:simpleType>
                                                      </xsd:element>
                                                      <xsd:element name="Hframed07_FramingSize" >
                                                         <xsd:annotation>
                                                            <xsd:documentation source="FieldText"></xsd:documentation>
                                                            <xsd:documentation source="CalculationsAndRules">User selects from list:  2x4, 2x6, 2x8 that has the correct material and spacing
 
Mandatory Measure Triggers:  
Multi-field triggers: See HFramed06_WallType CalculationsAndRules for trigger values from this field</xsd:documentation>
                                                         </xsd:annotation>
                                                         <xsd:simpleType>
                                                            <xsd:restriction base="env:FramingSize">
                                                               <xsd:enumeration value="2x4"/>
                                                               <xsd:enumeration value="2x6"/>
                                                               <xsd:enumeration value="2x8"/>
                                                            </xsd:restriction>
                                                         </xsd:simpleType>
                                                      </xsd:element>
                                                   </xsd:sequence>
                                                </xsd:complexType>
                                             </xsd:element>
                                             <xsd:choice maxOccurs="1">
                                                <xsd:element name="Hframed08_CavityInsulationNR" >
                                                   <xsd:annotation>
                                                      <xsd:documentation source="FieldText">Cavity Insulation per Design</xsd:documentation>
                                                      <xsd:documentation source="CalculationsAndRules">If Hframed05_UfactorSpecSource == JA4Other Or ApprovedSoftware this row is grayed out;  
Else If Hframed05_UfactorSpecSource == JA4Tables 
user selects from list customized as follows: 
 
If Hframed07_PartitionFramingMaterial == Metal,
Switch (Hframed07_FramingSize)
case 2x4: list: R0 (display term None), R5, R11, R13, R15;
case 2x6: list: R0 (display term None),  R19,  R20, R21;
case 2x8:  list: R0 (display term None),  R19, R22, R25, R30; 
End Switch;
 
Else If Hframed07_PartitionFramingMaterial == WoodFraming And Hframed07_FramingSize == 2x6
list: R0 (display term None), R19,  R20, R21, R22, R23, R25;
Else If Hframed07_PartitionFramingMaterial == Wood0_625Gypsum  And Hframed07_FramingSize == 2x6
list: R0 (display term None), R19,  R21, R22
End If;
 
If Hframed07_PartitionFramingMaterial == WoodFraming Or Wood0_625Gypsum
If Hframed07_FramingSize == 2x4
list R0 (display term None), R11, R13, R15 
Else If Hframed07_FramingSize == 2x8
list: R0 (display term None), R19, R22, R25, R30; 
End If
End If</xsd:documentation>
                                                   </xsd:annotation>
                                                   <xsd:simpleType>
                                                      <xsd:restriction base="env:CavityInsulationNR">
                                                         <xsd:enumeration value="R0"/>
                                                         <xsd:enumeration value="R11"/>
                                                         <xsd:enumeration value="R13"/>
                                                         <xsd:enumeration value="R15"/>
                                                         <xsd:enumeration value="R19"/>
                                                         <xsd:enumeration value="R20"/>
                                                         <xsd:enumeration value="R21"/>
                                                         <xsd:enumeration value="R22"/>
                                                         <xsd:enumeration value="R23"/>
                                                         <xsd:enumeration value="R25"/>
                                                         <xsd:enumeration value="R30"/>
                                                         <xsd:enumeration value="R5"/>
                                                      </xsd:restriction>
                                                   </xsd:simpleType>
                                                </xsd:element>
                                                <xsd:element name="Hframed08_CavityInsulationOther" type="com:CavityInsulationOther" >
                                                   <xsd:annotation>
                                                      <xsd:documentation source="FieldText">Cavity Insulation per Design</xsd:documentation>
                                                      <xsd:documentation source="CalculationsAndRules">If Hframed05_UfactorSpecSource == JA4Tables, this row is grayed out; 
Else If Hframed05_UfactorSpecSource == JA4Other Or ApprovedSoftware user  enters number for R-value
End If</xsd:documentation>
                                                   </xsd:annotation>
                                                </xsd:element>
                                             </xsd:choice>
                                             <xsd:choice maxOccurs="1">
                                                <xsd:element name="Hframed09_ContinuousInsulationNR" >
                                                   <xsd:annotation>
                                                      <xsd:documentation source="FieldText">Continuous Insulation per Design</xsd:documentation>
                                                      <xsd:documentation source="CalculationsAndRules">If Hframed05_UfactorSpecSource == JA4Other Or ApprovedSoftware this row is grayed out; 
Else If Hframed05_UfactorSpecSource == JA4Tables user selects from list customized as follows: 
 
If Hframed07_PartitionFramingMaterial == Metal, list: R0ci display term None, R2ci display term R-2, R4ci display term R-4, R5ci display term R-5, R6ci display term R-6, R7ci display term R-7, R8ci display term R-8, R10ci display term R-10, R12ci display term R-12, R14ci display term R-14, R15ci display term R-15; 
 
Else If Hframed07_PartitionFramingMaterial == WoodFraming, list: R0ci display term None, R2ci display term R-2, R4ci display term R-4, R5ci display term R-5, R6ci display term R-6, R7ci display term R-7, R8ci display term R-8, R10ci display term R-10, R12ci display term R-12, R15ci display term R-15; 
 
Else If Hframed07_PartitionFramingMaterial == Wood0_625Gypsum, list: R0ci display term None, R2ci display term R-2, R4ci display term R-4, R5ci display term R-5, R6ci display term R-6, R7ci display term R-7, R8ci display term R-8, R10ci display term R-10
End If
End If</xsd:documentation>
                                                   </xsd:annotation>
                                                   <xsd:simpleType>
                                                      <xsd:restriction base="env:ContinuousInsulationNR">
                                                         <xsd:enumeration value="R0ci"/>
                                                         <xsd:enumeration value="R10ci"/>
                                                         <xsd:enumeration value="R12ci"/>
                                                         <xsd:enumeration value="R14ci"/>
                                                         <xsd:enumeration value="R15ci"/>
                                                         <xsd:enumeration value="R2ci"/>
                                                         <xsd:enumeration value="R4ci"/>
                                                         <xsd:enumeration value="R5ci"/>
                                                         <xsd:enumeration value="R6ci"/>
                                                         <xsd:enumeration value="R7ci"/>
                                                         <xsd:enumeration value="R8ci"/>
                                                      </xsd:restriction>
                                                   </xsd:simpleType>
                                                </xsd:element>
                                                <xsd:element name="Hframed09_ContinuousInsulationOther" type="com:ContinuousInsulationOther" >
                                                   <xsd:annotation>
                                                      <xsd:documentation source="FieldText">Continuous Insulation per Design</xsd:documentation>
                                                      <xsd:documentation source="CalculationsAndRules">If Hframed05_UfactorSpecSource == JA4Tables, this row is grayed out; 
Else If Hframed05_UfactorSpecSource == JA4Other Or ApprovedSoftware user enters number for R-value
End If</xsd:documentation>
                                                   </xsd:annotation>
                                                </xsd:element>
                                                <xsd:element name="Hframed09_NotApplicableMessage" >
                                                   <xsd:annotation>
                                                      <xsd:documentation source="FieldText">Continuous Insulation per Design</xsd:documentation>
                                                      <xsd:documentation source="CalculationsAndRules">If Hframed10_ConstructionSpecificationMethod == RValue result = BlankGrayedOut stored in NotApplicableMessage and both rows for Hframed09 are grayed out;
End If</xsd:documentation>
                                                   </xsd:annotation>
                                                   <xsd:simpleType>
                                                      <xsd:restriction base="comp:NotApplicableMessage">
                                                         <xsd:enumeration value="BlankGrayedOut"/>
                                                      </xsd:restriction>
                                                   </xsd:simpleType>
                                                </xsd:element>
                                             </xsd:choice>
                                             <xsd:element name="Hframed10_ConstructionSpecificationMethod" >
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">Thermal Performance Unit</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">If Hframed04b_Status == ExistingAlteration  And Hframed01_UseInAreaWeightedAveUfactor  == false And Hframed02_UseInAreaWeightedAveUfactor  == false list RValue, UFactor ; 
Else result = UFactor and cannot be changed by user
End If</xsd:documentation>
                                                </xsd:annotation>
                                                <xsd:simpleType>
                                                   <xsd:restriction base="env:ConstructionSpecificationMethod">
                                                      <xsd:enumeration value="RValue"/>
                                                      <xsd:enumeration value="UFactor"/>
                                                   </xsd:restriction>
                                                </xsd:simpleType>
                                             </xsd:element>
                                             <xsd:choice maxOccurs="1">
                                                <xsd:element name="Hframed11_InsulationUFactorLimit" type="env:InsulationUFactorLimit" >
                                                   <xsd:annotation>
                                                      <xsd:documentation source="FieldText">Required Thermal Performance</xsd:documentation>
                                                      <xsd:documentation source="CalculationsAndRules">If Hframed04b_Status == ExistingAlteration And Hframed10_ConstructionSpecificationMethod == UFactor 
 If Hframed04a_TopLevelOccupancy == AlteredOccupancy Then 
 If Hframed07_PartitionFramingMaterial == WoodFraming Or Wood0_625Gypsum result = 0.110; 
 Else If Hframed07_PartitionFramingMaterial == Metal result = 0.217; 
End If 
End If
 
Else If Hframed04b_Status == New Then 
If Hframed06_WallType == DemisingWall 
If Hframed07_PartitionFramingMaterial == WoodFraming Or Wood0_625Gypsum result = 0.099; 
Else If Hframed07_PartitionFramingMaterial == Metal result = 0.151;
End if  
 
Else If Hframed06_WallType == ExteriorWall 
Switch (Hframed04a_TopLevelOccupancy) 
case  Nonresidential: 
If Hframed07_PartitionFramingMaterial == WoodFraming Or Wood0_625Gypsum 
If A03_ClimateZoneNumber == 1 result = 0.095; 
Else  If A03_ClimateZoneNumber == 2, 4, 9, 10, 12, 13, 14, Or 16 result = 0.059; 
Else If A03_ClimateZoneNumber == 3, 6, Or 7 result = 0.110; 
Else If A03_ClimateZoneNumber == 5 Or 8 result = 0.102; 
Else If A03_ClimateZoneNumber == 11 result = 0.045;
Else If A03_ClimateZoneNumber == 15 result = 0.042; 
End If       
Else If Hframed07_PartitionFramingMaterial == Metal  
If A03_ClimateZoneNumber == 1, 6 Or 7 result = 0.060; 
Else If A03_ClimateZoneNumber == 2, 4, 5, Or  InRange(8, 16) result = 0.055; 
Else If A03_ClimateZoneNumber == 3 result = 0.071; 
End If
End If  
 
case HotelMotel:
If Hframed07_PartitionFramingMaterial == WoodFraming Or Wood0_625Gypsum 
If A03_ClimateZoneNumber == InRange(1,10) Or 12 Or 13  result = 0.059
Else If A03_ClimateZoneNumber == 11 Or 14 Or 15 Or 16  result = 0.042
End If
Else If Hframed07_PartitionFramingMaterial == Metal  
If A03_ClimateZoneNumber == InRange(1,6) Or InRange(8,14) Or 16  result = 0.069
Else If A03_ClimateZoneNumber == 7  result = 0.105
Else If A03_ClimateZoneNumber == 15  result = 0.048
End If
End If
 
case RelocableSchoolBuildingAllZones:
If Hframed07_PartitionFramingMaterial == WoodFraming Or Wood0_625Gypsum 
result = 0.042
Else If Hframed07_PartitionFramingMaterial == Metal 
result = 0.057
End If
End If

case ResidentialMF:
If Hframed06a_StructureFireRating == GT1hr
If A03_ClimateZoneNumber == InRange(1,5) Or 8 Or 9 Or 10 Or 12 Or 13   result = 0.059
Else If A03_ClimateZoneNumber == 11 Or 14 Or 15 Or 16   result = 0.051
Else If A03_ClimateZoneNumber == 6 Or 7   result = 0.065
End If
Else If Hframed06a_StructureFireRating == LTE1hr
If A03_ClimateZoneNumber == InRange(1,5) Or InRange(8,16)  result = 0.051
Else If A03_ClimateZoneNumber == 6 Or 7   result = 0.065
End If
End If
End If
 
End Switch
 
</xsd:documentation>
                                                   </xsd:annotation>
                                                </xsd:element>
                                                <xsd:element name="Hframed11_InsulationRValueLimit" >
                                                   <xsd:annotation>
                                                      <xsd:documentation source="FieldText">Required Thermal Performance</xsd:documentation>
                                                      <xsd:documentation source="CalculationsAndRules">If Hframed04b_Status == ExistingAlteration And Hframed10_ConstructionSpecificationMethod == RValue 
If Hframed04a_TopLevelOccupancy == AlteredOccupancy  
If Hframed07_PartitionFramingMaterial == WoodFraming Or Wood0_625Gypsum result = R11; 
Else result = R13;
End If
End If
End If</xsd:documentation>
                                                   </xsd:annotation>
                                                   <xsd:simpleType>
                                                      <xsd:restriction base="env:InsulationRValueLimit">
                                                         <xsd:enumeration value="R11"/>
                                                         <xsd:enumeration value="R13"/>
                                                      </xsd:restriction>
                                                   </xsd:simpleType>
                                                </xsd:element>
                                             </xsd:choice>
                                             <xsd:element name="Hframed12UF" >
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">U-factor per Design</xsd:documentation>
                                                </xsd:annotation>
                                                <xsd:complexType>
                                                   <xsd:choice >
                                                      <xsd:element name="Hframed12_NotApplicableMessage" >
                                                         <xsd:annotation>
                                                            <xsd:documentation source="FieldText"></xsd:documentation>
                                                            <xsd:documentation source="CalculationsAndRules">If Hframed10_ConstructionSpecificationMethod == RValue result = BlankGrayedOut stored in NotApplicableMessage and all fields in Hframed12 are grayed out;
End If</xsd:documentation>
                                                         </xsd:annotation>
                                                         <xsd:simpleType>
                                                            <xsd:restriction base="comp:NotApplicableMessage">
                                                               <xsd:enumeration value="BlankGrayedOut"/>
                                                            </xsd:restriction>
                                                         </xsd:simpleType>
                                                      </xsd:element>
                                                      <xsd:element name="Hframed12JA4" >
                                                         <xsd:annotation>
                                                            <xsd:documentation source="FieldText">per JA4</xsd:documentation>
                                                         </xsd:annotation>
                                                         <xsd:complexType>
                                                            <xsd:choice >
                                                               <xsd:element name="Hframed12_UfactorJA431_16OC" type="comp:UfactorJA431_16OC" >
                                                                  <xsd:annotation>
                                                                     <xsd:documentation source="FieldText"></xsd:documentation>
