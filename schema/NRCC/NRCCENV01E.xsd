case R0: Rvalue = 1.3; case R1: Rvalue = 1.5;  case R2: Rvalue = 2.4;  case R3: Rvalue = 3.1; case R4: Rvalue = 3.8;  case R5: Rvalue = 4.4;  case R6: Rvalue = 4.9; case R7: Rvalue = 5.4; case R8: Rvalue = 5.8;  case R9:  Rvalue = 6.2; case R10: Rvalue = 6.5; case  R11: Rvalue = 6.8; case R12: Rvalue = 7.1;  
