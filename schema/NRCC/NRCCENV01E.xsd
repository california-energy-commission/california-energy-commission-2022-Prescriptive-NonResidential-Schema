                                                   <xsd:documentation source="FieldText">Radiant Barrier Req?</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">Calculated field: 
If FframedMF05_RoofType == AtticOptionB
If A03_ClimateZoneNumber == 2, 3 ,5, 6,Or 7
result = true display term Yes
Else result = false display term No
End If
Else If FframedMF05_RoofType == AtticOptionC
If A03_ClimateZoneNumber == InRnge (2, 15) 
result = true display term Yes
Else result = false display term No
End If
Else  result = false display term No
End If</xsd:documentation>
                                                </xsd:annotation>
                                             </xsd:element>
                                             <xsd:element name="FframedMF20_ConstructionSpecificationMethod" >
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">Thermal Performance Unit</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">Result = RValue and cannot be changed by user</xsd:documentation>
                                                </xsd:annotation>
                                                <xsd:simpleType>
                                                   <xsd:restriction base="env:ConstructionSpecificationMethod">
                                                      <xsd:enumeration value="RValue"/>
                                                   </xsd:restriction>
                                                </xsd:simpleType>
                                             </xsd:element>
                                             <xsd:element name="FframedMF21Label" minOccurs="0">
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">Minimum R-value Required [d:sup]4[/d:sup]</xsd:documentation>
                                                </xsd:annotation>
                                             </xsd:element>
                                             <xsd:choice maxOccurs="1">
                                                <xsd:element name="FframedMF21a_RoofBelowDeckInsulation" type="env:RoofBelowDeckInsulation" >
                                                   <xsd:annotation>
                                                      <xsd:documentation source="FieldText">Below Roof Deck [d:sup]5[/d:sup]</xsd:documentation>
                                                      <xsd:documentation source="CalculationsAndRules">If FframedMF05_RoofType == AtticOptionB
If A03_ClimateZoneNumber == 4 Or 8 Or 9 Or InRange(11,15)
result = 19
Else If  A03_ClimateZoneNumber == 10 Or 16
result = 13
Else If  A03_ClimateZoneNumber == 1 Or 2 Or 3 Or 5 Or 6 Or 7
result = BlankGrayedOut stored in NotApplicableMessage
End If
 
Else If FframedMF05_RoofType == NotVentedAttic 
If  FframedMF04_RoofSlope IsPresent And == LowSlope 
If  A03_ClimateZoneNumber == 1 Or 2 Or 4 Or InRange(8, 16)
result = 14
Else If  A03_ClimateZoneNumber == 3 Or 5 Or 6 Or 7
result = BlankGrayedOut stored in NotApplicableMessage
End If
End If
Else If FframedMF05_RoofType == AtticOptionC Or NonAtticOptionD Or VentedAttic
result = BlankGrayedOut stored in NotApplicableMessage
End If</xsd:documentation>
                                                   </xsd:annotation>
                                                </xsd:element>
                                                <xsd:element name="FframedMF21a_NotApplicableMessage" >
                                                   <xsd:annotation>
                                                      <xsd:documentation source="FieldText">Below Roof Deck [d:sup]5[/d:sup]</xsd:documentation>
                                                      <xsd:documentation source="CalculationsAndRules">If FframedMF05_RoofType == AtticOptionB
If A03_ClimateZoneNumber == 1 Or 2 Or 3 Or 5 Or 6 Or 7
result = BlankGrayedOut stored in NotApplicableMessage
End If
 
Else If FframedMF05_RoofType == NotVentedAttic 
If  FframedMF04_RoofSlope IsPresent And == LowSlope 
If  A03_ClimateZoneNumber ==  3 Or 5 Or 6 Or 7
result = BlankGrayedOut stored in NotApplicableMessage
End If
 
Else If FframedMF05_RoofType == AtticOptionC Or NonAtticOptionD Or VentedAttic Or RafterRoof
result = BlankGrayedOut stored in NotApplicableMessage
End If</xsd:documentation>
                                                   </xsd:annotation>
                                                   <xsd:simpleType>
                                                      <xsd:restriction base="comp:NotApplicableMessage">
                                                         <xsd:enumeration value="BlankGrayedOut"/>
                                                      </xsd:restriction>
                                                   </xsd:simpleType>
                                                </xsd:element>
                                             </xsd:choice>
                                             <xsd:choice maxOccurs="1">
                                                <xsd:element name="FframedMF21b_RoofAtCeilingInsulation" type="env:RoofAtCeilingInsulation" >
                                                   <xsd:annotation>
                                                      <xsd:documentation source="FieldText">At Ceiling</xsd:documentation>
                                                      <xsd:documentation source="CalculationsAndRules">If FframedMF05_RoofType == AtticOptionB
If A03_ClimateZoneNumber == 1 Or 2 Or 4 Or InRange(8,16)
result = 38
Else If  A03_ClimateZoneNumber == 3 Or 5 Or 6 Or 7
result = 30
End If
Else If FframedMF05_RoofType == AtticOptionC 
If A03_ClimateZoneNumber == 1 Or InRange(11,16)
result = 38
Else If  A03_ClimateZoneNumber == InRange (2, 10)
result = 30
End If
Else If  
FfamedMF05_RoofType == VentedAttic
If FframedMF04_Status IsPresent And == AddLTE700sf
If A03_ClimateZoneNumber == 1 Or 2 Or 4 Or InRange(8,16)
result = 38
Else If  A03_ClimateZoneNumber == 3 Or 5 Or 6 Or 7
result = 30
End If
Else If FframedMF04_RoofSlope IsPresent And == LowSlope Or SteepSlope
If A03_ClimateZoneNumber == InRange(1, 4) Or InRange(8,16)
result = 49
Else If  A03_ClimateZoneNumber == 5 Or 6 Or 7
result = BlankGrayedOut stored in NotApplicableMessage
End If
End If
Else If FframedMF05_RoofType == NonAtticOptionD Or RafterRoof Or NotVentedAttic
result = BlankGrayedOut stored in NotApplicableMessage
End If</xsd:documentation>
                                                   </xsd:annotation>
                                                </xsd:element>
                                                <xsd:element name="FframedMF21b_NotApplicableMessage" >
                                                   <xsd:annotation>
                                                      <xsd:documentation source="FieldText">At Ceiling</xsd:documentation>
                                                      <xsd:documentation source="CalculationsAndRules">If FframedMF04_RoofSlope IsPresent And == LowSlope Or SteepSlope
If  A03_ClimateZoneNumber == 5 Or 6 Or 7
result = BlankGrayedOut stored in NotApplicableMessage
End If
Else If FframedMF05_RoofType == NonAtticOptionD Or RafterRoof Or NotVentedAttic
result = BlankGrayedOut stored in NotApplicableMessage
End If</xsd:documentation>
                                                   </xsd:annotation>
                                                   <xsd:simpleType>
                                                      <xsd:restriction base="comp:NotApplicableMessage">
                                                         <xsd:enumeration value="BlankGrayedOut"/>
                                                      </xsd:restriction>
                                                   </xsd:simpleType>
                                                </xsd:element>
                                             </xsd:choice>
                                             <xsd:element name="FframedMF22Label" minOccurs="0">
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">R-value per Design</xsd:documentation>
                                                </xsd:annotation>
                                             </xsd:element>
                                             <xsd:choice maxOccurs="1">
                                                <xsd:element name="FframedMF22a_RoofBelowDeckInsulation" type="env:RoofBelowDeckInsulation" >
                                                   <xsd:annotation>
                                                      <xsd:documentation source="FieldText">Below Roof Deck</xsd:documentation>
                                                      <xsd:documentation source="CalculationsAndRules">If FframedMF21a_RoofBelowDeckInsulation  IsPresent 
User enters R-value for below roof deck insulation
End If</xsd:documentation>
                                                   </xsd:annotation>
                                                </xsd:element>
                                                <xsd:element name="FframedMF22a_NotApplicableMessage" >
                                                   <xsd:annotation>
                                                      <xsd:documentation source="FieldText">Below Roof Deck</xsd:documentation>
                                                      <xsd:documentation source="CalculationsAndRules">If FframedMF21a_NotApplicableMessage IsPresent and == BlankGrayedOut
result = FframedMF22a_NotApplicableMessage
End If</xsd:documentation>
                                                   </xsd:annotation>
                                                   <xsd:simpleType>
                                                      <xsd:restriction base="comp:NotApplicableMessage">
                                                         <xsd:enumeration value="BlankGrayedOut"/>
                                                      </xsd:restriction>
                                                   </xsd:simpleType>
                                                </xsd:element>
                                             </xsd:choice>
                                             <xsd:choice maxOccurs="1">
                                                <xsd:element name="FframedMF22b_RoofAtCeilingInsulation" type="env:RoofAtCeilingInsulation" >
                                                   <xsd:annotation>
                                                      <xsd:documentation source="FieldText">At Ceiling</xsd:documentation>
                                                      <xsd:documentation source="CalculationsAndRules">If   FframedMF21b_RoofAtCeilingInsulation IsPresent
User enters R-value for R-value at ceiling insulation</xsd:documentation>
                                                   </xsd:annotation>
                                                </xsd:element>
                                                <xsd:element name="FframedMF22b_NotApplicableMessage" >
                                                   <xsd:annotation>
                                                      <xsd:documentation source="FieldText">At Ceiling</xsd:documentation>
                                                      <xsd:documentation source="CalculationsAndRules">If FframedMF21b_NotApplicableMessage IsPresent and == BlankGrayedOut
result = FframedMF22a_NotApplicableMessage
End If</xsd:documentation>
                                                   </xsd:annotation>
                                                   <xsd:simpleType>
                                                      <xsd:restriction base="comp:NotApplicableMessage">
                                                         <xsd:enumeration value="BlankGrayedOut"/>
                                                      </xsd:restriction>
                                                   </xsd:simpleType>
                                                </xsd:element>
                                             </xsd:choice>
                                          </xsd:sequence>
                                       </xsd:complexType>
                                    </xsd:element>
                                 </xsd:sequence>
                              </xsd:complexType>
                           </xsd:element>
                           <xsd:element name="FframedMFFootnote4" minOccurs="0">
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText"></xsd:documentation>
                                 <xsd:documentation source="AdditionalRequirements">[d:i] [d:sup]3[/d:sup] When R-value is used for low-sloped roof alterations, the value shown here is for continuous insulation per [d:t24refnolink/]180.2(b)1Aiii.[/d:i]</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                           <xsd:element name="FframedMFFootnote5" minOccurs="0">
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText"></xsd:documentation>
                                 <xsd:documentation source="AdditionalRequirements">[d:i] [d:sup]4[/d:sup] Install the specified R-value with an air space present between the roofing and the roof deck.  Such as standard installation of concrete or clay tile.  R-values shown  are for wood-frame construction with insulation installed between the framing members. Alternatives including insulation above rafters or above roof deck shall comply with the performance standards.[/d:i]</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                        </xsd:sequence>
                     </xsd:complexType>
                  </xsd:element>
                  <xsd:element name="FCalc1UF" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">[d:b]Area-Weighted Average U-factor Compliance Calculation for Framed/ SIPs/ Span Deck &amp; Concrete/ Metal Panel Roofs[/d:b]</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">This subsection is required If  Fframed01_UseInAreaWeightedAveUfactor,  FSIPS01_UseInAreaWeightedAveUfactor, Fspan01_UseInAreaWeightedAveUfactor Or Fmetalp01_UseInAreaWeightedAveUfactor  == true</xsd:documentation>
                     </xsd:annotation>
                     <xsd:complexType>
                        <xsd:sequence>
                           <xsd:element name="TableFCalc1UF" >
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText"></xsd:documentation>
                              </xsd:annotation>
                              <xsd:complexType>
                                 <xsd:sequence>
                                    <xsd:element name="Row" minOccurs="1" maxOccurs="unbounded">
                                       <xsd:complexType>
                                          <xsd:sequence>
                                             <xsd:element name="FCalc1UF01_AreaWeightAvgType" >
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">Roof Type</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">Calculated field: If any of the following conditions are true, a row is added and the result loaded into this field: 
 If Fframed01_UseInAreaWeightedAveUfactor == true  result = Framed;  
If  FSIPS01_UseInAreaWeightedAveUfactor == true, result = SIPS; 
If Fspan01_UseInAreaWeightedAveUfactor ==  true result = Concrete;  
If Fmetalp01_UseInAreaWeightedAveUfactor  == true; result = MetalPanel</xsd:documentation>
                                                </xsd:annotation>
                                                <xsd:simpleType>
                                                   <xsd:restriction base="comp:AreaWeightAvgType">
                                                      <xsd:enumeration value="Concrete"/>
                                                      <xsd:enumeration value="Framed"/>
                                                      <xsd:enumeration value="MetalPanel"/>
                                                      <xsd:enumeration value="SIPS"/>
                                                   </xsd:restriction>
                                                </xsd:simpleType>
                                             </xsd:element>
                                             <xsd:element name="FCalc1UF02_RoofTypeTotalArea" type="comp:RoofTypeTotalArea" >
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">Total Area of Roof Type (ft[d:sup]2[/d:sup])</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">Calculated field: For current row, 
Switch (FCalc1UF01_AreaWeightAvgRoofType) 
case Framed: result = SUM ALL (Fframed16_RoofNetArea);  
case SIPS:  result = SUM ALL (FSIPS16_RoofNetArea); 
case Concrete:  result = SUM ALL (Fspan15_RoofNetArea); 
case MetalPanel:  result = SUM ALL (Fmetalp13_RoofNetArea).
End Switch</xsd:documentation>
                                                </xsd:annotation>
                                             </xsd:element>
                                             <xsd:element name="FCalc1UF03to04Label" minOccurs="0">
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">Area-weighted U-factor for Roof Type</xsd:documentation>
                                                </xsd:annotation>
                                             </xsd:element>
                                             <xsd:element name="FCalc1UF03_AreaWeightAvgRequiredUfactor" type="comp:AreaWeightAvgRequiredUfactor" >
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">Required</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">Calculated field: 
Switch ( FCalc1UF01_AreaWeightAvgRoofType) 
case Framed: result = (SUM ALL (Fframed16_RoofNetArea x Fframed14_InsulationUFactorLimit)) / FCalc1UF02_RoofTypeTotalArea; 
 
case SIPS: (SUM ALL (FSIPS16_RoofNetArea x FSIPS14_InsulationUFactorLimit)) / FCalc1UF02_RoofTypeTotalArea;  
 
case Concrete: (SUM ALL (Fspan15_RoofNetArea x Fspan13_InsulationUFactorLimit)) / FCalc1UF02_RoofTypeTotalArea; 
 
case MetalPanel: (SUM ALL (Fmetalp13_RoofNetArea x Fmetalp11_InsulationUFactorLimit)) / FCalc1UF02_RoofTypeTotalArea;
End Switch</xsd:documentation>
                                                </xsd:annotation>
                                             </xsd:element>
                                             <xsd:choice maxOccurs="1">
                                                <xsd:element name="FCalc1UF04Framed_AreaWeightAvgUfactor" type="comp:AreaWeightAvgUfactor" >
                                                   <xsd:annotation>
                                                      <xsd:documentation source="FieldText">Designed</xsd:documentation>
                                                      <xsd:documentation source="CalculationsAndRules">Calculated field: 
 
If FCalc1UF01_AreaWeightAvgRoofType == Framed result= (SUM ALL (Fframed16_RoofNetArea x Ufactor) ) / FCalc1UF02_RoofTypeTotalArea.  
End if
 
Ufactor for row is the property that IsPresent: 
Fframed15_UfactorJA4Other Or  
Fframed15_UfactorJA421_16OC Or  
Fframed15_UfactorJA421_24OC Or 
Fframed15_UfactorJA422_16OC Or 
Fframed15_UfactorJA422_24OC Or 
Fframed15_UfactorJA424_16OC or 
Fframed15_UfactorJA424_24OC Or 
Fframed15_UfactorJA425_16OC Or 
Fframed15_UfactorJA425_24OC</xsd:documentation>
                                                   </xsd:annotation>
                                                </xsd:element>
                                                <xsd:element name="FCalc1UF04SIPS_AreaWeightAvgUfactor" type="comp:AreaWeightAvgUfactor" >
                                                   <xsd:annotation>
                                                      <xsd:documentation source="FieldText">Designed</xsd:documentation>
                                                      <xsd:documentation source="CalculationsAndRules">Calculated field: 
 
If FCalc1UF01_AreaWeightAvgRoofType == SIPS result= (SUM ALL (FSIPS16_RoofNetArea x Ufactor) ) / FCalc1UF02_RoofTypeTotalArea.  
End if
 
Ufactor for row is the property that IsPresent: 
FSIPS15_UfactorJA4Other Or 
FSIPS15_UfactorJA423OSB Or 
FSIPS15_UfactorJA423Single or 
FSIPS15_UfactorJA423Double Or 
FSIPS15_UfactorJA423Ijoist Or 
FSIPS15_UfactorJA423Steel</xsd:documentation>
                                                   </xsd:annotation>
                                                </xsd:element>
                                                <xsd:element name="FCalc1UF04Span_AreaWeightAvgUfactor" type="comp:AreaWeightAvgUfactor" >
                                                   <xsd:annotation>
                                                      <xsd:documentation source="FieldText">Designed</xsd:documentation>
                                                      <xsd:documentation source="CalculationsAndRules">Calculated field: 
 
If FCalc1UF01_AreaWeightAvgRoofType == Concrete result= (SUM ALL (Fspan15_RoofNetArea x Ufactor) ) / FCalc1UF02_RoofTypeTotalArea.  
End If
Ufactor for row is the property that IsPresent: 
Fspan14_UfactorJA4Other Or 
Fspan14_UfactorJA426FireProof Or  
Fspan14_UfactorJA426NoFireProof</xsd:documentation>
                                                   </xsd:annotation>
                                                </xsd:element>
                                                <xsd:element name="FCalc1UF04Metalp_AreaWeightAvgUfactor" type="comp:AreaWeightAvgUfactor" >
                                                   <xsd:annotation>
                                                      <xsd:documentation source="FieldText">Designed</xsd:documentation>
                                                      <xsd:documentation source="CalculationsAndRules">Calculated field: 
 
If FCalc1UF01_AreaWeightAvgRoofType == MetalPanel result= (SUM ALL (Fmetalp13_RoofNetArea x Ufactor) ) / FCalc1UF02_RoofTypeTotalArea.  
End If
 
Ufactor for row is the property that IsPresent: 
Fmetalp12_ UfactorJA4Other Or 
Fmetalp12_UfactorJA429</xsd:documentation>
                                                   </xsd:annotation>
                                                </xsd:element>
                                             </xsd:choice>
                                          </xsd:sequence>
                                       </xsd:complexType>
                                    </xsd:element>
                                 </xsd:sequence>
                              </xsd:complexType>
                           </xsd:element>
                           <xsd:element name="FCalc1UF02to04TotalsLabel" minOccurs="0">
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText">[d:b]Total for all Roof Types:[/d:b]</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                           <xsd:element name="FCalc1UF02Totals_AreaWeightAvgTotalArea" type="comp:AreaWeightAvgTotalArea" >
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText"></xsd:documentation>
                                 <xsd:documentation source="CalculationsAndRules">Calculated field: result = SUM ALL (FCalc1UF02_RoofTypeTotalArea)</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                           <xsd:element name="FCalc1UF03Totals_AreaWeightAvgUfactorRequiredAll" type="comp:AreaWeightAvgUfactorRequiredAll" >
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText"></xsd:documentation>
                                 <xsd:documentation source="CalculationsAndRules">Calculated field: result = (SUM ALL ( FCalc1UF02_RoofTypeTotalArea x FCalc1UF03_AreaWeightAvgUfactorRequired)) / FCalc1UF02Totals_AreaWeightAvgTotalArea</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                           <xsd:element name="FCalc1UF04Totals_AreaWeightAvgUfactorAll" type="comp:AreaWeightAvgUfactorAll" >
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText"></xsd:documentation>
                                 <xsd:documentation source="CalculationsAndRules">Calculated field: result = (SUM ALL ( FCalc1UF02_RoofTypeTotalArea x FCalc1UF04_AreaWeightAvgUfactor)) / FCalc1UF02Totals_AreaWeightAvgTotalArea</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                           <xsd:element name="FCalc1UF05_DoesAreaWeightAvgComplyUfactor" type="comp:DoesAreaWeightAvgComplyUfactor" >
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText">Compliance Results Using Area-Weighted Calculation Option</xsd:documentation>
                                 <xsd:documentation source="CalculationsAndRules">Calculated field: 
 
If FCalc1UF03Totals_AreaWeightAvgUfactorRequiredAll GTE FCalc1UF04Totals_AreaWeightAvgUfactorAll  result = true display term COMPLIES; 
 
Else result = false display term DOES NOT COMPLY
End If</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                        </xsd:sequence>
                     </xsd:complexType>
                  </xsd:element>
                  <xsd:element name="FCalc2UF" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">[d:b]Area-Weighted Average U-factor Compliance Calculation for  Metal Building Roof[/d:b]</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">This subsection is required If Fmetalb01_UseInAreaWeightedAveUfactor == true</xsd:documentation>
                     </xsd:annotation>
                     <xsd:complexType>
                        <xsd:sequence>
                           <xsd:element name="FCalc2UF01_AreaWeightAvgType" >
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText">Roof Type</xsd:documentation>
                                 <xsd:documentation source="CalculationsAndRules">Calculated field: result = MetalBuilding display term Metal Building Roof</xsd:documentation>
                              </xsd:annotation>
                              <xsd:simpleType>
                                 <xsd:restriction base="comp:AreaWeightAvgType">
                                    <xsd:enumeration value="MetalBuilding"/>
                                 </xsd:restriction>
                              </xsd:simpleType>
                           </xsd:element>
                           <xsd:element name="FCalc2UF02_RoofTypeTotalArea" type="comp:RoofTypeTotalArea" >
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText">Total Area of Roof Type (ft[d:sup]2[/d:sup])</xsd:documentation>
                                 <xsd:documentation source="CalculationsAndRules">Calculated field: SUM ALL (Fmetalb15_RoofNetArea)</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                           <xsd:element name="FCalc2UF03to04Label" minOccurs="0">
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText">Area-weighted U-factor for Roof Type</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                           <xsd:element name="FCalc2UF03_AreaWeightAvgRequiredUfactor" type="comp:AreaWeightAvgRequiredUfactor" >
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText">Required</xsd:documentation>
                                 <xsd:documentation source="CalculationsAndRules">Calculated field: result = (SUM ALL (Fmetalb15_RoofNetArea x Fmetalb13_InsulationUFactorLimit)) / FCalc2UF02_RoofTypeTotalArea</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                           <xsd:element name="FCalc2UF04_AreaWeightAvgUfactor" type="comp:AreaWeightAvgUfactor" >
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText">Designed</xsd:documentation>
                                 <xsd:documentation source="CalculationsAndRules">Calculated field: result= (SUM ALL (Fmetalb15_RoofNetArea x Ufactor) ) / FCalc2UF02_RoofTypeTotalArea.  
 
Ufactor for row is the property that IsPresent: 
Fmetalb14_UfactorJA4Other Or 
Fmetalb14_UfactorJA427Screw Or 
Fmetalb14_UfactorJA427SeamSingle Or 
Fmetalb14_UfactorJA427SeamDouble Or 
Fmetalb14_UfactorJA427Blocks</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                           <xsd:element name="FCalc2UF05_DoesAreaWeightAvgComplyUfactor" type="comp:DoesAreaWeightAvgComplyUfactor" >
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText">Compliance Results Using Area-Weighted Calculation Option</xsd:documentation>
                                 <xsd:documentation source="CalculationsAndRules">Calculated field: If FCalc2UF03_AreaWeightAvgRequiredUfactor GTE FCalc2UF04_AreaWeightAvgUfactor result = true display term COMPLIES; 
 
Else result = false display term DOES NOT COMPLY
End If</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                        </xsd:sequence>
                     </xsd:complexType>
                  </xsd:element>
                  <xsd:element name="FCalc3UF" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">[d:b]Area-Weighted Average U-factor Compliance Calculation for Multifamily-Framed[/d:b]</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">This subsection is required If FframedMF01_UseInAreaWeightedAveUfactor == true</xsd:documentation>
                     </xsd:annotation>
                     <xsd:complexType>
                        <xsd:sequence>
                           <xsd:element name="FCalc3UF01_AreaWeightAvgType" >
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText">Roof Type</xsd:documentation>
                                 <xsd:documentation source="CalculationsAndRules">Calculated field: result = FramedMF display term Multifamily Framed Roof</xsd:documentation>
                              </xsd:annotation>
                              <xsd:simpleType>
                                 <xsd:restriction base="comp:AreaWeightAvgType">
                                    <xsd:enumeration value="FramedMF"/>
                                 </xsd:restriction>
                              </xsd:simpleType>
                           </xsd:element>
                           <xsd:element name="FCalc3UF02_RoofTypeTotalArea" type="comp:RoofTypeTotalArea" >
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText">Total Area of Roof Type (ft[d:sup]2[/d:sup])</xsd:documentation>
                                 <xsd:documentation source="CalculationsAndRules">Calculated field: SUM ALL (FframedMF17_RoofNetArea)</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                           <xsd:element name="FCalc3UF03to04Label" minOccurs="0">
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText">Area-weighted U-factor for Roof Type</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                           <xsd:element name="FCalc3UF03_AreaWeightAvgRequiredUfactor" type="comp:AreaWeightAvgRequiredUfactor" >
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText">Required</xsd:documentation>
                                 <xsd:documentation source="CalculationsAndRules">Calculated field: result = (SUM ALL (FframedMF17_RoofNetArea x Fframed15_InsulationUFactorLimit)) / FCalc3UF02_RoofTypeTotalArea</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                           <xsd:element name="FCalc3UF04_AreaWeightAvgUfactor" type="comp:AreaWeightAvgUfactor" >
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText">Designed</xsd:documentation>
                                 <xsd:documentation source="CalculationsAndRules">Calculated field: result= (SUM ALL (FframedMF17_RoofNetArea x Ufactor) ) / FCalc3UF02_RoofTypeTotalArea.  
 
Ufactor for row is the property that IsPresent: 
FframedMF16_UfactorJA4Other Or  
FframedMF16_UfactorJA421_16OC Or  
FframedMF16_UfactorJA421_24OC Or 
FframedMF16_UfactorJA422_16OC Or 
FframedMF16_UfactorJA422_24OC Or 
FframedMF16_UfactorJA424_16OC or 
FframedMF16_UfactorJA424_24OC Or 
FframedMF16_UfactorJA425_16OC Or 
FframedMF16_UfactorJA425_24OC</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                           <xsd:element name="FCalc3UF05_DoesAreaWeightAvgComplyUfactor" type="comp:DoesAreaWeightAvgComplyUfactor" >
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText">Compliance Results Using Area-Weighted Calculation Option</xsd:documentation>
                                 <xsd:documentation source="CalculationsAndRules">Calculated field: If FCalc3UF03_AreaWeightAvgRequiredUfactor GTE FCalc3UF04_AreaWeightAvgUfactor result = true display term COMPLIES; 
 
Else result = false display term DOES NOT COMPLY
End If</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                        </xsd:sequence>
                     </xsd:complexType>
                  </xsd:element>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
         <xsd:element name="Section_G" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation source="FieldText">[d:b]G. RATED ROOFING MATERIAL (COOL ROOF)[/d:b]</xsd:documentation>
               <xsd:documentation source="CalculationsAndRules">This section is required If B02NewRoof_ComponentApplies, B02AddRoof_ComponentApplies,  Or B02AltRoofmat_ComponentApplies  == true
End If
 
Mandatory Measure Triggers:
If this section is required    
MM110_8i_MeasureApplies  = true and is displayed
End If</xsd:documentation>
               <xsd:documentation source="MinOccurs">If this section doesn't apply, do not display anything for this section</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="G_BeginNote1" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">[d:i]This table demonstrates compliance with prescriptive roof material requirements in [d:t24refnolink/]140.3(a)1A/ [d:t24refnolink/]170.2(a)1A for new construction, [d:t24refnolink/]141.0(a)/ [d:t24refnolink/]180.1 for additions, and [d:t24refnolink/]141.0(b)2B/  [d:t24refnolink/]180.2 for alterations.  Roof recovers and replacements must also document compliance with insulation requirements in Table F.  Roof recoats may document compliance with roof material only in Table G. [/d:i]</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="TableGcoolroof" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                     </xsd:annotation>
                     <xsd:complexType>
                        <xsd:sequence>
                           <xsd:element name="Row" minOccurs="1" maxOccurs="unbounded">
                              <xsd:complexType>
                                 <xsd:sequence>
                                    <xsd:element name="G01_RoofAssemblyName" type="env:RoofAssemblyName" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Tag/Plan Detail ID</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">User input</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="G02_RoofDescription" type="env:RoofDescription" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Name/ Description/ Location</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">User input</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="G03_Status" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Status</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">User selects from list: New display term New, ExistingAlteration display term Altered</xsd:documentation>
                                       </xsd:annotation>
                                       <xsd:simpleType>
                                          <xsd:restriction base="com:Status">
                                             <xsd:enumeration value="ExistingAlteration"/>
                                             <xsd:enumeration value="New"/>
                                          </xsd:restriction>
                                       </xsd:simpleType>
                                    </xsd:element>
                                    <xsd:element name="G04_TopLevelOccupancy" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Occupancy Type</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">User selects from list: 
Nonresidential display term Nonresidential/ Relocatable 1 CZ, 
HotelMotel display erm Hotel/Motel,    
RelocableSchoolBuildingAllZones display term Relocatable all CZ
ResidentialMF display term Multifamily - Attic Roof
OtherOccupancy display term Multifamily - Non-attic Roof</xsd:documentation>
                                          <xsd:appinfo source="displayterm">
                                             <dtyp:displayterm value="Nonresidential">Nonresidential/ Relocatable 1 CZ</dtyp:displayterm>
                                             <dtyp:displayterm value="RelocableSchoolBuildingAllZones">Relocatable all CZ</dtyp:displayterm>
                                             <dtyp:displayterm value="ResidentialMF">Multifamily - Attic Roof</dtyp:displayterm>
                                             <dtyp:displayterm value="OtherOccupancy">Multifamily - Non-attic Roof
</dtyp:displayterm>
                                          </xsd:appinfo>
                                          
                                       </xsd:annotation>
                                       <xsd:simpleType>
                                          <xsd:restriction base="com:TopLevelOccupancy">
                                             <xsd:enumeration value="HotelMotel"/>
                                             <xsd:enumeration value="Nonresidential"/>
                                             <xsd:enumeration value="OtherOccupancy"/>
                                             <xsd:enumeration value="RelocableSchoolBuildingAllZones"/>
                                             <xsd:enumeration value="ResidentialMF"/>
                                          </xsd:restriction>
                                       </xsd:simpleType>
                                    </xsd:element>
                                    <xsd:element name="G05_RoofSlope" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Roof Slope</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">User selects from list</xsd:documentation>
                                          <xsd:appinfo source="displayterm">
                                             <dtyp:displayterm value="LowSlope">[d:lt/]2:12 (Low)</dtyp:displayterm>
                                             <dtyp:displayterm value="SteepSlope">[d:gte/] 2:12 (Steep)</dtyp:displayterm>
                                          </xsd:appinfo>
                                          
                                       </xsd:annotation>
                                       <xsd:simpleType>
                                          <xsd:restriction base="env:RoofSlope">
                                             <xsd:enumeration value="LowSlope"/>
                                             <xsd:enumeration value="SteepSlope"/>
                                          </xsd:restriction>
                                       </xsd:simpleType>
                                    </xsd:element>
                                    <xsd:element name="G06_RoofProductType" type="env:RoofProductType" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Roof Material</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">User selects from list: 
ToBeDetermined display term To Be Determined, 
AsphaltShingles display term Asphalt shingles, 
StoneAggregateBallast display term Aggregate or Ballast, 
ModifiedBitumin display term Bitumen, 
FieldAppliedCoating display term Field applied coating, 
FluidAppliedMembrane display term Fluid applied membrane, 
MetalRoof display term Metal , 
MetalShakesShingles display term Metal shakes or shingles, 
RoofPavers display term Paver, 
PolymerCompositeSteepSlope display term Polymer or Composite , 
SinglePlyThermoplastic display term Single-Ply,  
SprayPolyurethaneFoam display term Spray polyurethane foam, 
ClayRoofTiles display term Tile or Slate, 
WoodShakesShingles display term Wood, 
MetalBdgRoof display term Metal Building</xsd:documentation>
                                          <xsd:appinfo source="displayterm">
                                             <dtyp:displayterm value="StoneAggregateBallast"> Aggregate or Ballast</dtyp:displayterm>
                                             <dtyp:displayterm value="ModifiedBitumin"> Bitumen</dtyp:displayterm>
                                             <dtyp:displayterm value="MetalRoof"> Metal</dtyp:displayterm>
                                             <dtyp:displayterm value="RoofPavers"> Paver</dtyp:displayterm>
                                             <dtyp:displayterm value="PolymerCompositeSteepSlope"> Polymer or Composite</dtyp:displayterm>
                                             <dtyp:displayterm value="SinglePlyThermoplastic"> Single-Ply</dtyp:displayterm>
                                             <dtyp:displayterm value="ClayRoofTiles"> Tile or Slate</dtyp:displayterm>
                                             <dtyp:displayterm value="WoodShakesShingles"> Wood</dtyp:displayterm>
                                          </xsd:appinfo>
                                          
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="G07toG10Choice" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText"></xsd:documentation>
                                       </xsd:annotation>
                                       <xsd:complexType>
                                          <xsd:choice >
                                             <xsd:element name="G07toG10NA_NotApplicableMessage" >
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText"></xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">For any of these conditions G07 - G10 result = BlankGrayedOut; and fields G07 - G10 are blank and grayed out  Note: G04_TopLevelOccupancy value OtherOccupancy is for Multifamily - Non-attic Roof  and ResidentialMF is for Multifamily - Attic Roof
 
Condition:  G04_TopLevelOccupancy == HotelMotel And G05_RoofSlope == SteepSlope And G03_Status == New Or ExistingAlteration And A03_ClimateZoneNumber == 1 Or 16
 
Condition:  G04_TopLevelOccupancy == OtherOccupancy And G05_RoofSlope == SteepSlope And G03_Status == New And A03_ClimateZoneNumber == 1 Or 16
 
Condition:  G04_TopLevelOccupancy == HotelMotel Or OtherOccupancy And G05_RoofSlope == LowSlope And G03_Status == New And A03_ClimateZoneNumber ==  InRange(1,8) Or 12 Or 16; 
 
Condition: G04_TopLevelOccupancy == HotelMotel And G05_RoofSlope == LowSlope And  G03_Status == ExistingAlteration And A03_ClimateZoneNumber ==  InRange(1,8) Or 12 Or 16
 
Condition:  G04_TopLevelOccupancy == ResidentialMF And G05_RoofSlope == LowSlope And G03_Status == New And A03_ClimateZoneNumber ==  InRange(1,12) Or 14 Or 16; 
 
Condition:  G04_TopLevelOccupancy == ResidentialMF And G05_RoofSlope == SteepSlope And G03_Status == New And A03_ClimateZoneNumber ==  InRange(1,9) Or 16 
 
Condition:  G04_TopLevelOccupancy == ResidentialMF Or OtherOccupancy And G05_RoofSlope == LowSlope And G03_Status == ExistingAlteration And A03_ClimateZoneNumber ==  1,3, 5 Or 16 
 
 
Condition:  G04_TopLevelOccupancy == ResidentialMF Or OtherOccupancy And G05_RoofSlope == SteepSlope And G03_Status == ExistingAlteration And A03_ClimateZoneNumber ==  1,2, 3, 5, 6, 7 Or 16</xsd:documentation>
                                                </xsd:annotation>
                                                <xsd:simpleType>
                                                   <xsd:restriction base="comp:NotApplicableMessage">
                                                      <xsd:enumeration value="BlankGrayedOut"/>
                                                   </xsd:restriction>
                                                </xsd:simpleType>
                                             </xsd:element>
                                             <xsd:element name="G07toG10" >
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText"></xsd:documentation>
                                                </xsd:annotation>
                                                <xsd:complexType>
                                                   <xsd:sequence>
                                                      <xsd:element name="G07_CoolRoofComplianceMethod" type="comp:CoolRoofComplianceMethod" >
                                                         <xsd:annotation>
                                                            <xsd:documentation source="FieldText">Compliance Method</xsd:documentation>
                                                            <xsd:documentation source="CalculationsAndRules">User selects from list customized as follows: 
Always list: 
InitialSolarReflectanceThermaEmittance display term Solar Reflectance (Initial)/ Emittance, 
AgedSolarReflectanceThermaEmittance display term Solar Reflectance (Aged)/ Emittance, 
DemisingRoof display term NA: Demising Roof; 
 
Customize the rest of the list as follows: 
If G03_Status == ExistingAlteration And G04_TopLevelOccupancy == Nonresidential Or HotelMotel And  G05_RoofSlope == LowSlope add to list:
SolarReflectanceIndex display term Solar Reflectance Index (SRI),
RoofSolarPV display term NA: Roof-integrated PV or Solar Thermal, 
MassRoofGTE25Lbs display term NA: Weight [d:gte/] 25lb/ft [d:sup]2[/d:sup] over membrane 
RoofAlteredLT2000Ft2 display term NA:[d:lte/] 50% roof area &amp; [d:lt/]2,000ft2 altered
UfactorTradeOffAlterations display term U-factor Trade-off per Table 141.0-B
 
Else If G03_Status == ExistingAlteration And G04_TopLevelOccupancy == Nonresidential Or HotelMotel And  G05_RoofSlope == SteepSlope add to list: 
SolarReflectanceIndex display term Solar Reflectance Index (SRI),
RoofSolarPV display term NA: Roof-integrated PV or Solar Thermal, 
MassRoofGTE25Lbs display term NA: Weight [d:gte/] 25lb/ft [d:sup]2[/d:sup] over membrane 
RoofAlteredLT2000Ft2 display term NA:[d:lte/] 50% roof area &amp; [d:lt/]2,000ft2 altered
 
Else If G04_TopLevelOccupancy == Nonresidential And G05_RoofSlope == LowSlope And A03_ClimateZoneNumber == 1, 2, 4 Or InRange (6, 16) add to list
SolarReflectanceIndex display term Solar Reflectance Index (SRI),
RoofSolarPV display term NA: Roof-integrated PV or Solar Thermal, 
MassRoofGTE25Lbs display term NA: Weight [d:gte/] 25lb/ft [d:sup]2[/d:sup] over membrane 
UfactorTradeOff display term U-factor Trade-off per Table 140.3;
 
Else If G04_TopLevelOccupancy == Nonresidential And G05_RoofSlope == LowSlope And A03_ClimateZoneNumber == 3 Or 5 add to list
SolarReflectanceIndex display term Solar Reflectance Index (SRI),
RoofSolarPV display term NA: Roof-integrated PV or Solar Thermal, 
MassRoofGTE25Lbs display term NA: Weight [d:gte/] 25lb/ft [d:sup]2[/d:sup] over membrane 
UfactorTradeOff display term U-factor Trade-off per Table 140.3;
WoodFrameLTEU034 idsplay term NA: Wood Frame Roof U :[d:lte/] 0.034
 
Else If G04_TopLevelOccupancy == HotelMotel And G05_RoofSlope == LowSlope 
Or G04_TopLevelOccupancy == ResidentialMF  Or OtherOccupancy And G03_Status ==  New  add to list
SolarReflectanceIndex display term Solar Reflectance Index (SRI),
RoofSolarPV display term NA: Roof-integrated PV or Solar Thermal, 
MassRoofGTE25Lbs display term NA: Weight [d:gte/] 25lb/ft [d:sup]2[/d:sup] over membrane
 
Else If G03_Status ==  New  And G05_RoofSlope == SteepSlope add to list:
SolarReflectanceIndex display term Solar Reflectance Index (SRI),
RoofSolarPV display term NA: Roof-integrated PV or Solar Thermal,
 
Else If G03_Status == ExistingAlteration And G04_TopLevelOccupancy == ResidentialMF  Or OtherOccupancy And G05_RoofSlope == LowSlope  add to list
SolarReflectanceIndex display term Solar Reflectance Index (SRI),
RoofSolarPV display term NA: Roof-integrated PV or Solar Thermal, 
MassRoofGTE25Lbs display term NA: Weight [d:gte/] 25lb/ft [d:sup]2[/d:sup] over membrane 
RoofAlteredLT2000Ft2 display term NA:[d:lte/] 50% roof area &amp; [d:lt/]2,000ft2 altered
RvalueTradeOff display term R-value Trade-off per Table 180.2-A
 
Else If G03_Status == ExistingAlteration And G04_TopLevelOccupancy == ResidentialMF  Or OtherOccupancy And G05_RoofSlope == SteepSlope  add to list
SolarReflectanceIndex display term Solar Reflectance Index (SRI),
RoofSolarPV display term NA: Roof-integrated PV or Solar Thermal, 
MassRoofGTE25Lbs display term NA: Weight [d:gte/] 25lb/ft [d:sup]2[/d:sup] over membrane 
RoofAlteredLT2000Ft2 display term NA:[d:lte/] 50% roof area &amp; [d:lt/]2,000ft2 altered
CeilingUfactorNA display term NA: Ceiling U-factor [d:lte/]0.25 
AtticCeilingInsulationNA display term NA: Attic ceiling insulation [d:gte/]R-38
AtticRadiantBarrierNA display term NA: Attic radiant barrier per [d:t24refnolink/]170.2(a)1C
NoAtticDuctsInCZNA display term NA: no ducts in attic &amp; CZ 2, 4, 9, 10, 12 or 14
ContinuousInsulationNA display term NA: Continuous insulation above/below roof deck  [d:gte/]R-2
End If</xsd:documentation>
                                                            <xsd:appinfo source="displayterm">
                                                               <dtyp:displayterm value="AgedSolarReflectanceThermaEmittance"> Solar Reflectance (Aged)/ Emittance</dtyp:displayterm>
                                                               <dtyp:displayterm value="InitialSolarReflectanceThermaEmittance"> Solar Reflectance (Initial)/ Emittance</dtyp:displayterm>
                                                               <dtyp:displayterm value="SolarReflectanceIndex"> Solar Reflectance Index (SRI)</dtyp:displayterm>
                                                            </xsd:appinfo>
                                                            
                                                         </xsd:annotation>
                                                      </xsd:element>
                                                      <xsd:element name="G08Label" minOccurs="0">
                                                         <xsd:annotation>
                                                            <xsd:documentation source="FieldText">Required Minimum Material Performance</xsd:documentation>
                                                         </xsd:annotation>
                                                      </xsd:element>
                                                      <xsd:choice maxOccurs="1">
                                                         <xsd:element name="G08a_SolarReflectanceAgedLimit" type="env:SolarReflectanceAgedLimit" >
                                                            <xsd:annotation>
                                                               <xsd:documentation source="FieldText">Reflectance</xsd:documentation>
                                                               <xsd:documentation source="CalculationsAndRules">If G07_CoolRoofComplianceMethod == AgedSolarReflectanceThermaEmittance Or InitialSolarReflectanceThermaEmittance  
 
Switch ( G04_TopLevelOccupancy) 
case Nonresidential: 
If G05_RoofSlope == LowSlope  
result = 0.63; 
Else If G05_RoofSlope == SteepSlope  
If A03_ClimateZoneNumber == 1 Or 3
result = 0.20; 
Else 
End If A03_ClimateZoneNumber == 2 Or InRange(4, 16)9, 
result = 0.25; 
End If
End If
 
case HotelMotel:  
If G05_RoofSlope == LowSlope 
If G03_Status == New And A03_ClimateZoneNumber == 9, 10, 11, 13, 14 Or 15 
result = 0.55; 
Else If G03_Status == ExistingAlteration And A03_ClimateZoneNumber == 9, 10, 11, 13, 14 Or 15 
result = 0.55; 
End If
Else If G05_RoofSlope == SteepSlope And A03_ClimateZoneNumber InRange (2,15) 
result = 0.20; 
End If 
 
case RelocableSchoolBuildingAllZones:
 If G05_RoofSlope == LowSlope result = 0.63; 
Else If G05_RoofSlope == SteepSlope result = 0.25; 
End If
 
case ResidentialMF  (display term Multifamily Attic Roof)
If G05_RoofSlope == LowSlope 
If G03_Status == New And A03_ClimateZoneNumber == 13 Or 15
result = 0.63
Else If G03_Status == ExistingAlteration And A03_ClimateZoneNumber == 2 Or 4 Or InRange(6,15)
result = 0.63
End If
Else If G05_RoofSlope == SteepSlope
If G03_Status == New And A03_ClimateZoneNumber == InRange(10,15)
result = 0.20
Else If G03_Status == ExistingAlteration And A03_ClimateZoneNumber == 4 Or InRange(8,15)
result = 0.20
End If
End If
 
case OtherOccupancy  (display term Multifamily Non-Attic Roof)
If G05_RoofSlope == LowSlope 
If G03_Status == New And A03_ClimateZoneNumber == 9 Or 10 Or 11 Or 13 Or 14 Or 15
result = 0.63
Else If G03_Status == ExistingAlteration And A03_ClimateZoneNumber == 2 Or 4 Or InRange(6,15)
result = 0.63
End If
Else If G05_RoofSlope == SteepSlope
If G03_Status == New And A03_ClimateZoneNumber == InRange(2,15)
result = 0.20
Else If G03_Status == ExistingAlteration And A03_ClimateZoneNumber == 4 Or InRange(8,15)
result = 0.20
End If
End If
 
End Switch 
 
Else If G07_CoolRoofComplianceMethod == UfactorTradeOff Or  UfactorTradeOffAlterations 
 
If G10_UfactorTradeOffRange == LTE0_024 Or LTE0_026 Or LTE0_028 Or LTE0_031 Or LTE0_037 result = 0.25; 
Else If  G10_UfactorTradeOffRange ==  0_0311To0_033, Or 0_0371To0_039 result = 0.36;  
Else  If  G10_UfactorTradeOffRange == 0_0291To0_030 Or 0_0331To0_035 Or 0_0391To0_042  result = 0.46;  
Else If  G10_UfactorTradeOffRange == 0_0301To0_032 Or  0_0351To0_038 Or 0_0421To0_045 result = 0.56; 
Else If  G10_UfactorTradeOffRange == 0_0241To0_025 result = 0.3; 
Else If  G10_UfactorTradeOffRange == 00_0251To0_026 result = 0.35; 
Else If  G10_UfactorTradeOffRange == 0_0291To0_032  result = 0.40; 
Else If  G10_UfactorTradeOffRange == 0_0321To0_034 result = 0.45; 
Else If  G10_UfactorTradeOffRange == 0_0341To0_038 Or 0_0291To0_031 result = 0.5;  
Else If  G10_UfactorTradeOffRange == 0_0381To0_041 Or 0_0311To0_034 result = 0.55; 
Else If  G10_UfactorTradeOffRange == 0_0411To0_043 Or 0_0341To0_035 result = 0.60
Else If  G10_UfactorTradeOffRange == 0_0281To0_029
If G03_Status == New result = 36
Else If ExistingAlteration And A03_ClimateZoneNumber == 6 Or 7 Or 8  result = 0.35
Else If ExistingAlteration And A03_ClimateZoneNumber == InRange(1,5) Or InRange (9,16)  result = 0.45
End If
 
Else If  G10_UfactorTradeOffRange == 0_0261To0_028
If ExistingAlteration And A03_ClimateZoneNumber == 6 Or 7 Or 8  result = 0.30
Else If ExistingAlteration And A03_ClimateZoneNumber == InRange(1,5) Or InRange (9,16)  result = 0.40
End If
End If 
 
Else If G07_CoolRoofComplianceMethod == RvalueTradeOff
 If  G10_RvalueTradeOff == GTE10 Or GTE24 result = 0
Else If  G10_RvalueTradeOff == 8To9_99 Or 22To23_99 result = 0.45
Else If  G10_RvalueTradeOff == 6To7_99 Or 20To21_99 result = 0.5
Else If  G10_RvalueTradeOff == 4To5_99 Or 18To19_99 result = 0.55
Else If  G10_RvalueTradeOff == 2To3_99 Or 16To17_99 result = 0.6
End If
 
End If</xsd:documentation>
                                                            </xsd:annotation>
                                                         </xsd:element>
                                                         <xsd:element name="G08a_NotApplicableMessage" >
                                                            <xsd:annotation>
                                                               <xsd:documentation source="FieldText">Reflectance</xsd:documentation>
                                                               <xsd:documentation source="CalculationsAndRules">If G07_CoolRoofComplianceMethod != AgedSolarReflectanceThermaEmittance Or InitialSolarReflectanceThermaEmittance  Or UfactorTradeOff Or  UfactorTradeOffAlterations Or RvalueTradeOff
result = BlankGrayedOut
Else 
Switch ( G04_TopLevelOccupancy) 
case HotelMotel:  
If G05_RoofSlope == LowSlope And A03_ClimateZoneNumber == InRange (1, 8) Or 12 Or 16
result = BlankGrayedOut 
Else If G05_RoofSlope == SteepSlope And A03_ClimateZoneNumber == 1 Or 16
result = BlankGrayedOut 
End If
 
case ResidentialMF  (display term Multifamily Attic Roof)
If G05_RoofSlope == LowSlope 
If G03_Status == New And A03_ClimateZoneNumber == InRange (1, 12) Or 14 Or 16
result = BlankGrayedOut 
Else If G03_Status == ExistingAlteration And A03_ClimateZoneNumber == 1, 3, 5 Or 16
result = BlankGrayedOut
End If
Else If G05_RoofSlope == SteepSlope
If G03_Status == New And A03_ClimateZoneNumber == InRange(1, 9) Or 16
result = BlankGrayedOut
Else If G03_Status == ExistingAlteration And A03_ClimateZoneNumber == 1, 2, 3, 5, 6, 7, Or 16
result = BlankGrayedOut
End If
End If
 
case OtherOccupancy  (display term Multifamily Non-Attic Roof)
If G05_RoofSlope == LowSlope 
If G03_Status == New And A03_ClimateZoneNumber == InRange (1, 8) Or 12 Or 16
result = BlankGrayedOut 
Else If G03_Status == ExistingAlteration And A03_ClimateZoneNumber == 1, 3, 5 Or 16
result = BlankGrayedOut
End If
Else If G05_RoofSlope == SteepSlope
If G03_Status == New And A03_ClimateZoneNumber == 1 Or 16
result = BlankGrayedOut
Else If G03_Status == ExistingAlteration And A03_ClimateZoneNumber == 1, 2, 3, 5, 6, 7, Or 16
result = BlankGrayedOut
End If
End If
 
End Switch
End If</xsd:documentation>
                                                            </xsd:annotation>
                                                            <xsd:simpleType>
                                                               <xsd:restriction base="comp:NotApplicableMessage">
                                                                  <xsd:enumeration value="BlankGrayedOut"/>
                                                               </xsd:restriction>
                                                            </xsd:simpleType>
