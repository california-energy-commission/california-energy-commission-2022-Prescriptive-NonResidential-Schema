result = 0.75; 
Else If G05_RoofSlope == SteepSlope 
result = 0.80
End If
 
case ResidentialMF  (display term Multifamily Attic Roof)
If G05_RoofSlope == LowSlope 
If G03_Status == New And A03_ClimateZoneNumber == 13 Or 15
result = 0.75
Else If G03_Status == ExistingAlteration And A03_ClimateZoneNumber == 2 Or 4 Or InRange(6,15)
result = 0.75
End If
Else If G05_RoofSlope == SteepSlope
If G03_Status == New And A03_ClimateZoneNumber == InRange(10,15)
result = 0.75
Else If G03_Status == ExistingAlteration And A03_ClimateZoneNumber == 4 Or InRange(8,15)
result = 0.75
End If
End If
 
case OtherOccupancy  (display term Multifamily Non-Attic Roof)
If G05_RoofSlope == LowSlope 
If G03_Status == New And A03_ClimateZoneNumber == 9 Or 10 Or 11 Or 13 Or 14 Or 15
result = 0.75
Else If G03_Status == ExistingAlteration And A03_ClimateZoneNumber == 2 Or 4 Or InRange(6,15)
result = 0.75
End If
Else If G05_RoofSlope == SteepSlope
If G03_Status == New And A03_ClimateZoneNumber == InRange(2,15)
result = 0.75
Else If G03_Status == ExistingAlteration And A03_ClimateZoneNumber == 4 Or InRange(8,15)
result = 0.75
End If
End If
 
End Switch</xsd:documentation>
