                                                <xsd:element name="Iframed10_InsulationUFactorLimit" type="env:InsulationUFactorLimit" >
                                                   <xsd:annotation>
                                                      <xsd:documentation source="FieldText">Required Thermal Performance [d:sup]2[/d:sup]</xsd:documentation>
                                                      <xsd:documentation source="CalculationsAndRules">Calculated field: 
If Iframed09_ConstructionSpecificationMethod == UFactor 
If Iframed03b_Status == ExistingAlteration  
If Iframed03a_TopLevelOccupancy == AlteredOccupancy  result = 0.071; 
End If
Else If Iframed03b_Status == New
Switch (  Iframed03a_TopLevelOccupancy) 
case  Nonresidential: 
If A03_ClimateZoneNumber == 1 result = 0.048; 
Else  If A03_ClimateZoneNumber == 2, 11, 14, 15 Or 16 result = 0.039; 
Else If A03_ClimateZoneNumber == 12 Or 13 Or InRange (3, 10) result = 0.071;   
End If
case HotelMotel: 
If A03_ClimateZoneNumber ==  1, 2,14 Or 16 result = 0.034; 
Else If A03_ClimateZoneNumber == InRange(3, 6) Or InRange(8, 13) Or 15  result = 0.039; 
Else If A03_ClimateZoneNumber == 7 result = 0.071; 
End If
case RelocableSchoolBuildingAllZones: result = 0.048;
 
case ResidentialMF:
If Iframed06a_PartitionFramingMaterial == WoodFraming
result = 0.037
Else If Iframed06a_PartitionFramingMaterial == Metal
If A03_ClimateZoneNumber == 1 result = 0.048; 
Else  If A03_ClimateZoneNumber == 2, 11, 14, 15 Or 16 result = 0.039; 
Else If A03_ClimateZoneNumber == 12 Or 13 Or InRange (3, 10) result = 0.071;   
End If
 
End Switch
End If
End If</xsd:documentation>
