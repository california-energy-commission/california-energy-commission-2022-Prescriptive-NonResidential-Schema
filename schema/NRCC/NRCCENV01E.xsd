                                                      <xsd:documentation source="CalculationsAndRules">If Kvertperf07_ComplianceMethodSHGC_VT == NoOverHang result = 0.56;
 
Else If Kvertperf07_ComplianceMethodSHGC_VT == Table140_3BCD  
If Kvertperf06_TopLevelOccupancy == Nonresidential
Switch (Kvertperf05_FenestrationProductType) 
case FixedWindow:  
If A03_ClimateZoneNumber == InRange (1,8) Or 10 Or 16
result = 0.25;
Else If A03_ClimateZoneNumber == 9 Or InRange (11,15)
result = 0.22
End If; 
case OperableWindow:  
result = 0.22;  
 
case StoreFront:  case CurtainWall: 
If A03_ClimateZoneNumber == 1 Or 7
result = 0.25;
Else If A03_ClimateZoneNumber == InRange (2,6) Or InRange(8, 16)
result = 0.26; 
End If
 
case GlazedDoor:
result = 0.23; 
End Switch
 
If Kvertperf06_TopLevelOccupancy == HotelMotel 
Switch (Kvertperf05_FenestrationProductType) 
case FixedWindow:  
result = 0.25
case OperableWindow:  
result = 0.22;
case StoreFront:  case CurtainWall: 
result = 0.26; 
case GlazedDoor:
result = 0.23; 
End Switch
 
Else If Kvertperf06_TopLevelOccupancy == RelocableSchoolBuildingAllZones 
If Kvertperf05_FenestrationProductType == GlazedDoor
result = 0.23
Else
result = 0.26;  
End If
End If
 
Else  If Kvertperf07_ComplianceMethodSHGC_VT == Table141_0A 
If A03_ClimateZoneNumber == 1, 3, 5 Or 16 result = 0.41; 
Else If A03_ClimateZoneNumber == 2, 4 Or InRange (6,15) result = 0.31;  
End If
 
Else If Kvertperf07_ComplianceMethodSHGC_VT == Table170_2A 
If Kvertperf06_TopLevelOccupancy == HighriseResidential 
If Kvertperf05_FenestrationProductType == StoreFront Or CurtainWall  
If A03_ClimateZoneNumber == 1 
resut = 0.35
Else If A03_ClimateZoneNumber ==  InRange(2, 13) Or 15
result = 0.26; 
Else If A03_ClimateZoneNumber == 14 Or 15
result = 0.25
End If
Else Kvertperf05_FenestrationProductType != StoreFront Or CurtainWall  
If A03_ClimateZoneNumber == 1 
resut = 0.35
Else If A03_ClimateZoneNumber == InRange (2, 15)
 result = 0.23;  
End If
End If
End If
 
Else If Kvertperf07_ComplianceMethodSHGC_VT == Table180_2B
If Kvertperf05_FenestrationProductType == StoreFront Or CurtainWall
 If A03_ClimateZoneNumber == 1
result = 0.35
Else If A03_ClimateZoneNumber == InRange (2,15) 
 result = 0.26; 
