Else If HSIPS07_CoreInsulation == R18 
Switch (HSIPS08_ContinuousInsulationNR) 
case R0: result = 0.066; case R2: result = 0.056; case R4: result = 0.050; case R5: result = 0.048; case R6: result = 0.045; case R8: result = 0.041;  
End Switch
End If
 
Else If HSIPS06_PanelThickness == 6_5In  
If HSIPS07_CoreInsulation == R22 
Switch (HSIPS08_ContinuousInsulationNR) 
case R0: result = 0.054; case R2: result = 0.048; case R4: result = 0.043; case R5: result = 0.041; case R6: result = 0.039; case R8: result = 0.036;  
End Switch
Else If HSIPS07_CoreInsulation == R33 
Switch (HSIPS08_ContinuousInsulationNR) 
case R0: result = 0.043; case R2: result = 0.038; case R4: result = 0.034; case R5: result = 0.033; case R6: result = 0.031; case R8: result = 0.029;  
