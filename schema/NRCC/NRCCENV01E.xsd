                                                      <xsd:choice maxOccurs="1">
                                                         <xsd:element name="Kskyperf16a_UfactorNA6" type="env:UfactorNA6" >
                                                            <xsd:annotation>
                                                               <xsd:documentation source="FieldText">Product Performance per  NA6</xsd:documentation>
                                                               <xsd:documentation source="CalculationsAndRules">Calculated field: 
Switch (Kskyperf13_FenestrationFramingNR) 
case MetalFraming: 
If Kskyperf05_FenestrationProductType == SkylightGlassCurb Or SkylightPlasticCurb Or SkylightTubular result = 0.711 + (1.065 x Kskyperf15a_UFactorCenterOfGlass); 
 
Else If If Kskyperf05_FenestrationProductType == SkylightGlassDeck result =  0.310 + (0.878 x Kskyperf15a_UFactorCenterOfGlass);   
End If
 
case MetalFramingWithThermalBreak: 
If Kskyperf05_FenestrationProductType == SkylightGlassCurb Or SkylightPlasticCurb Or SkylightTubular result =  0.437 + (1.229 x Kskyperf15a_UFactorCenterOfGlass);  
 
Else Kskyperf05_FenestrationProductType == SkylightGlassDeck result =   0.195 + (0.882 x  Kskyperf15a_UFactorCenterOfGlass);
 
case NonMetalFraming: 
If Kskyperf05_FenestrationProductType == SkylightGlassCurb Or SkylightPlasticCurb Or SkylightTubular result =  0.437 + (1.229 x Kskyperf15a_UFactorCenterOfGlass);  
Else Kskyperf05_FenestrationProductType == SkylightGlassDeck result =   0.310 + (0.878 x   Kskyperf15a_UFactorCenterOfGlass);
 
 
End If
End Switch</xsd:documentation>
                                                            </xsd:annotation>
