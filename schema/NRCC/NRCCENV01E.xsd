Else If Hlog06_FramingDepth == 2In 
Switch (Hlog07_CavityInsulationNR) 
case R0: Rvalue = 1.4; case R1: Rvalue = 1.5;  case R2: Rvalue = 2.5;  case R3: Rvalue = 3.3; case R4: Rvalue = 4.0;  case R5: Rvalue = 4.7;  case R6: Rvalue = 5.3; case R7: Rvalue = 5.9; case R8: Rvalue = 6.4;  case R9:  Rvalue = 6.9; case R10: Rvalue =7.3; case  R11: Rvalue = 7.7; case R12: Rvalue = 8.1;  case R13: Rvalue = 8.4; case R14: Rvalue = 8.7;  case R15: Rvalue = 9.0; case R16: Rvalue = 9.3; 
