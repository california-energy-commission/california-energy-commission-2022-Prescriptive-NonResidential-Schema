                                                                  <xsd:element name="Hconcrete11_UfactorJA437" type="comp:UfactorJA437" >
                                                                     <xsd:annotation>
                                                                        <xsd:documentation source="FieldText">NOT REPORTED IN PDF</xsd:documentation>
                                                                        <xsd:documentation source="CalculationsAndRules">Calculated field: If Hconcrete08_NotAplicableMessge != FramingNA use this choice for not reported data from JA Table 4.3.7
If Hconcrete05_ConcreteWebPercent == 0pct  
If Hconcrete06_ SteelThroughInsulation == false 
Switch (Hconcrete07_ConcreteSandwichInsulation ) 
case R7_0: result = 0.122; case R9_3: result = 0.095; case R14_0: 0.066; case R18_6: 0.051; case R27_9: result = 0.034; 
End Switch
 
Else If Hconcrete06_ SteelThroughInsulation == true 
Switch (Hconcrete07_ConcreteSandwichInsulation ) 
case R7_0: result = 0.164; case R9_3: result = 0.128; case R14_0: 0.091; case R18_6: 0.070; case R27_9: result = 0.048;   
End Switch
End If
 
Else If Hconcrete05_ConcreteWebPercent == 10pct  
If Hconcrete06_ SteelThroughInsulation == false 
Switch (Hconcrete07_ConcreteSandwichInsulation ) 
case R7_0: result = 0.476; case R9_3: result = 0.435; case R14_0: 0.345; case R18_6: 0.286; case R27_9: result = 0.217;  
End Switch
 
Else If Hconcrete06_ SteelThroughInsulation == true 
Switch (Hconcrete07_ConcreteSandwichInsulation ) 
case R7_0: result = 0.500; case R9_3: result = 0.435; case R14_0: 0.357; case R18_6: 0.303; case R27_9: result = 0.227; 
End Switch 
End If
 
Else If Hconcrete05_ConcreteWebPercent == 20pct  
If Hconcrete06_ SteelThroughInsulation == false 
Switch (Hconcrete07_ConcreteSandwichInsulation ) 
case R7_0: result = 0.588; case R9_3: result = 0.556; case R14_0: 0.476; case R18_6: 0.417; case R27_9: result = 0.333; 
End Switch
 
Else If Hconcrete06_ SteelThroughInsulation == true 
Switch (Hconcrete07_ConcreteSandwichInsulation ) 
case R7_0: result = 0.588; case R9_3: result = 0.556; case R14_0: 0.476; case R18_6: 0.417; case R27_9: result = 0.333;
End Switch
End if
End If</xsd:documentation>
