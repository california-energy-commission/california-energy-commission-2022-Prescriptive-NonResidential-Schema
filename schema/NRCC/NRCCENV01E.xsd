                  <xsd:element name="Hmetalb" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">[d:b]Metal Building Walls [/d:b]</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">This subsection is required If H01MetalBldg_WallTypeApplies == true</xsd:documentation>
                     </xsd:annotation>
                     <xsd:complexType>
                        <xsd:sequence>
                           <xsd:element name="Hmetalb01_UseInAreaWeightedAveUfactor" type="comp:UseInAreaWeightedAveUfactor" >
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText">Calculate Area-Weighted Average U-factor for Metal Building Walls [d:sup]1[/d:sup]</xsd:documentation>
                                 <xsd:documentation source="CalculationsAndRules">User input: If checked result = true; Else result = false.  If result == true   D03_ExceptionalConditionMessageCCENVE += WallAreaWeightedAveInH</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                           <xsd:element name="TableHmetalb" >
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText"></xsd:documentation>
                              </xsd:annotation>
                              <xsd:complexType>
                                 <xsd:sequence>
                                    <xsd:element name="Row" minOccurs="1" maxOccurs="unbounded">
                                       <xsd:complexType>
                                          <xsd:sequence>
                                             <xsd:element name="Hmetalb02_WallAssemblyName" type="env:WallAssemblyName" >
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">Tag/Plan Detail ID</xsd:documentation>
                                                </xsd:annotation>
                                             </xsd:element>
                                             <xsd:element name="Hmetalb03a_TopLevelOccupancy" >
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">Occupancy &amp; Status</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">User selects from list: Nonresidential display term Nonresidential/ Relocatable 1 CZ: New, HotelMotel  display term Hotel/ Motel: New, RelocableSchoolBuildingAllZones display term Relocatable all CZ: New,  AlteredOccupancy display term Any occupancy: Altered, and ResidentialMF  display term Multifamily: New</xsd:documentation>
                                                   <xsd:appinfo source="displayterm">
                                                      <dtyp:displayterm value="Nonresidential">Nonresidential/ Relocatable 1 CZ: New</dtyp:displayterm>
                                                      <dtyp:displayterm value="HotelMotel">Hotel/Motel: New</dtyp:displayterm>
                                                      <dtyp:displayterm value="RelocableSchoolBuildingAllZones">Relocatable all CZ: New</dtyp:displayterm>
                                                      <dtyp:displayterm value="ResidentialMF">Multifamily: New</dtyp:displayterm>
                                                      <dtyp:displayterm value="AlteredOccupancy">Any occupancy: Altered</dtyp:displayterm>
                                                   </xsd:appinfo>
                                                   
                                                </xsd:annotation>
