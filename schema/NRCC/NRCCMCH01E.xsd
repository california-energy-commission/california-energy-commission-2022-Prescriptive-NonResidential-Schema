                                                            <xsd:annotation>
                                                               <xsd:documentation source="FieldText">Minimum Efficiency Required per Tables 110.2 / Title 20</xsd:documentation>
                                                               <xsd:documentation source="CalculationsAndRules">If F02_HvacSystemType == UnitaryHeatPumps Or UnitaryHeatPumpsNoElectR use this choice. For the following evaluations result1 is the EER value and result2 is the IEER:  
 
If F03_UnitaryHeatPumps  == AirCooledSplit1PhaseHP, AirCooledSplit3PhaseHP, AirCooledPkg1PhaseHP, AirCooledPkg3PhaseHP, AirCooledSplitConstrainedHP Or AirCooledPkgConstrainedHP  
 
Switch (Fa02_SizeRangeUnitaryHeatPumps) 
case GTE65000AndLT135000: 
If F02_HvacSystemType == UnitaryHeatPumps
result1 = 11.0 (EER) and result2 = 14.1 (IEER); 
Else If F02_HvacSystemType == UnitaryHeatPumpsNoElectR
result1 = 10.8 (EER) and result2 = 12.0 (IEER); 
End If
 
case GTE135000AndLT240000: 
If F02_HvacSystemType == UnitaryHeatPumps
result1 = 10.6 (EER) and result2 = 13.5 (IEER); 
Else If F02_HvacSystemType == UnitaryHeatPumpsNoElectR
result1 = 10.4 (EER) and result2 = 11.4 (IEER); 
End If
 
 
case GTE240000: 
If F02_HvacSystemType == UnitaryHeatPumps
