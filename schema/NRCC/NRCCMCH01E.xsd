                                                         <xsd:annotation>
                                                            <xsd:documentation source="FieldText">Efficiency Unit</xsd:documentation>
                                                            <xsd:documentation source="CalculationsAndRules">Two types are assigned for this choice as follows: 
If F02_HvacSystemType == FurnaceAC 
If F03_FurnaceAC == AirCooledSinglePkgAC_CentralFurnaceWeatherized1Phase Or AirCooledSplitAC_CentralFurnaceWeatherized1Phase 
If Fa02_SizeRangeFurnaceAC == LT45000CoolLT225000Heat Or LT45000CoolGTE225000Heat Or LT65000CoolLT225000Heat Or LT65000CoolGTE225000Heat Or GTE45000CoolLT225000Heat Or GTE45000CoolGTE225000Heat
result1 = EER2 and result2 == SEER2
Else if Fa02_SizeRangeFurnaceAC != LT45000CoolLT225000Heat Or LT45000CoolGTE225000Heat Or LT65000CoolLT225000Heat Or LT65000CoolGTE225000Heat Or GTE45000CoolLT225000Heat Or GTE45000CoolGTE225000Heat
result1 = EER and result2 == IEER
End If
Else If F03_FurnaceAC ==  AirCooledSplitAC_CentralFurnace Or AirCooledSinglePkgAC_CentralFurnace
If Fa02_SizeRangeFurnaceAC == LT45000CoolLT225000Heat Or LT45000CoolGTE225000Heat Or LT65000CoolLT225000Heat Or LT65000CoolGTE225000Heat Or GTE45000CoolLT225000Heat Or GTE45000CoolGTE225000Heat
result1 = EER and result2 = SEER
Else if Fa02_SizeRangeFurnaceAC != LT45000CoolLT225000Heat Or LT45000CoolGTE225000Heat Or LT65000CoolLT225000Heat Or LT65000CoolGTE225000Heat Or GTE45000CoolLT225000Heat Or GTE45000CoolGTE225000Heat
result 1 = EER and result 2 = IEER; 
End If
End If
 
Else If F02_HvacSystemType == UnitaryCondensersAC Or UnitaryCondensersAC_NoElectR : 
If SizeRangeUnitaryCondensersAC == LT45000  Or GTE45000AndLT65000 
result 1 = EER2 and result 2 = SEER2;
Else If SizeRangeUnitaryCondensersAC  == LT65000 And F03_ UnitaryCondensersAC  == AirCooledPkg1PhaseAC, AirCooledSmallDuctHighVelocityAC Or AirCooledSplitConstrainedAC
result 1 = EER2 and result 2 = SEER2;
Else If SizeRangeUnitaryCondensersAC  == LT30000 And  F03_ UnitaryCondensersAC == AirCooledPkgConstrainedAC Or AirCooledSplitConstrainedAC
result 1 = EER2 and result 2 = SEER2;
Else
result 1 =EER and result 2 = IEER;  
End If
 
Else If F02_HvacSystemType == UnitaryHeatPumps Or UnitaryHeatPumpsNoElectR  then 
If F03_UnitaryHeatPumps  == AirCooledSplit1PhaseHP, AirCooledSplit3PhaseHP, AirCooledPkg1PhaseHP, AirCooledPkg3PhaseHP, AirCooledSplitConstrainedHP Or AirCooledPkgConstrainedHP And SizeRangeUnitaryHeatPumps != LT65000 
result1 = EER and result2 = IEER: 
End If
 
Else If F02_HvacSystemType == VariableRefrigerantFlow 
If F03_ VariableRefrigerantFlow != GroundWaterSourceVRF Or GroundSourceVRF 
Or If F03_ VariableRefrigerantFlow == AirConditionerVRF Or AirCooledHP_VRF And  Fa02_SizeRangeVariableRefrigerantFlow != LT65000  
result1 = EER and result2 = IEER:
End If
End If</xsd:documentation>
                                                         </xsd:annotation>
