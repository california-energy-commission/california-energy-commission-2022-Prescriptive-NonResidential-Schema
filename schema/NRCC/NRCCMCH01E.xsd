         ExhuastFanBaseAllowance = 0.236
      End If
   Else If H00d_SystemZoning = AllOtherZoneTypes
      If H00f_FanSystemFlowCapacity LTE 5000
         ExhuastFanBaseAllowance = 0.186
      Else If H00f_FanSystemFlowCapacity LTE 10000
         ExhuastFanBaseAllowance = 0.184
      Else If H00f_FanSystemFlowCapacity GT 10000
         ExhuastFanBaseAllowance = 0.190
      End If
   Else If H00d_SystemZoning ==  DOASZone
      ExhuastFanBaseAllowance is not applicable for calculation below
   End If
End If
</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="H12b_NotApplicableMessage" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Exhuast/Return/Relief/Transfer Fan Base Allowance(kW)</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">If H00e_SystemZoning == MultiDwellingUnits Or SingleDwellingUnit
Or
H00d_SystemZoning == DOASZone
Or 
H13_FanSystemInputPower  LT 1.0 kW 
result = BlankGrayedOut stored in NotApplicableMessage
End If</xsd:documentation>
                                          </xsd:annotation>
                                          <xsd:simpleType>
                                             <xsd:restriction base="comp:NotApplicableMessage">
                                                <xsd:enumeration value="BlankGrayedOut"/>
                                             </xsd:restriction>
                                          </xsd:simpleType>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="H12_FanSystemPowerAllowance" type="comp:FanSystemPowerAllowance" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Fan System Allowance (kW)[d:sup]3[/d:sup]</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">Calculated value:
If H13_FanSystemInputPower GTE 1.0
If H00d_SystemZoning != DOAS
If H00g_Elevation LTE 3,000 ft
result = ((H12a_SupplyFanBaseAllowance + H12b_ExhaustFanBaseAllowance + SUM ALL (H03_FanCount x H08_FanAllowance))  x  H00f_FanSystemFlowCapacity) / 1000   
Else If H00g_Elevation GTE 3,000 ft And LT 4,000
result = 0.896  x (((H12a_SupplyFanBaseAllowance + H12b_ExhaustFanBaseAllowance + SUM ALL (H03_FanCount x H08_FanAllowance)) x  H00f_FanSystemFlowCapacity) / 1000)   
Else If H00g_Elevation GTE 4,000 ft And LT 5,000
result = 0.864 x (((H12a_SupplyFanBaseAllowance + H12b_ExhaustFanBaseAllowance + SUM ALL (H03_FanCount x H08_FanAllowance)) x  H00f_FanSystemFlowCapacity) / 1000)  
Else If H00g_Elevation GTE 5,000 ft And LT 6,000
result = 0.832 x (((H12a_SupplyFanBaseAllowance + H12b_ExhaustFanBaseAllowance + SUM ALL (H03_FanCount x H08_FanAllowance)) x  H00f_FanSystemFlowCapacity) / 1000)   
Else If H00g_Elevation GTE 6,000 ft 
result = 0.801 x (((H12a_SupplyFanBaseAllowance + H12b_ExhaustFanBaseAllowance + SUM ALL (H03_FanCount x H08_FanAllowance)) x  H00f_FanSystemFlowCapacity) / 1000)   
End If
Else If H00d_SystemZoning == DOASZone
result = H00f_FanSystemFlowCapacity / 1000 
End If
End If</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="H12_NotApplicableMessage" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Fan System Allowance (kW)[d:sup]3[/d:sup]</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">If H00e_SystemZoning == MultiDwellingUnits Or SingleDwellingUnit
Or
H13_FanSystemInputPower  LT 1.0 kW  
result = BlankGrayedOut stored in NotApplicableMessage
End If</xsd:documentation>
                                          </xsd:annotation>
                                          <xsd:simpleType>
                                             <xsd:restriction base="comp:NotApplicableMessage">
                                                <xsd:enumeration value="BlankGrayedOut"/>
                                             </xsd:restriction>
                                          </xsd:simpleType>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="H13_FanSystemInputPower" type="hvac:FanSystemInputPower" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Fan System Electrical Output (kW)</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">Calculated value: result = for  fans in system Sum All (H03_FanCount x H11_FanPower)</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="H13_NotApplicableMessage" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Fan System Electrical Output (kW)</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">If H00e_SystemZoning == MultiDwellingUnits Or SingleDwellingUnit
result = BlankGrayedOut
End If</xsd:documentation>
                                          </xsd:annotation>
                                          <xsd:simpleType>
                                             <xsd:restriction base="comp:NotApplicableMessage">
                                                <xsd:enumeration value="BlankGrayedOut"/>
                                             </xsd:restriction>
                                          </xsd:simpleType>
                                       </xsd:element>
                                    </xsd:choice>
                                 </xsd:sequence>
                              </xsd:complexType>
                           </xsd:element>
                        </xsd:sequence>
                     </xsd:complexType>
                  </xsd:element>
                  <xsd:element name="HFootnote1" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">[d:i][d:sup]1[/d:sup] FOOTNOTES: Fans serving spaces with design background noise goals below NC35 [/d:i]</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="HFootnote2" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">[d:i][d:sup]2[/d:sup] Low-turndown single-zone VAV fan system must be capable of and configured to reduce airflow to 50 percent of design airflow and use no more than 30 percent of the design wattage at that airflow. No more than 10 percent of the design load served by the equipment shall have fixed loads.[/d:i]</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="HFootnote3" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">[d:i][d:sup]3[/d:sup] Fan system allowance includes fan system base allowance.[/d:i]</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="HFootnote4" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">[d:i][d:sup]4[/d:sup] Filter pressure loss can only be counted once per fan system.[/d:i]</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="HFootnote5" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">[d:i][d:sup]5[/d:sup] Complex Fan System means a fan system that combines a single cabinet fan system with other supply fans, exhaust fans, or both.[/d:i]</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="HFootnote6" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">[d:i][d:sup]6[/d:sup] Computer room economizers must meet requirements of [d:t24refnolink/]140.9(a) and will be documented on the NRCC-PRC-E document..[/d:i]</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="TableDwellingFan" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">[d:b]H. DWELLING UNIT FAN EFFICACY &amp; ENERGY/HEAT RECOVERY 170.2(c)3B [/d:b]</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">This table is required If H00e_SystemZoning == MultiDwellingUnits Or SingleDwellingUnit</xsd:documentation>
                     </xsd:annotation>
                     <xsd:complexType>
                        <xsd:sequence>
                           <xsd:element name="Row" minOccurs="1" maxOccurs="unbounded">
                              <xsd:complexType>
                                 <xsd:sequence>
                                    <xsd:element name="Hdwell01_FanSystemName" type="hvac:FanSystemName" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Fan System Name or Item Tag</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">result = H00a_FanSystemName when H00e_SystemZoning == MultiDwellingUnits Or SingleDwellingUnit</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="Hdwell02_SystemCount" type="hvac:SystemCount" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Qty</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">result - H00b_HvacSystemCount;</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="Hdwell03_SystemZoning" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">System Zoning</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">result = H00e_SystemZoning</xsd:documentation>
                                       </xsd:annotation>
                                       <xsd:simpleType>
                                          <xsd:restriction base="hvac:SystemZoning">
                                             <xsd:enumeration value="MultiDwellingUnits"/>
                                             <xsd:enumeration value="NotServingDwellingUnit"/>
                                             <xsd:enumeration value="SingleDwellingUnit"/>
                                          </xsd:restriction>
                                       </xsd:simpleType>
                                    </xsd:element>
                                    <xsd:element name="Hdwell04_FanSystemAHU" type="hvac:FanSystemAHU" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">System Type Serving</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">User selects from list</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="Hdwell05_FanSystemFlowCapacity" type="hvac:FanSystemFlowCapacity" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">System Airflow (cfm)</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">User input</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="Hdwell05_NotApplicableMessage" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">System Airflow (cfm)</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">See Hdwell08_NotAppicableMessage logic to see if this result = BlankGrayedOut</xsd:documentation>
                                          </xsd:annotation>
                                          <xsd:simpleType>
                                             <xsd:restriction base="comp:NotApplicableMessage">
                                                <xsd:enumeration value="BlankGrayedOut"/>
                                             </xsd:restriction>
                                          </xsd:simpleType>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="Hdwell06_FanSystemInputPower" type="hvac:FanSystemInputPower" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Design Power (kW)</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">User input</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="Hdwell06_NotApplicableMessage" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Design Power (kW)</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">See Hdwell08_NotAppicableMessage logic to see if this result = BlankGrayedOut</xsd:documentation>
                                          </xsd:annotation>
                                          <xsd:simpleType>
                                             <xsd:restriction base="comp:NotApplicableMessage">
                                                <xsd:enumeration value="BlankGrayedOut"/>
                                             </xsd:restriction>
                                          </xsd:simpleType>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="Hdwell07_FanPowerPerFlow" type="hvac:FanPowerPerFlow" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Design Watts/CFM [d:t24refnolink/]170.2(c)3B</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">result = (Hdwell06_FanSystemInputPower / 1000)  / Hdwell05_FanSystemFlowCapacity</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="Hdwell07_NotApplicableMessage" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Design Watts/CFM [d:t24refnolink/]170.2(c)3B</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">If Hdwell05_NotApplicableMessage Or Hdwell06_NotApplicableMessage IsPresent
result = BlankGrayedOut
End If</xsd:documentation>
                                          </xsd:annotation>
                                          <xsd:simpleType>
                                             <xsd:restriction base="comp:NotApplicableMessage">
                                                <xsd:enumeration value="BlankGrayedOut"/>
                                             </xsd:restriction>
                                          </xsd:simpleType>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="Hdwell08_FanPowerPerFlow" type="hvac:FanPowerPerFlow" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Maximum Watts/CFM [d:t24refnolink/]170.2(c)3B</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">Calculated field:
If Hdwell03_SystemZoning == MultiDwellingUnits
If Hdwell09_EnergyHeatRecoveryVent == ProvidesERV Or ProvidesHRV
If A02_ClimateZoneNumber == 1, 2 Or InRange(11,16)
If A03All_WholeBuildingOccupancy == HighRiseResidential
result = BlankGrayedOut stored in NotApplicableMessage And this system must be transferred to TableFanSystems to meet common area fan power requirements
End If
End If
End If
Else If Hdwell03_SystemZoning == SingleDwellingUnit
If Hdwell04_FanSystemAHU == NonAHU
If Hdwell09_EnergyHeatRecoveryVent == ProvidesERV Or ProvidesHRV
If A02_ClimateZoneNumber == 1, 2 Or InRange(11,16)
result = 0.60
End If
End If
Else If Hdwell04_FanSystemAHU == NonGasFurnaceAHU
result = 0.58
Else if Hdwell04_FanSystemAHU == FurnaceGasAHU
result = 0.45
End If
Else
result =  BlankGrayedOut stored in NotApplicableMessage And Hdwell05_NotApplicableMessage  And Hdwell06_NotApplicableMessage  And Hdwell07_NotApplicableMessage = BlanGrayedOut also
End If</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="Hdwell08_NotApplicableMessage" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Maximum Watts/CFM [d:t24refnolink/]170.2(c)3B</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">Calculated field:
If Hdwell03_SystemZoning == MultiDwellingUnits
If Hdwell09_EnergyHeatRecoveryVent == ProvidesERV Or ProvidesHRV
If A02_ClimateZoneNumber == 1, 2 Or InRange(11,16)
If A03All_WholeBuildingOccupancy == HighRiseResidential
result = BlankGrayedOut stored in NotApplicableMessage And this system must be transferred to TableFanSystems to meet common area fan power requirements
End If
End If
End If
Else If Hdwell03_SystemZoning == SingleDwellingUnit
If Hdwell04_FanSystemAHU == NonAHU
If Hdwell09_EnergyHeatRecoveryVent == ProvidesERV Or ProvidesHRV
If A02_ClimateZoneNumber == 1, 2 Or InRange(11,16)
result = 0.60 stored in Hdwell08_FanPowerPerFlow
End If
End If
Else If Hdwell04_FanSystemAHU == NonGasFurnaceAHU
result = 0.58 stored in Hdwell08_FanPowerPerFlow
Else if Hdwell04_FanSystemAHU == FurnaceGasAHU
result = 0.45 stored in Hdwell08_FanPowerPerFlow
End If
Else
result =  BlankGrayedOut stored in NotApplicableMessage And Hdwell05_NotApplicableMessage  And Hdwell06_NotApplicableMessage  And Hdwell07_NotApplicableMessage = BlanGrayedOut also
End If</xsd:documentation>
                                          </xsd:annotation>
                                          <xsd:simpleType>
                                             <xsd:restriction base="comp:NotApplicableMessage">
                                                <xsd:enumeration value="BlankGrayedOut"/>
                                             </xsd:restriction>
                                          </xsd:simpleType>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:element name="Hdwell09_EnergyHeatRecoveryVent" type="comp:EnergyHeatRecoveryVent" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">ERV/HRV</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">User selects from dropdown list</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="Hdwell10_SensibleRecoveryEfficiency" type="hvac:SensibleRecoveryEfficiency" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Design Sensible Recovery/Effectiveness</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">User input integer percent value</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="Hdwell10_NotApplicableMessage" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Design Sensible Recovery/Effectiveness</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">Calcualted field:  If Hdwell09_EnergyHeatRecoveryVent != ProvidesERV And  != ProvidesHRV
result = BlankGrayedOut stored in NotApplicableMessage
End If</xsd:documentation>
                                          </xsd:annotation>
                                          <xsd:simpleType>
                                             <xsd:restriction base="comp:NotApplicableMessage">
                                                <xsd:enumeration value="BlankGrayedOut"/>
                                             </xsd:restriction>
                                          </xsd:simpleType>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="Hdwell11_SensibleRecoveryEfficiency" type="hvac:SensibleRecoveryEfficiency" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Required Sensible Recovery/Effectiveness [d:t24refnolink/]170.2(c)3Biv</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">Calculated field:  If Hdwell09_EnergyHeatRecoveryVent == ProvidesERV Or ProvidesHRV
result = 67%
Else 
result = BlankGrayedOut stored in NotApplicableMessage
End If</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="Hdwell11_NotApplicableMessage" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Required Sensible Recovery/Effectiveness [d:t24refnolink/]170.2(c)3Biv</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">Calcualted field:  If Hdwell09_EnergyHeatRecoveryVent != ProvidesERV And  != ProvidesHRV
result = BlankGrayedOut stored in NotApplicableMessage
End If</xsd:documentation>
                                          </xsd:annotation>
                                          <xsd:simpleType>
                                             <xsd:restriction base="comp:NotApplicableMessage">
                                                <xsd:enumeration value="BlankGrayedOut"/>
                                             </xsd:restriction>
                                          </xsd:simpleType>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="Hdwell12_RecoveryBypassType" type="comp:RecoveryBypassType">
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Recovery Bypass/ Directly Economize Controls [d:t24refnolink/]170.2(c)3Biv</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">If H00e_SystemZoning != SingleDwellingUnit And A02_ClimateZoneNumber != InRange(3,10)
User selects from dropdown list
Else
result = BlankGrayedOut
End If</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="Hdwell12_NotApplicableMessage" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Recovery Bypass/ Directly Economize Controls [d:t24refnolink/]170.2(c)3Biv</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">If H00e_SystemZoning == SingleDwellingUnit Or A02_ClimateZoneNumber == InRange(3,10)
result = BlankGrayedOut
End If</xsd:documentation>
                                          </xsd:annotation>
                                          <xsd:simpleType>
                                             <xsd:restriction base="comp:NotApplicableMessage">
                                                <xsd:enumeration value="BlankGrayedOut"/>
                                             </xsd:restriction>
                                          </xsd:simpleType>
                                       </xsd:element>
                                    </xsd:choice>
                                 </xsd:sequence>
                              </xsd:complexType>
                           </xsd:element>
                        </xsd:sequence>
                     </xsd:complexType>
                  </xsd:element>
                  <xsd:element name="TableExhaustFan" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">[d:b]H. EXHAUST AIR HEAT RECOVERY 140.4(q), 170.2(c)4O [/d:b]</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">This table is required If B03c_ProjectHasFanSystems == true  And A03_ClimateZoneNumber != 6 And != 7</xsd:documentation>
                     </xsd:annotation>
                     <xsd:complexType>
                        <xsd:sequence>
                           <xsd:element name="Row" minOccurs="1" maxOccurs="unbounded">
                              <xsd:complexType>
                                 <xsd:sequence>
                                    <xsd:element name="Hexhaust01_FanSystemName" type="hvac:FanSystemName" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Fan System Name</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">result = H00a_FanSystemName</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="Hexhaust02_SystemCount" type="hvac:SystemCount" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Qty</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">If Hexhaust07_HeatRecoveryExemptions == NoExemptApply 
result - H00b_HvacSystemCount;</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="Hexhaust02_NotApplicableMessage" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Qty</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">If Hexhaust07_HeatRecoveryExemptions != NoExemptApply
result = BlankGrayedOut
End If</xsd:documentation>
                                          </xsd:annotation>
                                          <xsd:simpleType>
                                             <xsd:restriction base="comp:NotApplicableMessage">
                                                <xsd:enumeration value="BlankGrayedOut"/>
                                             </xsd:restriction>
                                          </xsd:simpleType>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="Hexhaust03_HeatRecoveryHours" type="comp:HeatRecoveryHours" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Hours of Operation per Year</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">User selects from list</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="Hexhaust03_NotApplicableMessage" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Hours of Operation per Year</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">If Hexhaust07_HeatRecoveryExemptions != NoExemptApply
result = BlankGrayedOut
End If</xsd:documentation>
                                          </xsd:annotation>
                                          <xsd:simpleType>
                                             <xsd:restriction base="comp:NotApplicableMessage">
                                                <xsd:enumeration value="BlankGrayedOut"/>
                                             </xsd:restriction>
                                          </xsd:simpleType>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:element name="Hexhaust04_AirflowDesignRate" type="hvac:AirflowDesignRate" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Design Supply Airflow Rate</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">User input</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="Hexhaust05_OutdoorAirflow" type="hvac:OutdoorAirflow" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Outdoor Airflow</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">User input</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="Hexhaust06_OutdoorAirflowPct" type="hvac:OutdoorAirflowPct" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">% Outdoor Air at Full Design Airflow</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">Calculated field: 
result =  Hexhaust05_OutdoorAirflow / Hexhaust04_AirflowDesignRate </xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="Hexhaust07_HeatRecoveryExemptions" type="comp:HeatRecoveryExemptions" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Exemptions to Exhaust Air Heat Recovery Requirement per [d:t24refnolink/]140.4(q) &amp; [d:t24refnolink/]170.2(c)4O</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">User selects from list
 
If result != NoExemptApply  
Report Display Term = Not Required
End If</xsd:documentation>
                                          <xsd:appinfo source="displayterm">
                                             <dtyp:displayterm value="CZ15HeatingNA">Not Required</dtyp:displayterm>
                                             <dtyp:displayterm value="CZ1CoolingNA">Not Required</dtyp:displayterm>
                                             <dtyp:displayterm value="LabFactoryExhaustNA">Not Required</dtyp:displayterm>
                                             <dtyp:displayterm value="LT75%AirFlowWithin20FtNA">Not Required</dtyp:displayterm>
