                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Minimum Efficiency Required per Tables 110.2 / Title 20</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">If F05_NotApplicableMessage IsPresent Or F05_HeatingDesignCapacity LT 1 
result = BlankGrayedOut stored in Fa04_NotApplicableMessage and this field is greyed out
End If</xsd:documentation>
                                          </xsd:annotation>
                                          <xsd:simpleType>
                                             <xsd:restriction base="comp:NotApplicableMessage">
                                                <xsd:enumeration value="BlankGrayedOut"/>
