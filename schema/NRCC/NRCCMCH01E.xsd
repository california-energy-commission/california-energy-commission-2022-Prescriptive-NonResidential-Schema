Else If F03_VariableRefrigerantFlow == WaterSourceVRF  
Switch (Fa02_SizeRangeUnitaryCondensersAC)  
case LT65000: Or  GTE65000AndLT135000: 
result1 = 12.0 (EER) and result2 = 16.0 (IEER); 
 
case GTE135000AndLT240000: 
result1 = 10.0 (EER) and result2 = 14.0 (IEER); 
 
case GTE240000: 
result1 = 10.0 (EER) and result2 = 12.0 (IEER);
End Switch
End If</xsd:documentation>
                                                            </xsd:annotation>
                                                         </xsd:element>
                                                         <xsd:element name="Fa08_CoolEfficiencyMinUnitAC_EERIEER" type="hvac:CoolEfficiencyMinUnitAC_EERIEER"  minOccurs="2" maxOccurs="2">
                                                            <xsd:annotation>
                                                               <xsd:documentation source="FieldText">Minimum Efficiency Required per Tables 110.2 / Title 20</xsd:documentation>
                                                               <xsd:documentation source="CalculationsAndRules">If F02_HvacSystemType == UnitaryCondensersAC Or UnitaryCondensersAC _NoElectR use this choice.  For the following evaluations result1 is the EER value and result2 is the IEER:  
 
If Fa02_SizeRangeUnitaryCondensersAC != LT45000  GTE45000AndLT65000 Or LT65000  
If F03_ UnitaryCondensersAC == WaterCooledAC 
 
Switch Fa02_SizeRangeUnitaryCondensersAC 
case GTE65000AndLT135000: 
If F02_HvacSystemType == UnitaryCondensersAC
result1 = 12.1 (EER) and result2 = 13.9 (IEER); 
Else If F02_HvacSystemType == UnitaryCondensersAC _NoElectR
result1 = 11.9 (EER) and result2 = 13.7 (IEER); 
End If
 
case GTE135000AndLT240000: 
If F02_HvacSystemType == UnitaryCondensersAC
result1 = 12.5 (EER) and result2 = 13.9 (IEER); 
Else If F02_HvacSystemType == UnitaryCondensersAC _NoElectR
result1 = 12.3 (EER) and result2 = 13.7 (IEER); 
End If
 
case GTE240000AndLT760000: 
If F02_HvacSystemType == UnitaryCondensersAC
result1 = 12.4 (EER) and result2 = 13.6 (IEER);  
Else If F02_HvacSystemType == UnitaryCondensersAC _NoElectR
result1 = 12.2 (EER) and result2 = 13.4 (IEER); 
End If
 
case GTE760000: 
If F02_HvacSystemType == UnitaryCondensersAC
result1 = 12.2 (EER) and result2 = 13.5 (IEER);   
Else If F02_HvacSystemType == UnitaryCondensersAC _NoElectR
result1 = 12.0 (EER) and result2 = 13.3 (IEER); 
End If
 
End Switch
 
Else If F03_ UnitaryCondensersAC == EvaporativeCooledAC  
Switch (Fa02_SizeRangeUnitaryCondensersAC) 
case GTE65000AndLT135000: 
If F02_HvacSystemType == UnitaryCondensersAC
result1 = 12.1 (EER) and result2 = 12.3 (IEER); 
Else If F02_HvacSystemType == UnitaryCondensersAC_NoElectR
result1 = 11.9 (EER) and result2 = 12.1 (IEER); 
End If
 
case GTE135000AndLT240000: 
If F02_HvacSystemType == UnitaryCondensersAC
result1 = 12.0 (EER) and result2 = 12.2 (IEER); 
Else If F02_HvacSystemType == UnitaryCondensersAC_NoElectR
result1 = 11.8 (EER) and result2 = 12.0 (IEER); 
End If
 
case GTE240000AndLT760000: 
If F02_HvacSystemType == UnitaryCondensersAC
result1 = 11.9 (EER) and result2 = 12.1 (IEER);  
Else If F02_HvacSystemType == UnitaryCondensersAC_NoElectR
result1 = 11.7 (EER) and result2 = 11.9 (IEER); 
End If
 
 
case GTE760000: 
If F02_HvacSystemType == UnitaryCondensersAC
result1 = 11.7 (EER) and result2 = 11.9 (IEER);  
Else If F02_HvacSystemType == UnitaryCondensersAC_NoElectR
result1 = 11.5 (EER) and result2 = 11.7 (IEER); 
End If
 
End Switch
 
Else If F03_ UnitaryCondensersAC == AirCooledCondensingUnit  
If Fa02_SizeRangeUnitaryCondensersAC == GTE135000 
result1 = 10.5 (EER) and result2 = 11.8 (IEER); 
End If
 
Else If F03_ UnitaryCondensersAC == WaterCooledCondensingUnit Or EvaporativeCooledCondensingUnit   
If Fa02_SizeRangeUnitaryCondensersAC == GTE135000 
result1 = 13.5 (EER) and result2 = 14.0 (IEER);   
End If
 
Else If F03_ UnitaryCondensersAC == AirCooledSplit1PhaseAC, AirCooledSplit3PhaseAC, AirCooledPkg1PhaseAC, AirCooledPkg3PhaseAC, AirCooledSplitConstrainedAC Or AirCooledPkgConstrainedAC  
Switch (Fa02_SizeRangeUnitaryCondensersAC) 
case GTE65000AndLT135000: 
If F02_HvacSystemType == UnitaryCondensersAC
result1 = 11.2 (EER) and result2 = 14.8 (IEER); 
Else If F02_HvacSystemType == UnitaryCondensersAC_NoElectR
result1 = 11.0 (EER) and result2 = 14.6 (IEER); 
End If
 
case GTE135000AndLT240000: 
If F02_HvacSystemType == UnitaryCondensersAC
result1 = 11.0 (EER) and result2 = 14.2 (IEER); 
Else If F02_HvacSystemType == UnitaryCondensersAC_NoElectR
result1 = 10.8 (EER) and result2 = 14.0 (IEER); 
End If
  
case GTE240000AndLT760000: 
If F02_HvacSystemType == UnitaryCondensersAC
result1 = 10.0 (EER) and result2 = 13.2 (IEER); 
Else If F02_HvacSystemType == UnitaryCondensersAC_NoElectR
result1 = 9.8 (EER) and result2 = 13.0 (IEER); 
End If
 
 
case GTE760000: 
If F02_HvacSystemType == UnitaryCondensersAC
result1 = 9.7 (EER) and result2 = 12.5 (IEER);
Else If F02_HvacSystemType == UnitaryCondensersAC_NoElectR
result1 = 9.5 (EER) and result2 = 12.3 (IEER); 
End If
 
 
End Switch
End If
End if</xsd:documentation>
                                                            </xsd:annotation>
                                                         </xsd:element>
                                                         <xsd:element name="Fa08_CoolEfficiencyMinUnitAC_EERSEER" type="hvac:CoolEfficiencyMinUnitAC_EERSEER"  minOccurs="2" maxOccurs="2">
                                                            <xsd:annotation>
                                                               <xsd:documentation source="FieldText">Minimum Efficiency Required per Tables 110.2 / Title 20</xsd:documentation>
                                                               <xsd:documentation source="CalculationsAndRules">If F02_HvacSystemType == UnitaryCondensersAC Or UnitaryCondensersAC_NoElectR use this choice. For the following evaluations result1 is the EER value and result2 is the SEER:  
If   SizeRangeUnitaryCondensersAC ==GTE45000AndLT65000  
result1 = 11.7 (EER) And result 2 =  14.0 (SEER) 
End If</xsd:documentation>
                                                            </xsd:annotation>
                                                         </xsd:element>
                                                      </xsd:choice>
                                                      <xsd:choice maxOccurs="1">
                                                         <xsd:element name="Fa09_EfficiencyEER_IEER" type="hvac:EfficiencyEER_IEER"  minOccurs="2" maxOccurs="2">
                                                            <xsd:annotation>
                                                               <xsd:documentation source="FieldText">Design Efficiency</xsd:documentation>
                                                               <xsd:documentation source="CalculationsAndRules">If Fa07_EfficiencyType result 1== EER and  result 2== IEER use this choice to store user input of design  EER  for result1 and IEER for result2</xsd:documentation>
                                                            </xsd:annotation>
                                                         </xsd:element>
                                                         <xsd:element name="Fa09_EfficiencySEER_EER" type="hvac:EfficiencySEER_EER"  minOccurs="2" maxOccurs="2">
                                                            <xsd:annotation>
                                                               <xsd:documentation source="FieldText">Design Efficiency</xsd:documentation>
                                                               <xsd:documentation source="CalculationsAndRules">If Fa07_EfficiencyType result 1== EER and  result 2== SEER use this choice to store user input of design  EER  for result1 and SEER for result2</xsd:documentation>
                                                            </xsd:annotation>
                                                         </xsd:element>
                                                         <xsd:element name="Fa09_EfficiencySEER2_EER2" type="hvac:EfficiencySEER2_EER2"  minOccurs="2" maxOccurs="2">
                                                            <xsd:annotation>
                                                               <xsd:documentation source="FieldText">Design Efficiency</xsd:documentation>
                                                               <xsd:documentation source="CalculationsAndRules">If Fa07_EfficiencyType result 1== EER2 and  result 2== SEER2 use this choice to store user input of design  EER2  for result1 and SEER2 for result2</xsd:documentation>
                                                            </xsd:annotation>
                                                         </xsd:element>
                                                      </xsd:choice>
                                                   </xsd:sequence>
                                                </xsd:complexType>
                                             </xsd:element>
                                             <xsd:element name="CoolingNA" >
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText"></xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">Use this choice If F08_NotApplicableMessage IsPresent
 Or F08_CoolingSensibleDesignCapacity LT 1 for Fa01 
Or If F03_UnitaryHeatPumps == WaterSourceHP And If Fa02_SizeRangeUnitaryHeatPumps == GTE135000AndLT240000
Or If F03_ CeilingCRAC == AirCooledFreeAirDucted: And Fa02_SizeRangeCeilingCRAC ==LT29000</xsd:documentation>
                                                </xsd:annotation>
                                                <xsd:complexType>
                                                   <xsd:sequence>
                                                      <xsd:element name="Fa07_NotApplicableMessage" >
                                                         <xsd:annotation>
                                                            <xsd:documentation source="FieldText">Efficiency Unit</xsd:documentation>
                                                            <xsd:documentation source="CalculationsAndRules">Result = = BlankGrayedOut stored in NotApplicableMessage and cell is blank and grayed out</xsd:documentation>
                                                         </xsd:annotation>
                                                         <xsd:simpleType>
                                                            <xsd:restriction base="comp:NotApplicableMessage">
                                                               <xsd:enumeration value="BlankGrayedOut"/>
                                                            </xsd:restriction>
                                                         </xsd:simpleType>
                                                      </xsd:element>
                                                      <xsd:element name="Fa08_NotApplicableMessage" >
                                                         <xsd:annotation>
                                                            <xsd:documentation source="FieldText">Minimum Efficiency Required per Tables 110.2 / Title 20</xsd:documentation>
                                                            <xsd:documentation source="CalculationsAndRules">Result = = BlankGrayedOut stored in NotApplicableMessage and cell is blank and grayed out</xsd:documentation>
                                                         </xsd:annotation>
                                                         <xsd:simpleType>
                                                            <xsd:restriction base="comp:NotApplicableMessage">
                                                               <xsd:enumeration value="BlankGrayedOut"/>
                                                            </xsd:restriction>
                                                         </xsd:simpleType>
                                                      </xsd:element>
                                                      <xsd:element name="Fa09_NotApplicableMessage" >
                                                         <xsd:annotation>
                                                            <xsd:documentation source="FieldText">Design Efficiency</xsd:documentation>
                                                            <xsd:documentation source="CalculationsAndRules">Result = = BlankGrayedOut stored in NotApplicableMessage and cell is blank and grayed out</xsd:documentation>
                                                         </xsd:annotation>
                                                         <xsd:simpleType>
                                                            <xsd:restriction base="comp:NotApplicableMessage">
                                                               <xsd:enumeration value="BlankGrayedOut"/>
                                                            </xsd:restriction>
                                                         </xsd:simpleType>
                                                      </xsd:element>
                                                   </xsd:sequence>
                                                </xsd:complexType>
                                             </xsd:element>
