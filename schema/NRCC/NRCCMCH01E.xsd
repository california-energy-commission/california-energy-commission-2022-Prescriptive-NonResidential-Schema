End Switch
 
Else If F03_VariableRefrigerantFlow == WaterSourceVRF  
Switch (Fa02_SizeRangeUnitaryCondensersAC)  
case LT65000: Or  GTE65000AndLT135000: 
result1 = 12.0 (EER) and result2 = 16.0 (IEER); 
 
case GTE135000AndLT240000: 
result1 = 10.0 (EER) and result2 = 14.0 (IEER); 
 
case GTE240000: 
result1 = 10.0 (EER) and result2 = 12.0 (IEER);
End Switch
End If</xsd:documentation>
