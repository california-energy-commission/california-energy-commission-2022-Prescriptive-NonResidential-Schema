                                                      <xsd:choice maxOccurs="1">
                                                         <xsd:element name="J14_ExhaustSpaceRequiredMin" type="com:ExhaustSpaceRequiredMin" >
                                                            <xsd:annotation>
                                                               <xsd:documentation source="FieldText">Required Min CFM</xsd:documentation>
                                                               <xsd:documentation source="CalculationsAndRules">Calculated Field: 
Switch (J09_VentilationSpaceFunction) 
case ClassroomArt, FoodServiceKitchen: 
result = 0.7 x J10_SpaceFloorArea; 
 
case AutoRepairVent, StorageOccupiableLiquids: 
result = 1.5 x J10_SpaceFloorArea; 
 
case BarberShop, CopyPrintRoom, ArenasVent,  LockerAthleticIndustrial,  SportsSpectatorAreaVent, WoodMetalShop: 
result = 0.5 x J10_SpaceFloorArea; 
 
case BeautyNailSalon: 
result = 0.6 x J10_SpaceFloorArea; 
 
case  CellWithToiletVent, DarkRoom, JanitorRoom, SoiledLaundryStorage, ScienceLab, TrashRecyclingRoom, CollegeLab: 
result = 1.0 x J10_SpaceFloorArea; 
 
case  KitchenetteVent: 
result = 0.3 x J10_SpaceFloorArea; 
 
case LockerAllOthers: 
result = 0.25 x J10_SpaceFloorArea;   
 
case ParkingGarageVent: 
result = 0.75 x J10_SpaceFloorArea;  
 
case PetShop: result = 0.9 x J10_SpaceFloorArea;  
 
case ShowerRoom, RestroomPrivate, RestroomPublicContExhaust: 
result = 50 x J11_ExhaustNumberFixtures; 
 
case ShowerContExhaust: 
result = 20 x J11_ExhaustNumberFixtures;  
 
case RestroomPrivateContExhaust: 
result = 25 x J11_ExhaustNumberFixtures; 
 
case   RestroomPublic: 
result = 70 x J11_ExhaustNumberFixtures;
End Switch
 
Mandatory Measure Triggers:
If result GT 0  
MM120_1c4_MeasureApplies = true and is displayed
End If</xsd:documentation>
