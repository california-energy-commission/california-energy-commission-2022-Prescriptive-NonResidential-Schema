<?xml version="1.0" encoding="UTF-8"?>
<xsd:schema
   targetNamespace="http://www.lmonte.com/besm/NRCCMCH01E"
   xmlns="http://www.lmonte.com/besm/NRCCMCH01E"
   xmlns:d="http://www.lmonte.com/besm/d"
   xmlns:bld="http://www.lmonte.com/besm/bld"
   xmlns:env="http://www.lmonte.com/besm/env"
   xmlns:lit="http://www.lmonte.com/besm/lit"
   xmlns:hvac="http://www.lmonte.com/besm/hvac"
   xmlns:comp="http://www.lmonte.com/besm/comp"
   xmlns:com="http://www.lmonte.com/besm/com"
   xmlns:dtyp="http://www.lmonte.com/besm/dtyp"
   xmlns:xsd="http://www.w3.org/2001/XMLSchema"
   xmlns:altova="http://www.altova.com/xml-schema-extensions"
   elementFormDefault="qualified" attributeFormDefault="unqualified" version="2022.0.000">
   <xsd:import namespace="http://www.lmonte.com/besm/com" schemaLocation="../base/NrPresCommon.xsd"/>
   <xsd:import namespace="http://www.lmonte.com/besm/bld" schemaLocation="../base/NrPresBuilding.xsd"/>
   <xsd:import namespace="http://www.lmonte.com/besm/env" schemaLocation="../base/NrPresEnvelope.xsd"/>
   <xsd:import namespace="http://www.lmonte.com/besm/lit" schemaLocation="../base/NrPresLighting.xsd"/>
   <xsd:import namespace="http://www.lmonte.com/besm/hvac" schemaLocation="../base/NrPresHvac.xsd"/>
   <xsd:import namespace="http://www.lmonte.com/besm/comp" schemaLocation="../base/NrPresCompliance.xsd"/>
   <xsd:import namespace="http://www.lmonte.com/besm/dtyp" schemaLocation="../base/DataTypes.xsd"/>
   <xsd:element name="ComplianceDocumentPackage">
      <xsd:complexType>
         <xsd:sequence>
            <xsd:element name="DocID" minOccurs="0">
               <xsd:complexType>
                  <xsd:attribute name="doc" type="comp:ComplianceDocumentTagNR" fixed="NRCCMCHE"/>
                  <xsd:attribute name="docType" type="comp:ComplianceDocumentTypeNR" fixed="CERTIFICATE OF COMPLIANCE NR"/>
                  <xsd:attribute name="docTitle" type="comp:ComplianceDocumentTitleNR" fixed="Mechanical Systems"/>
               </xsd:complexType>
            </xsd:element>
            <xsd:element name="Payload" type="comp:Payload"/>
            <xsd:element name="DocumentData">
               <xsd:complexType>
                  <xsd:sequence>
                     <xsd:element name="Header" type="comp:HeaderNRCC" />
                     <xsd:element ref="nRCCMCH01E" />
                     <xsd:element name="DeclarationsBlock" type="comp:DeclarationsNRCC" maxOccurs="unbounded" />
                     <xsd:element name="Footer" type="comp:FooterNRCC" />
                  </xsd:sequence>
               </xsd:complexType>
            </xsd:element>
            <xsd:element name="Report" type="xsd:base64Binary" minOccurs="0"/>
            <xsd:element name="ReportMMB" type="xsd:base64Binary" minOccurs="0"/>
         </xsd:sequence>
         <xsd:attribute name="revision" use="required" type="xsd:string" fixed="rev 20220101"/>
         <xsd:attribute name="doc" use="required" type="comp:ComplianceDocumentTagNR" fixed="NRCCMCHE"/>
      </xsd:complexType>
   </xsd:element>
   <xsd:complexType name="NRCCMCH01E">
      <xsd:annotation>
         <xsd:documentation>Mechanical Systems</xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="Section_A" minOccurs="1" maxOccurs="1">
            <xsd:annotation>
               <xsd:documentation source="FieldText">[d:b]A. GENERAL INFORMATION[/d:b]</xsd:documentation>
               <xsd:documentation source="MinOccurs">This section is required.</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="A_BeforeNote1" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">[d:i]This document is used to demonstrate compliance for mechanical systems that are within the scope of the permit application and are demonstrating compliance using the prescriptive path outlined in [d:t24refnolink/]140.4, or  [d:t24refnolink/]141.0(b)2 for alterations.[/d:i]</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="A01_ProjectCity" type="comp:ProjectCity" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Project Location (city)</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">User input</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="A02_ClimateZoneNumber" type="com:ClimateZoneNumber" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Climate Zone</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">User input selected from 16 California climate zones</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="A03All_WholeBuildingOccupancy"  maxOccurs="unbounded">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Occupancy Types Within Project:</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">This field accumulates all the occupancy types in the project that the user selected in the UI</xsd:documentation>
                        <xsd:appinfo source="displayterm">
                           <dtyp:displayterm value="RestaurantBuilding">Restaurant/ Commercial Kitchen</dtyp:displayterm>
                        </xsd:appinfo>
                        
                     </xsd:annotation>
                     <xsd:simpleType>
                        <xsd:restriction base="com:WholeBuildingOccupancy">
                           <xsd:enumeration value="AllOthersBuildings"/>
                           <xsd:enumeration value="AuditoriumBuilding"/>
                           <xsd:enumeration value="ClassroomBuilding"/>
                           <xsd:enumeration value="CommercialIndustrialBuilding"/>
                           <xsd:enumeration value="ConventionCenterBuilding"/>
                           <xsd:enumeration value="DataCenterBuilding"/>
                           <xsd:enumeration value="FinancialInstitutionBuilding"/>
                           <xsd:enumeration value="GroceryStoreBuilding"/>
                           <xsd:enumeration value="GymnasiumBuilding"/>
                           <xsd:enumeration value="HighRiseResidential"/>
                           <xsd:enumeration value="HospitalBuilding"/>
                           <xsd:enumeration value="HotelMotel"/>
                           <xsd:enumeration value="LibraryBuilding"/>
                           <xsd:enumeration value="MedicalClinicBuilding"/>
                           <xsd:enumeration value="OfficeBuilding"/>
                           <xsd:enumeration value="ParkingGarageBuilding"/>
                           <xsd:enumeration value="ReligiousFacilityBuilding"/>
                           <xsd:enumeration value="RelocatablePublicSchoolBuilding"/>
                           <xsd:enumeration value="RestaurantBuilding"/>
                           <xsd:enumeration value="RetailBuilding"/>
                           <xsd:enumeration value="SchoolBuilding"/>
                           <xsd:enumeration value="SportsArenaBuilding"/>
                           <xsd:enumeration value="SupportArea"/>
                           <xsd:enumeration value="TheaterBuilding"/>
                           <xsd:enumeration value="Warehouse"/>
                        </xsd:restriction>
                     </xsd:simpleType>
                  </xsd:element>
                  <xsd:element name="A04_TotalConditionedFloorArea" type="com:TotalConditionedFloorArea" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Total Conditioned Floor Area</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">User input</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="A05_TotalUnconditionedFloorArea" type="com:TotalUnconditionedFloorArea" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Total Unconditioned Floor Area</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">User input</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="A06_AboveGradeStoryCount" type="env:AboveGradeStoryCount" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"># of Stories (Habitable  Above Grade)</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="A07_RequestedMMB" type="comp:RequestedMMB"  minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">NOT REPORTED IN PDF</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">This is a flag set by the agent that generates the report XML data to indicate that the user requested a Mandatory Measures Note Block which is placed after the DeclarationBlock in the XML output.</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="A08_ProjectScopeNR" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">NOT REPORTED IN PDF</xsd:documentation>
                        <xsd:appinfo source="displayterm">
                           <dtyp:displayterm value="AdditionComplete">Addition</dtyp:displayterm>
                           <dtyp:displayterm value="ExistingAdditionAndAlteration">Addition Alteration</dtyp:displayterm>
                           <dtyp:displayterm value="ExistingAlteration">Alteration</dtyp:displayterm>
                           <dtyp:displayterm value="NewComplete">New</dtyp:displayterm>
                        </xsd:appinfo>
                        
                     </xsd:annotation>
                     <xsd:simpleType>
                        <xsd:restriction base="comp:ProjectScopeNR">
                           <xsd:enumeration value="AdditionComplete"/>
                           <xsd:enumeration value="ExistingAdditionAndAlteration"/>
                           <xsd:enumeration value="ExistingAlteration"/>
                           <xsd:enumeration value="NewComplete"/>
                        </xsd:restriction>
                     </xsd:simpleType>
                  </xsd:element>
                  <xsd:element name="A09_IsProjectAllElectric" type="comp:IsProjectAllElectric" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">NOT REPORTED IN PDF</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="A10_GasType" type="com:GasType" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">NOT REPORTED IN PDF</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">Values are NaturalGas, Propane and None</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
         <xsd:element name="Section_B" minOccurs="1" maxOccurs="1">
            <xsd:annotation>
               <xsd:documentation source="FieldText">[d:b]B. PROJECT SCOPE[/d:b]</xsd:documentation>
               <xsd:documentation source="MinOccurs">This section is required.</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="B_BeginNote1" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">[d:i]This table Includes mechanical systems or components that are within the scope of the permit application and are demonstrating compliance using the prescriptive path outlined in [d:t24refnolink/]140.4, [d:t24refnolink/]170.2(b) or [d:t24refnolink/]141.0(b)2 and [d:t24refnolink/]180.2(b)2 for alterations.[/d:i]</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="B_Subheading" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">[d:b] My project consists of (check all that apply)[/d:b]</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="B01_Label1" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Air System(s)</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="B01a_ProjectHasHeatingAirSystem" type="comp:ProjectHasHeatingAirSystem" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Heating Air System</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">User input.  If checked result = true, Else If unchecked result = false</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="B01b_ProjectHasCoolingAirSystem" type="comp:ProjectHasCoolingAirSystem" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Cooling Air System</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">User input.  If checked result = true, Else If unchecked result = false</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="B01_Label2" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Mechanical Controls</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="B01c_ProjectHasControls" type="comp:ProjectHasControls" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Mechanical Controls (existing to remain, altered or new)</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">User input.  If checked result = true, Else If unchecked result = false</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="B02_Label" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Wet System Components</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="B02a_ProjectHasWaterEconomizer" type="comp:ProjectHasWaterEconomizer" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Water Economizer</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">User input.  If checked result = true, Else If unchecked result = false</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="B02b_ProjectHasPumps" type="comp:ProjectHasPumps" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Pumps</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">User input.  If checked result = true, Else If unchecked result = false</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="B02c_ProjectHasSystemPiping" type="comp:ProjectHasSystemPiping" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">System Piping</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">User input.  If checked result = true, Else If unchecked result = false</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="B02d_ProjectHasCoolingTowers" type="comp:ProjectHasCoolingTowers" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Cooling Towers</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">User input.  If checked result = true, Else If unchecked result = false</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="B02e_ProjectHasChillers" type="comp:ProjectHasChillers" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Chillers</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">User input.  If checked result = true, Else If unchecked result = false</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="B02f_ProjectHasBoilers" type="comp:ProjectHasBoilers" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Boilers</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">User input.  If checked result = true, Else If unchecked result = false
 
Mandatory Measure Triggers:
If result == true
MM120_9_MeasureApplies = true and is displayed 
End If</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="B03_Label" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Dry System Components</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="B03a_ProjectHasAirEconomizer" type="comp:ProjectHasAirEconomizer" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Air Economizer</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">User input.  If checked result = true, Else If unchecked result = false</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="B03b_ProjectHasElectricHeat" type="comp:ProjectHasElectricHeat" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Electric Resistance Heat</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">User input.  If checked result = true, Else If unchecked result = false</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="B03c_ProjectHasFanSystems" type="comp:ProjectHasFanSystems" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Fan Systems</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">User input.  If checked result = true, Else If unchecked result = false
If B03e_ProjectHasVentilation == true 
result = true
End If</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="B03d_ProjectHasDuctwork" type="comp:ProjectHasDuctwork" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Ductwork (existing to remain, altered or new)</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">User input.  If checked result = true, Else If unchecked result = false
 
Mandatory Measure Triggers:
If result == true
MM120_4_MeasureApplies = true and is displayed And
MM120_4b_MeasureApplies = true and is displayed And
MM120_4c_MeasureApplies = true and is displayed And
MM120_4d_MeasureApplies = true and is displayed And
MM120_4e_MeasureApplies = true and is displayed And
MM120_4f_MeasureApplies = true and is displayed And
End If</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="B03e_ProjectHasVentilation" type="comp:ProjectHasVentilation" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Ventilation</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">User input.  If checked result = true, Else If unchecked result = false
If B03c_ProjectHasFanSystem == true 
result = true
End If</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="B03f_ProjectHasZonalSystems" type="comp:ProjectHasZonalSystems" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Zonal Systems/ Terminal Boxes</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">User input.  If checked result = true, Else If unchecked result = false
 
Mandatory Measure Triggers:
If result == true And Section J is required
MM120_1e_MeasureApplies = true and is displayed 
End If</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="B_Alert1" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">[d:b] [d:i]ALERT! Healthcare facilities must be ventilated in accordance with Chapter 4 of the CMC as amended by OSHPD and do not need to show compliance with Title 24, Part 6 ventilation requirements in Table J.[/d:i][/d:b]</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">Display this alert If A03_WholeBuildingOccupancy includes HospitalBuilding And B03e_ProjectHasVentilation == true</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
         <xsd:element name="Section_C" minOccurs="1">
            <xsd:annotation>
               <xsd:documentation source="FieldText">[d:b]C. COMPLIANCE RESULTS[/d:b]</xsd:documentation>
               <xsd:documentation source="MinOccurs">This section is required.</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="C_BeginNote1" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">[d:i]Table C will indicate if the project data input into the compliance document is compliant with mechanical requirements.  This table is not editable by the user.   If this table says "DOES NOT COMPLY" or "COMPLIES with Exceptional Conditions" refer to Table D., or the table indicated as not compliant for guidance.[/d:i]</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="C01ColumnLabel1" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">System Summary [d:t24refnolink/]110.1, [d:t24refnolink/]110.2, [d:t24refnolink/]140.4, [d:t24refnolink/]170.2(c)</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="C01ColumnLabel2" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">(See Table F)</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:choice maxOccurs="1">
                     <xsd:element name="C01_CompliantSectionF" type="comp:CompliantSectionF" >
                        <xsd:annotation>
                           <xsd:documentation source="FieldText"></xsd:documentation>
                           <xsd:documentation source="CalculationsAndRules">Calculated field: 
If All of the following conditions are true: 
Condition 1
If TableDrySizing IsPresent  Then for every row 
fields F01-F04 have data;  
 
Condition 2
If Table1DryEquipment IsPresent  Then for every row 
any field in range Fa01-Fa09 != BlankGrayedOut  has data And 
If Fa05 and Fa06 != BlankGrayedOut Then Fa05 LT Fa06 And 
If Fa08 And Fa09 != BlankGrayedOut Then Fa08 LT Fa09; 
 
Condition 3
If Table2DryEquipment IsPresent  Then for every row 
any field in range Fb01-Fb07 != BlankGrayedOut  has data And
 If Fb03 and Fb04  != BlankGrayedOut  Then Fb03 LT Fb04 And 
If Fb06 And Fb07 != BlankGrayedOut Then Fb06 LT Fb07; 
 
Condition 4
If TableBoiler IsPresent  Then  for every row 
any field in range Fc01 - Fc12 != BlankGrayedOut has data And 
If Fc11 and Fc12 != BlankGrayedOut Then Fc11 GT Fc12; 
 
Condition 5
If TableChiller IsPresent  Then for every row  
If any field in range Fd01 - Fd10 != BlankGrayedOut has data Then
 
Switch (Fd08a_EfficiencyType )
case EER: 
If Fd06a_EfficiencyEER And Fd07a_RangeMinEER != BlankGrayedOut Then 
Fd06a_EfficiencyEER GTE Fd07a_RangeMinEER;
End If
 
case COP
If Fd06a_EfficiencyCOP And Fd07a_RangeMinCOP != BlankGrayedOut Then 
Fd06a_EfficiencyCOP GTE Fd07a_RangeMinCOP; 
Else If Fd06a_ChillerCOP And Fd07a_RangeMinCOP != BlankGrayedOut Then
Fd06a_ChillerCOP GTE Fd07a_RangeMinCOP; 
End If
 
case kWperTon:
If Fd06a_EfficiencykWperTon And Fd07a_RangeMinkWperTon != BlankGrayedOut Then 
Fd07a_RangeMinkWperTon GTE Fd06a_EfficiencykWperTon;
End If
End Switch
 
Switch (Fd08b_EfficiencyType)
case COP_IPLV:
If Fd06b_EfficiencyCOP_IPLV And Fd07b_RangeMinCOP_IPLV != BlankGrayedOut Then 
Fd06b_EfficiencyCOP_IPLV GTE Fd07b_RangeMinCOP_IPLV; 
End If
 
case EER_IPLV:
If Fd06b_EfficiencyEER_IPLV And Fd07b_RangeMinEER_IPLV != BlankGrayedOut Then 
Fd06b_EfficiencyEER_IPLV GTE Fd07b_RangeMinEER_IPLV; 
End If
 
case kWperTon_IPLV:
If Fd06b_EfficiencykWperTon_IPLV And Fd07b_RangeMinkWperTon_IPLV != BlankGrayedOut Then 
Fd07b_RangeMinkWperTon_IPLV GTE Fd06b_EfficiencykWperTon_IPLV;
End If
End Switch
End If 
End If 
 
Condition 6
If TableHeatRejection IsPresent  Then for every row 
any field in range  Fe01 - Fe12 != BlankGrayedOut has data And 
If Fe05 and Fe06 != BlankGrayedOut Then Fe05 GT Fe06; 
 
Condition 7
If TableElectricHeat IsPresent  Then for every row 
any field in range  Ff01 - Ff04 != BlankGrayedOut has data.  
 
ondition 8
If TableDOAS IsPresent Then for every row 
any field in range Fdoas01 - Fdoas08 != BlankGrayedOut has data. And
If Fdoas06a And Fdoas08a != BlankGrayedOut Then Fdoas06a LT Fdoas08a And
If Fdoas06b And Fdoas08b != BlankGrayedOut Then Fdoas06b LT Fdoas08b 
 
Condition 9 
 If TableDFHP IsPresent Then for every row
any field in range Fdfhp01- Fdfhp11 != BlankGrayedOut has data. And
If Fdfhp07a (min effic) And Fdfhp08a (design effic)  != BlankGrayedOut Then Fdfhp07a GT Fdfhp08a And 
If Fdfhp07b (min effic) And Fdfhp08b (design effic)  != BlankGrayedOut Then Fdfhp07b GT Fdfhp08b And 
If Fdfhp10a (min effic) And Fdfhp11a (design effic)  != BlankGrayedOut Then Fdfhp10a GT Fdfhp11a
 
 
If all conditions are true
Then result = true display term Yes;
Else If any of the conditions are not true
 result = false display term No.</xsd:documentation>
                        </xsd:annotation>
                     </xsd:element>
                     <xsd:element name="C01_NotApplicableMessage" >
                        <xsd:annotation>
                           <xsd:documentation source="FieldText"></xsd:documentation>
                           <xsd:documentation source="CalculationsAndRules">Calculated field: 
If Section_F NotPresent  result = BlankGrayedOut stored in NotApplicableMessage and field is grayed out  
End If</xsd:documentation>
                        </xsd:annotation>
                        <xsd:simpleType>
                           <xsd:restriction base="comp:NotApplicableMessage">
                              <xsd:enumeration value="BlankGrayedOut"/>
                           </xsd:restriction>
                        </xsd:simpleType>
                     </xsd:element>
                  </xsd:choice>
                  <xsd:element name="C01AndLabel" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">AND</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="C02ColumnLabel1" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Pumps [d:t24refnolink/]140.4(k), [d:t24refnolink/]170.2(c)4I</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="C02ColumnLabel2" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">(See Table G)</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:choice maxOccurs="1">
                     <xsd:element name="C02_CompliantSectionG" type="comp:CompliantSectionG" >
                        <xsd:annotation>
                           <xsd:documentation source="FieldText"></xsd:documentation>
                           <xsd:documentation source="CalculationsAndRules">Calculated field:  
If B02b_ProjectHasPumps == true Then for every row 
any field in range G01 - G08 != BlankGrayedOut has data 
Then result = true display term Yes; 
Else result = false display term No
End If</xsd:documentation>
                        </xsd:annotation>
                     </xsd:element>
                     <xsd:element name="C02_NotApplicableMessage" >
                        <xsd:annotation>
                           <xsd:documentation source="FieldText"></xsd:documentation>
                           <xsd:documentation source="CalculationsAndRules">Calculated field: 
If B02b_ProjectHasPumps == false  result = BlankGrayedOut stored in NotApplicableMessage and field is grayed out
End If</xsd:documentation>
                        </xsd:annotation>
                        <xsd:simpleType>
                           <xsd:restriction base="comp:NotApplicableMessage">
                              <xsd:enumeration value="BlankGrayedOut"/>
                           </xsd:restriction>
                        </xsd:simpleType>
                     </xsd:element>
                  </xsd:choice>
                  <xsd:element name="C02AndLabel" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">AND</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="C03ColumnLabel1" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Fans/ Economizers  [d:t24refnolink/]140.4(c), [d:t24refnolink/]140.4(e), [d:t24refnolink/]170.2(c)</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="C03ColumnLabel2" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">(See Table H)</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:choice maxOccurs="1">
                     <xsd:element name="C03_CompliantSectionH" type="comp:CompliantSectionH" >
                        <xsd:annotation>
                           <xsd:documentation source="FieldText"></xsd:documentation>
                           <xsd:documentation source="CalculationsAndRules">Calculated field:  
If B03c_ProjectHasFanSystems == true Or B03a_ProjectHasAirEconomizer== true Then for every row in TableFanSystems fields H00a-H13 If  != BlankGrayedOut have data 
And for every row H12_FanSystemPowerAllowance GTE H13_FanSystemInputPower
 
And  If TableDwellingFan IsPresent 
If Hdwell01- Hdwell12 If != BlankGrayedOut have data 
And Hdwell07_FanPowerPerFlow LT  Hdwell08_FanPowerPerFlow
And Hdwell10_SensibleRecoveryEfficiency GT Hdwell11_SensibleRecoveryEfficiency
 
And If TableExhaustFan IsPresent 
If Hexhaust01- Hexhaust11 If != BlankGrayedOut have data 
And If TableDOAS IsPresent
If Hdoas01-Hdoas06 If != BlankGrayedOut have data
 
Then result = true display term Yes; 
Else result = false display term No And D03_ExceptionalConditionMessageCCMCHE += FanPowerIndexExceedsMaxInH; 
End If</xsd:documentation>
                        </xsd:annotation>
                     </xsd:element>
                     <xsd:element name="C03_NotApplicableMessage" >
                        <xsd:annotation>
                           <xsd:documentation source="FieldText"></xsd:documentation>
                           <xsd:documentation source="CalculationsAndRules">Calculated field: 
If B03c_ProjectHasFanSystems == false And B03a_ProjectHasAirEconomizer== false  result =  BlankGrayedOut stored in NotApplicableMessage and field is grayed out  
End If</xsd:documentation>
                        </xsd:annotation>
                        <xsd:simpleType>
                           <xsd:restriction base="comp:NotApplicableMessage">
                              <xsd:enumeration value="BlankGrayedOut"/>
                           </xsd:restriction>
                        </xsd:simpleType>
                     </xsd:element>
                  </xsd:choice>
                  <xsd:element name="C03AndLabel" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">AND</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="C04ColumnLabel1" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">System Controls [d:t24refnolink/] 110.2, [d:t24refnolink/]120.2, [d:t24refnolink/]140.4(f), [d:t24refnolink/]170.2(c)</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="C04ColumnLabel2" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">(See Table I)</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:choice maxOccurs="1">
                     <xsd:element name="C04_CompliantSectionI" type="comp:CompliantSectionI" >
                        <xsd:annotation>
                           <xsd:documentation source="FieldText"></xsd:documentation>
                           <xsd:documentation source="CalculationsAndRules">Calculated field: 
 If B01a_ProjectHasHeatingAirSystem Or B01b_ProjectHasCoolingAirSystem Or B01c_ProjectHasControls == true  Then for every row
 
If fields I01-I10 have data And 
 
Then  result = true display term Yes; 
 
Else result = false display term No
 
End if
End If</xsd:documentation>
                        </xsd:annotation>
                     </xsd:element>
                     <xsd:element name="C04_NotApplicableMessage" >
                        <xsd:annotation>
                           <xsd:documentation source="FieldText"></xsd:documentation>
                           <xsd:documentation source="CalculationsAndRules">Calculated field: 
If B01a_ProjectHasHeatingAirSystem And B01b_ProjectHasCoolingAirSystem And B01c_ProjectHasControls == false result = BlankGrayedOut stored in NotApplicableMessage and field is grayed out  
End If</xsd:documentation>
                        </xsd:annotation>
                        <xsd:simpleType>
                           <xsd:restriction base="comp:NotApplicableMessage">
                              <xsd:enumeration value="BlankGrayedOut"/>
                           </xsd:restriction>
                        </xsd:simpleType>
                     </xsd:element>
                  </xsd:choice>
                  <xsd:element name="C04AndLabel" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">AND</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="C05ColumnLabel1" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Ventilation [d:t24refnolink/]120.1, [d:t24refnolink/]160.2</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="C05ColumnLabel2" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">(See Table J)</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:choice maxOccurs="1">
                     <xsd:element name="C05_CompliantSectionJ" type="comp:CompliantSectionJ" >
                        <xsd:annotation>
                           <xsd:documentation source="FieldText"></xsd:documentation>
                           <xsd:documentation source="CalculationsAndRules">Calculated field: 
If Section_J IsPresent
If J01_CalculationsAttached == true Or
If J04 - J16 If ! BlankGrayedOut have data
And J18_DoesSystemVentialtionComply == true
 
And If TableVentMF IsPresent
If J19 - J29 If ! BlankGrayedOut have data
And all instances of J28_IsVentSystemBalanced == true And J29_DoesVentMeetRequiredOA == true
 
result = true
Else
result = false
End If</xsd:documentation>
                        </xsd:annotation>
                     </xsd:element>
                     <xsd:element name="C05_NotApplicableMessage" >
                        <xsd:annotation>
                           <xsd:documentation source="FieldText"></xsd:documentation>
                           <xsd:documentation source="CalculationsAndRules">Calculated field:  
If B03e_ProjectHasVentilation == false result = BlankGrayedOut stored in NotApplicableMessage and field is grayed out  
End If</xsd:documentation>
                        </xsd:annotation>
                        <xsd:simpleType>
                           <xsd:restriction base="comp:NotApplicableMessage">
                              <xsd:enumeration value="BlankGrayedOut"/>
                           </xsd:restriction>
                        </xsd:simpleType>
                     </xsd:element>
                  </xsd:choice>
                  <xsd:element name="C05AndLabel" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">AND</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="C06ColumnLabel1" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Terminal Box Controls [d:t24refnolink/]140.4(d), [d:t24refnolink/]170.2(c)4B</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="C06ColumnLabel2" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">(See Table K)</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:choice maxOccurs="1">
                     <xsd:element name="C06_CompliantSectionK" type="comp:CompliantSectionK" >
                        <xsd:annotation>
                           <xsd:documentation source="FieldText"></xsd:documentation>
                           <xsd:documentation source="CalculationsAndRules">Calculated field: 
If B03f_ProjectHasZonalSystems == true Then  for every row 
If any field in range K01-K11If  != BlankGrayedOut has data And all instances of K12_ZonalControlComplies == true 
result = true display term Yes; 
Else 
result = false display term No
End If
End If</xsd:documentation>
                        </xsd:annotation>
                     </xsd:element>
                     <xsd:element name="C06_NotApplicableMessage" >
                        <xsd:annotation>
                           <xsd:documentation source="FieldText"></xsd:documentation>
                           <xsd:documentation source="CalculationsAndRules">Calculated field:  
If  B03f_ProjectHasZonalSystems == false result = BlankGrayedOut stored in NotApplicableMessage and field is grayed out 
End If</xsd:documentation>
                        </xsd:annotation>
                        <xsd:simpleType>
                           <xsd:restriction base="comp:NotApplicableMessage">
                              <xsd:enumeration value="BlankGrayedOut"/>
                           </xsd:restriction>
                        </xsd:simpleType>
                     </xsd:element>
                  </xsd:choice>
                  <xsd:element name="C06AndLabel" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">AND</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="C07ColumnLabel1" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Distribution [d:t24refnolink/]120.3, [d:t24refnolink/]140.4(l), [d:t24refnolink/]160.2, [d:t24refnolink/]160.3</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="C07ColumnLabel2" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">(See Table L)</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:choice maxOccurs="1">
                     <xsd:element name="C07_CompliantSectionL" type="comp:CompliantSectionL" >
                        <xsd:annotation>
                           <xsd:documentation source="FieldText"></xsd:documentation>
                           <xsd:documentation source="CalculationsAndRules">Calculated field: 
If B02c_ProjectHasSystemPiping == true  Then  for every row  
 
If L01_ PipeInsulationProtectionComplies == true And 
Either L07_PipeInsulationMinByDiameter LTE L09_PipeInsulationThickness Or 
L08_PipeInsulationMinimum LTE L09_PipeInsulationThickness Or L10_PipeInsulationException != NoException 
result = true; 
 
Else result = false;  
End If
 
If  B03d_ProjectHasDuctwork == true then 
If any field in range L11-L20 If != BlankGrayedOut has data 
result = true display term Yes; 
Else result = false display term No.
End If
End If
End If</xsd:documentation>
                        </xsd:annotation>
                     </xsd:element>
                     <xsd:element name="C07_NotApplicableMessage" >
                        <xsd:annotation>
                           <xsd:documentation source="FieldText"></xsd:documentation>
                           <xsd:documentation source="CalculationsAndRules">Calculated field: 
If  B02c_ProjectHasSystemPiping == false And B03d_ProjectHasDuctwork == false  result = BlankGrayedOut stored in NotApplicableMessage and field is grayed out
End If</xsd:documentation>
                        </xsd:annotation>
                        <xsd:simpleType>
                           <xsd:restriction base="comp:NotApplicableMessage">
                              <xsd:enumeration value="BlankGrayedOut"/>
                           </xsd:restriction>
                        </xsd:simpleType>
                     </xsd:element>
                  </xsd:choice>
                  <xsd:element name="C07AndLabel" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">AND</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="C08ColumnLabel1" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Cooling Towers [d:t24refnolink/]110.2(e)2</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="C08ColumnLabel2" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">(See Table M)</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:choice maxOccurs="1">
                     <xsd:element name="C08_CompliantSectionM" type="comp:CompliantSectionM" >
                        <xsd:annotation>
                           <xsd:documentation source="FieldText"></xsd:documentation>
                           <xsd:documentation source="CalculationsAndRules">Calculated field: 
If B02d_ProjectHasCoolingTowers == true Then 
If M01_CalculationasAttached == true 
result = true display term Yes; 
 
Else Ifor every row 
If fields M02-M14 have data And M15_DoesCoolingTowerComply == true 
result = true display term Yes; 
Else result = false display term No
End If
End If</xsd:documentation>
                        </xsd:annotation>
                     </xsd:element>
                     <xsd:element name="C08_NotApplicableMessage" >
                        <xsd:annotation>
                           <xsd:documentation source="FieldText"></xsd:documentation>
                           <xsd:documentation source="CalculationsAndRules">Calculated field: 
If B02d_ProjectHasCoolingTowers == false  result = BlankGrayedOut stored in NotApplicableMessage and field is grayed out
End If</xsd:documentation>
                        </xsd:annotation>
                        <xsd:simpleType>
                           <xsd:restriction base="comp:NotApplicableMessage">
                              <xsd:enumeration value="BlankGrayedOut"/>
                           </xsd:restriction>
                        </xsd:simpleType>
                     </xsd:element>
                  </xsd:choice>
                  <xsd:element name="C09_DoesFormComplyNRCC" type="comp:DoesFormComplyNRCC" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Compliance Results</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">Calculated field: 
Perform evaluation only for fields C01 - C08 != BlankGrayedOut:
 
If  C01_CompliantSectionF == false Or C02_CompliantSectionG ==  false Or  C03_CompliantSectionH ==  false Or C04_CompliantSectionI == false Or  C05_CompliantSectionJ ==  false Or  C06_CompliantSectionK ==  false Or  C07_CompliantSectionL == false Or  C08_CompliantSectionM == false
result = false  display term DOES NOT COMPLY;
 
Else If D03_  ExceptionalConditionMessageCCMCHE NotPresent 
result = Complies display term COMPLIES; 
 
Else If D03_  ExceptionalConditionMessageCCMCHE IsPresent result = CompliesExceptionalConditions result = true display term COMPLIES with Exceptional Conditions; 
 
Else result = DoesNotComply  display term DOES NOT COMPLY
End If</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="C10_MandatoryMeasuresComply" type="comp:MandatoryMeasuresComply" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">[d:b] Mandatory Measures Compliance (See Table Q for Details) [/d:b]</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">Calculated field:  
If Q01_UsingMandMeasNoteBlock ==  true And Q02_ DocLocationMandMeasBlock NotNull; 
Or 
If Q01_UsingMandMeasNoteBlock == false And Q03a-m have data 
result = true display term COMPLIES;  
Else result = false display term DOES NOT COMPLY;
End If</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
         <xsd:element name="Section_D" minOccurs="1">
            <xsd:annotation>
               <xsd:documentation source="FieldText">[d:b]D. EXCEPTIONAL CONDITIONS[/d:b]</xsd:documentation>
               <xsd:documentation source="MinOccurs">This section is required.</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="D_BeginNote1" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">[d:i]This table is auto-filled with uneditable comments because of selections made or data entered in tables throughout the form.[/d:i]</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="TableAllExceptionalConditions" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">This table is required if there are any exceptional conditions in sections</xsd:documentation>
                     </xsd:annotation>
                     <xsd:complexType>
                        <xsd:sequence>
                           <xsd:element name="Row" minOccurs="1" maxOccurs="unbounded">
                              <xsd:complexType>
                                 <xsd:sequence>
                                    <xsd:element name="D01_SectionTitle" type="comp:SectionTitle" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText"></xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="D02_SectionExceptionalConditionNotes" type="comp:SectionExceptionalConditionNotes"  maxOccurs="unbounded">
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText"></xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                 </xsd:sequence>
                              </xsd:complexType>
                           </xsd:element>
                        </xsd:sequence>
                     </xsd:complexType>
                  </xsd:element>
                  <xsd:element name="D03_ExceptionalConditionMessageCCMCHE" type="comp:ExceptionalConditionMessageCCMCHE"  minOccurs="0" maxOccurs="unbounded">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Exceptional Messages</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">Auto-filled with any system generated messages that explain exceptional conditions encountered in the  completed compliance document</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
         <xsd:element name="Section_E" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation source="FieldText">[d:b]E. ADDITIONAL REMARKS[/d:b]</xsd:documentation>
               <xsd:documentation source="CalculationsAndRules">This section is required if there are  comments from the permit applicant to the Authority Having Jurisdiction</xsd:documentation>
               <xsd:documentation source="MinOccurs">If this section doesn't apply, do not display anything for this section</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="E_BeginNote1" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">[d:i]This table includes remarks made by the permit applicant to the Authority Having Jurisdiction.[/d:i]</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="E01_CommentsToPermitAuthority" type="comp:CommentsToPermitAuthority"  minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
         <xsd:element name="Section_F" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation source="FieldText">[d:b]F. HVAC SYSTEM SUMMARY (DRY &amp; WET SYSTEMS)[/d:b]</xsd:documentation>
               <xsd:documentation source="CalculationsAndRules">This section is required if any of the following values == true: 
B01aProjectHasHeatingAirSystem,  B01b_ProjectHasCoolingAirSystem,  B02a_ProjectHasWaterEconomizer, B02d_ProjectHasCoolingTowers, B02e_ProjectHasChillers, B02f_ProjectHasBoilers, B03b_ProjectHasElectricHeat
 
Mandatory Measure Triggers:
If this section is required
MM110_2_MeasureApplies = true and is displayed
End If</xsd:documentation>
               <xsd:documentation source="MinOccurs">If this section doesn't apply, do not display anything for this section</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="F_BeginNote1" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">[d:i]This table is used to demonstrate compliance for mechanical equipment with mandatory requirements found in [d:t24refnolink/]110.1 and [d:t24refnolink/]110.2(a) and prescriptive requirements found in [d:t24refnolink/]140.4(a), [d:t24refnolink/]140.4(b), [d:t24refnolink/]170.2(c)1, [d:t24refnolink/]170.2(c)3, [d:t24refnolink/]140.4(k) or  [d:t24refnolink/]141.0(b)2 and [d:t24refnolink/]180.2(b)2 for alterations.  [/d:i]</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="Fscs" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Space Conditioning System Information</xsd:documentation>
                     </xsd:annotation>
                     <xsd:complexType>
                        <xsd:sequence>
                           <xsd:element name="Row" minOccurs="1" maxOccurs="unbounded">
                              <xsd:complexType>
                                 <xsd:sequence>
                                    <xsd:element name="Fscs01_SpaceConditioningSystemName" type="hvac:SpaceConditioningSystemName" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">System Name</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">User input</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="Fscs02_SpaceConditioningSystemCount" type="hvac:SpaceConditioningSystemCount" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Quantity</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">User input</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="Fscs03_SystemZoning" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">System Serving</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">User selects from list</xsd:documentation>
                                          <xsd:appinfo source="displayterm">
                                             <dtyp:displayterm value="SingleZone">Individual DwellingUnit</dtyp:displayterm>
                                          </xsd:appinfo>
                                          
                                       </xsd:annotation>
                                       <xsd:simpleType>
                                          <xsd:restriction base="hvac:SystemZoning">
                                             <xsd:enumeration value="MultiDwellingUnits"/>
                                             <xsd:enumeration value="MultiZone"/>
                                             <xsd:enumeration value="SingleDwellingUnit"/>
                                             <xsd:enumeration value="SingleZone"/>
                                          </xsd:restriction>
                                       </xsd:simpleType>
                                    </xsd:element>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="Fscs04_Status" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">System Status</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">If Fscs03_SystemZoning == MultiZone Or MultiDwellingUnits
result = BlankGrayedOut stored in NotApplicableMessage
Else user selects from list
End If</xsd:documentation>
                                          </xsd:annotation>
                                          <xsd:simpleType>
                                             <xsd:restriction base="hvac:Status">
                                                <xsd:enumeration value="Alteration"/>
                                                <xsd:enumeration value="NewOrAddition"/>
                                             </xsd:restriction>
                                          </xsd:simpleType>
                                       </xsd:element>
                                       <xsd:element name="Fscs04_NotApplicableMessage" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">System Status</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">If Fscs03_SystemZoning == MultiZone Or MultiDwellingUnits
result = BlankGrayedOut 
End If</xsd:documentation>
                                          </xsd:annotation>
                                          <xsd:simpleType>
                                             <xsd:restriction base="comp:NotApplicableMessage">
                                                <xsd:enumeration value="BlankGrayedOut"/>
                                             </xsd:restriction>
                                          </xsd:simpleType>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="Fscs05_WholeBuildingOccupancy" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Space Type</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">If Fscs03_SystemZoning == MultiZone Or MultiDwellingUnits
Of If Fscs04_Status == Alteration
result = BlankGrayedOut stored in NotApplicableMessage
Else user selects from list
End If</xsd:documentation>
                                             <xsd:appinfo source="displayterm">
                                                <dtyp:displayterm value="Warehouse">Office in Warehouse</dtyp:displayterm>
                                             </xsd:appinfo>
                                             
                                          </xsd:annotation>
                                          <xsd:simpleType>
                                             <xsd:restriction base="com:WholeBuildingOccupancy">
                                                <xsd:enumeration value="AllOthersBuildings"/>
                                                <xsd:enumeration value="FinancialInstitutionBuilding"/>
                                                <xsd:enumeration value="GroceryStoreBuilding"/>
                                                <xsd:enumeration value="LibraryBuilding"/>
                                                <xsd:enumeration value="OfficeBuilding"/>
                                                <xsd:enumeration value="RetailBuilding"/>
                                                <xsd:enumeration value="SchoolBuilding"/>
                                                <xsd:enumeration value="Warehouse"/>
                                             </xsd:restriction>
                                          </xsd:simpleType>
                                       </xsd:element>
                                       <xsd:element name="Fscs05_NotApplicableMessage" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Space Type</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">If Fscs03_SystemZoning == MultiZone Or MultiDwellingUnits
Of If Fscs04_Status == Alteration
result = BlankGrayedOut 
End If</xsd:documentation>
                                          </xsd:annotation>
                                          <xsd:simpleType>
                                             <xsd:restriction base="comp:NotApplicableMessage">
                                                <xsd:enumeration value="BlankGrayedOut"/>
                                             </xsd:restriction>
                                          </xsd:simpleType>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="Fscs06_HeatRecovery" type="hvac:HeatRecovery" >
