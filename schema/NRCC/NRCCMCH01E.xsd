Else If Hexhaust06_OutdoorAirflowPct GTE 80%
If A02_ClimateZoneNumber  ==  6, Or 7 
result = HeatCoolNotRequired
Else If A02_ClimateZoneNumber == 1,2, 12, 13, 14, 15, Or 16
If Hexhaust04_AirflowDesignRate LT 2000
result = HeatCoolNotRequired
Else If Hexhaust07_HeatRecoveryExemptions == CZ15HeatingNA
result = CoolOnlyRequired
Else If Hexhaust07_HeatRecoveryExemptions == CZ1CoolingNA
result = HeatOnlyRequired
Else
result = HeatCoolRequired
End If
Else If A02_ClimateZoneNumber == 3
If Hexhaust04_AirflowDesignRate LT 6000
result = HeatCoolNotRequired
Else If Hexhaust07_HeatRecoveryExemptions == CZ15HeatingNA
result = CoolOnlyRequired
Else If Hexhaust07_HeatRecoveryExemptions == CZ1CoolingNA
result = HeatOnlyRequired
Else
result = HeatCoolRequired
End If
Else If A02_ClimateZoneNumber == 4, Or 5
If Hexhaust04_AirflowDesignRate LT 5000
result = HeatCoolNotRequired
Else If Hexhaust07_HeatRecoveryExemptions == CZ15HeatingNA
result = CoolOnlyRequired
Else If Hexhaust07_HeatRecoveryExemptions == CZ1CoolingNA
result = HeatOnlyRequired
Else
result = HeatCoolRequired
End If
Else If A02_ClimateZoneNumber == 10, Or 11
If Hexhaust04_AirflowDesignRate LT 3000
result = HeatCoolNotRequired
Else If Hexhaust07_HeatRecoveryExemptions == CZ15HeatingNA
result = CoolOnlyRequired
Else If Hexhaust07_HeatRecoveryExemptions == CZ1CoolingNA
result = HeatOnlyRequired
Else
result = HeatCoolRequired
End If
Else If A02_ClimateZoneNumber == 8
If Hexhaust04_AirflowDesignRate LT 12000
result = HeatCoolNotRequired
Else If Hexhaust07_HeatRecoveryExemptions == CZ15HeatingNA
result = CoolOnlyRequired
Else If Hexhaust07_HeatRecoveryExemptions == CZ1CoolingNA
result = HeatOnlyRequired
Else
result = HeatCoolRequired
End If
Else If A02_ClimateZoneNumber == 9
If Hexhaust04_AirflowDesignRate LT 7000
result = HeatCoolNotRequired
Else If Hexhaust07_HeatRecoveryExemptions == CZ15HeatingNA
result = CoolOnlyRequired
Else If Hexhaust07_HeatRecoveryExemptions == CZ1CoolingNA
result = HeatOnlyRequired
Else
result = HeatCoolRequired
End If
 
End If
End If</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="Hexhaust08_NotApplicableMessage" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Exhaust Air Heat Recovery [d:t24refnolink/]140.4(q) &amp; [d:t24refnolink/]170.2(c)4O</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">If Hexhaust07_HeatRecoveryExemptions != NoExemptApply
result = BlankGrayedOut
End If</xsd:documentation>
                                          </xsd:annotation>
                                          <xsd:simpleType>
                                             <xsd:restriction base="comp:NotApplicableMessage">
                                                <xsd:enumeration value="BlankGrayedOut"/>
                                             </xsd:restriction>
                                          </xsd:simpleType>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="Hexhaust09_HeatRecoveryRating" type="comp:HeatRecoveryRating" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Type Of Heat Recovery Rating</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">User selects from list: SensibleRecoveryRatio display term Sensible, EnthalpyRecoveryRatio display term Enthalpy</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="Hexhaust09_NotApplicableMessage" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Type Of Heat Recovery Rating</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">If Hexhaust07_HeatRecoveryExemptions != NoExemptApply 
Or Hexhaust08lt8000_HeatRecoveryRequirements == HeatCoolNotRequired
Or Hexhaust08gte8000_HeatRecoveryRequirements == HeatCoolNotRequired
result = BlankGrayedOut
End If</xsd:documentation>
                                          </xsd:annotation>
                                          <xsd:simpleType>
                                             <xsd:restriction base="comp:NotApplicableMessage">
                                                <xsd:enumeration value="BlankGrayedOut"/>
                                             </xsd:restriction>
                                          </xsd:simpleType>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="Hexhaust10_HeatRecoveryRatio" type="comp:HeatRecoveryRatio" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Required Recovery Ratio</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">Calculated field: 
If Hexhaust09_HeatRecoveryRating IsPresent
If Hexhaust09_HeatRecoveryRating == SensibleRecoveryRatio
result = 0.60
Else Hexhaust09_HeatRecoveryRating == EnthalpyRecoveryRatio
result = 0.50
End If
End If</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="Hexhaust10_NotApplicableMessage" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Required Recovery Ratio</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">If Hexhaust07_HeatRecoveryExemptions != NoExemptApply Or 
Hexhaust09_NotApplicableMessage IsPresent
Or Hexhaust08lt8000_HeatRecoveryRequirements == HeatCoolNotRequired
Or Hexhaust08gte8000_HeatRecoveryRequirements == HeatCoolNotRequired
result = BlankGrayedOut
End If</xsd:documentation>
                                          </xsd:annotation>
                                          <xsd:simpleType>
                                             <xsd:restriction base="comp:NotApplicableMessage">
                                                <xsd:enumeration value="BlankGrayedOut"/>
                                             </xsd:restriction>
                                          </xsd:simpleType>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="Hexhaust11_RecoveryBypassType" type="comp:RecoveryBypassType">
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Energy Recovery Bypass</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">User selects from dropdown list</xsd:documentation>
                                             <xsd:appinfo source="displayterm">
                                                <dtyp:displayterm value="ProvidesRecoveryBypass">Energy Recovery Bypass or similar control Installed per 140.4(q)2</dtyp:displayterm>
                                             </xsd:appinfo>
                                             
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="Hexhaust11_NotApplicableMessage" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Energy Recovery Bypass</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">If Hexhaust07_HeatRecoveryExemptions != NoExemptApply Or 
Hexhaust09_NotApplicableMessage IsPresent  
Or Hexhaust08lt8000_HeatRecoveryRequirements == HeatCoolNotRequired
Or Hexhaust08gte8000_HeatRecoveryRequirements == HeatCoolNotRequired                                          
result = BlankGrayedOut
End If</xsd:documentation>
                                          </xsd:annotation>
                                          <xsd:simpleType>
                                             <xsd:restriction base="comp:NotApplicableMessage">
                                                <xsd:enumeration value="BlankGrayedOut"/>
                                             </xsd:restriction>
                                          </xsd:simpleType>
                                       </xsd:element>
                                    </xsd:choice>
                                 </xsd:sequence>
                              </xsd:complexType>
                           </xsd:element>
                        </xsd:sequence>
                     </xsd:complexType>
                  </xsd:element>
                  <xsd:element name="TableDOAS" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">[d:b]Dedicated Outdoor Air System (DOAS)[/d:b]</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">This table is required  at least one row where F02_HvacSystemType == DedicatedOAS</xsd:documentation>
                     </xsd:annotation>
                     <xsd:complexType>
                        <xsd:sequence>
                           <xsd:element name="Row" minOccurs="1" maxOccurs="unbounded">
                              <xsd:complexType>
                                 <xsd:sequence>
                                    <xsd:element name="Hdoas01_HvacSystemName" type="comp:HvacSystemName" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">System Name</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">Auto-filled from F01_HvcSystemName where F02_HvacSystemType == DedicatedOAS</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="Hdoas02_SystemCount" type="hvac:SystemCount" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Quantity</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">User input</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="Hdoas03_DeliveryDOAS" type="comp:DeliveryDOAS" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Delivered Directly to Space</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">User selects from dropdown list</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="Hdoas04_FanControlDOAS" type="hvac:FanControlDOAS" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">DOAS Fan Control</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">User selects from dropdown list</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="Hdoas05_MultizoneDOAS" type="comp:MultizoneDOAS" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Multi-Zone DOAS with Cooling [d:t24refnolink/]140.4(p)4 &amp; [d:t24refnolink/]170.2(c)4N</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">User selects from dropdown list</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="Hdoas06_MultifamilyDOAS" type="comp:MultifamilyDOAS" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Multifamily DOAS</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">User selects from dropdown list</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                 </xsd:sequence>
                              </xsd:complexType>
                           </xsd:element>
                        </xsd:sequence>
                     </xsd:complexType>
                  </xsd:element>
                  <xsd:element name="TableFEI" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">[d:b]Fan Energy Index  (FEI)[/d:b]</xsd:documentation>
                     </xsd:annotation>
                     <xsd:complexType>
                        <xsd:sequence>
                           <xsd:element name="Row" minOccurs="1" maxOccurs="unbounded">
                              <xsd:complexType>
                                 <xsd:sequence>
                                    <xsd:element name="Hfei01_FanName" type="hvac:FanName" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Name or Item Tag</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">If H01_FanName IsPresent
Autofilled from H01_FanName
Else
User Input
End If</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="Hfei02_FanEnergyIndexException" type="comp:FanEnergyIndexException" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">FEI Exception</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">User selects from dropdown list</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="Hfei03_FanEnergyIndex" type="comp:FanEnergyIndex" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">FEI</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">If Hfei02_FanEnergyIndexException  == NoneApplyFEI Or ExceptVavFan
User input
End If</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="Hfei03_NotApplicableMessage" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">FEI</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">If Hfei02_FanEnergyIndexException  != NoneApplyFEI or ExceptVavFan
result = NotApplicableMessage
End If</xsd:documentation>
                                          </xsd:annotation>
                                          <xsd:simpleType>
                                             <xsd:restriction base="comp:NotApplicableMessage">
                                                <xsd:enumeration value="BlankGrayedOut"/>
                                             </xsd:restriction>
                                          </xsd:simpleType>
