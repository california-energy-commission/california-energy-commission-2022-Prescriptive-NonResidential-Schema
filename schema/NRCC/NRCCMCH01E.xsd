                                             <xsd:documentation source="CalculationsAndRules">For value in Fdfhp01_HvacSystemName  If F05_NotApplicableMessage IsPresent Or F05_HeatingDesignCapacity LT 1 
result = BlankGrayedOut  stored in NotApplicableMessage and this field is greyed out
End If</xsd:documentation>
                                          </xsd:annotation>
                                          <xsd:simpleType>
                                             <xsd:restriction base="comp:NotApplicableMessage">
                                                <xsd:enumeration value="BlankGrayedOut"/>
                                             </xsd:restriction>
                                          </xsd:simpleType>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="Fdfhp08b_CombustionEfficiency" type="hvac:CombustionEfficiency" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Design Efficiency</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">If Fdfhp06b_EfficiencyType == CombustionEfficiency use this choice to store user input of design CombustionEfficiency value</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="Fdfhp08b_ThermalEfficiency" type="hvac:ThermalEfficiency" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Design Efficiency</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">If Fdfhp06b_EfficiencyType == ThermalEfficiency use this choice to store user input of design ThermalEfficiency value</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="Fdfhp08b_EfficiencyAFUE" type="hvac:EfficiencyAFUE" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Design Efficiency</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">If Fdfhp06b_EfficiencyType == AFUE use this choice to store user input of design AFUE value</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="Fdfhp08b_NotApplicableMessage" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Design Efficiency</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">For value in Fdfhp01_HvacSystemName  If F05_NotApplicableMessage IsPresent Or F05_HeatingDesignCapacity LT 1  
result = BlankGrayedOut  stored in NotApplicableMessage and this field is greyed out
End If</xsd:documentation>
                                          </xsd:annotation>
                                          <xsd:simpleType>
                                             <xsd:restriction base="comp:NotApplicableMessage">
                                                <xsd:enumeration value="BlankGrayedOut"/>
                                             </xsd:restriction>
                                          </xsd:simpleType>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:element name="Fdfhp09to11Label" minOccurs="0">
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Cooling Mode</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="CoolingEfficDfhp" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText"></xsd:documentation>
                                       </xsd:annotation>
                                       <xsd:complexType>
                                          <xsd:choice >
                                             <xsd:element name="CoolingSingleEfficDfhp" >
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText"></xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">Use this choice If F08_CoolingSensibleDesignCapacity GTE 1 And If there is only one efficiency value for the equipment.  
 
All results are for result1, the top cell and result2 bottom cell is blank</xsd:documentation>
                                                </xsd:annotation>
                                                <xsd:complexType>
                                                   <xsd:sequence>
                                                      <xsd:element name="Fdfhp09a_EfficiencyType" >
                                                         <xsd:annotation>
                                                            <xsd:documentation source="FieldText">Efficiency Unit</xsd:documentation>
                                                            <xsd:documentation source="CalculationsAndRules">User selects from Efficiency units for Cooling mode  for F02_HvacSystemType assigned to Fdfhp01_HvacSystemName  that only have one efficiency type.  
 
The type is stored in result1, the top displayed cell and the bottom cell is blank.  Calculated values:  
 
If Fdfhp03a_UnitaryHeatPumps IsPresent 
If Fdfhp03a_UnitaryHeatPumps  == GroundSourceHP, GroundWaterSourceHP, GroundWaterSourceWaterToWaterHP, WaterSourceHP, WaterToWaterHP or GroundSourceBrineToWaterHP 
result1 = EER; 
Else If Fdfhp03a_UnitaryHeatPumps == AirCooledGasEngineHP 
result1 = COP; 
Else If Fdfhp03a_UnitaryHeatPumps == AirCooledSplit1PhaseHP, AirCooledSplit3PhaseHP, AirCooledPkg1PhaseHP, AirCooledPkg3PhaseHP,  AirCooledSplitConstrainedHP, AirCooledPkgConstrainedHP Or AirCooledSmallDuctHighVelocityHP And If Fdfhp04a_SizeRangeUnitaryHeatPumps == LT65000 
result1 = SEER; 
End If
 
End If</xsd:documentation>
                                                         </xsd:annotation>
                                                         <xsd:simpleType>
                                                            <xsd:restriction base="hvac:EfficiencyType">
                                                               <xsd:enumeration value="COP"/>
                                                               <xsd:enumeration value="EER"/>
                                                               <xsd:enumeration value="SEER"/>
                                                            </xsd:restriction>
                                                         </xsd:simpleType>
                                                      </xsd:element>
                                                      <xsd:element name="Fdfhp09b_NotApplicableMessage" >
                                                         <xsd:annotation>
                                                            <xsd:documentation source="FieldText">Efficiency Unit</xsd:documentation>
                                                            <xsd:documentation source="CalculationsAndRules">result = BlankGrayedOut - no furnace value for cooling</xsd:documentation>
                                                         </xsd:annotation>
                                                         <xsd:simpleType>
                                                            <xsd:restriction base="comp:NotApplicableMessage">
                                                               <xsd:enumeration value="BlankGrayedOut"/>
                                                            </xsd:restriction>
                                                         </xsd:simpleType>
                                                      </xsd:element>
                                                      <xsd:choice maxOccurs="1">
                                                         <xsd:element name="Fdfhp10a_CoolEfficiencyMinUnitHP" type="hvac:CoolEfficiencyMinUnitHP" >
                                                            <xsd:annotation>
                                                               <xsd:documentation source="FieldText">Minimum Efficiency Required per Tables 110.2 / Title 20</xsd:documentation>
                                                               <xsd:documentation source="CalculationsAndRules">The value is stored in result1, the top displayed cell and the bottom cell is blank. 
 
 If Fdfhp03a_UnitaryHeatPumps == AirCooledGasEngineHP 
result1 = 0.60 (COP); 
 
Else If Fdfhp04a_SizeRangeUnitaryHeatPumps == LT65000 
Switch (Fdfhp03a_UnitaryHeatPumps)  
case AirCooledSplit1PhaseHP Or AirCooledPkg1PhaseHP: 
result1 = 14.0 (SEER); 
 
cade AirCooledSplit3PhaseHP Or AirCooledPkg3PhaseHP 
result1 = 14.0 (SEER);  
 
cade AirCooledSmallDuctHighVelocityHP: 
result1 = 12.0 (SEER); 
 
case AirCooledSplitConstrainedHP Or AirCooledPkgConstrainedHP: 
result1 = 12.0 (SEER); 
End Switch
 
Else If Fdfhp04a_SizeRangeUnitaryHeatPumps == LT135000 
 
Switch (Fdfhp03a_ UnitaryHeatPumps) 
case GroundWaterSourceHP: 
If Fdfhp02a_HvacSystemType == UnitaryHeatPumps   
result1 = 18.0 (EER); 
Else If Fdfhp02a_HvacSystemType == UnitaryHeatPumpsNoElectR 
 
result1 = 17.8 (EER);
End If
 
case GroundSourceHP: 
If Fdfhp02a_HvacSystemType == UnitaryHeatPumps
result1= 14.1 (EER); 
Else If Fdfhp02a_HvacSystemType == UnitaryHeatPumpsNoElectR
result1= 13.9 (EER);
End If
 
case WaterToWaterHP: 
If Fdfhp02a_HvacSystemType == UnitaryHeatPumps
result1= 10.6 (EER); 
Else If Fdfhp02a_HvacSystemType == UnitaryHeatPumpsNoElectR
result1= 10.4 (EER);
End If
 
 
case GroundWaterSourceWaterToWaterHP:
If Fdfhp02a_HvacSystemType == UnitaryHeatPumps 
result1= 16.3 (EER);
Else If Fdfhp02a_HvacSystemType == UnitaryHeatPumpsNoElectR
result1= 16.1 (EER);
End If 
 
case GroundSourceBrineToWaterHP: 
If Fdfhp02a_HvacSystemType == UnitaryHeatPumps
result1= 12.1 (EER); 
Else If Fdfhp02a_HvacSystemType == UnitaryHeatPumpsNoElectR
result1= 11.9 (EER);
End If 
End Switch
 
Else If Fdfhp03a_UnitaryHeatPumps == WaterSourceHP:
If Fdfhp04a_SizeRangeUnitaryHeatPumps == LT17000  
If Fdfhp02a_HvacSystemType == UnitaryHeatPumps
result1 =12.2 (EER);  
Else If Fdfhp02a_HvacSystemType == UnitaryHeatPumpsNoElectR
result1= 12.2 (EER);
End If 
Else If Fdfhp04a_SizeRangeUnitaryHeatPumps == GTE17000AndLT135000
If Fdfhp02a_HvacSystemType == UnitaryHeatPumps
result1 =13.0 (EER);  
Else If Fdfhp02a_HvacSystemType == UnitaryHeatPumpsNoElectR
result1= 12.8 (EER);
End If 
End If</xsd:documentation>
                                                            </xsd:annotation>
                                                         </xsd:element>
                                                         <xsd:element name="Fdfhp10a_NotApplicableMessage" >
                                                            <xsd:annotation>
                                                               <xsd:documentation source="FieldText">Minimum Efficiency Required per Tables 110.2 / Title 20</xsd:documentation>
                                                               <xsd:documentation source="CalculationsAndRules">If Fdfhp03a_UnitaryHeatPumps == WaterSourceHP:
If Fdfhp04a_SizeRangeUnitaryHeatPumps == GTE135000AndLT1240000
result = BlankGrayedOut
End If</xsd:documentation>
                                                            </xsd:annotation>
                                                            <xsd:simpleType>
                                                               <xsd:restriction base="comp:NotApplicableMessage">
                                                                  <xsd:enumeration value="BlankGrayedOut"/>
                                                               </xsd:restriction>
                                                            </xsd:simpleType>
                                                         </xsd:element>
                                                      </xsd:choice>
                                                      <xsd:element name="Fdfhp10b_NotApplicableMessage" >
                                                         <xsd:annotation>
                                                            <xsd:documentation source="FieldText">Minimum Efficiency Required per Tables 110.2 / Title 20</xsd:documentation>
                                                            <xsd:documentation source="CalculationsAndRules">result = BlankGrayedOut - no furnace value for cooling</xsd:documentation>
                                                         </xsd:annotation>
                                                         <xsd:simpleType>
                                                            <xsd:restriction base="comp:NotApplicableMessage">
                                                               <xsd:enumeration value="BlankGrayedOut"/>
                                                            </xsd:restriction>
                                                         </xsd:simpleType>
                                                      </xsd:element>
                                                      <xsd:choice maxOccurs="1">
                                                         <xsd:element name="Fdfhp11a_EfficiencyCOP" type="hvac:EfficiencyCOP" >
                                                            <xsd:annotation>
                                                               <xsd:documentation source="FieldText">Design Efficiency</xsd:documentation>
                                                               <xsd:documentation source="CalculationsAndRules">If Fdfhp09a_EfficiencyType == COP use this choice to store user input of design COP.  
 
The value is stored in result1, the top displayed cell and the bottom cell is blank.</xsd:documentation>
                                                            </xsd:annotation>
                                                         </xsd:element>
                                                         <xsd:element name="Fdfhp11a_EfficiencyEER" type="hvac:EfficiencyEER" >
                                                            <xsd:annotation>
                                                               <xsd:documentation source="FieldText">Design Efficiency</xsd:documentation>
                                                               <xsd:documentation source="CalculationsAndRules">If Fdfhp09a_EfficiencyType == EER use this choice to store user input of design  EER. 
 
 The value is stored in result1, the top displayed cell and the bottom cell is blank.</xsd:documentation>
                                                            </xsd:annotation>
                                                         </xsd:element>
                                                         <xsd:element name="Fdfhp11a_EfficiencySEER" type="hvac:EfficiencySEER" >
                                                            <xsd:annotation>
                                                               <xsd:documentation source="FieldText">Design Efficiency</xsd:documentation>
                                                               <xsd:documentation source="CalculationsAndRules">If Fdfhp09a_EfficiencyType == SEER use this choice to store user input of design  SEER.  
 
The value is stored in result1, the top displayed cell and the bottom cell is blank.</xsd:documentation>
                                                            </xsd:annotation>
                                                         </xsd:element>
                                                      </xsd:choice>
                                                      <xsd:element name="Fdfhp11b_NotApplicableMessage" >
                                                         <xsd:annotation>
                                                            <xsd:documentation source="FieldText">Design Efficiency</xsd:documentation>
                                                            <xsd:documentation source="CalculationsAndRules">result = BlankGrayedOut - no furnace value for cooling</xsd:documentation>
                                                         </xsd:annotation>
                                                         <xsd:simpleType>
                                                            <xsd:restriction base="comp:NotApplicableMessage">
                                                               <xsd:enumeration value="BlankGrayedOut"/>
                                                            </xsd:restriction>
                                                         </xsd:simpleType>
                                                      </xsd:element>
                                                   </xsd:sequence>
                                                </xsd:complexType>
                                             </xsd:element>
                                             <xsd:element name="CoolingEER_IEER_Dfhp" >
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText"></xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">Use this choice If F08_CoolingSensibleDesignCapacity GTE 1 And  If there are two  efficiency values, 
EER in result1and IEER in result2</xsd:documentation>
                                                </xsd:annotation>
                                                <xsd:complexType>
                                                   <xsd:sequence>
                                                      <xsd:element name="Fdfhp09a_EfficiencyType"  minOccurs="2" maxOccurs="2">
                                                         <xsd:annotation>
                                                            <xsd:documentation source="FieldText">Efficiency Unit</xsd:documentation>
                                                            <xsd:documentation source="CalculationsAndRules">If Fdfhp03a_UnitaryHeatPumps  == AirCooledSplit1PhaseHP, AirCooledSplit3PhaseHP, AirCooledPkg1PhaseHP, AirCooledPkg3PhaseHP, AirCooledSplitConstrainedHP Or AirCooledPkgConstrainedHP And Dfhp03a_SizeRangeUnitaryHeatPumps != LT65000 
result1 = EER and result2 = IEER: 
End If</xsd:documentation>
                                                         </xsd:annotation>
                                                         <xsd:simpleType>
                                                            <xsd:restriction base="hvac:EfficiencyType">
                                                               <xsd:enumeration value="EER"/>
                                                               <xsd:enumeration value="IEER"/>
                                                               <xsd:enumeration value="SEER"/>
                                                            </xsd:restriction>
                                                         </xsd:simpleType>
                                                      </xsd:element>
                                                      <xsd:element name="Fdfhp09b_NotApplicableMessage" >
                                                         <xsd:annotation>
                                                            <xsd:documentation source="FieldText">Efficiency Unit</xsd:documentation>
                                                            <xsd:documentation source="CalculationsAndRules">result = BlankGrayedOut - no furnace value for cooling</xsd:documentation>
                                                         </xsd:annotation>
                                                         <xsd:simpleType>
                                                            <xsd:restriction base="comp:NotApplicableMessage">
                                                               <xsd:enumeration value="BlankGrayedOut"/>
                                                            </xsd:restriction>
                                                         </xsd:simpleType>
                                                      </xsd:element>
                                                      <xsd:element name="Fdfhp10a_CoolEfficiencyMinUnitHP_EERIEER" type="hvac:CoolEfficiencyMinUnitHP_EERIEER"  minOccurs="2" maxOccurs="2">
                                                         <xsd:annotation>
                                                            <xsd:documentation source="FieldText">Minimum Efficiency Required per Tables 110.2 / Title 20</xsd:documentation>
                                                            <xsd:documentation source="CalculationsAndRules">For the following evaluations result1 is the EER value and result2 is the IEER:  
 
If Fdfhp03a_ UnitaryHeatPumps  == AirCooledSplit1PhaseHP, AirCooledSplit3PhaseHP, AirCooledPkg1PhaseHP, AirCooledPkg3PhaseHP, AirCooledSplitConstrainedHP Or AirCooledPkgConstrainedHP  
 
Switch (Fdfhp03a_SizeRangeUnitaryHeatPumps) 
case GTE65000AndLT135000: 
If Fdfhp02a_HvacSystemType == UnitaryHeatPumps
result1 = 11.0 (EER) and result2 = 14.1 (IEER); 
Else If Fdfhp02a_HvacSystemType == UnitaryHeatPumpsNoElectR NEED NON-ELEC FLAG
result1 = 10.8 (EER) and result2 = 12.0 (IEER); 
End If
 
case GTE135000AndLT240000: 
If Fdfhp02a_HvacSystemType == UnitaryHeatPumps
result1 = 10.6 (EER) and result2 = 13.5 (IEER); 
Else If Fdfhp02a_HvacSystemType == UnitaryHeatPumpsNoElectR
result1 = 10.4 (EER) and result2 = 11.4 (IEER); 
End If
 
case GTE240000: 
If Fdfhp02a_HvacSystemType == UnitaryHeatPumps
result1 = 9.5 (EER) and result2 = 12.5 (IEER);
Else If Fdfhp02a_HvacSystemType == UnitaryHeatPumpsNoElectR
result1 = 9.3 (EER) and result2 = 10.4 (IEER); 
End If
 
End Switch
End If</xsd:documentation>
                                                         </xsd:annotation>
                                                      </xsd:element>
                                                      <xsd:element name="Fdfhp10b_NotApplicableMessage" >
                                                         <xsd:annotation>
                                                            <xsd:documentation source="FieldText">Minimum Efficiency Required per Tables 110.2 / Title 20</xsd:documentation>
                                                            <xsd:documentation source="CalculationsAndRules">result = BlankGrayedOut - no furnace value for cooling</xsd:documentation>
                                                         </xsd:annotation>
                                                         <xsd:simpleType>
                                                            <xsd:restriction base="comp:NotApplicableMessage">
                                                               <xsd:enumeration value="BlankGrayedOut"/>
                                                            </xsd:restriction>
                                                         </xsd:simpleType>
                                                      </xsd:element>
                                                      <xsd:element name="Fdfhp11a_EfficiencyEER_IEER" type="hvac:EfficiencyEER_IEER"  minOccurs="2" maxOccurs="2">
                                                         <xsd:annotation>
                                                            <xsd:documentation source="FieldText">Design Efficiency</xsd:documentation>
                                                            <xsd:documentation source="CalculationsAndRules">User input of design  EER  for result1 and IEER for result2</xsd:documentation>
                                                         </xsd:annotation>
                                                      </xsd:element>
                                                      <xsd:element name="Fdfhp11b_NotApplicableMessage" >
                                                         <xsd:annotation>
                                                            <xsd:documentation source="FieldText">Design Efficiency</xsd:documentation>
                                                            <xsd:documentation source="CalculationsAndRules">result = BlankGrayedOut - no furnace value for cooling</xsd:documentation>
                                                         </xsd:annotation>
                                                         <xsd:simpleType>
                                                            <xsd:restriction base="comp:NotApplicableMessage">
                                                               <xsd:enumeration value="BlankGrayedOut"/>
                                                            </xsd:restriction>
                                                         </xsd:simpleType>
                                                      </xsd:element>
                                                   </xsd:sequence>
                                                </xsd:complexType>
                                             </xsd:element>
                                          </xsd:choice>
                                       </xsd:complexType>
                                    </xsd:element>
                                 </xsd:sequence>
                              </xsd:complexType>
                           </xsd:element>
                        </xsd:sequence>
                     </xsd:complexType>
                  </xsd:element>
                  <xsd:element name="TableBoilerSystem" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">[d:b]Boiler Efficiency and Controls [/d:b]</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">This table is required if B02f_ProjectHasBoilers == true</xsd:documentation>
                     </xsd:annotation>
                     <xsd:complexType>
                        <xsd:sequence>
                           <xsd:element name="Row" minOccurs="1" maxOccurs="unbounded">
                              <xsd:complexType>
                                 <xsd:sequence>
                                    <xsd:element name="Fc00a_BoilerSystemAreaServed" type="hvac:BoilerSystemAreaServed" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Boiler System Serving</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">User input text</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="Fc00b_Heating25PctRenewRecover" type="comp:Heating25PctRenewRecover" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">25% of annual space heating is provided by on site renewable energy, site recovered energy, or heat recovery chillers</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">User input - If checked result = true Else result = false</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="Fc00c_Heating50PctConvectiveRadiant" type="comp:Heating50PctConvectiveRadiant" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">50% or more of the design heating load is served using perimeter convective heating, and/or radiant panels</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">User input - If checked result = true Else result = false</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="Fc00d_InMultifamilyBldg" type="comp:InMultifamilyBldg" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Installed in Multifamily Building</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">User input - If checked result = true Else result = false</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="Fc00e_InstalledInExistingBldg" type="comp:InstalledInExistingBldg" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Boiler system added to existing building</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">User input - If checked result = true Else result = false</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="TableBoiler" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText"></xsd:documentation>
                                       </xsd:annotation>
                                       <xsd:complexType>
                                          <xsd:sequence>
                                             <xsd:element name="Row" minOccurs="1" maxOccurs="unbounded">
                                                <xsd:complexType>
                                                   <xsd:sequence>
                                                      <xsd:element name="Fc01_BoilerName" type="hvac:BoilerName" >
                                                         <xsd:annotation>
                                                            <xsd:documentation source="FieldText">Boiler Name or Item Tag</xsd:documentation>
                                                            <xsd:documentation source="CalculationsAndRules">User input</xsd:documentation>
                                                         </xsd:annotation>
                                                      </xsd:element>
                                                      <xsd:element name="Fc02_BoilerType" type="hvac:BoilerType" >
                                                         <xsd:annotation>
                                                            <xsd:documentation source="FieldText">Equipment Type[d:sup]1[/d:sup]</xsd:documentation>
                                                            <xsd:documentation source="CalculationsAndRules">User selects from list: HotWaterGas display term Hot Water, Gas-Fired; HotWaterOil display term Hot Water, Oil-Fired; SteamGasNotNaturalDraft display term Steam, Gas-Fired, not natural draft; SteamGas display term Steam, Gas-Fired; 
 SteamOil display term Steam, Oil-Fired</xsd:documentation>
                                                         </xsd:annotation>
                                                      </xsd:element>
                                                      <xsd:element name="Fc03_BoilerCount" type="hvac:BoilerCount" >
                                                         <xsd:annotation>
                                                            <xsd:documentation source="FieldText">Qty</xsd:documentation>
                                                            <xsd:documentation source="CalculationsAndRules">User input</xsd:documentation>
                                                         </xsd:annotation>
                                                      </xsd:element>
                                                      <xsd:element name="Fc04_BoilerRatedInput" type="hvac:BoilerRatedInput" >
                                                         <xsd:annotation>
                                                            <xsd:documentation source="FieldText">Rated Input Capacity (Btu/h)[d:sup]2,3[/d:sup]</xsd:documentation>
                                                            <xsd:documentation source="CalculationsAndRules">User input numeric value for boiler rated Btu/h</xsd:documentation>
                                                         </xsd:annotation>
                                                      </xsd:element>
                                                      <xsd:choice maxOccurs="1">
                                                         <xsd:element name="Fc05_EfficiencyAFUE" type="hvac:EfficiencyAFUE" >
                                                            <xsd:annotation>
                                                               <xsd:documentation source="FieldText">Rated Efficiency</xsd:documentation>
                                                               <xsd:documentation source="CalculationsAndRules">User input efficiency of the type appropriate for the size of their boiler as follows: 
 
If Fc04_BoilerRatedInput LT 300000 
user enters rated AFUE efficiency for the boiler.
End If</xsd:documentation>
                                                            </xsd:annotation>
                                                         </xsd:element>
                                                         <xsd:element name="Fc05_ThermalEfficiency" type="hvac:ThermalEfficiency" >
                                                            <xsd:annotation>
                                                               <xsd:documentation source="FieldText">Rated Efficiency</xsd:documentation>
                                                               <xsd:documentation source="CalculationsAndRules">User input efficiency of the type appropriate for the size of their boiler as follows:
If  Fc04_BoilerRatedInput GT 2500000  And Fc02_BoilerType == SteamGas Or SteamGasNotNaturalDraft Or SteamOil 
user enters rated Thermal Efficiency for the boiler
 
Else If Fc04_BoilerRatedInput  GTE300000 And LTE 2500000:
user enters rated Thermal Efficiency for the boiler
End If</xsd:documentation>
                                                            </xsd:annotation>
                                                         </xsd:element>
                                                         <xsd:element name="Fc05_CombustionEfficiency" type="hvac:CombustionEfficiency" >
                                                            <xsd:annotation>
                                                               <xsd:documentation source="FieldText">Rated Efficiency</xsd:documentation>
                                                               <xsd:documentation source="CalculationsAndRules">User input efficiency of the type appropriate for the size of their boiler as follows: 
 
If Fc04_BoilerRatedInput   GT 2500000 And 
Fc02_BoilerType == HotWaterGas Or HotWaterOil 
 
user enters rated Combustion Efficiency for the boiler
End If</xsd:documentation>
                                                            </xsd:annotation>
                                                         </xsd:element>
                                                      </xsd:choice>
                                                      <xsd:choice maxOccurs="1">
                                                         <xsd:element name="Fc06_EfficiencyMinimumValueAFUE" type="hvac:EfficiencyMinimumValueAFUE" >
                                                            <xsd:annotation>
                                                               <xsd:documentation source="FieldText">Minimum Efficiency Required per  [d:t24refnolink/]110.2</xsd:documentation>
                                                               <xsd:documentation source="CalculationsAndRules">Calculated Field: 
If Fc04_BoilerRatedInput  LT 300000 
Switch (Fc02_BoilerType) 
case: HotWaterGas 
result = .82 display term 82% AFUE; 
case: HotWaterOil result = .84 display term 84% AFUE  
case: SteamGas result = .80 display term 80% AFUE 
case: SteamOil result = .82 display term 82% AFUE
End Switch
End If</xsd:documentation>
                                                            </xsd:annotation>
                                                         </xsd:element>
                                                         <xsd:element name="Fc06_ThermalEfficiencyMinimum" type="hvac:ThermalEfficiencyMinimum" >
                                                            <xsd:annotation>
                                                               <xsd:documentation source="FieldText">Minimum Efficiency Required per  [d:t24refnolink/]110.2</xsd:documentation>
                                                               <xsd:documentation source="CalculationsAndRules">Calculated Field: 
Switch (Fc02_BoilerType) 
case: HotWaterGas
If Fc04_BoilerRatedInput GTE 300000 And LTE 2500000 
result = .84 display term 84% Et;  
End If
 
case: HotWaterOil 
If Fc04_BoilerRatedInput GTE 300000 And LTE 2500000 
result = .87 display term 87% Et; 
End If
 
case SteamGas:  case SteamGasNotNaturalDraft:  
If Fc04_BoilerRatedInput GTE 300000 And LTE 2500000 
 result = .81 display term 81% Et  
Else If Fc04_BoilerRatedInput  GT 2500000 And LT 10000000 
result = .81 display term 82% Et  
Else If Fc04_BoilerRatedInput  GT 10000000
result = .79 display term 79% Et  
End If
 
case: SteamOil 
If Fc04_BoilerRatedInput GTE 300000 And LTE 2500000  
 result = .84 display term 84% Et  
Else If Fc04_BoilerRatedInput  GT 2500000 And LT 10000000 
result = .85 display term 85% Et  
Else If Fc04_BoilerRatedInput  GT 10000000
result = .81 display term 81% Et  
End If
End Switch</xsd:documentation>
                                                            </xsd:annotation>
                                                         </xsd:element>
                                                         <xsd:element name="Fc06_CombustionEfficiencyMinimum" type="hvac:CombustionEfficiencyMinimum" >
                                                            <xsd:annotation>
                                                               <xsd:documentation source="FieldText">Minimum Efficiency Required per  [d:t24refnolink/]110.2</xsd:documentation>
                                                               <xsd:documentation source="CalculationsAndRules">Calculated Field: 
Switch (Fc02_BoilerType) 
 
case: HotWaterGas 
If Fc04_BoilerRatedInput GT 2500000 And LT 10000000 
result = .85 display term 85% Ec 
Else If Fc04_BoilerRatedInput GT 10000000 
result = .82 display term 82% Ec
End If
 
 case: HotWaterOil  
If Fc04_BoilerRatedInput GT 2500000 And LT 10000000 
result = .88 display term 88% Ec 
Else If Fc04_BoilerRatedInput  GT 10000000 
result = .84 display term 84% Ec
End If
End Switch</xsd:documentation>
                                                            </xsd:annotation>
                                                         </xsd:element>
                                                         <xsd:element name="Fc06_NotApplicableMessage" >
                                                            <xsd:annotation>
                                                               <xsd:documentation source="FieldText">Minimum Efficiency Required per  [d:t24refnolink/]110.2</xsd:documentation>
                                                               <xsd:documentation source="CalculationsAndRules">If combination of boiler type and boiler size has no assigned minimum efficiency, 
result  BlankGrayedOut
End If</xsd:documentation>
                                                            </xsd:annotation>
                                                            <xsd:simpleType>
                                                               <xsd:restriction base="comp:NotApplicableMessage">
                                                                  <xsd:enumeration value="BlankGrayedOut"/>
                                                               </xsd:restriction>
                                                            </xsd:simpleType>
                                                         </xsd:element>
                                                      </xsd:choice>
                                                      <xsd:element name="Fc07_EfficiencyType" >
                                                         <xsd:annotation>
                                                            <xsd:documentation source="FieldText">Efficiency Unit</xsd:documentation>
                                                            <xsd:documentation source="CalculationsAndRules">Calculated Field: 
If Fc04_BoilerRatedInput LT 300000 
 result = AFUE; 
 
Else If Fc04_BoilerRatedInput  GTE 300000 And  LTE2500000:
result = ThermalEfficiency; 
 
Else If Fc04_BoilerRatedInput GT 2500000 
If Fc02_BoilerType == HotWaterGas Or HotWaterOil result = CombustionEfficiency 
 
ElseIf Fc02_BoilerType == SteamGas Or SteamGasNotNaturalDraft Or SteamOil 
result = ThermalEfficiency.
End If</xsd:documentation>
                                                         </xsd:annotation>
                                                         <xsd:simpleType>
                                                            <xsd:restriction base="hvac:EfficiencyType">
                                                               <xsd:enumeration value="AFUE"/>
                                                               <xsd:enumeration value="CombustionEfficiency"/>
                                                               <xsd:enumeration value="ThermalEfficiency"/>
                                                            </xsd:restriction>
                                                         </xsd:simpleType>
                                                      </xsd:element>
                                                      <xsd:element name="Fc08toFc09ColumnHeading" minOccurs="0">
                                                         <xsd:annotation>
                                                            <xsd:documentation source="FieldText">Boiler Controls per [d:t24refnolink/]140.4(k), [d:t24refnolink/]170.2(c)Iiii</xsd:documentation>
                                                         </xsd:annotation>
                                                      </xsd:element>
                                                      <xsd:element name="Fc08_BoilerIsolationValve" type="hvac:BoilerIsolationValve" >
                                                         <xsd:annotation>
                                                            <xsd:documentation source="FieldText">Isolation Valve</xsd:documentation>
                                                            <xsd:documentation source="CalculationsAndRules">User selects from list: HasValve display term Yes and No1Boiler display term NA: only 1 boiler in plant
 
If result = No1Boiler And Fc03_BoilerCount GT 1
D03_ExceptionalConditionMessageCCMCHE += MultipleBoilerConflictInF
End If</xsd:documentation>
                                                         </xsd:annotation>
                                                      </xsd:element>
                                                      <xsd:element name="Fc09_TempResetControl" type="hvac:TempResetControl" >
                                                         <xsd:annotation>
                                                            <xsd:documentation source="FieldText">Temperature Reset</xsd:documentation>
                                                            <xsd:documentation source="CalculationsAndRules">User selects from list: 
HasTempReset display term Yes, 
NoLTE500000 display term NA: less than 500,000 Btu/h, 
NoVariableFlowControls display term NA: Variable Flow Controls, 
NoServesHealthcare display term NA: System serves Healthcare
 
If result = NoLTE500000 And Fc04_BoilerRatedInput GT 500000
D03_ExceptionalConditionMessageCCMCHE += BoilerSizeConflictInF
End If</xsd:documentation>
                                                         </xsd:annotation>
                                                      </xsd:element>
                                                      <xsd:element name="Fc10_HighCapBoilerExceptions" type="comp:HighCapBoilerExceptions" >
                                                         <xsd:annotation>
                                                            <xsd:documentation source="FieldText">High Capacity Boiler Exceptions</xsd:documentation>
                                                            <xsd:documentation source="CalculationsAndRules">User selects from list:</xsd:documentation>
                                                         </xsd:annotation>
                                                      </xsd:element>
                                                   </xsd:sequence>
                                                </xsd:complexType>
                                             </xsd:element>
                                          </xsd:sequence>
                                       </xsd:complexType>
                                    </xsd:element>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="Fc11_BoilerSystemEfficiency" type="hvac:BoilerSystemEfficiency" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">System Efficiency</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">If Sum All (Fc04_BoilerRatedInput that are GTE 300000) is GTE 1000000
result = (sum (Fc05 x Fc04_BoilerRatedInput)) / total input capacty of boilers between 1000000 and 10000000</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="Fc11_NotApplicableMessage" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">System Efficiency</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">If Sum All (Fc04_BoilerRatedInput ) LT  300000  Or
Sum All (Fc04_BoilerRatedInput that are GTE 300000) is LT 1000000
result = BlankGrayedOut
End If</xsd:documentation>
                                          </xsd:annotation>
                                          <xsd:simpleType>
                                             <xsd:restriction base="comp:NotApplicableMessage">
                                                <xsd:enumeration value="BlankGrayedOut"/>
                                             </xsd:restriction>
                                          </xsd:simpleType>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="Fc12_BoilerSystemMinEfficiency" type="hvac:BoilerSystemMinEfficiency" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Minimum System Efficiency Required per Tables 110.2 / Title 20</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">result = 90 % Thermal Efficiency</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="Fc12_NotApplicableMessage" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Minimum System Efficiency Required per Tables 110.2 / Title 20</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">If Fc10_HighCapBoilerExceptions != NoExceptionsHCB  Or 
Sum All (Fc04_BoilerRatedInput ) LT  300000
result = BlankGryedOut
End If</xsd:documentation>
                                          </xsd:annotation>
                                          <xsd:simpleType>
                                             <xsd:restriction base="comp:NotApplicableMessage">
                                                <xsd:enumeration value="BlankGrayedOut"/>
                                             </xsd:restriction>
                                          </xsd:simpleType>
                                       </xsd:element>
                                    </xsd:choice>
                                 </xsd:sequence>
                              </xsd:complexType>
                           </xsd:element>
                        </xsd:sequence>
                     </xsd:complexType>
                  </xsd:element>
                  <xsd:element name="TableBoilerFootnote1" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">[d:i][d:sup]1[/d:sup]FOOTNOTES: Use NRCC-PLB to document compliance with domestic hot water and service water heating systems.[/d:i]</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="TableBoilerFootnote2" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">[d:i][d:sup]2[/d:sup]Maximum capacity- minimum and maximum ratings as provided for and allowed by the unit's controls[/d:i]</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="TableBoilerFootnote3" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">[d:i][d:sup]3[/d:sup] Includes oil-fired (residual)[/d:i]</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="TableChiller" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">[d:b]Chiller Efficiency and Controls [/d:b]</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">This table is required if B02e_ProjectHasChillers == true</xsd:documentation>
                     </xsd:annotation>
                     <xsd:complexType>
                        <xsd:sequence>
                           <xsd:element name="Row" minOccurs="1" maxOccurs="unbounded">
                              <xsd:complexType>
                                 <xsd:sequence>
                                    <xsd:element name="Fd01_ChillerName" type="hvac:ChillerName" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Name or Item Tag</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">User input</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="Fd02_ChillerType" type="hvac:ChillerType" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Equipment Type</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">User selects from list:
AirCooledWithCondenser display term Air Cooled: Condenser elec. Operated; 
AirCooledWithOutCondenser display term Air Cooled: w/o condenser elec. Operated; 
Reciprocating display term Water Cooled: Reciprocating; 
PositiveDisplacement display term Water Cooled: Positive displacement; 
Centrifugal display term Water Cooled: Centrifugal; 
AbsorptionSingleEffect display term Air Cooled: Absorption, single effect; 
AbsorptionSingleEffectWater display term Water Cooled: Absorption, single effect; 
AbsorptionDoubleEffectDirectFired display term Absorption: Double Effect, direct-fired; 
AbsorptionDoubleEffectIndirectFired display term Absorption: Double Effect, indirect-fired; 
GasEngineDriven display term Water Cooled: Gas Engine</xsd:documentation>
                                          <xsd:appinfo source="displayterm">
                                             <dtyp:displayterm value="AbsorptionSingleEffect">Air Cooled: Absorption, single effect</dtyp:displayterm>
                                             <dtyp:displayterm value="Reciprocating"> Water Cooled: Reciprocating</dtyp:displayterm>
                                             <dtyp:displayterm value="Centrifugal"> Water Cooled: Centrifugal</dtyp:displayterm>
                                          </xsd:appinfo>
                                          
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="Fd03_Count" type="hvac:Count" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Qty</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">User input</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="Fd04_ChillerTonsRange" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Size Category[d:sup]1[/d:sup] [d:line1/] (tons)</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">User selects from list loaded as follows:  : 
LT75 display term [d:lt/]75 Tons; 
GTE75ToLT150 display term [d:gte/]75 and [d:lt/]150 Tons; 
GTE150ToLT300 display term [d:gte/]150 and [d:lt/]300 Tons; 
GTE300ToLT400 display term [d:gte/]300 and [d:lt/]400 Tons; 
GTE400ToLT600 display term [d:gte/]400 and [d:lt/]600 Tons; 
GTE600 display term [d:gte/]600 Tons;</xsd:documentation>
                                       </xsd:annotation>
                                       <xsd:simpleType>
                                          <xsd:restriction base="hvac:ChillerTonsRange">
                                             <xsd:enumeration value="GTE150ToLT300"/>
                                             <xsd:enumeration value="GTE300ToLT400"/>
                                             <xsd:enumeration value="GTE400ToLT600"/>
                                             <xsd:enumeration value="GTE600"/>
                                             <xsd:enumeration value="GTE75ToLT150"/>
                                             <xsd:enumeration value="LT75"/>
                                          </xsd:restriction>
                                       </xsd:simpleType>
                                    </xsd:element>
                                    <xsd:element name="Fd05_ChillerExceptionPathB" type="comp:ChillerExceptionPathB" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Chiller Efficiency "Path B" Exception per [d:t24refnolink/]140.4(i) &amp; [d:t24refnolink/]170.2(c)g</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">User selects from list: 
NoExceptionTaken display term No Exception Taken, 
EfficiencyException1 display term 1. Electrical service [d:gt/] 600V, 
EfficiencyException2 display term 2. Heat recovery w/ design capacity GT 40% of the design chiller cooling capacity, 
EfficiencyException3 display term 3. Charging energy storage systems w/ charging temperature  LT 40[d:deg/]F, 
EfficiencyException4 display term 4. 4th (or more) chiller in this building.  
 
If result != NoExceptionTaken minimum efficiency values for path A should be used
End If</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="FdEfficiency" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText"></xsd:documentation>
                                       </xsd:annotation>
                                       <xsd:complexType>
                                          <xsd:choice >
                                             <xsd:element name="FdCOP" >
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText"></xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">Use this choice 
If  Fd02_ChillerType == AbsorptionSingleEffect  Or AbsorptionSingleEffectWater.  
End If
There is only one efficiency value COP assigned to result1, the top cell and result2 bottom cell is blank.</xsd:documentation>
                                                </xsd:annotation>
                                                <xsd:complexType>
                                                   <xsd:sequence>
                                                      <xsd:element name="Fd06a_ChillerCOP" type="hvac:ChillerCOP" >
                                                         <xsd:annotation>
                                                            <xsd:documentation source="FieldText">Rated Efficiencies</xsd:documentation>
                                                            <xsd:documentation source="CalculationsAndRules">User enters efficiency value COP assigned to result1, the top cell and result2 bottom cell is blank.</xsd:documentation>
                                                         </xsd:annotation>
                                                      </xsd:element>
                                                      <xsd:element name="Fd07a_RangeMinCOP" type="hvac:RangeMinCOP" >
                                                         <xsd:annotation>
                                                            <xsd:documentation source="FieldText">Efficiencies Required per [d:t24refnolink/]110.2[d:sup]2[/d:sup]</xsd:documentation>
                                                            <xsd:documentation source="CalculationsAndRules">Calculated Value: 
If Fd02_ChillerType == AbsorptionSingleEffect result1 = GTE0.600; 
Else If Fd02_ChillerType == AbsorptionSingleEffectWater. result1 = GTE0.700.   
End If
result2 bottom cell is blank.</xsd:documentation>
                                                         </xsd:annotation>
                                                      </xsd:element>
                                                      <xsd:element name="Fd08a_EfficiencyType" >
                                                         <xsd:annotation>
                                                            <xsd:documentation source="FieldText">Efficiency Unit</xsd:documentation>
                                                            <xsd:documentation source="CalculationsAndRules">Calculated Value: result1 = COP and  result2 bottom cell is blank.</xsd:documentation>
                                                         </xsd:annotation>
                                                         <xsd:simpleType>
                                                            <xsd:restriction base="hvac:EfficiencyType">
                                                               <xsd:enumeration value="COP"/>
                                                            </xsd:restriction>
                                                         </xsd:simpleType>
                                                      </xsd:element>
                                                   </xsd:sequence>
                                                </xsd:complexType>
                                             </xsd:element>
                                             <xsd:element name="FdCOP_IPLV" >
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText"></xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">Use this choice 
If Fd02_ChillerType == AbsorptionDoubleEffectDirectFired, AbsorptionDoubleEffectIndirectFired Or GasEngineDriven. 
End If
Two  efficiency values are required: COP  in result1 and IPLV in result2</xsd:documentation>
                                                </xsd:annotation>
                                                <xsd:complexType>
                                                   <xsd:sequence>
                                                      <xsd:element name="Fd06a_EfficiencyCOP" type="hvac:EfficiencyCOP" >
                                                         <xsd:annotation>
                                                            <xsd:documentation source="FieldText">Rated Efficiencies</xsd:documentation>
                                                            <xsd:documentation source="CalculationsAndRules">User enters the rated COP efficiency for the chiller in result1, top cell in table</xsd:documentation>
                                                         </xsd:annotation>
                                                      </xsd:element>
                                                      <xsd:element name="Fd06b_EfficiencyCOP_IPLV" type="hvac:EfficiencyCOP_IPLV" >
                                                         <xsd:annotation>
                                                            <xsd:documentation source="FieldText">Rated Efficiencies</xsd:documentation>
                                                            <xsd:documentation source="CalculationsAndRules">User enters the rated COP IPLV efficiency for the chiller in result2, bottom cell in table</xsd:documentation>
                                                         </xsd:annotation>
                                                      </xsd:element>
                                                      <xsd:element name="Fd07a_RangeMinCOP" type="hvac:RangeMinCOP" >
                                                         <xsd:annotation>
                                                            <xsd:documentation source="FieldText">Efficiencies Required per [d:t24refnolink/]110.2[d:sup]2[/d:sup]</xsd:documentation>
                                                            <xsd:documentation source="CalculationsAndRules">Calculation for result1, top cell in table:  
If Fd02_ChillerType == AbsorptionDoubleEffectIndirectFired Or AbsorptionDoubleEffectDirectFired result1 = GTE1_000;
 
 Else If Fd02_ChillerType ==   GasEngineDriven result1 = GTE1_200
End If</xsd:documentation>
                                                         </xsd:annotation>
                                                      </xsd:element>
                                                      <xsd:element name="Fd07b_RangeMinCOP_IPLV" type="hvac:RangeMinCOP_IPLV" >
                                                         <xsd:annotation>
                                                            <xsd:documentation source="FieldText">Efficiencies Required per [d:t24refnolink/]110.2[d:sup]2[/d:sup]</xsd:documentation>
                                                            <xsd:documentation source="CalculationsAndRules">Calculation for result2, bottom cell in table:   
If Fd02_ChillerType == AbsorptionDoubleEffectIndirectFired result2 = GTE1_050 
Else If Fd02_ChillerType == AbsorptionDoubleEffectDirectFired result2 = GTE1_000; 
Else If Fd02_ChillerType ==   GasEngineDriven result2 = GTE2_000
End If</xsd:documentation>
                                                         </xsd:annotation>
                                                      </xsd:element>
                                                      <xsd:element name="Fd08a_EfficiencyType" >
                                                         <xsd:annotation>
                                                            <xsd:documentation source="FieldText">Efficiency Unit</xsd:documentation>
                                                            <xsd:documentation source="CalculationsAndRules">Result1, top cell in table: = COP</xsd:documentation>
                                                         </xsd:annotation>
                                                         <xsd:simpleType>
                                                            <xsd:restriction base="hvac:EfficiencyType">
                                                               <xsd:enumeration value="COP"/>
                                                            </xsd:restriction>
                                                         </xsd:simpleType>
                                                      </xsd:element>
                                                      <xsd:element name="Fd08b_EfficiencyType" >
                                                         <xsd:annotation>
                                                            <xsd:documentation source="FieldText">Efficiency Unit</xsd:documentation>
                                                            <xsd:documentation source="CalculationsAndRules">Result2, bottom cell in table: = COP_IPLV</xsd:documentation>
                                                         </xsd:annotation>
                                                         <xsd:simpleType>
                                                            <xsd:restriction base="hvac:EfficiencyType">
                                                               <xsd:enumeration value="COP_IPLV"/>
                                                            </xsd:restriction>
                                                         </xsd:simpleType>
                                                      </xsd:element>
                                                   </xsd:sequence>
                                                </xsd:complexType>
                                             </xsd:element>
                                             <xsd:element name="FdEER_IPLV" >
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText"></xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">Use this choice If Fd02_ChillerType == AirCooledWithCondenser Or AirCooledWithOutCondenser. 
 
If Fd05_ChillerExceptionPathB == NoExceptionTaken Path B values are used; 
Else Path A values are used.  
End If
Two  efficiency values are required: EER  in result1 and IPLV in result2</xsd:documentation>
                                                </xsd:annotation>
                                                <xsd:complexType>
                                                   <xsd:sequence>
                                                      <xsd:element name="Fd06a_EfficiencyEER" type="hvac:EfficiencyEER" >
                                                         <xsd:annotation>
                                                            <xsd:documentation source="FieldText">Rated Efficiencies</xsd:documentation>
                                                            <xsd:documentation source="CalculationsAndRules">User enters the rated EER efficiency for the chiller in result1, top cell in table</xsd:documentation>
                                                         </xsd:annotation>
                                                      </xsd:element>
                                                      <xsd:element name="Fd06b_EfficiencyEER_IPLV" type="hvac:EfficiencyEER_IPLV" >
                                                         <xsd:annotation>
                                                            <xsd:documentation source="FieldText">Rated Efficiencies</xsd:documentation>
                                                            <xsd:documentation source="CalculationsAndRules">User enters the rated EER  IPLV efficiency for the chiller in result2, bottom cell in table</xsd:documentation>
                                                         </xsd:annotation>
                                                      </xsd:element>
                                                      <xsd:element name="Fd07a_RangeMinEER" type="hvac:RangeMinEER" >
                                                         <xsd:annotation>
                                                            <xsd:documentation source="FieldText">Efficiencies Required per [d:t24refnolink/]110.2[d:sup]2[/d:sup]</xsd:documentation>
                                                            <xsd:documentation source="CalculationsAndRules">Calculation for result1, top cell in table:  
 
If Fd02_ChillerType == AirCooledWithCondenser Or AirCooledWithOutCondenser Then 
If Fd05_ChillerExceptionPathB == NoExceptionTaken result1 = GTE9_700; 
Else result1 = GTE10_100;
End If
End If</xsd:documentation>
                                                         </xsd:annotation>
                                                      </xsd:element>
                                                      <xsd:element name="Fd07b_RangeMinEER_IPLV" type="hvac:RangeMinEER_IPLV" >
                                                         <xsd:annotation>
                                                            <xsd:documentation source="FieldText">Efficiencies Required per [d:t24refnolink/]110.2[d:sup]2[/d:sup]</xsd:documentation>
                                                            <xsd:documentation source="CalculationsAndRules">Calculation for result2, bottom cell in table:   
If Fd02_ChillerType == AirCooledWithCondenser Or AirCooledWithOutCondenser 
Switch (Fd04_ChillerTonsRange) 
case LT75 Or GTE75ToLT150: 
If Fd05_ChillerExceptionPathB == NoExceptionTaken result1 = GTE15_800; 
Else result1 = GTE13_700; 
End If
 
case GTE150ToLT300, GTE300ToLT400, GTE400ToLT600 or GTE600: 
If Fd05_ChillerExceptionPathB == NoExceptionTaken result1 = GTE16_100; 
Else result1 = GTE14_000;
End If
End Switch
End If</xsd:documentation>
                                                         </xsd:annotation>
                                                      </xsd:element>
                                                      <xsd:element name="Fd08a_EfficiencyType" >
                                                         <xsd:annotation>
                                                            <xsd:documentation source="FieldText">Efficiency Unit</xsd:documentation>
                                                            <xsd:documentation source="CalculationsAndRules">Result1, top cell in table: = EER</xsd:documentation>
                                                         </xsd:annotation>
                                                         <xsd:simpleType>
                                                            <xsd:restriction base="hvac:EfficiencyType">
                                                               <xsd:enumeration value="EER"/>
                                                            </xsd:restriction>
                                                         </xsd:simpleType>
                                                      </xsd:element>
                                                      <xsd:element name="Fd08b_EfficiencyType" >
                                                         <xsd:annotation>
                                                            <xsd:documentation source="FieldText">Efficiency Unit</xsd:documentation>
                                                            <xsd:documentation source="CalculationsAndRules">Result2, bottom cell in table: = EER_IPLV</xsd:documentation>
                                                         </xsd:annotation>
                                                         <xsd:simpleType>
                                                            <xsd:restriction base="hvac:EfficiencyType">
                                                               <xsd:enumeration value="EER_IPLV"/>
                                                            </xsd:restriction>
                                                         </xsd:simpleType>
                                                      </xsd:element>
                                                   </xsd:sequence>
                                                </xsd:complexType>
                                             </xsd:element>
                                             <xsd:element name="FdKWperTon_IPLV" >
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText"></xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">Use this choice If Fd02_ChillerType Reciprocating,  PositiveDisplacement Or Centrifugal.  
 
If Fd05_ChillerExceptionPathB == NoExceptionTaken Path B values are used; 
Else Path A values are used.  
End If
Two  efficiency values are required: kWperTon  in result1 and IPLV in result2</xsd:documentation>
                                                </xsd:annotation>
                                                <xsd:complexType>
                                                   <xsd:sequence>
                                                      <xsd:element name="Fd06a_EfficiencyKWperTon" type="hvac:EfficiencyKWperTon" >
                                                         <xsd:annotation>
                                                            <xsd:documentation source="FieldText">Rated Efficiencies</xsd:documentation>
                                                            <xsd:documentation source="CalculationsAndRules">User enters the rated kWperTon  efficiency for the chiller in result1, top cell in table</xsd:documentation>
                                                         </xsd:annotation>
                                                      </xsd:element>
                                                      <xsd:element name="Fd06b_EfficiencyKWperTon_IPLV" type="hvac:EfficiencyKWperTon_IPLV" >
                                                         <xsd:annotation>
                                                            <xsd:documentation source="FieldText">Rated Efficiencies</xsd:documentation>
                                                            <xsd:documentation source="CalculationsAndRules">User enters the rated kWperTon   IPLV efficiency for the chiller in result2, bottom cell in table</xsd:documentation>
                                                         </xsd:annotation>
                                                      </xsd:element>
                                                      <xsd:element name="Fd07a_RangeMinKWperTon" type="hvac:RangeMinKWperTon" >
                                                         <xsd:annotation>
                                                            <xsd:documentation source="FieldText">Efficiencies Required per [d:t24refnolink/]110.2[d:sup]2[/d:sup]</xsd:documentation>
                                                            <xsd:documentation source="CalculationsAndRules">Calculation for result1, top cell in table:   
If Fd02_ChillerType == Reciprocating Or   PositiveDisplacement  
Switch (Fd04_ChillerTonsRange)  
case LT75: 
If Fd05_ChillerExceptionPathB == NoExceptionTaken result1 = LTE0_780; 
Else result1 =LTE0_750; 
End If
case GTE75ToLT150: 
If Fd05_ChillerExceptionPathB == NoExceptionTaken result1 = LTE0_750; 
Else result1 =LTE0_720; 
End if
case GTE150ToLT300: 
If Fd05_ChillerExceptionPathB == NoExceptionTaken result1 = LTE0_680; 
Else result1 = LTE0_660; 
End if
case GTE300ToLT400: case GTE400ToLT600: 
If Fd05_ChillerExceptionPathB == NoExceptionTaken result1 = LTE0_625; 
Else result1 =LTE0_610; 
End if
case GTE600: 
If Fd05_ChillerExceptionPathB == NoExceptionTaken result1 = LTE0_585; 
Else result1 = LTE0_560; 
End If 
End Switch
 
Else If Fd02_ChillerType == Centrifugal 
Switch (Fd04_ChillerTonsRange)  
case LT75 Or GTE75ToLT150: 
If Fd05_ChillerExceptionPathB == NoExceptionTaken result1 = LTE0_695; 
Else result1 =LTE0_610; 
End If
case GTE150ToLT300: 
If Fd05_ChillerExceptionPathB == NoExceptionTaken result1 = LTE0_635; 
Else result1 = LTE0_610; 
End If
case GTE300ToLT400: 
If Fd05_ChillerExceptionPathB == NoExceptionTaken result1 = LTE0_595; 
Else result1 =LTE0_560; 
End If
case GTE400ToLT600 Or GTE600: 
If Fd05_ChillerExceptionPathB == NoExceptionTaken result1 = LTE0_585; 
Else result = LTE0_560; 
End if
End Switch
End if</xsd:documentation>
                                                         </xsd:annotation>
                                                      </xsd:element>
                                                      <xsd:element name="Fd07b_RangeMinKWperTon_IPLV" type="hvac:RangeMinKWperTon_IPLV" >
                                                         <xsd:annotation>
                                                            <xsd:documentation source="FieldText">Efficiencies Required per [d:t24refnolink/]110.2[d:sup]2[/d:sup]</xsd:documentation>
                                                            <xsd:documentation source="CalculationsAndRules">Calculation for result2, bottom cell in table:   
If Fd02_ChillerType == Reciprocating Or   PositiveDisplacement  
Switch (Fd04_ChillerTonsRange)  
case LT75: 
If Fd05_ChillerExceptionPathB == NoExceptionTaken result1 = LTE0_500; 
Else result1 =LTE0_600; 
End If
case GTE75ToLT150: 
If Fd05_ChillerExceptionPathB == NoExceptionTaken result1 = LTE0_490; 
Else result1 =LTE0_560; 
End if
case GTE150ToLT300: 
If Fd05_ChillerExceptionPathB == NoExceptionTaken result1 = LTE0_440; 
Else result1 = LTE0_540; 
End If
case GTE300ToLT400: case GTE400ToLT600:
If Fd05_ChillerExceptionPathB == NoExceptionTaken result1 = LTE0_410; 
Else result1 =LTE0_520; 
End If
case GTE600: 
If Fd05_ChillerExceptionPathB == NoExceptionTaken result1 = LTE0_380; 
Else result1 = LTE0_500;  
End If
End Switch
 
ElseIf Fd02_ChillerType == Centrifugal 
Switch (Fd04_ChillerTonsRange)  
case LT75 Or GTE75ToLT150: 
If Fd05_ChillerExceptionPathB == NoExceptionTaken result1 = LTE0_440; 
Else result1 = LTE0_550; 
End If
case GTE150ToLT300: 
If Fd05_ChillerExceptionPathB == NoExceptionTaken result1 = LTE0_400; 
Else result1 = LTE0_550; 
End If
case GTE300ToLT400: 
If Fd05_ChillerExceptionPathB == NoExceptionTaken result1 = LTE0_390; 
Else result1 =LTE0_520; 
End If
case GTE400ToLT600 Or GTE600: 
If Fd05_ChillerExceptionPathB == NoExceptionTaken result1 = LTE0_380; 
Else result = LTE0_500; 
End If
End Switch
End If</xsd:documentation>
                                                         </xsd:annotation>
                                                      </xsd:element>
                                                      <xsd:element name="Fd08a_EfficiencyType" >
                                                         <xsd:annotation>
                                                            <xsd:documentation source="FieldText">Efficiency Unit</xsd:documentation>
                                                            <xsd:documentation source="CalculationsAndRules">Result1, top cell in table: = kWperTon</xsd:documentation>
                                                         </xsd:annotation>
                                                         <xsd:simpleType>
                                                            <xsd:restriction base="hvac:EfficiencyType">
                                                               <xsd:enumeration value="KWperTon"/>
                                                            </xsd:restriction>
                                                         </xsd:simpleType>
                                                      </xsd:element>
                                                      <xsd:element name="Fd08b_EfficiencyType" >
                                                         <xsd:annotation>
                                                            <xsd:documentation source="FieldText">Efficiency Unit</xsd:documentation>
                                                            <xsd:documentation source="CalculationsAndRules">Result2, bottom cell in table: = kWperTon_IPLV</xsd:documentation>
                                                         </xsd:annotation>
                                                         <xsd:simpleType>
                                                            <xsd:restriction base="hvac:EfficiencyType">
                                                               <xsd:enumeration value="KWperTon_IPLV"/>
                                                            </xsd:restriction>
                                                         </xsd:simpleType>
                                                      </xsd:element>
                                                   </xsd:sequence>
                                                </xsd:complexType>
                                             </xsd:element>
                                          </xsd:choice>
                                       </xsd:complexType>
                                    </xsd:element>
                                    <xsd:element name="Fd09toFd10ColumnHeading" minOccurs="0">
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText"></xsd:documentation>
                                          <xsd:documentation source="AdditionalRequirements">Controls per [d:t24refnolink/]140.4(k), [d:t24refnolink/]170.2(c)I</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="Fd09_ChillerIsolationValve" type="hvac:ChillerIsolationValve" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Isolation Valve</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">User selects from list; HasValve display term Yes, No1Chiller display term NA: only 1 chiller in plant, NoChillersInSeries display term NA: chillers in series.  
 
If result == No1Chiller And Fd03_ChillerCount GT 1  D03_ExceptionalConditionMessageCCMCHE += MultipleChillerConflictInF
End If</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="Fd10_TempResetControl" type="hvac:TempResetControl" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Temperature Reset</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">User selects from list; HasTempReset display term Yes, NoLTE500000 display term NA: LTE 500,00 Btu/h, NoVariableFlowControls display term NA: Variable Flow Controls,  NoServesHealthcare display term NA: System serves Healthcare</xsd:documentation>
                                          <xsd:appinfo source="displayterm">
                                             <dtyp:displayterm value="NoLTE500000">NA: [d:lte/]500,00 Btu/h (41.67 tons)</dtyp:displayterm>
                                          </xsd:appinfo>
                                          
                                       </xsd:annotation>
                                    </xsd:element>
                                 </xsd:sequence>
                              </xsd:complexType>
                           </xsd:element>
                        </xsd:sequence>
                     </xsd:complexType>
                  </xsd:element>
                  <xsd:element name="FdFootnote1" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">[d:i][d:sup]1[/d:sup]FOOTNOTES: Chilled water plants shall not have more than 300 tons provided by air-cooled chillers.  Exceptions may apply per [d:t24refnolink/]140.4(j).[/d:i]</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="FdFootnote2" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">[d:i][d:sup]2[/d:sup] Efficiency required is a minimum when EER or COP is the Efficiency Unit in column 08.  It is also a minimum when IPLV is the unit for air-cooled, absorption, and water cooled gas engine chillers.  Efficiency required is a maximum when kW/ton is the Efficiency Unit and when IPLV is the unit for water cooled electrically operated chillers..[/d:i]</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="TableHeatRejection" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">[d:b]Heat Rejection Equipment (Cooling Towers, Condensers, Waterside Economizers) Efficiency and Controls [/d:b]</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">This table is required If B02a_ProjectHasWaterEconomizer Or B02d_ProjectHasCoolingTowers == true</xsd:documentation>
                     </xsd:annotation>
                     <xsd:complexType>
                        <xsd:sequence>
                           <xsd:element name="Row" minOccurs="1" maxOccurs="unbounded">
                              <xsd:complexType>
                                 <xsd:sequence>
                                    <xsd:element name="Fe01_HeatRejectionName" type="hvac:HeatRejectionName" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Name or Item Tag</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">User enters identifying name</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="Fe02_HeatRejectionType" type="hvac:HeatRejectionType" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Equipment Type[d:sup]1[/d:sup]</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">User selects from dropdown list: 
OpenTowerAxialFan display term Propeller/ axial fan open-circuit cooling tower, 
 
AxialFanDryCooler display term Propeller/ axial fan dry cooler
 
OpenTowerAxialGTE900gpm display term Axial fan open-circuit cooling tower, CW loop in CHW plant [d:gte/] 900 gpm, 
 
OpenTowerReplacementAxialGTE900gpm display term Replacement axial fan open-circuit cooling tower, CW loop in CHW plant [d:gte/] 900 gpm,  
 
ClosedTowerAxialFan display term Propeller/ axial fan closed-circuit cooling tower, 
 
OpenTowerCentrifugalFan display term Centrifugal fan open-circuit cooling tower, 
 
ClosedTowerCentrifugalFan display term Centrifugal fan closed-circuit cooling tower, 
 
EvaporativeCondenserAxialFan display term Propeller/ axial fan evaporative condensers,
 
EvaporativeCondenserCentrifugalFan display term Centrifugal fan evaporative condensers, 
 
AirCooledCondenser display term Air cooled condensers, 
 
WatersideEconomizer display term Air Waterside Economizer
 
Mandatory Measure Triggers:
If result == OpenTowerAxialFan Or OpenTowerAxialGTE900gpm Or 
OpenTowerReplacementAxialGTE900gpm Or ClosedTowerAxialFan Or  OpenTowerCentrifugalFan Or ClosedTowerCentrifugalFan  
MM110_2e_MeasureApplies = true and is displayed
End If</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="Fe03_HeatRejectionEquipmentCount" type="hvac:HeatRejectionEquipmentCount" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Qty</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">User enters quantity as integer</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="Fe03_NotApplicableMessage" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Qty</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">If Fe02_HeatRejectionType == WatersideEconomizer 
result = BlankGrayedOut Stored in the NotApplicableMessage
End If</xsd:documentation>
                                          </xsd:annotation>
                                          <xsd:simpleType>
                                             <xsd:restriction base="comp:NotApplicableMessage">
                                                <xsd:enumeration value="BlankGrayedOut"/>
                                             </xsd:restriction>
                                          </xsd:simpleType>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="Fe04_EvaporativeCondenserRatingCondition" type="hvac:EvaporativeCondenserRatingCondition" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Rating Condition ([d:deg/]F)</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">If Fe02_HeatRejectionType == EvaporativeCondenserAxialFan Or EvaporativeCondenserCentrifugalFan, user slects from list: 
Entering140Condensing96_3 display term 140 [:deg/]F entering gas temp   96.3 [d:deg/]F condensing temp
Entering165Condensing105 display term 165 [:deg/]F entering gas temp   105 [d:deg/]F condensing temp
 
Else result = BlankGrayedOut stored in NotApplicableMessage and cell is grayed out
End If</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="Fe04_NotApplicableMessage" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Rating Condition ([d:deg/]F)</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">If Fe02_HeatRejectionType == EvaporativeCondenserAxialFan Or EvaporativeCondenserCentrifugalFan, user input
Else result = BlankGrayedOut stored in NotApplicableMessage and cell is grayed out
End If</xsd:documentation>
                                          </xsd:annotation>
                                          <xsd:simpleType>
                                             <xsd:restriction base="comp:NotApplicableMessage">
                                                <xsd:enumeration value="BlankGrayedOut"/>
                                             </xsd:restriction>
                                          </xsd:simpleType>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="Fe05_CoolingTowerCapacityRated" type="hvac:CoolingTowerCapacityRated" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Rated Performance</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">If Fe02_HeatRejectionType == ClosedTowerAxialFan,  ClosedTowerCentrifugalFan, OpenTowerAxialFan, AxialFanDryCooler, OpenTowerAxialGTE900gpm, OpenTowerReplacementAxialGTE900gpm Or OpenTowerCentrifugalFan 
User input and result is stored in CoolingTowerCapacityRated with units of gpm/hp
End If</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="Fe05_HeatRejectionCondenserCapacityRated" type="hvac:HeatRejectionCondenserCapacityRated" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Rated Performance</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">If Fe02_HeatRejectionType == EvaporativeCondenserAxialFan,   EvaporativeCondenserCentrifugalFan Or AirCooledCondenser 
User input and result is stored in HeatRejectionCondenserCapacityRated with units of Btu/h hp
End if</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="Fe05_NotApplicableMessage" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Rated Performance</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">If Fe02_HeatRejectionType == WatersideEconomizer 
result = BlankGrayedOut Stored in the NotApplicableMessage
End If</xsd:documentation>
                                          </xsd:annotation>
                                          <xsd:simpleType>
                                             <xsd:restriction base="comp:NotApplicableMessage">
                                                <xsd:enumeration value="BlankGrayedOut"/>
                                             </xsd:restriction>
                                          </xsd:simpleType>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="Fe06_CoolingTowerCapacityRequiredRange" type="hvac:CoolingTowerCapacityRequiredRange" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Minimum Required Performance per Table 110.2-F and [d:t24refnolink/]140.4(h)5 &amp;  [d:t24refnolink/]170.2(c)fv</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">If Fe02_HeatRejectionType == ClosedTowerAxialFan,  ClosedTowerCentrifugalFan, OpenTowerAxialFan, AxialFanDryCooler Or OpenTowerCentrifugalFan result is stored in HeatRejectionCapacityRequiredTower as follows: 
 
If Fe02_HeatRejectionType == OpenTowerAxialFan result = GTE42_1; 
Else If Fe02_HeatRejectionType == OpenTowerCentrifugalFan result = GTE20_0; 
Else If Fe02_HeatRejectionType == ClosedTowerAxialFan result = GTE16_1; 
Else If Fe02_HeatRejectionType == ClosedTowerCentrifugalFan result = GTE7_0.  
 
Else If Fe02_HeatRejectionType == OpenTowerAxialGTE900gpm Then 
If A02_ClimateZone is InRange(2,15) result = 60;
Else If A02_ClimateZone == 1 Or 16 result = 42.1;  
End If
 
Else If Fe02_HeatRejectionType == OpenTowerReplacementAxialGTE900gpm 
result = 42.1;
End If
 
If Fe02_HeatRejectionType == AxialFanDryCooler  result = GTE4_5;
End If</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="Fe06_HeatRejectionCondenserCapacityRequired" type="hvac:HeatRejectionCondenserCapacityRequired" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Minimum Required Performance per Table 110.2-F and [d:t24refnolink/]140.4(h)5 &amp;  [d:t24refnolink/]170.2(c)fv</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">If Fe02_HeatRejectionType == EvaporativeCondenserAxialFan,   EvaporativeCondenserCentrifugalFan Or AirCooledCondenser result is stored in HeatRejectionCapacityRequiredCondenser as follows:.  
 
If Fe02_HeatRejectionType == EvaporativeCondenserAxialFan Then 
If  Fe04_EvaporativeCondenserRatingCondition == Entering165Condensing105 result =  GTE157000; 
Else if  Fe04_EvaporativeCondenserRatingCondition == Entering140Condensing96_3 result = GTE134000; 
End If
 
Else If Fe02_HeatRejectionType == EvaporativeCondenserCentrifugalFan Then 
If  Fe04_EvaporativeCondenserRatingCondition == Entering165Condensing105 result =  GTE135000; 
Else if  Fe04_EvaporativeCondenserRatingCondition == Entering140Condensing96_3 result = GTE110000;  
End If
 
Else If Fe02_HeatRejectionType == AirCooledCondenser result = GTE176000
End If
End If</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="Fe06_NotApplicableMessage" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Minimum Required Performance per Table 110.2-F and [d:t24refnolink/]140.4(h)5 &amp;  [d:t24refnolink/]170.2(c)fv</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">If Fe02_HeatRejectionType == WatersideEconomizer result = BlankGrayedOut Stored in the NotApplicableMessage
End If</xsd:documentation>
                                          </xsd:annotation>
                                          <xsd:simpleType>
                                             <xsd:restriction base="comp:NotApplicableMessage">
                                                <xsd:enumeration value="BlankGrayedOut"/>
                                             </xsd:restriction>
                                          </xsd:simpleType>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="Fe07_HeatRejectionCapacityUnits" type="hvac:HeatRejectionCapacityUnits" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Performance Unit</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">If Fe02_HeatRejectionType == ClosedTowerAxialFan,  ClosedTowerCentrifugalFan, OpenTowerAxialFan,  AxialFanDryCooler,  OpenTowerCentrifugalFan, OpenTowerAxialGTE900gpm Or OpenTowerReplacementAxialGTE900gpm  
result = GalPerMinHp 
 
Else result = BtuHrHp
End If</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="Fe07_NotApplicableMessage" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Performance Unit</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">If Fe02_HeatRejectionType == WatersideEconomizer result = BlankGrayedOut Stored in the NotApplicableMessage
End If</xsd:documentation>
                                          </xsd:annotation>
                                          <xsd:simpleType>
                                             <xsd:restriction base="comp:NotApplicableMessage">
                                                <xsd:enumeration value="BlankGrayedOut"/>
                                             </xsd:restriction>
                                          </xsd:simpleType>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="Fe08_HeatRejectionFanSpeedControl" type="hvac:HeatRejectionFanSpeedControl" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Fan Speed Control [d:t24refnolink/]140.4(h)1</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">User selects from list: 
HasSpeedControl display term Yes, 
FanMotorLT7_5hp display term NA: Fan motor LT 7.5 hp (5.6kW), 
IntegralToOtherEquipment display term NA: Integral to other equip., 
FanForMultipleCircuits display term NA: Cond. fans serving  GT 1 refrigerant circuits, 
FanForFloodedCondenser display term NA: Cond. fans serving flooded cond.,  
LeadFansHaveControls display term NA: Mult. fans &amp; lead fans meet req.</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="Fe08_NotApplicableMessage" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Fan Speed Control [d:t24refnolink/]140.4(h)1</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">If Fe02_HeatRejectionType == WatersideEconomizer result = BlankGrayedOut Stored in the NotApplicableMessage
End If</xsd:documentation>
                                          </xsd:annotation>
                                          <xsd:simpleType>
                                             <xsd:restriction base="comp:NotApplicableMessage">
                                                <xsd:enumeration value="BlankGrayedOut"/>
                                             </xsd:restriction>
                                          </xsd:simpleType>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="Fe09_TowerFlowTurnDown" type="hvac:TowerFlowTurnDown" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Tower Flow Turndown [d:t24refnolink/]140.4(h)2</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">If Fe02_HeatRejectionType == OpenTowerAxialFan, OpenTowerCentrifugalFan, OpenTowerAxialGTE900gpm Or OpenTowerReplacementAxialGTE900gpm
 
 user selects from list: 
HasTowerFlowTurnDown display term Yes, 
SingleCondenserPump display term NA: Single condenser water pump, 
NotOpenTower display term NA: Not open-circuit tower
 
Else result = BlankGrayedOut stored in the NotApplicableMessage and cell is grayed out.  
End If</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="Fe09_NotApplicableMessage" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Tower Flow Turndown [d:t24refnolink/]140.4(h)2</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">If Fe02_HeatRejectionType != OpenTowerAxialFan, OpenTowerCentrifugalFan, OpenTowerAxialGTE900gpm or OpenTowerReplacementAxialGTE900gpm
 result = BlankGrayedOut  stored in the NotApplicableMessage and cell is grayed out
End If</xsd:documentation>
                                          </xsd:annotation>
                                          <xsd:simpleType>
                                             <xsd:restriction base="comp:NotApplicableMessage">
                                                <xsd:enumeration value="BlankGrayedOut"/>
                                             </xsd:restriction>
                                          </xsd:simpleType>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="Fe10_FanControlMultipleCells" type="hvac:FanControlMultipleCells" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Fan Control in Multiple Cell Equipment [d:t24refnolink/]140.4(h)4</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">User selects from list: 
HasControlInMultipleCells display term Yes, 
NotMultipleCells display term NA: Not multiple cell, 
NotVariableSpeedDrive display term NA: Not variable speed drives</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="Fe10_NotApplicableMessage" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Fan Control in Multiple Cell Equipment [d:t24refnolink/]140.4(h)4</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">If Fe02_HeatRejectionType == WatersideEconomizer 
result = BlankGrayedOut Stored in the NotApplicableMessage
End If</xsd:documentation>
                                          </xsd:annotation>
                                          <xsd:simpleType>
                                             <xsd:restriction base="comp:NotApplicableMessage">
                                                <xsd:enumeration value="BlankGrayedOut"/>
                                             </xsd:restriction>
                                          </xsd:simpleType>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="Fe11_HasWaterEconomizerControls" type="hvac:HasWaterEconomizerControls" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Economizer Controls [d:t24refnolink/]140.4(e)</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">If Fe02_HeatRejectionType == WatersideEconomizer user selects from list: Designed per 140.4(e) (result = true);  
 
Else result = BlankGrayedOut stored in the NotApplicableMessage and cell is grayed out.  EndIf</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="Fe11_NotApplicableMessage" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Economizer Controls [d:t24refnolink/]140.4(e)</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">If Fe02_HeatRejectionType != WatersideEconomizer result = BlankGrayedOut stored in the NotApplicableMessage and cell is grayed out
End If</xsd:documentation>
                                          </xsd:annotation>
                                          <xsd:simpleType>
                                             <xsd:restriction base="comp:NotApplicableMessage">
                                                <xsd:enumeration value="BlankGrayedOut"/>
                                             </xsd:restriction>
                                          </xsd:simpleType>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="Fe12_HasTempResetControls" type="hvac:HasTempResetControls" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Condenser Water Temp Reset Controls</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">If Fe02_HeatRejectionType != AirCooledCondenser 
user selects Yes (result = true) and No (result = false)
 
Else result = BlankGrayedOut stored in the NotApplicableMessage and cell is grayed out.  End If</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="Fe12_NotApplicableMessage" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Condenser Water Temp Reset Controls</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">If Fe02_HeatRejectionType == AirCooledCondenser  
result = BlankGrayedOut stored in the NotApplicableMessage and cell is grayed out.
End If</xsd:documentation>
                                          </xsd:annotation>
                                          <xsd:simpleType>
                                             <xsd:restriction base="comp:NotApplicableMessage">
                                                <xsd:enumeration value="BlankGrayedOut"/>
                                             </xsd:restriction>
                                          </xsd:simpleType>
                                       </xsd:element>
                                    </xsd:choice>
                                 </xsd:sequence>
                              </xsd:complexType>
                           </xsd:element>
                        </xsd:sequence>
                     </xsd:complexType>
                  </xsd:element>
                  <xsd:element name="TableHeatRejectionFootnote1" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">[d:i][d:sup]1[/d:sup]FOOTNOTES: Centrifugal fan open-circuit towers are not allowed for rated capacities [d:gte/] 900 gpm at 95[d:deg/]F condenser water return, 85[d:deg/]F condenser water supply and 75[d:deg/]F outdoor wet-bulb temperature.  Exceptions may apply per [d:t24refnolink/]140.4(h)4.  [/d:i]</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="TableElectricHeat" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">[d:b]Electric Resistance Heating[/d:b]</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">This table is required If B03b_ProjectHasElectricHeat == true</xsd:documentation>
                     </xsd:annotation>
                     <xsd:complexType>
                        <xsd:sequence>
                           <xsd:element name="Row" minOccurs="1" maxOccurs="unbounded">
                              <xsd:complexType>
                                 <xsd:sequence>
                                    <xsd:element name="Ff01_ElectricResistanceName" type="hvac:ElectricResistanceName" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Name or Item Tag</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">User enters name of equipment</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="Ff02_ElectricResistanceHeatingType" type="hvac:ElectricResistanceHeatingType" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Equipment Description</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">User selects from list: Baseboard, Other, UnderMat display term Under-mat</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="Ff03_ElectricResistanceHeatingCapacity" type="hvac:ElectricResistanceHeatingCapacity" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Output Capacity (kW)</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">User enters capacity in kW</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="Ff04_ElectricResistanceHeatingException" type="hvac:ElectricResistanceHeatingException" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Applicable Exception to [d:t24refnolink/]140.4(g) Allowing Electric Resistance Heating</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">User selects from list: 
Exception1PvRecovered60pctEnergy display term Exception1: Supplement to a heating system in which at least 60% of the annual energy requirement is supplied by site-solar or recovered energy.  
 
Exception2HP75pctLoad display term Exception2: Supplement to a heat pump heating system and the heating capacity of the heat pump is more than 75% of the design heating load calculated in accordance with 140.4
 
Exception3ElectricHeatingLT10pctTotal display term Exception3: Total capacity of electric-resistance heating systems serving the entire building is less than 10% of the total design output capacity of all heating equipment serving the entire building 
 
Exception4ElectricHeatingCapacity3kW display term Exception4: Total capacity of electric-resistance heating systems serving the entire building excluding those allowed under Exception 2, is no more than 3 kW 
 
Exception5NoCoolingNoGasNonRes display term Exception5: Serving an entire building that is not hotel/motel and has a conditioned floor area no greater than 5,000 ft2, no mechanical cooling and is in an area without natural gas access  
 
ExceptionElecResistanceNA display term NA: Alteration, replacement of existing electric resistance heating system.  
 
For any result   D03_ExceptionalConditionMessageCCMCHE += ElectricHeatExceptionInF</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                 </xsd:sequence>
                              </xsd:complexType>
                           </xsd:element>
                        </xsd:sequence>
                     </xsd:complexType>
                  </xsd:element>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
         <xsd:element name="Section_G" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation source="FieldText">[d:b]G. PUMPS[/d:b]</xsd:documentation>
               <xsd:documentation source="CalculationsAndRules">This section is required If B02b_ProjectHasPumps == true</xsd:documentation>
               <xsd:documentation source="MinOccurs">If this section doesn't apply, do not display anything for this section</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="G_BeginNote1" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">[d:i]This table is used to demonstrate compliance with Prescriptive hydronic system requirements found in [d:t24refnolink/]140.4(k)  [d:t24refnolink/]170.2(c)4I applicable to pumps [d:lt/] 5hp.[/d:i]</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="PumpsTable" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                     </xsd:annotation>
                     <xsd:complexType>
                        <xsd:sequence>
                           <xsd:element name="Row" minOccurs="1" maxOccurs="unbounded">
                              <xsd:complexType>
                                 <xsd:sequence>
                                    <xsd:element name="G01_PumpName" type="hvac:PumpName" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Name or Item Tag</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">User input</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="G02_PumpOperationType" type="hvac:PumpOperationType" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Equipment Type</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">User selects from list: 
PrimaryChilledWater display term Primary chilled water, 
SecondaryChilledWater display term Secondary chilled water, 
PrimaryHeatingHotWater display term Primary heating hot water, 
SecondaryHeatingHotWater display term Secondary heating hot water, 
TertiaryHeatingHotWater display term Tertiary heating hot water, 
CondenserWater display term Condenser water</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="G03_PumpCount" type="hvac:PumpCount" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Qty</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">User enters number of pumps</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="G04_PumpMotorHP" type="hvac:PumpMotorHP" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">HP</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">User enters pump horsepower</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="G05toG08ColumnHeading" minOccurs="0">
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Controls per [d:t24refnolink/]140.4(k) &amp; [d:t24refnolink/]170.2(c)4I</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="G05_PumpControlVF" type="hvac:PumpControlVF" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Variable Flow</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">If G02_PumpOperationType == CondenserWater result = BlankGrayedOut  stored in G05_NotApplicableMessage and field is grayed out; 
 
Else User selects from list 
HasVariableFlow display term Yes, 
LTE3ControlValves display term NA: [d:lte/] 3 control valves, 
LTE1_5HP display term NA: pump system  [d:lte/] 1.5 hp
End If</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="G05_NotApplicableMessage" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Variable Flow</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">If G02_PumpOperationType== CondenserWater result = BlankGrayedOut stored in G05_NotApplicableMessage and field is grayed out
End If</xsd:documentation>
                                          </xsd:annotation>
                                          <xsd:simpleType>
                                             <xsd:restriction base="comp:NotApplicableMessage">
                                                <xsd:enumeration value="BlankGrayedOut"/>
                                             </xsd:restriction>
                                          </xsd:simpleType>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="G06_HeatPumpIsolation" type="hvac:HeatPumpIsolation" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Hydronic Heat Pump Isolation</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">If G02_PumpOperationType != CondenserWater result = BlankGrayedOut stored in G06_NotApplicableMessage and field is grayed out; 
 
Else user selects from list: 
HasHeatPumpIsolation display term Yes, 
NotOnWaterLoop display term NA: Not connected to common water loop, 
NoHydronicHP display term NA: No hydronic heat pump
End If</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="G06_NotApplicableMessage" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Hydronic Heat Pump Isolation</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">If G02_PumpOperationType != CondenserWater result = BlankGrayedOut  stored in G06_NotApplicableMessage and field is grayed out
End If</xsd:documentation>
                                          </xsd:annotation>
                                          <xsd:simpleType>
                                             <xsd:restriction base="comp:NotApplicableMessage">
                                                <xsd:enumeration value="BlankGrayedOut"/>
                                             </xsd:restriction>
                                          </xsd:simpleType>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="G07_PumpControlVSD" type="hvac:PumpControlVSD" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">VSD on Pumps [d:gt/] 5HP</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">If G02_PumpOperationType == PrimaryHeatingHotWater, SecondaryHeatingHotWater Or TertiaryHeatingHotWater Or If G04_PumpMotorHP LTE 5  result = BlankGrayedOut stored in G07_NotApplicableMessage and field is grayed out; 
 
Else User selects from list: 
HasVariableSpeedDrive display term Yes, 
NotVariableFlowSystem display term NA: Not serving variable flow system, 
OnlyWaterCooledChillers display term NA: Condenser water serving only water cooled chillers
End If</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="G07_NotApplicableMessage" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">VSD on Pumps [d:gt/] 5HP</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">If G02_PumpOperationType == PrimaryHeatingHotWater, SecondaryHeatingHotWater Or TertiaryHeatingHotWater Or If G04_PumpMotorHP LTE 5  result = BlankGrayedOut stored in G07_NotApplicableMessage and field is grayed out;
End if</xsd:documentation>
                                          </xsd:annotation>
                                          <xsd:simpleType>
                                             <xsd:restriction base="comp:NotApplicableMessage">
                                                <xsd:enumeration value="BlankGrayedOut"/>
                                             </xsd:restriction>
                                          </xsd:simpleType>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:element name="G08_DifferentialPressureSensorRequired" type="hvac:DifferentialPressureSensorRequired" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Differential Pressure Sensor</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">Calculated value: 
If G04_PumpMotorHP LTE 5 Or If G07_PumpControlVSD != HasVariableSpeedDrive  result = false and field is grayed out ; 
 
Else result = true with display term Required;
End If</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                 </xsd:sequence>
                              </xsd:complexType>
                           </xsd:element>
                        </xsd:sequence>
                     </xsd:complexType>
                  </xsd:element>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
         <xsd:element name="Section_H" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation source="FieldText">[d:b]H. FAN SYSTEMS &amp; AIR ECONOMIZERS[/d:b]</xsd:documentation>
               <xsd:documentation source="CalculationsAndRules">This section is required If B03c_ProjectHasFanSystems == true Or If B03a_ProjectHasAirEconomizer== true</xsd:documentation>
               <xsd:documentation source="MinOccurs">If this section doesn't apply, do not display anything for this section</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="H_BeginNote1" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
