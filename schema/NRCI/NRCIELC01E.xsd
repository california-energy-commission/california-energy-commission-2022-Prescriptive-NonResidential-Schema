   If  I03_ControlledReceptacleShutOffControl IsPresent
   result = I03_ControlledReceptacleShutOffControl
   End If
   End If</xsd:documentation>
                                                               <xsd:appinfo source="CIsrc">
                                                                  <dtyp:srcSchema form="CCELC01E"  usage="copy">I03_ControlledReceptacleShutOffControl</dtyp:srcSchema>
