                                             <dtyp:srcSchema form="CCLTI01E"  usage="copy">Q13_LuminaireCountByType</dtyp:srcSchema>
                                             <dtyp:srcSchema form="CCPRF01E"  usage="copy">LtgSched05_LuminaireCountByType</dtyp:srcSchema>
                                          </xsd:appinfo>
