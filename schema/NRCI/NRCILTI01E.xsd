                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Is this in your scope?</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">User selects from true value display term Yes and false value display term No, by others
 If result == false Paf02cc-Paf06cc and Paf03-Paf06As-built fields are blank and grayed out
And D01_ExceptionalConditionMessageNRCILTIE += PafNotInScope  
End If</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="PafScopeChoice" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText"></xsd:documentation>
                                       </xsd:annotation>
                                       <xsd:complexType>
                                          <xsd:choice >
                                             <xsd:element name="PafScopeNA_NotApplicableMessage" >
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText"></xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">If Pafl01_InMyScope == false
result = BlankGrayedOut for Paf02cc-Paf06cc and Paf03-Paf06 As-built fields are blank and grayed out fields
End If</xsd:documentation>
                                                </xsd:annotation>
                                                <xsd:simpleType>
                                                   <xsd:restriction base="comp:NotApplicableMessage">
                                                      <xsd:enumeration value="BlankGrayedOut"/>
                                                   </xsd:restriction>
                                                </xsd:simpleType>
                                             </xsd:element>
                                             <xsd:element name="PafInScope" >
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText"></xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">This subsection is required if If Pafl01_InMyScope == true</xsd:documentation>
                                                </xsd:annotation>
                                                <xsd:complexType>
                                                   <xsd:sequence>
                                                      <xsd:element name="Paf01_InstalledSameAsCC" type="comp:InstalledSameAsCC"  minOccurs="0">
                                                         <xsd:annotation>
                                                            <xsd:documentation source="FieldText">NOT REPORTED IN PDF</xsd:documentation>
                                                            <xsd:documentation source="CalculationsAndRules">User selects true if installed is the same as CC value  Or false if installed is different than CC value</xsd:documentation>
                                                         </xsd:annotation>
                                                      </xsd:element>
                                                      <xsd:element name="Paf02cc_PAFCreditType" maxOccurs="2">
                                                         <xsd:annotation>
                                                            <xsd:documentation source="FieldText">Controls for Wattage Credit</xsd:documentation>
                                                            <xsd:documentation source="CalculationsAndRules">Only 2 PAF's are allowed for a space.  Start with Count == 0  and when Count == 2 stop assignments
 
If CCLTIE parent
Evaluate the Pc02 value If Paf01cc is loaded from Pc01_SpaceDescription. Evaluate the Pu02 value If Paf01cc is loaded from Pu01_SpaceDescription:
 
If Pc02a_ApplyDaylightDimmingPlusOff == true Or Pu02a_ApplyDaylightDimmingPlusOff == true
result += DaylightDimmingPlusOff
Count  += 1
End If
If Count LT 2 And Pc02b_ApplyOccupantSensingControlsUpTo125SF == true Or Pu02b_ApplyOccupantSensingControlsUpTo125SF == true
result += OccupantSensingControls_1To125SF
Count  += 1
End If
If Count LT 2 And Pc02c_ApplyOccupantSensingControlsFrom126To250SF == true Or Pu02c_ApplyOccupantSensingControlsFrom126To250SF == true
result += OccupantSensingControls_126To250SF
Count += 1
End If
If Count LT 2 And Pc02d_ApplyOccupantSensingControlsFrom251To500SF == true Or Pu02d_ApplyOccupantSensingControlsFrom251To500SF == true
result += OccupantSensingControls_251To500SF
Count += 1
End If
If Count LT 2 And Pc02e_ApplyInstitutionalTuningNonDaylit == true Or Pu02e_ApplyInstitutionalTuningNonDaylit == true
result += InstitutionalTuningNonDaylit
Count += 1
End If
If Count LT 2 And Pc02f_ApplyInstitutionalTuningDaylit == true Or Pu02f_ApplyInstitutionalTuningDaylit == true
result += InstitutionalTuning
Count += 1
End If
If Count LT 2 And Pc02g_ApplyDemandResponsiveControl == true Or Pu02g_ApplyDemandResponsiveControl == true
result += DemandResponsiveControl
Count += 1
End If
If Count LT 2 And Pc02h_ApplyClerestoryFenestrationPAF == true Or Pu02h_ApplyClerestoryFenestrationPAF == true
result += Clerestories
Count += 1
End If
If Count LT 2 And Pc02i_ApplyHorizontalSlatsPAF == true Or Pu02i_ApplyHorizontalSlatsPAF == true
result += HorizontalSlats
Count += 1
End If
If Count LT 2 And Pc02i_ApplyLightShelvesPAF == true Or Pu02i_ApplyLightShelvesPAF == true
result += LightShelves
Else result = SeePlans
 
Else If CCPRFE parent
                                                                If LtgCredits03_PAFCreditType[0] IsPresent
// this property was not restricted to 2 values, so need to copy upto two values and then stop
result = LtgCredits03_PAFCreditType[0] //first value using zero based index
If LtgCredits03_PAFCreditType[1] not null
result += LtgCredits03_PAFCreditType[1] //second value
Else result = SeePlans
End If</xsd:documentation>
