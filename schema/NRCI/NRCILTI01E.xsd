                                    <xsd:element name="Paf07_ComplianceResultCI" type="comp:ComplianceResultCI" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Controls for Credit Compliance</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">If any user input field is blank
 result = TableIncomplete  
Else If Paf01_InMyScope == false
result = NotInScope 
Else If for all rows
If Paf02cc_PAFCreditType == SeePlans
Or
Paf04cc_LuminairePower == SeePlans
Or
Paf05cc_LuminaireCountByType == SeePlans
Or
Paf06cc_LuminairePowerByType == SeePlans
result = MayNotComply
Else If Paf04cc_LuminairePower == Paf04_LuminairePower
And 
Paf05cc_LuminaireCountByType == Paf05_LuminaireCountByType
result = ExactMatch
 
Else If for all rows 
Paf06_LuminairePowerByType GTE Paf06cc_LuminairePowerByType
result = EqualBetter
Else If any row has 
Paf06_LuminairePowerByType LT Paf06cc_LuminairePowerByType
