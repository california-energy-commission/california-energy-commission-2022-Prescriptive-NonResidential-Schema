                                          <xsd:documentation source="FieldText">Room name or Description</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">User input</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="Controls02cc_NotApplicableMessage" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Location/ Type of Controlled Receptacles[d:sup]1[/d:sup]</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">No parent Certificate of Compliance data</xsd:documentation>
                                       </xsd:annotation>
                                       <xsd:simpleType>
                                          <xsd:restriction base="comp:NotApplicableMessage">
                                             <xsd:enumeration value="BlankGrayedOut"/>
                                             <xsd:enumeration value="NoDataGrayedOut"/>
                                          </xsd:restriction>
                                       </xsd:simpleType>
                                    </xsd:element>
                                    <xsd:element name="Controls02_ControlledReceptacleComplianceType" type="comp:ControlledReceptacleComplianceType"  maxOccurs="unbounded">
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Location/ Type of Controlled Receptacles[d:sup]1[/d:sup]</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">User selects all that apply in this space from dropdown list which includes ControlledReceptacleComplianceType</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="Controls02_ControlledReceptacleNA" type="comp:ControlledReceptacleNA"  minOccurs="0" maxOccurs="unbounded">
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Location/ Type of Controlled Receptacles[d:sup]1[/d:sup]</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">User selects all that apply in this space from dropdown list ControlledReceptacleNA</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="Controls03cc_NotApplicableMessage" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Shut-Off Controls</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">No parent Certificate of Compliance data</xsd:documentation>
                                       </xsd:annotation>
                                       <xsd:simpleType>
                                          <xsd:restriction base="comp:NotApplicableMessage">
                                             <xsd:enumeration value="BlankGrayedOut"/>
                                             <xsd:enumeration value="NoDataGrayedOut"/>
                                          </xsd:restriction>
                                       </xsd:simpleType>
                                    </xsd:element>
                                    <xsd:element name="Controls03_ControlledReceptacleShutOffControl" type="comp:ControlledReceptacleShutOffControl"  maxOccurs="unbounded">
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Shut-Off Controls</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">User selects all that apply in this space from ControlledReceptacleShutOffControl</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="Controls03_ControlledReceptacleNA" type="comp:ControlledReceptacleNA"  minOccurs="0">
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Shut-Off Controls</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">User selects all that apply in this space from ControlledReceptacleNA</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="Controls04cc_NotApplicableMessage" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Permanent Durable Marking Is Used</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">No parent Certificate of Compliance data</xsd:documentation>
                                       </xsd:annotation>
                                       <xsd:simpleType>
                                          <xsd:restriction base="comp:NotApplicableMessage">
                                             <xsd:enumeration value="BlankGrayedOut"/>
                                             <xsd:enumeration value="NoDataGrayedOut"/>
                                          </xsd:restriction>
                                       </xsd:simpleType>
                                    </xsd:element>
                                    <xsd:element name="Controls04_AreControlledReceptaclesPermanentlyMarked" type="comp:AreControlledReceptaclesPermanentlyMarked" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Permanent Durable Marking Is Used</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">User selects true or false for AreControlledReceptaclesPermanentlyMarked 
If result = true value displayed is Controlled and Uncontrolled Receptacles Identified with Permanent Marking, 
If result = false
user must select at least one value from ControlledReceptacleNA</xsd:documentation>
                                          <xsd:appinfo source="displayterm">
                                             <dtyp:displayterm value="true">Controlled and Uncontrolled Receptacles Identified with Permanent Marking</dtyp:displayterm>
                                          </xsd:appinfo>
                                          
                                       </xsd:annotation>
                                    </xsd:element>
