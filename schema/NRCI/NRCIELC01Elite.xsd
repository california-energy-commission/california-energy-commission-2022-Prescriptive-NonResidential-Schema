                        <xsd:documentation source="FieldText">[d:b]Separation of Electrical Circuits for Energy Monitoring[/d:b]</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">This subsection is required and displayed If B01b_ProjectHasLoadSeparation == true</xsd:documentation>
                     </xsd:annotation>
                     <xsd:complexType>
                        <xsd:sequence>
                           <xsd:element name="Circuits00cc_NotApplicableMessage" >
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText">Electrical Service Designation/Description</xsd:documentation>
                                 <xsd:documentation source="CalculationsAndRules">No parent Certificate of Compliance data</xsd:documentation>
                              </xsd:annotation>
                              <xsd:simpleType>
                                 <xsd:restriction base="comp:NotApplicableMessage">
                                    <xsd:enumeration value="BlankGrayedOut"/>
                                    <xsd:enumeration value="NoDataGrayedOut"/>
                                 </xsd:restriction>
                              </xsd:simpleType>
                           </xsd:element>
                           <xsd:element name="Circuits00_ElectricalServiceName" type="comp:ElectricalServiceName" >
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText">Electrical Service Designation/Description</xsd:documentation>
                                 <xsd:documentation source="CalculationsAndRules">User input</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                           <xsd:element name="TableCircuits" >
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText"></xsd:documentation>
                              </xsd:annotation>
