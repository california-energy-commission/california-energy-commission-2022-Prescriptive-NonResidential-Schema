                                                   <xsd:restriction base="comp:NotApplicableMessage">
                                                      <xsd:enumeration value="BlankGrayedOut"/>
                                                      <xsd:enumeration value="NoDataGrayedOut"/>
                                                   </xsd:restriction>
                                                </xsd:simpleType>
                                             </xsd:element>
                                             <xsd:element name="Circuits02_ElectricalLoadSeparationRequirement" type="comp:ElectricalLoadSeparationRequirement" >
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">Minimum Required Separation of Load</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">User input</xsd:documentation>
                                                </xsd:annotation>
                                             </xsd:element>
                                             <xsd:element name="Circuits03cc_NotApplicableMessage" >
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">Separation Method[d:sup]2[/d:sup]</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">No parent Certificate of Compliance data</xsd:documentation>
                                                </xsd:annotation>
                                                <xsd:simpleType>
                                                   <xsd:restriction base="comp:NotApplicableMessage">
                                                      <xsd:enumeration value="BlankGrayedOut"/>
                                                      <xsd:enumeration value="NoDataGrayedOut"/>
                                                   </xsd:restriction>
                                                </xsd:simpleType>
                                             </xsd:element>
