                                                <xsd:element name="Circuits03_ElectricalLoadComplianceMethod" type="comp:ElectricalLoadComplianceMethod" >
                                                   <xsd:annotation>
                                                      <xsd:documentation source="FieldText">Separation Method[d:sup]2[/d:sup]</xsd:documentation>
                                                      <xsd:documentation source="CalculationsAndRules">User selects from dropdown list</xsd:documentation>
                                                   </xsd:annotation>
                                                </xsd:element>
                                                <xsd:element name="Circuits03_NotApplicableExplanation" type="comp:NotApplicableExplanation" >
                                                   <xsd:annotation>
                                                      <xsd:documentation source="FieldText">NO DATA Separation Method[d:sup]2[/d:sup]</xsd:documentation>
                                                      <xsd:documentation source="CalculationsAndRules">If not required for load type user inputs short explanation why no data</xsd:documentation>
                                                   </xsd:annotation>
                                                </xsd:element>
                                             </xsd:choice>
                                             <xsd:element name="Circuits04_ComplianceResultNRCI" type="comp:ComplianceResultNRCI"  minOccurs="0">
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText">Compliance</xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">If any user input field is blank
result = TableIncomplete  
Else  
result =  MaynotComply</xsd:documentation>
                                                </xsd:annotation>
                                             </xsd:element>
                                          </xsd:sequence>
                                       </xsd:complexType>
                                    </xsd:element>
                                 </xsd:sequence>
                              </xsd:complexType>
