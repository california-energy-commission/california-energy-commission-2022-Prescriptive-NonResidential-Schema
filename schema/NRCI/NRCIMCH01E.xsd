                                                         </xsd:element>
                                                      </xsd:choice>
                                                      <xsd:element name="Dfhp12cc_EfficiencyType" type="hvac:EfficiencyType" >
                                                         <xsd:annotation>
                                                            <xsd:documentation source="FieldText">Efficiency Unit</xsd:documentation>
                                                            <xsd:documentation source="CalculationsAndRules">If CC-MCH parent
result =result 2 Fdfhp09a_EfficiencyType in CoolingEER_IEER_Dfhp element
Else If CC-PRF parent
result = result 2 Dfhp06a_EfficiencyType in CoolingEER_IEER_Dfhp element
 
End If</xsd:documentation>
                                                            <xsd:appinfo source="CIsrc">
                                                               <dtyp:srcSchema form="CCMCH01E"  usage="copy">Fdfhp09a_EfficiencyType</dtyp:srcSchema>
                                                            </xsd:appinfo>
                                                         </xsd:annotation>
                                                      </xsd:element>
                                                      <xsd:element name="Dfph13_ComplianceResultCI" type="comp:ComplianceResultCI" >
                                                         <xsd:annotation>
                                                            <xsd:documentation source="FieldText">Equipment Compliance</xsd:documentation>
                                                            <xsd:documentation source="CalculationsAndRules">If any user input field is blank
result = TableIncomplete  
 Else If Dfhp01_InMyScope == false 
result = NotInScope
 
Else If 
(
Dfhp03_HvacSystemType IsPresent And Dfhp03_HvacSystemType != Dfhp03cc_HvacSystemType
Or 
Dfhp03_FurnaceUnitHeater  IsPresent And Dfhp03_FurnaceUnitHeater  != Dfhp03cc_FurnaceUnitHeater 
Or
Dfhp04_SizeRangeFurnaceUnitHeater != Dfhp4cc_SizeRangeFurnaceUnitHeater 
Or 
(
Dfhp05_EfficiencyAFUE IsPresent And Dfhp05_EfficiencyAFUE LT  Dfhp05cc_EfficiencyAFUE 
Or 
Dfhp05_CombustionEfficiency  IsPresent And Dfhp05_CombustionEfficiency LT  Dfhp05cc_CombustionEfficiency 
Or 
Dfhp05_ThermalEfficiency IsPresent And Dfhp05_ThermalEfficiency LT Dfhp05cc_ThermalEfficiency
)
Or
(
Dfhp07_HvacSystemType IsPresent And Dfhp07_HvacSystemType != Dfhp07cc_HvacSystemType
Or
Dfhp07_UnitaryHeatPumps IsPresent And Dfhp07_UnitaryHeatPumps != Dfhp07cc_UnitaryHeatPumps
Or
Dfhp08_SizeRangeFurnaceUnitHeatPumps != Dfhp4cc_SizeRangeFurnaceHeatPumps 
Or 
(
Dfhp09_EfficiencyCOP IsPresent And Dfhp09_EfficiencyCOP LTDfhp09cc_EfficiencyCOP 
Or
Dfhp09_EfficiencyEER IsPresent And Dfhp09_EfficiencyEER LT Dfhp09cc_EfficiencyEER 
Or
Dfhp09_EfficiencyEER_IEER IsPresent And Dfhp09_EfficiencyEER_IEER LT Dfhp09cc_EfficiencyEER_IEER
Or
Dfhp11_EfficiencyEER_IEER IsPresent And Dfhp11_EfficiencyEER_IEER LT Dfhp11cc_EfficiencyEER_IEER
)
result = MayNotComply
 
Else If 
Dfhp03_HvacSystemType IsPresent And Dfhp03_HvacSystemType == Dfhp03cc_HvacSystemType
Or
Dfhp03_FurnaceUnitHeater  IsPresent And Dfhp03_FurnaceUnitHeater  == Dfhp03cc_FurnaceUnitHeater 
And
Dfhp04_SizeRangeFurnaceUnitHeater == Dfhp4cc_SizeRangeFurnaceUnitHeater 
And
(
Dfhp05_EfficiencyAFUE IsPresent And Dfhp05_EfficiencyAFUE ==  Dfhp05cc_EfficiencyAFUE 
Or 
Dfhp05_CombustionEfficiency  IsPresent And Dfhp05_CombustionEfficiency ==  Dfhp05cc_CombustionEfficiency 
Or 
Dfhp05_ThermalEfficiency IsPresent And Dfhp05_ThermalEfficiency == Dfhp05cc_ThermalEfficiency
)
And 
Dfhp07_HvacSystemType IsPresent And Dfhp07_HvacSystemType == Dfhp07cc_HvacSystemType
Or
Dfhp07_UnitaryHeatPumps IsPresent And Dfhp07_UnitaryHeatPumps == Dfhp07cc_UnitaryHeatPumps
 
And
Dfhp08_SizeRangeFurnaceUnitHeatPumps == Dfhp4cc_SizeRangeFurnaceHeatPumps 
And
(
Dfhp09_EfficiencyCOP IsPresent And Dfhp09_EfficiencyCOP == Dfhp09cc_EfficiencyCOP 
Or
Dfhp09_EfficiencyEER IsPresent And Dfhp09_EfficiencyEER == Dfhp09cc_EfficiencyEER 
Or
Dfhp09_EfficiencyEER_IEER IsPresent And Dfhp09_EfficiencyEER_IEER == Dfhp09cc_EfficiencyEER_IEER
)
And
Dfhp11_EfficiencyEER_IEER IsPresent And Dfhp11_EfficiencyEER_IEER == Dfhp11cc_EfficiencyEER_IEER
result = ExactMatch
 
Else If 
(
Dfhp05_EfficiencyAFUE IsPresent And Dfhp05_EfficiencyAFUE GTE  Dfhp05cc_EfficiencyAFUE 
Or 
Dfhp05_CombustionEfficiency  IsPresent And Dfhp05_CombustionEfficiency GTE  Dfhp05cc_CombustionEfficiency 
Or 
Dfhp05_ThermalEfficiency IsPresent And Dfhp05_ThermalEfficiency GTE Dfhp05cc_ThermalEfficiency
)
Or 
(
Dfhp09_EfficiencyCOP IsPresent And Dfhp09_EfficiencyCOP GTE Dfhp09cc_EfficiencyCOP 
Or
Dfhp09_EfficiencyEER IsPresent And Dfhp09_EfficiencyEER GTE Dfhp09cc_EfficiencyEER 
Or
Dfhp09_EfficiencyEER_IEER IsPresent And Dfhp09_EfficiencyEER_IEER GTE Dfhp09cc_EfficiencyEER_IEER
)
Or
Dfhp11_EfficiencyEER_IEER IsPresent And Dfhp11_EfficiencyEER_IEER GTE Dfhp11cc_EfficiencyEER_IEER
)
result = EqualBetter
End If
End If</xsd:documentation>
                                                         </xsd:annotation>
                                                      </xsd:element>
                                                   </xsd:sequence>
                                                </xsd:complexType>
                                             </xsd:element>
                                          </xsd:choice>
                                       </xsd:complexType>
                                    </xsd:element>
                                 </xsd:sequence>
                              </xsd:complexType>
                           </xsd:element>
                        </xsd:sequence>
