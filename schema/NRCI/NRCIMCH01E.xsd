                                                            <xsd:annotation>
                                                               <xsd:documentation source="FieldText">Rating Condition</xsd:documentation>
                                                               <xsd:documentation source="CalculationsAndRules">If CC-MCH parent
If Fdoas05_NotApplicableMessage isPresent
result = Fdoas05_NotApplicableMessageEnd If</xsd:documentation>
                                                               <xsd:appinfo source="CIsrc">
                                                                  <dtyp:srcSchema form="CCMCH01E"  usage="copy">Fdoas05_NotApplicableMessage</dtyp:srcSchema>
                                                               </xsd:appinfo>
                                                            </xsd:annotation>
                                                            <xsd:simpleType>
