                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Equipment Compliance</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">If any user input field is blank
result = TableIncomplete  
 Else If Doas01_InMyScope == false 
result = NotInScope
 
Else If 
(
Doas03_EquipmentDX  != Doas03cc_EquipmentDX
Or 
Doas04_EnergyRecovery   != Doas04cc_EnergyRecovery
Or
If Doas05_RatingConditionDOAS isPresent And Doas05cc_RatingConditionDOAS !=  Doas05cc_RatingConditionDOAS 
Or
Doas06_EfficiencyISMRE LT Doas06cc_EfficiencyISMRE
Or 
Doas08_EfficiencyISCOP IsPresent And Doas08_EfficiencyISCOP  LT Doas08cc_EfficiencyISCOP
)
result = MayNotComply
 
Else If 
(
Doas03_EquipmentDX  == Doas03cc_EquipmentDX
Or 
Doas04_EnergyRecovery   == Doas04cc_EnergyRecovery
Or
If Doas05_NotApplicableMessage IsPresent Or Doas05cc_RatingConditionDOAS ==  Doas05cc_RatingConditionDOAS 
Or
Doas06_EfficiencyISMRE == Doas06cc_EfficiencyISMRE
Or 
Doas08_NotApplicableMessage IsPresent Or Doas08_EfficiencyISCOP  == Doas08cc_EfficiencyISCOP
 
result = ExactMatch
 Else If
Doas06_EfficiencyISMREGTE Doas06cc_EfficiencyISMRE
Or 
Doas08_EfficiencyISCOP IsPresent And Doas08_EfficiencyISCOP GTE Doas08cc_EfficiencyISCOP
 
(
result = EqualBetter
End If
End If</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                 </xsd:sequence>
                              </xsd:complexType>
                           </xsd:element>
                        </xsd:sequence>
