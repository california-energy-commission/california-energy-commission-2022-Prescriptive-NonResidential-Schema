                                                               <xsd:documentation source="FieldText">Efficiency</xsd:documentation>
                                                               <xsd:documentation source="CalculationsAndRules">If CC-MCH parent
If Fdoas06b_NotApplicableMessage isPresent
result = Fdoas06b_NotApplicableMessageEnd If</xsd:documentation>
                                                               <xsd:appinfo source="CIsrc">
                                                                  <dtyp:srcSchema form="CCMCH01E"  usage="copy">Fdoas06b_NotApplicableMessage</dtyp:srcSchema>
                                                               </xsd:appinfo>
                                                            </xsd:annotation>
                                                            <xsd:simpleType>
                                                               <xsd:restriction base="comp:NotApplicableMessage">
                                                                  <xsd:enumeration value="BlankGrayedOut"/>
                                                               </xsd:restriction>
                                                            </xsd:simpleType>
                                                         </xsd:element>
                                                      </xsd:choice>
                                                      <xsd:choice maxOccurs="1">
                                                         <xsd:element name="Doas08_EfficiencyISCOP" type="hvac:EfficiencyISCOP" >
                                                            <xsd:annotation>
                                                               <xsd:documentation source="FieldText">Efficiency</xsd:documentation>
                                                               <xsd:documentation source="CalculationsAndRules">If Doas08cc_EfficiencyISCOP IsPresent
If Doas01_InstalledSameAsCC == true
result = Doas08cc_EfficiencyISCOP
If Doas01_InstalledSameAsCC == false
User input
End If
End If</xsd:documentation>
                                                            </xsd:annotation>
                                                         </xsd:element>
                                                         <xsd:element name="Doas08_NotApplicableMessage" >
                                                            <xsd:annotation>
                                                               <xsd:documentation source="FieldText">Efficiency</xsd:documentation>
                                                               <xsd:documentation source="CalculationsAndRules">If Doas08cc_NotApplicableMessage isPresent
If Doas01_InstalledSameAsCC == true
result = Doas08cc_NotApplicableMessage 
End If</xsd:documentation>
                                                            </xsd:annotation>
                                                            <xsd:simpleType>
                                                               <xsd:restriction base="comp:NotApplicableMessage">
                                                                  <xsd:enumeration value="BlankGrayedOut"/>
                                                               </xsd:restriction>
                                                            </xsd:simpleType>
                                                         </xsd:element>
