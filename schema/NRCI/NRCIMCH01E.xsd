                                                               <xsd:documentation source="FieldText">Differential Pressure Sensor</xsd:documentation>
                                                               <xsd:documentation source="CalculationsAndRules">If CC-MCH parent  
result = G08_DifferentialPressureSensorRequired
End If</xsd:documentation>
                                                               <xsd:appinfo source="displayterm">
                                                                  <dtyp:displayterm value="true">Provided</dtyp:displayterm>
                                                                  <dtyp:displayterm value="false">Not required</dtyp:displayterm>
                                                               </xsd:appinfo>
                                                               
                                                               <xsd:appinfo source="CIsrc">
                                                                  <dtyp:srcSchema form="CCMCH01E"  usage="copy">G08_DifferentialPressureSensorRequired</dtyp:srcSchema>
                                                               </xsd:appinfo>
                                                            </xsd:annotation>
                                                         </xsd:element>
