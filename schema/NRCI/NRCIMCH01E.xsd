                                             </xsd:element>
                                          </xsd:choice>
                                       </xsd:complexType>
                                    </xsd:element>
                                    <xsd:element name="Hrej12_ComplianceResultCI" type="comp:ComplianceResultCI" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Equipment Compliance</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">If any user input field is blank
result = TableIncomplete 
Else If Hrej01_InMyScope == false 
result = NotInScope
Else If 
Hrej03_HeatRejectionType != Hrej03cc_HeatRejectionType
Or
Hrej05_CoolingTowerCapacityRated IsPresent And Hrej05_CoolingTowerCapacityRated  LT Hrej05cc_CoolingTowerCapacityRated 
Or 
Hrej05_HeatRejectionCondenserCapacityRated IsPresent And Hrej05_HeatRejectionCondenserCapacityRated LT Hrej05cc_HeatRejectionCondenserCapacityRated  
result = MayNotComply
 
Else If 
Hrej03_HeatRejectionType ==  Hrej3cc_HeatRejectionType
And
Hrej04_NotApplicableMessage IsPresent Or Hrej04_HeatRejectionEquipmentCount ==  Hrej04cc_HeatRejectionEquipmentCount    
And 
Hrej07_NotApplicableMessage IsPresent Or Hrej07_HeatRejectionFanSpeedControl ==  Hrej07cc_HeatRejectionFanSpeedControl
And 
Hrej08_NotApplicableMessage IsPresent Or Hrej08_TowerFlowTurnDown ==  Hrej08cc_TowerFlowTurnDown 
And  
Hrej09_NotApplicableMessage IsPresent Or Hrej09_FanControlMultipleCells == Hrej09cc_FanControlMultipleCells
And
Hrej10_NotApplicableMessage IsPresent Or Hrej10_HasWaterEconomizerControls == Hrej10cc_HasWaterEconomizerControls
And
Hrej11_NotApplicableMessage IsPresent Or Hrej11_HasTempResetControls == Hrej11cc_HasTempResetControls
 
Then 
If
( 
Hrej05_NotApplicableMessage IsPresent 
Or 
Hrej05_CoolingTowerCapacityRated IsPresent  And Hrej05_CoolingTowerCapacityRated == Hrej05cc_CoolingTowerCapacityRated 
Or
Hrej05_HeatRejectionCondenserCapacityRated IsPresent And Hrej05_HeatRejectionCondenserCapacityRated == Hrej05cc_HeatRejectionCondenserCapacityRated
) 
result = ExactMatch
 
Else  If 
Hrej05_CoolingTowerCapacityRated IsPresent  And Hrej05_CoolingTowerCapacityRated GT Hrej05cc_CoolingTowerCapacityRated 
Or 
Hrej05_HeatRejectionCondenserCapacityRated IsPresent And Hrej05_HeatRejectionCondenserCapacityRated GT  Hrej05cc_HeatRejectionCondenserCapacityRated
  
result = EqualBetter
End If
End If
 
 
If Hrej07_HeatRejectionFanSpeedControl IsPresent And Hrej07_HeatRejectionFanSpeedControl !=  Hrej07cc_HeatRejectionFanSpeedControl
Or
Hrej08_TowerFlowTurnDown IsPresent And Hrej08_TowerFlowTurnDown !=  Hrej08cc_TowerFlowTurnDown 
Or
Hrej09_FanControlMultipleCells IsPresent And Hrej09_FanControlMultipleCells != Hrej09cc_FanControlMultipleCells
Or
Hrej10_HasWaterEconomizerControls IsPresent And Hrej10_HasWaterEconomizerControls != Hrej10cc_HasWaterEconomizerControls
Or
Hrej11_HasTempResetControls IsPresent And Hrej11_HasTempResetControls != Hrej11cc_HasTempResetControls
 
 D03_ExceptionalConditoinMessageCIMCHE += HeatRejNoMatch
End If</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                 </xsd:sequence>
                              </xsd:complexType>
                           </xsd:element>
                        </xsd:sequence>
                     </xsd:complexType>
