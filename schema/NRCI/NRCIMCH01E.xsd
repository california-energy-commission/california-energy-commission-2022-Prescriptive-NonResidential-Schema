Or 
Dry09_EfficiencySEER  IsPresent And Dry09_EfficiencySEER ==  Dry09cc_EfficiencySEER
)
 
result = ExactMatch
 
Else If 
(
Dry06_EfficiencyAFUE IsPresent And Dry06_EfficiencyAFUE GTE Dry06cc_EfficiencyAFUE 
Or
Dry06_EfficiencyCOP IsPresent And  Dry06_EfficiencyCOP GTE Dry06cc_EfficiencyCOP 
