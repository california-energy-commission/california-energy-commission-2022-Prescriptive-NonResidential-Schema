                                                         <xsd:element name="Pump08_DifferentialPressureSensorRequired" type="hvac:DifferentialPressureSensorRequired" >
                                                            <xsd:annotation>
                                                               <xsd:documentation source="FieldText">Differential Pressure Sensor</xsd:documentation>
                                                               <xsd:documentation source="CalculationsAndRules">If Pump08cc_DifferentialPressureSensorRequired IsPresent 
If Pump01_InstalledSameAsCC == true
result = Pump08cc_DifferentialPressureSensorRequired but user can change value
Else If Pump01_InstalledSameAsCC == false
User input
End If
End If</xsd:documentation>
                                                               <xsd:appinfo source="displayterm">
                                                                  <dtyp:displayterm value="true">Provided</dtyp:displayterm>
                                                                  <dtyp:displayterm value="false">Not required</dtyp:displayterm>
                                                               </xsd:appinfo>
                                                               
                                                            </xsd:annotation>
                                                         </xsd:element>
