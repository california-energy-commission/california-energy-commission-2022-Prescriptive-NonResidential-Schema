                                                               <xsd:appinfo source="CIsrc">
                                                                  <dtyp:srcSchema form="CCMCH01E"  usage="copy">Fdoas05_RatingConditionDOAS</dtyp:srcSchema>
                                                               </xsd:appinfo>
                                                            </xsd:annotation>
                                                         </xsd:element>
