                                    <xsd:element name="Chlr12_ComplianceResultCI" type="comp:ComplianceResultCI" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Equipment Compliance</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">If any user input field is blank
result = TableIncomplete display term INCOMPLETE
Else If Chlr01_InMyScope == false 
result = NotInScope 
Else If 
Chlr03_ChillerType != Chlr03cc_ChillerType 
Or
(
Chlr05_ChillerTonsRange IsPresent And  Chlr05_ChillerTonsRange != Chlr05cc_ChillerTonsRange 
Or
Chlr05_ChillerCapacity IsPresent And Chlr05_ChillerCapacity != Chlr05cc_ChillerCapacity 
)
 
Or
(
Chlr06_EfficiencyCOP IsPresent And Chlr06_EfficiencyCOP LT Chlr06cc_EfficiencyCOP  
Or
Chlr06_EfficiencyEER  IsPresent And  Chlr06_EfficiencyEER LT Chlr06cc_EfficiencyEER  
Or 
Chlr06_EfficiencyKWperTon  IsPresent And  Chlr06_EfficiencyKWperTon GT Chlr06cc_EfficiencyKWperTon
)
 
Or
(
Chlr08_EfficiencyCOP_IPLV IsPresent  And Chlr08_EfficiencyCOP_IPLV LT Chlr08cc_EfficiencyCOP_IPLV 
Or 
Chlr08_EfficiencyEER_IPLV IsPresent  And Chlr08_EfficiencyEER_IPLV LT Chlr08cc_EfficiencyEER_IPLV 
Or
Chlr08_EfficiencyKWperTon_IPLV IsPresent And Chlr08_EfficiencyKWperTon_IPLV GT Chlr08cc_EfficiencyKWperTon_IPLV  
)
result = MayNotComply
 
Else If 
Chlr03_ChillerType == Chlr03cc_ChillerType 
And
Chlr04_NotApplicableMessage IsPresent Or Chlr04_Count ==  Chlr04cc_Count
And
Chlr05_ChillerTonsRange IsPresent And  Chlr05_ChillerTonsRange IsPresent == Chlr05cc_ChillerTonsRange 
Or Chlr05_ChillerCapacity IsPresent And Chlr05_ChillerCapacity == Chlr05cc_ChillerCapacity
And 
Chlr10_NotApplicableMessage IsPresent Or Chlr10_ChillerIsolationValve IsPresent ==  Chlr10cc_ChillerIsolationValve 
And 
Chlr11_NotApplicableMessage IsPresent Or Chlr11_TempResetControl IsPresent And ==  Chlr11cc_TempResetControl
 
Then If
(
Chlr06_EfficiencyCOP IsPresent And Chlr06_EfficiencyCOP == Chlr06cc_EfficiencyCOP  
Or 
Chlr06_EfficiencyEER  IsPresent And  Chlr06_EfficiencyEER == Chlr06cc_EfficiencyEER  
Or 
Chlr06_EfficiencyKWperTon  IsPresent And Chlr06_EfficiencyKWperTon == Chlr06cc_EfficiencyKWperTon
)
 
And
(
Chlr08_NotApplicableMessage IsPresent 
Or 
Chlr08_EfficiencyCOP_IPLV IsPresent  And Chlr08_EfficiencyCOP_IPLV == Chlr08cc_EfficiencyCOP_IPLV 
Or 
Chlr08_EfficiencyEER_IPLV IsPresent  And Chlr08_EfficiencyEER_IPLV == Chlr08cc_EfficiencyEER_IPLV 
Or 
Chlr08_EfficiencyKWperTon_IPLV IsPresent And Chlr08_EfficiencyKWperTon_IPLV == Chlr08cc_EfficiencyKWperTon_IPLV
) 
 
result = ExactMatch
 
Else  If 
(
Chlr06_EfficiencyCOP IsPresent And Chlr06_EfficiencyCOP GTE Chlr06cc_EfficiencyCOP  
Or 
Chlr06_EfficiencyEER  IsPresent And  Chlr06_EfficiencyEER GTE Chlr06cc_EfficiencyEER  
Or
Chlr06_EfficiencyKWperTon  IsPresent And Chlr06_EfficiencyKWperTon LTE Chlr06cc_EfficiencyKWperTon 
)
 
Or 
(
Chlr08_EfficiencyCOP_IPLV IsPresent And Chlr08_EfficiencyCOP_IPLV GTE Chlr08cc_EfficiencyCOP_IPLV 
Or 
Chlr08_EfficiencyEER_IPLV IsPresent And Chlr08_EfficiencyEER_IPLV GTE Chlr08cc_EfficiencyEER_IPLV 
Or 
Chlr08_EfficiencyKWperTon_IPLV IsPresent And Chlr08_EfficiencyKWperTon_IPLV LTE Chlr08cc_EfficiencyKWperTon_IPLV 
)
  
result = EqualBetter
End If 
End If
 
If Chlr10_ChillerIsolationValve IsPresent And Chlr10_ChillerIsolationValve !=  Chlr10cc_ChillerIsolationValve
Or 
Chlr11_TempResetControl IsPresent And Chlr11_TempResetControl !=  Chlr11cc_TempResetControl
D03_ExceptionalConditoinMessageCIMCHE += ChillerNoMatch
End If</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
