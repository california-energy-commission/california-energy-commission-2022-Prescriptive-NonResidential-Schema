<?xml version="1.0" encoding="UTF-8"?>
<xsd:schema
   targetNamespace="http://www.lmonte.com/besm/NRCIMCH01E"
   xmlns="http://www.lmonte.com/besm/NRCIMCH01E"
   xmlns:d="http://www.lmonte.com/besm/d"
   xmlns:bld="http://www.lmonte.com/besm/bld"
   xmlns:env="http://www.lmonte.com/besm/env"
   xmlns:lit="http://www.lmonte.com/besm/lit"
   xmlns:hvac="http://www.lmonte.com/besm/hvac"
   xmlns:comp="http://www.lmonte.com/besm/comp"
   xmlns:com="http://www.lmonte.com/besm/com"
   xmlns:dtyp="http://www.lmonte.com/besm/dtyp"
   xmlns:xsd="http://www.w3.org/2001/XMLSchema"
   xmlns:altova="http://www.altova.com/xml-schema-extensions"
   elementFormDefault="qualified" attributeFormDefault="unqualified" version="2022.0.000">
   <xsd:import namespace="http://www.lmonte.com/besm/com" schemaLocation="../base/NrPresCommon.xsd"/>
   <xsd:import namespace="http://www.lmonte.com/besm/bld" schemaLocation="../base/NrPresBuilding.xsd"/>
   <xsd:import namespace="http://www.lmonte.com/besm/env" schemaLocation="../base/NrPresEnvelope.xsd"/>
   <xsd:import namespace="http://www.lmonte.com/besm/lit" schemaLocation="../base/NrPresLighting.xsd"/>
   <xsd:import namespace="http://www.lmonte.com/besm/hvac" schemaLocation="../base/NrPresHvac.xsd"/>
   <xsd:import namespace="http://www.lmonte.com/besm/comp" schemaLocation="../base/NrPresCompliance.xsd"/>
   <xsd:import namespace="http://www.lmonte.com/besm/dtyp" schemaLocation="../base/DataTypes.xsd"/>
   <xsd:element name="ComplianceDocumentPackage">
      <xsd:complexType>
         <xsd:sequence>
            <xsd:element name="DocID" minOccurs="0">
               <xsd:complexType>
                  <xsd:attribute name="doc" type="comp:ComplianceDocumentTagNR" fixed="NRCIMCH01E"/>
                  <xsd:attribute name="docType" type="comp:ComplianceDocumentTypeNR" fixed="CertificateInstallationNR"/>
                  <xsd:attribute name="docTitle" type="comp:ComplianceDocumentTitleNR" fixed="Mechanical Systems"/>
               </xsd:complexType>
            </xsd:element>
            <xsd:element name="Payload" type="comp:Payload"/>
            <xsd:element name="DocumentData">
               <xsd:complexType>
                  <xsd:sequence>
                     <xsd:element name="Header" type="comp:HeaderNRCI" />
                     <xsd:element ref="nRCIMCH01E" />
                     <xsd:element name="DocAuthor" type="comp:DocumentAuthor" maxOccurs="unbounded" />
                     <xsd:element name="RespPerson" type="comp:ResponsiblePersonNRCI" maxOccurs="unbounded" />
                     <xsd:element name="Footer" type="comp:FooterNRCC" />
                     <xsd:element name="ComplianceID" minOccurs="0"/>
                  </xsd:sequence>
               </xsd:complexType>
            </xsd:element>
            <xsd:element name="Report" type="xsd:base64Binary" minOccurs="0"/>
         </xsd:sequence>
         <xsd:attribute name="revision" use="required" type="xsd:string" fixed="rev 20220901"/>
         <xsd:attribute name="doc" use="required" type="comp:ComplianceDocumentTagNR" fixed="NRCIMCH01E"/>
      </xsd:complexType>
   </xsd:element>
   <xsd:complexType name="NRCIMCH01E">
      <xsd:annotation>
         <xsd:documentation>Mechanical Systems</xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="Section_A" minOccurs="1" maxOccurs="1">
            <xsd:annotation>
               <xsd:documentation source="FieldText">[d:b]A. GENERAL INFORMATION[/d:b]</xsd:documentation>
               <xsd:documentation source="MinOccurs">This section is required.</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="A_BeforeNote1" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">[d:i]This  Certificate of Installation is used to document the installation of mechanical features, materials, components, and manufactured devices required to demonstrate compliance with Title 24, Part 6 per [d:t24refnolink/]110-103(a)3 for nonresidential, hotel/motel and high-rise residential occupancies.[/d:i]</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="A01_ProjectCity" type="comp:ProjectCity" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Project Location (city)</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">If CC-MCH parent, 
result = A01_ProjectCity
Else If  CC-PRF parent, 
result = Info01_ProjectCity
End If</xsd:documentation>
                        <xsd:appinfo source="CIsrc">
                           <dtyp:srcSchema form="CCMCH01E"  usage="copy">A01_ProjectCity</dtyp:srcSchema>
                           <dtyp:srcSchema form="CCPRF01E"  usage="copy">Info04_ProjectCity</dtyp:srcSchema>
                        </xsd:appinfo>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="A02_ProjectZipcode" type="comp:ProjectZipcode" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Zip Code</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">If CC-PRF parent
result = Info02_ProjectZipcode
Else If CC-MCH parent
User input
End If</xsd:documentation>
                        <xsd:appinfo source="CIsrc">
                           <dtyp:srcSchema form="CCPRF01E"  usage="copy">Info06_ProjectZipcode</dtyp:srcSchema>
                        </xsd:appinfo>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="A03_PermitSetDate" type="comp:PermitSetDate" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Date of Permit Set used for construction</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">User input</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="A04_AuthorityHavingJurisdiction" type="comp:AuthorityHavingJurisdiction" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Authority Having Jurisdiction</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">User input</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="A05_PermitNumber" type="comp:PermitNumber" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Building Permit #</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">User input</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="A06_AsBuiltSetDate" type="comp:AsBuiltSetDate" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Date of As-built Set</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">User input</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="A07_PermitSetName" type="comp:PermitSetName"  minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">NOT REPORTED IN PDF</xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">Name of Permit Set used for construction</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">This data is not currently required but may be needed in the future</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="A08_AsBuiltSetName" type="comp:AsBuiltSetName"  minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">NOT REPORTED IN PDF</xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">Name of As-built Set</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">This data is not currently required but may be needed in the future</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
         <xsd:element name="Section_B" minOccurs="1" maxOccurs="1">
            <xsd:annotation>
               <xsd:documentation source="FieldText">[d:b]B. INSTALLER SCOPE[/d:b]</xsd:documentation>
               <xsd:documentation source="MinOccurs">This section is required.</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="B_BeginNote1" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">[d:i]This table indicates construction systems and materials documented on this Certificate of Installation.[/d:i]</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="B01a_ProjectHasDrySystemEquipment" type="comp:ProjectHasDrySystemEquipment" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Dry System (Airside) Equipment</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">User input. 
If user selects this item result = true and item is checked
 Else result = false  and item is unchecked 
End If</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="B01b_ProjectHasBoilers" type="comp:ProjectHasBoilers" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Boiler</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">User input. 
If user selects this item result = true and item is checked 
Else result = false  and item is unchecked 
End If</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="B01c_ProjectHasChillers" type="comp:ProjectHasChillers" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Chiller</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">User input. 
If user selects this item result = true and item is checked 
Else result = false  and item is unchecked 
End If</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="B01d_ProjectHasVentilation" type="comp:ProjectHasVentilation" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Ventilation</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">User input. 
If user selects this item result = true and item is checked 
Else result = false  and item is unchecked 
End If</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="B01e_ProjectHasPumps" type="comp:ProjectHasPumps" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Pumps</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">User input. 
If user selects this item result = true and item is checked 
Else result = false  and item is unchecked 
End If</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="B01f_ProjectHasFanSystems" type="comp:ProjectHasFanSystems" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Fans and Air Economizers</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">User input. 
If user selects this item result = true and item is checked 
Else result = false  and item is unchecked 
End If</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="B01g_ProjectHasSystemControls" type="comp:ProjectHasSystemControls" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">System Controls</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">User input. 
If user selects this item result = true and item is checked 
Else result = false  and item is unchecked 
End If</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="B01h_ProjectHasTerminalBoxControls" type="comp:ProjectHasTerminalBoxControls" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Terminal Box Controls</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">User input. 
If user selects this item result = true and item is checked 
Else result = false  and item is unchecked 
End If</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="B01i_ProjectHasHeatRejectionEquipment" type="comp:ProjectHasHeatRejectionEquipment" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Heat Rejection Equipment (cooling towers, condensers, waterside economizers)</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">User input. 
If user selects this item result = true and item is checked 
Else result = false  and item is unchecked 
End If</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="B01j_ProjectHasDuctwork" type="comp:ProjectHasDuctwork" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Ductwork</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">User input. 
If user selects this item result = true and item is checked 
Else result = false  and item is unchecked 
End If</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="B01k_ProjectHasSystemPiping" type="comp:ProjectHasSystemPiping" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Piping</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">If TablePipeInsulation IsPresent
User input.
End If 
If user selects this item result = true and item is checked 
Else result = false  and item is unchecked 
End If</xsd:documentation>
                        <xsd:appinfo source="CIsrc">
                           <dtyp:srcSchema form="CCMCH01E"  usage="reference">TablePipeInsulation</dtyp:srcSchema>
                        </xsd:appinfo>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="B01l_ProjectHasElectricHeat" type="comp:ProjectHasElectricHeat" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Electric Resistance Heat</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">User input. 
If user selects this item result = true and item is checked 
Else result = false  and item is unchecked 
End If</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
         <xsd:element name="Section_C" minOccurs="1">
            <xsd:annotation>
               <xsd:documentation source="FieldText">[d:b]C. COMPLIANCE RESULTS[/d:b]</xsd:documentation>
               <xsd:documentation source="MinOccurs">This section is required.</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="C_BeginNote1" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">[d:i]This table indicates whether the as-built conditions documented in this form are equal or better than what was documented on the permitted Certificate of Compliance.  If the installation is not equal or better, Section 10-103(a)2B requires the Certificate of Compliance form to be revised accordingly to demonstrate compliance.[/d:i]</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="C01_ComplianceResultCI" type="comp:ComplianceResultCI" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">Calculated result:  
If in any required tables or subsections in Section F  ComplianceResultCI ==  TableIncomplete
result = FormNotComplete
Else If in any required tables or subsections in Section F ComplianceResultCI == MayNotComply
result = MayNotComply
 
Else If All required tables or subsections in Section F  have a combination of ComplianceResultCI ==  ExactMatch or NotInScope
result = ExactMatch
Else If any required tables or subsections in Section F have  a combination of ComplianceResultCI ==  EqualBetter or ExactMatch or NotInScope
result = EqualBetter
End If</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="C01Message_ComplianceResultMessageCI" type="comp:ComplianceResultMessageCI" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">Calculated result:  
If C01_ComplianceResultCI == ExactMatch Or EqualBetter
result = ExactEqualBetterText
Else If C01_ComplianceResultCI == MayNotComply
result =  MayNotComplyText
End If</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
         <xsd:element name="Section_D" minOccurs="1">
            <xsd:annotation>
               <xsd:documentation source="FieldText">[d:b]D. EXCEPTIONAL CONDITIONS[/d:b]</xsd:documentation>
               <xsd:documentation source="MinOccurs">This section is required.</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="D_BeginNote1" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">[d:i]This table is auto-filled with uneditable comments because of field conditions noted by the installer that may impact requirements documented on the Certificate of Compliance.[/d:i]</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="D01_ExceptionalConditionMessageCIMCHE" type="comp:ExceptionalConditionMessageCIMCHE"  minOccurs="0" maxOccurs="unbounded">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Exceptional Messages</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">Auto-filled with any system generated messages that explain exceptional conditions encountered in the the completed compliance document
If ExceptionalConditionMessageCIMCHE == ""
result = NoExceptionNote
End If</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
         <xsd:element name="Section_E" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation source="FieldText">[d:b]E. INSTALLER NOTES[/d:b]</xsd:documentation>
               <xsd:documentation source="CalculationsAndRules">This section is required if there are  comments from the permit applicant to the Authority Having Jurisdiction</xsd:documentation>
               <xsd:documentation source="MinOccurs">If this section doesn't apply, do not display anything for this section</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="E_BeginNote1" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">[d:i]This table includes remarks made by the installer to the Authority Having Jurisdiction.[/d:i]</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="E01_CommentsToPermitAuthority" type="comp:CommentsToPermitAuthority"  minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
         <xsd:element name="Section_F" minOccurs="1">
            <xsd:annotation>
               <xsd:documentation source="FieldText">[d:b]F. INSTALLATION DETAILS [/d:b]</xsd:documentation>
               <xsd:documentation source="MinOccurs">This section is required.</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="F_BeginNote1" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">[d:i]The following tables indicate performance requirements as documented on the permitted Certificate of Compliance (NRCC) for all systems and components included in Table B. Installer Scope.  Also included are the as-built conditions documented by the installer/documentation author. [/d:i]</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="F_RowLabelTopCell" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Per C of C</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">This is the text displayed at the left end of a row in the top cell for all tables/subsections in Section F except the Ducts subsection and the Pipes subsection</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="F_RowLabelBottomCell" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">As-built Conditions</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">This is the text displayed at the left end of a row in the bottom cell for all tables/subsections in Section F except the Ducts subsection and the Pipes subsection</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="TableDrySystems" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">[d:b]Dry System Equipment  Schedule[/d:b]</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">This subsection is required if B01aProjectHasDrySystemEquipment  ==  true</xsd:documentation>
                     </xsd:annotation>
                     <xsd:complexType>
                        <xsd:sequence>
                           <xsd:element name="Row" minOccurs="1" maxOccurs="unbounded">
                              <xsd:complexType>
                                 <xsd:sequence>
                                    <xsd:element name="Dry01cc_HvacSystemName" type="comp:HvacSystemName" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Name or Item Tag</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">If CC-MCH parent 
result = CC-MCH F01_HvacSystemName
Else If CC-PRF parent
result = CC-PRF HvacDry01_HvacSystemName
End If
 
The result is displayed in both the Per CC and As-built conditions</xsd:documentation>
                                          <xsd:appinfo source="CIsrc">
                                             <dtyp:srcSchema form="CCMCH01E"  usage="copy">F01_HvacSystemName</dtyp:srcSchema>
                                             <dtyp:srcSchema form="CCPRF01E"  usage="copy">HvacDry01_HvacSystemName</dtyp:srcSchema>
                                          </xsd:appinfo>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="Dry01_InMyScope" type="comp:InMyScope" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Is this in your scope?</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">User selects from true value display term Yes and false value display term No, by others
 If result == false Dry02cc-Dry12cc and Dry02-Dry12 As-built fields are blank and grayed out
And D01_ExceptionalConditionMessageCIMCHE += DryNotInScope  
End If</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="DryScopeChoice" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText"></xsd:documentation>
                                       </xsd:annotation>
                                       <xsd:complexType>
                                          <xsd:choice >
                                             <xsd:element name="DryScopeNA_NotApplicableMessage" >
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText"></xsd:documentation>
                                                   <xsd:documentation source="CalculationsAndRules">If Dry01_InMyScope == false
result = BlankGrayedOut for Dry02cc-Dry12cc and Dry02-Dry12 As-built fields are blank and grayed out 
End If</xsd:documentation>
                                                </xsd:annotation>
                                                <xsd:simpleType>
                                                   <xsd:restriction base="comp:NotApplicableMessage">
                                                      <xsd:enumeration value="BlankGrayedOut"/>
                                                   </xsd:restriction>
                                                </xsd:simpleType>
                                             </xsd:element>
                                             <xsd:element name="DryInScope" >
                                                <xsd:annotation>
                                                   <xsd:documentation source="FieldText"></xsd:documentation>
                                                </xsd:annotation>
                                                <xsd:complexType>
                                                   <xsd:sequence>
                                                      <xsd:element name="Dry01_InstalledSameAsCC" type="comp:InstalledSameAsCC"  minOccurs="0">
                                                         <xsd:annotation>
                                                            <xsd:documentation source="FieldText">NOT REPORTED IN PDF</xsd:documentation>
                                                            <xsd:documentation source="CalculationsAndRules">User selects true if installed is the same as CC value  Or false if installed is different than CC value</xsd:documentation>
                                                         </xsd:annotation>
                                                      </xsd:element>
                                                      <xsd:element name="Dry02cc_NotApplicableMessage" >
                                                         <xsd:annotation>
                                                            <xsd:documentation source="FieldText">Model #</xsd:documentation>
                                                            <xsd:documentation source="CalculationsAndRules">The CC cell is always BlankGrayedOut</xsd:documentation>
                                                         </xsd:annotation>
                                                         <xsd:simpleType>
                                                            <xsd:restriction base="comp:NotApplicableMessage">
                                                               <xsd:enumeration value="BlankGrayedOut"/>
                                                            </xsd:restriction>
                                                         </xsd:simpleType>
                                                      </xsd:element>
                                                      <xsd:element name="Dry02_ModelNumber" type="hvac:ModelNumber" >
                                                         <xsd:annotation>
                                                            <xsd:documentation source="FieldText">Model #</xsd:documentation>
                                                            <xsd:documentation source="CalculationsAndRules">User input</xsd:documentation>
                                                         </xsd:annotation>
                                                      </xsd:element>
                                                      <xsd:choice maxOccurs="1">
                                                         <xsd:element name="Dry03cc_ComputerRoomAC" type="hvac:ComputerRoomAC" >
                                                            <xsd:annotation>
                                                               <xsd:documentation source="FieldText">Equipment Type</xsd:documentation>
                                                               <xsd:documentation source="CalculationsAndRules">If CC-MCH parent 
If CC-MCH F03_ComputerRoomAC IsPresent for system in Dry01nrcc_HvacSystemName
result = CC-MCH F03_ComputerRoomAC
End If
End If</xsd:documentation>
                                                               <xsd:appinfo source="CIsrc">
                                                                  <dtyp:srcSchema form="CCMCH01E"  usage="copy">F03_ComputerRoomAC</dtyp:srcSchema>
                                                               </xsd:appinfo>
                                                            </xsd:annotation>
                                                         </xsd:element>
                                                         <xsd:element name="Dry03cc_FurnaceAC" type="hvac:FurnaceAC" >
                                                            <xsd:annotation>
                                                               <xsd:documentation source="FieldText">Equipment Type</xsd:documentation>
                                                               <xsd:documentation source="CalculationsAndRules">If CC-MCH parent 
If CC-MCH F03_FurnaceAC IsPresent  for system in Dry01nrcc_HvacSystemName
result = CC-MCH F03_FurnaceAC
End If
End If</xsd:documentation>
                                                               <xsd:appinfo source="CIsrc">
                                                                  <dtyp:srcSchema form="CCMCH01E"  usage="copy">F03_FurnaceAC</dtyp:srcSchema>
                                                               </xsd:appinfo>
                                                            </xsd:annotation>
                                                         </xsd:element>
