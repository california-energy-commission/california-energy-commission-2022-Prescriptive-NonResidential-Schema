Ctrl06_ControlSystemTempReset IsPresent And  Ctrl06_ControlSystemTempReset  != Ctrl06cc_ControlSystemTempReset  
Or
Ctrl07_ControlSystemWindows IsPresent And  Ctrl07_ControlSystemWindows  != Ctrl07cc_ControlSystemWindows   
Ctrl08_ControlSystemDDC IsPresent Or Ctrl08_ControlSystemDDC != Ctrl08cc_ControlSystemDDC
 
D03_ExceptionalConditionMessageCIMCHE += SysControlsNoMatch
