                  <xsd:element name="TableBoiler" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">[d:b]Boiler Efficiency and Controls [/d:b]</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">This table is required if B01b_ProjectHasBoilers == true</xsd:documentation>
