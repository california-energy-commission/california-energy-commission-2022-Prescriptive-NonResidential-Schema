                                                                        <xsd:element name="Ventnr08comp_ComplianceResultCI" type="comp:ComplianceResultCI"  minOccurs="0">
                                                                           <xsd:annotation>
                                                                              <xsd:documentation source="FieldText">Space Vent  Compliance</xsd:documentation>
                                                                              <xsd:documentation source="CalculationsAndRules">If any user input field fields Ventnr05 - Ventnr08 is blank
result = TableIncomplete  
Else If all rows have Ventnr05_InMyScope == false 
result = NotInScope
Else If any row has 
Ventnr06_NotApplicableMessage NotPresent And Ventnr06_ExhaustPerSpace LT Ventnr06cc_ExhaustPerSpace 
result = MayNotComply
 
Else If all rows have
Ventnr06_NotApplicableMessage IsPresent Or Ventnr06_ExhaustPerSpace == Ventnr06cc_ExhaustPerSpace 
And 
Ventnr07_NotApplicableMessage IsPresent Or  Ventnr07_OccupantSensorVent  == Ventnr07cc_OccupantSensorVent 
And
Ventnr08_NotApplicableMessage IsPresent Or  Ventnr08_DemandControlVent   == Ventnr08cc_DemandControlVent 
result = ExactMatch
 
Else If any rows have
Ventnr06_ExhaustPerSpace IsPresent And Ventnr06_ExhaustPerSpace GTE Ventnr06cc_ExhaustPerSpace 
result = EqualBetter
End If
 
If Ventnr07_OccupantSensorVent IsPresent And Ventnr07_OccupantSensorVent  != Ventnr07cc_OccupantSensorVent 
Or
Ventnr08_DemandControlVent IsPresent Or  Ventnr08_DemandControlVent   != Ventnr08cc_DemandControlVent 
D03_ExceptionalConditionMessageCIMCHE += VentNoMatch
End If</xsd:documentation>
