                                    <xsd:element name="Blr10_ComplianceResultCI" type="comp:ComplianceResultCI" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Equipment Compliance</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">If any user input field is blank
result = TableIncomplete 
Else If Blr01_InMyScope == false 
result = NotInScope 
Else If 
Blr03_BoilerType != Blr03cc_BoilerType 
Or
(
Blr05_CapacityRated IsPresent And  Blr05_CapacityRated != Blr05cc_CapacityRated 
Or 
Blr05_BoilerRatedInput IsPresent And Blr05_BoilerRatedInput != Blr05cc_BoilerRatedInput
)
Or
(
Blr06_EfficiencyAFUE IsPresent And Blr06_EfficiencyAFUE LT Blr06cc_EfficiencyAFUE  
Or 
Blr06_ThermalEfficiency  IsPresent And  Blr06_ThermalEfficiency LT Blr06cc_ThermalEfficiency  
Or
Blr06_CombustionEfficiency  IsPresent And  LT Blr06cc_CombustionEfficiency 
)
 
result = MayNotComply
 
Else If 
Blr03_BoilerType == Blr03cc_BoilerType
And 
Blr04_NotApplicableMessage IsPresent Or Blr04_BoilerCount ==  Blr04cc_BoilerCount
And
(
Blr05_CapacityRated IsPresent And  Blr05_CapacityRated IsPresent == Blr05cc_CapacityRated 
Or 
Blr05_BoilerRatedInput IsPresent And Blr05_BoilerRatedInput == Blr05cc_BoilerRatedInput
)
And 
Blr08_NotApplicableMessage IsPresent Or Blr08_BoilerIsolationValve ==  Blr08cc_BoilerIsolationValve 
And 
Blr09_NotApplicableMessage IsPresent Or Blr09_TempResetControl ==  Blr09cc_TempResetControl
 
Then If
(
Blr06_EfficiencyAFUE IsPresent And Blr06_EfficiencyAFUE == Blr06cc_EfficiencyAFUE  
Or
Blr06_ThermalEfficiency  IsPresent And Blr06_ThermalEfficiency == Blr06cc_ThermalEfficiency  
Or
Blr06_CombustionEfficiency  IsPresent And Blr06_CombustionEfficiency == Blr06cc_CombustionEfficiency 
)
result = ExactMatch
 
Else If  
(
Blr06_EfficiencyAFUE IsPresent And  Blr06_EfficiencyAFUE GTE Blr06cc_EfficiencyAFUE  
Or
Blr06_ThermalEfficiency  IsPresent And  Blr06_ThermalEfficiency GTE Blr06cc_ThermalEfficiency  
Or
Blr06_CombustionEfficiency  IsPresent And Blr06_CombustionEfficiency  GTE Blr06cc_CombustionEfficiency 
)
result = EqualBetter
End If
End If 
 
If  Blr08_BoilerIsolationValve IsPresent And  Blr08_BoilerIsolationValve !=  Blr08cc_BoilerIsolationValve 
Or 
Blr09_TempResetControl IsPresent And Blr09_TempResetControl !=  Blr09cc_TempResetControl
 
D03_ExceptionalConditoinMessageCIMCHE += BoilerNoMatch
End If</xsd:documentation>
