                                                               <xsd:documentation source="CalculationsAndRules">If  Pump08cc_NotApplicableMessage IsPresent
result = Pump08cc_NotApplicableMessage
End If</xsd:documentation>
                                                            </xsd:annotation>
                                                            <xsd:simpleType>
                                                               <xsd:restriction base="comp:NotApplicableMessage">
                                                                  <xsd:enumeration value="BlankGrayedOut"/>
                                                               </xsd:restriction>
