                           <xsd:element name="Pv11_PV_SystemPowerOutput" type="hvac:PV_SystemPowerOutput" >
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText">Total As-built DC System Size (kW)</xsd:documentation>
                                 <xsd:documentation source="CalculationsAndRules">result = SUM ALL (PV02_ArrayPowerOutput)</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                           <xsd:element name="Pv12cc_PV_DesignSize" type="comp:PV_DesignSize" >
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText">Total DC System Size per C of C (kW)</xsd:documentation>
                                 <xsd:documentation source="CalculationsAndRules">If CC-SAB parent
result = C04_PV_SystemTotalRatedPowerDC / 1000
Else If CC-PRF parent
result = Pv01_PV_SystemTotalRatedPowerDC</xsd:documentation>
                                 <xsd:appinfo source="CIsrc">
                                    <dtyp:srcSchema form="CCSAB01E"  usage="copy">C04_PV_SystemTotalRatedPowerDC</dtyp:srcSchema>
                                    <dtyp:srcSchema form="CCPRF01E"  usage="copy">Pv01_PV_SystemTotalRatedPowerDC</dtyp:srcSchema>
                                 </xsd:appinfo>
                              </xsd:annotation>
                           </xsd:element>
                           <xsd:element name="PV13_ComplianceResultCI" type="comp:ComplianceResultCI" >
                              <xsd:annotation>
                                 <xsd:documentation source="FieldText">PV Compliance</xsd:documentation>
                                 <xsd:documentation source="CalculationsAndRules">If any user input field is blank
 result = TableIncomplete  
Else If PV11_PV_SystemPowerOutput == PV12cc_PV_DesignSize
result = ExactMatch
Else If PV11_PV_SystemPowerOutput GT PV12cc_PV_DesignSize
result = EqualBetter
Else If PV11_PV_SystemPowerOutput LT PV12cc_PV_DesignSize
result = MayNotComply
End If</xsd:documentation>
                              </xsd:annotation>
                           </xsd:element>
                        </xsd:sequence>
                     </xsd:complexType>
