<?xml version="1.0" encoding="UTF-8"?>
<xsd:schema
   targetNamespace="http://www.lmonte.com/besm/NRCISAB01E"
   xmlns="http://www.lmonte.com/besm/NRCISAB01E"
   xmlns:d="http://www.lmonte.com/besm/d"
   xmlns:bld="http://www.lmonte.com/besm/bld"
   xmlns:env="http://www.lmonte.com/besm/env"
   xmlns:lit="http://www.lmonte.com/besm/lit"
   xmlns:hvac="http://www.lmonte.com/besm/hvac"
   xmlns:comp="http://www.lmonte.com/besm/comp"
   xmlns:com="http://www.lmonte.com/besm/com"
   xmlns:dtyp="http://www.lmonte.com/besm/dtyp"
   xmlns:xsd="http://www.w3.org/2001/XMLSchema"
   xmlns:altova="http://www.altova.com/xml-schema-extensions"
   elementFormDefault="qualified" attributeFormDefault="unqualified" version="2022.0.000">
   <xsd:import namespace="http://www.lmonte.com/besm/com" schemaLocation="../base/NrPresCommon.xsd"/>
   <xsd:import namespace="http://www.lmonte.com/besm/bld" schemaLocation="../base/NrPresBuilding.xsd"/>
   <xsd:import namespace="http://www.lmonte.com/besm/env" schemaLocation="../base/NrPresEnvelope.xsd"/>
   <xsd:import namespace="http://www.lmonte.com/besm/lit" schemaLocation="../base/NrPresLighting.xsd"/>
   <xsd:import namespace="http://www.lmonte.com/besm/hvac" schemaLocation="../base/NrPresHvac.xsd"/>
   <xsd:import namespace="http://www.lmonte.com/besm/comp" schemaLocation="../base/NrPresCompliance.xsd"/>
   <xsd:import namespace="http://www.lmonte.com/besm/dtyp" schemaLocation="../base/DataTypes.xsd"/>
   <xsd:element name="ComplianceDocumentPackage">
      <xsd:complexType>
         <xsd:sequence>
            <xsd:element name="DocID" minOccurs="0">
               <xsd:complexType>
                  <xsd:attribute name="doc" type="comp:ComplianceDocumentTagNR" fixed="NRCISAB01E"/>
                  <xsd:attribute name="docType" type="comp:ComplianceDocumentTypeNR" fixed="CertificateInstallationNR"/>
                  <xsd:attribute name="docTitle" type="comp:ComplianceDocumentTitleNR" fixed="Solar And Battery"/>
               </xsd:complexType>
            </xsd:element>
            <xsd:element name="Payload" type="comp:Payload"/>
            <xsd:element name="DocumentData">
               <xsd:complexType>
                  <xsd:sequence>
                     <xsd:element name="Header" type="comp:HeaderNRCI" />
                     <xsd:element ref="nRCISAB01E" />
                     <xsd:element name="DocAuthor" type="comp:DocumentAuthor" maxOccurs="unbounded" />
                     <xsd:element name="RespPerson" type="comp:ResponsiblePersonNRCI" maxOccurs="unbounded" />
                     <xsd:element name="Footer" type="comp:FooterNRCC" />
                  </xsd:sequence>
               </xsd:complexType>
            </xsd:element>
            <xsd:element name="Report" type="xsd:base64Binary" minOccurs="0"/>
         </xsd:sequence>
         <xsd:attribute name="revision" use="required" type="xsd:string" fixed="rev 20220901"/>
         <xsd:attribute name="doc" use="required" type="comp:ComplianceDocumentTagNR" fixed="NRCISAB01E"/>
      </xsd:complexType>
   </xsd:element>
   <xsd:complexType name="NRCISAB01E">
      <xsd:annotation>
         <xsd:documentation>Solar And Battery</xsd:documentation>
      </xsd:annotation>
      <xsd:sequence>
         <xsd:element name="Section_A" minOccurs="1" maxOccurs="1">
            <xsd:annotation>
               <xsd:documentation source="FieldText">[d:b]A. GENERAL INFORMATION[/d:b]</xsd:documentation>
               <xsd:documentation source="MinOccurs">This section is required.</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="A_BeforeNote1" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">[d:i]This  Certificate of Installation is used to document the installation of solar and battery   features, materials, components, and manufactured devices required to demonstrate compliance with Title 24, Part 6 per [d:t24refnolink/]110-103(a)3 for nonresidential, hotel/motel and high-rise residential occupancies.[/d:i]</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="A01_ProjectCity" type="comp:ProjectCity" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Project Location (city)</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">If CC-SAB parent
result = A01_ProjectCity
End If</xsd:documentation>
                        <xsd:appinfo source="CIsrc">
                           <dtyp:srcSchema form="CCSAB01E"  usage="copy">A01_ProjectCity</dtyp:srcSchema>
                        </xsd:appinfo>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="A02_ProjectZipcode" type="comp:ProjectZipcode" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Zip Code</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">User input</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="A03_PermitSetDate" type="comp:PermitSetDate" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Date of Permit Set used for construction</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">User input</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="A04_AuthorityHavingJurisdiction" type="comp:AuthorityHavingJurisdiction" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Authority Having Jurisdiction</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">User input</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="A05_PermitNumber" type="comp:PermitNumber" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Building Permit #</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">User input</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="A06_AsBuiltSetDate" type="comp:AsBuiltSetDate" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Date of As-built Set</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">User input</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="A07_PermitSetName" type="comp:PermitSetName"  minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">NOT REPORTED IN PDF</xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">Name of Permit Set used for construction</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">This data is not currently required but may be needed in the future</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="A08_AsBuiltSetName" type="comp:AsBuiltSetName"  minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">NOT REPORTED IN PDF</xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">Name of As-built Set</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">This data is not currently required but may be needed in the future</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
         <xsd:element name="Section_B" minOccurs="1" maxOccurs="1">
            <xsd:annotation>
               <xsd:documentation source="FieldText">[d:b] B. INSTALLER SCOPE[/d:b]</xsd:documentation>
               <xsd:documentation source="MinOccurs">This section is required.</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="B_BeginNote1" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">[d:i]This table indicates construction systems and materials documented on this Certificate of Installation.[/d:i]</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="B01a_IsProjectAllocatedSolarZone" type="comp:IsProjectAllocatedSolarZone" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Allocated Solar Zone</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">User input</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="B01b_ProjectHasHomeAutomation" type="comp:ProjectHasHomeAutomation" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Home Automation System</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">User input</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="B01c_IsProjectInstalledPhotovoltaicSystem" type="comp:IsProjectInstalledPhotovoltaicSystem" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Photovoltaic System</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">User input</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="B01d_ProjectHasGreywater" type="comp:ProjectHasGreywater" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Greywater Irrigation System</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">User input</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="B01e_IsProjectInstalledSolarWaterHeating" type="comp:IsProjectInstalledSolarWaterHeating" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Solar Water Heater</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">User input</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="B01f_ProjectHasRainwater" type="comp:ProjectHasRainwater" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Rainwater Catchment System</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">User input</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="B01g_IsProjectSmartTstatPlusEE_Measure" type="comp:IsProjectSmartTstatPlusEE_Measure" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Smart Thermostat</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">User input</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="B01h_ProjectHasEVCharging" type="comp:ProjectHasEVCharging" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Electric Vehicle Charging Space</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">User input</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="B01i_ProjectHasEnergyStar" type="comp:ProjectHasEnergyStar" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">ENERGY STAR dishwasher and ENERGY STAR refrigerator</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">User input</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="B01j_ProjectHasEnergyStarAndFanEC" type="comp:ProjectHasEnergyStarAndFanEC" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">ENERGY STAR dishwasher and WHF w/ electronically commutated motor</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">User input</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="B01k_ProjectHasBatteryStorage" type="comp:ProjectHasBatteryStorage" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Battery Storage System</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">User input</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
         <xsd:element name="Section_C" minOccurs="1">
            <xsd:annotation>
               <xsd:documentation source="FieldText">[d:b]C. COMPLIANCE RESULTS[/d:b]</xsd:documentation>
               <xsd:documentation source="MinOccurs">This section is required.</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="C_BeginNote1" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">[d:i]This table indicates whether the as-built conditions documented in this form are equal or better than what was documented on the permitted Certificate of Compliance.  If the installation is not equal or better, Section 10-103(a)2B requires the Certificate of Compliance form to be revised accordingly to demonstrate compliance.[/d:i]</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="C01_ComplianceResultCI" type="comp:ComplianceResultCI" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">Calculated result:  
If in any required tables or subsections in Section F  ComplianceResultCI ==  TableIncomplete
result = FormNotComplete
Else If in any required tables or subsections in Section F ComplianceResultCI == MayNotComply
result = MayNotComply
Else If All required tables or subsections in Section F  have  ComplianceResultCI ==  ExactMatch
result = ExactMatch
Else If any required tables or subsections in Section F have ComplianceResultCI ==  EqualBetter
result = EqualBetter
End If</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="C01Message_ComplianceResultMessageCI" type="comp:ComplianceResultMessageCI" >
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">Calculated result:  
If C01_ComplianceResultCI == ExactMatch Or EqualBetter
result = ExactEqualBetterText
Else If C01_ComplianceResultCI == MayNotComply
result =  MayNotComplyText
End If</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
         <xsd:element name="Section_D" minOccurs="1">
            <xsd:annotation>
               <xsd:documentation source="FieldText">[d:b]D. EXCEPTIONAL CONDITIONS[/d:b]</xsd:documentation>
               <xsd:documentation source="MinOccurs">This section is required.</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="D_BeginNote1" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">[d:i]This table is auto-filled with uneditable comments because of field conditions noted by the installer that may impact requirements documented on the Certificate of Compliance.[/d:i]</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="D01_ExceptionalConditionMessageCISABE" type="comp:ExceptionalConditionMessageCISABE"  minOccurs="0" maxOccurs="unbounded">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Exceptional Messages</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">Auto-filled with any system generated messages that explain exceptional conditions encountered in the the completed compliance document
If ExceptionalConditionMessageCISABE == ""
result = NoExceptionNote
End If</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
         <xsd:element name="Section_E" minOccurs="0">
            <xsd:annotation>
               <xsd:documentation source="FieldText">[d:b]E. INSTALLER NOTES[/d:b]</xsd:documentation>
               <xsd:documentation source="CalculationsAndRules">This section is required if there are  comments from the permit applicant to the Authority Having Jurisdiction</xsd:documentation>
               <xsd:documentation source="MinOccurs">If this section doesn't apply, do not display anything for this section</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="E_BeginNote1" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">[d:i]This table includes remarks made by the installer to the Authority Having Jurisdiction.[/d:i]</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="E01_CommentsToPermitAuthority" type="comp:CommentsToPermitAuthority"  minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">User input</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
               </xsd:sequence>
            </xsd:complexType>
         </xsd:element>
         <xsd:element name="Section_F" minOccurs="1">
            <xsd:annotation>
               <xsd:documentation source="FieldText">[d:b]F. INSTALLATION DETAILS [/d:b]</xsd:documentation>
               <xsd:documentation source="MinOccurs">This section is required.</xsd:documentation>
            </xsd:annotation>
            <xsd:complexType>
               <xsd:sequence>
                  <xsd:element name="F_BeginNote1" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText"></xsd:documentation>
                        <xsd:documentation source="AdditionalRequirements">[d:i]The following tables indicate performance requirements as documented on the permitted Certificate of Compliance  for all systems and components included in Table B. Installer Scope.  Also included are the as-built conditions documented by the installer/documentation author. [/d:i]</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="F_RowLabelTopCell" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">Per C of C</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">This is the text displayed at the left end of a row in the top cell for all tables/subsections in Section F except the last subsection Smart Thermostats and Additional Efficiency Measure</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="F_RowLabelBottomCell" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">As-built Conditions</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">This is the text displayed at the left end of a row in the bottom cell for all tables/subsections in Section F except the last subsection Smart Thermostats and Additional Efficiency Measure</xsd:documentation>
                     </xsd:annotation>
                  </xsd:element>
                  <xsd:element name="TableZone" minOccurs="0">
                     <xsd:annotation>
                        <xsd:documentation source="FieldText">[d:b]Allocated Solar Zone on Roof for Future Use [/d:b]</xsd:documentation>
                        <xsd:documentation source="CalculationsAndRules">This section is required If B01a_IsProjectAllocatedSolarZone == true</xsd:documentation>
                     </xsd:annotation>
                     <xsd:complexType>
                        <xsd:sequence>
                           <xsd:element name="Row" minOccurs="1" maxOccurs="unbounded">
                              <xsd:complexType>
                                 <xsd:sequence>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="Zone01cc_SolarZoneSubAreaID" type="comp:SolarZoneSubAreaID" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Subarea Name</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">If CC-SAB parent
If F09_SolarZoneSubAreaID IsPresent
result = F09_SolarZoneSubAreaID
End If
End If</xsd:documentation>
                                             <xsd:appinfo source="CIsrc">
                                                <dtyp:srcSchema form="CCSAB01E"  usage="copy">F09_SolarZoneSubAreaID</dtyp:srcSchema>
                                             </xsd:appinfo>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="Zone01cc_NotApplicableMessage" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Subarea Name</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">If CC-SAB parent
If F09to19NA_NotApplicableMessage IsPresent
result = BlankGrayedOut
End If
End If</xsd:documentation>
                                             <xsd:appinfo source="CIsrc">
                                                <dtyp:srcSchema form="CCSAB01E"  usage="reference">F09to19NA_NotApplicableMessage</dtyp:srcSchema>
                                             </xsd:appinfo>
                                          </xsd:annotation>
                                          <xsd:simpleType>
                                             <xsd:restriction base="comp:NotApplicableMessage">
                                                <xsd:enumeration value="BlankGrayedOut"/>
                                             </xsd:restriction>
                                          </xsd:simpleType>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:element name="Zone01_InMyScope" type="comp:InMyScope" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Is this in your scope?</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">User selects from true value display term Yes and false value display term No, by others
 If result == false Zone02cc-Zone03cc and Zone03 As-built fields is blank and grayed out
And D01_ExceptionalConditionMessageCISABE += SolarZoneNotInScope  
End If</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="Zone01_InstalledSameAsCC" type="comp:InstalledSameAsCC"  minOccurs="0">
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">NOT REPORTED IN PDF</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">User selects true if installed is the same as CC value  Or false if installed is different than CC value</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="Zone02cc_BuildingPlanName" type="comp:BuildingPlanName" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Building Plan Reference Showing Solar Zone</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">If CC-SAB parent
If F10_BuildingPlanName IsPresent
result = SolarZoneSubAreaID
End If
End If</xsd:documentation>
                                             <xsd:appinfo source="CIsrc">
                                                <dtyp:srcSchema form="CCSAB01E"  usage="copy">F10_BuildingPlanName</dtyp:srcSchema>
                                             </xsd:appinfo>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="Zone02cc_NotApplicableMessage" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Building Plan Reference Showing Solar Zone</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">If CC-SAB parent
If F10_F09to19NA_NotApplicableMessage IsPresent
Or If Zone01_InMyScope == false 
result = BlankGrayedOut
End If
End If</xsd:documentation>
                                             <xsd:appinfo source="CIsrc">
                                                <dtyp:srcSchema form="CCSAB01E"  usage="reference">F09to19NA_NotApplicableMessage</dtyp:srcSchema>
                                             </xsd:appinfo>
                                          </xsd:annotation>
                                          <xsd:simpleType>
                                             <xsd:restriction base="comp:NotApplicableMessage">
                                                <xsd:enumeration value="BlankGrayedOut"/>
                                             </xsd:restriction>
                                          </xsd:simpleType>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="Zone03cc_IsSolarZoneSubAreaObstructionsFree" type="comp:IsSolarZoneSubAreaObstructionsFree" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Solar Zone Free from Obstructions?[d:sup]1[/d:sup]</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">If CC-SAB parent
If F14_IsSolarZoneSubAreaObstructionsFree IsPresent  
result =F14_IsSolarZoneSubAreaObstructionsFree 
End If
End If</xsd:documentation>
                                             <xsd:appinfo source="CIsrc">
                                                <dtyp:srcSchema form="CCSAB01E"  usage="copy">F14_IsSolarZoneSubAreaObstructionsFree</dtyp:srcSchema>
                                             </xsd:appinfo>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="Zone03cc_NotApplicableMessage" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Solar Zone Free from Obstructions?[d:sup]1[/d:sup]</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">If CC-SAB parent
If F10_F09to19NA_NotApplicableMessage IsPresent
Or If Zone01_InMyScope == false 
result = BlankGrayedOut
End If
End If</xsd:documentation>
                                             <xsd:appinfo source="CIsrc">
                                                <dtyp:srcSchema form="CCSAB01E"  usage="reference">F09to19NA_NotApplicableMessage</dtyp:srcSchema>
                                             </xsd:appinfo>
                                          </xsd:annotation>
                                          <xsd:simpleType>
                                             <xsd:restriction base="comp:NotApplicableMessage">
                                                <xsd:enumeration value="BlankGrayedOut"/>
                                             </xsd:restriction>
                                          </xsd:simpleType>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:choice maxOccurs="1">
                                       <xsd:element name="Zone03_IsSolarZoneSubAreaObstructionsFree" type="comp:IsSolarZoneSubAreaObstructionsFree" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Solar Zone Free from Obstructions?[d:sup]1[/d:sup]</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">If Zone03cc_IsSolarZoneSubAreaObstructionsFree IsPresent
If Zone01_InstalledSameAsCC == true
result = Zone03cc_IsSolarZoneSubAreaObstructionsFree
Else
User input
End If
End If</xsd:documentation>
                                          </xsd:annotation>
                                       </xsd:element>
                                       <xsd:element name="Zone03_NotApplicableMessage" >
                                          <xsd:annotation>
                                             <xsd:documentation source="FieldText">Solar Zone Free from Obstructions?[d:sup]1[/d:sup]</xsd:documentation>
                                             <xsd:documentation source="CalculationsAndRules">If Zone01_InMyScope == false 
Or Zone03cc_NotApplicableMessage IsPresent
result = Zone03cc_NotApplicableMessage
End If</xsd:documentation>
                                          </xsd:annotation>
                                          <xsd:simpleType>
                                             <xsd:restriction base="comp:NotApplicableMessage">
                                                <xsd:enumeration value="BlankGrayedOut"/>
                                             </xsd:restriction>
                                          </xsd:simpleType>
                                       </xsd:element>
                                    </xsd:choice>
                                    <xsd:element name="Zone04_ComplianceResultCI" type="comp:ComplianceResultCI" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Solar Zone Compliance</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">If any user input field is blank
result = TableIncomplete 
Else If Zone01_InMyScope == false 
result = NotInScope
 
Else If  Zone03_IsSolarZoneSubAreaObstructionsFree IsPresent  And Zone03_IsSolarZoneSubAreaObstructionsFree  == Zone03cc_IsSolarZoneSubAreaObstructionsFree 
result = ExactMatch
End If</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                 </xsd:sequence>
                              </xsd:complexType>
                           </xsd:element>
                        </xsd:sequence>
                     </xsd:complexType>
