                           <xsd:choice maxOccurs="1">
                              <xsd:element name="SHW05cc_IsDWHR_Installed" type="comp:IsDWHR_Installed" >
                                 <xsd:annotation>
                                    <xsd:documentation source="FieldText">Drainwater Heat Recovery</xsd:documentation>
                                    <xsd:documentation source="AdditionalRequirements">A drain water heat recovery system that is field verified by a HERS Rater as specified in the Title 24, Part 6 Reference Appendix RA3.6.9 is being used to lower the required Solar Savings Fraction and must be installed and HERS verified.</xsd:documentation>
                                    <xsd:documentation source="CalculationsAndRules">If CC-SAB parent
If H00c_IsDWHR_Installed == true
Display static text in AdditionalRequirements
End If
End If</xsd:documentation>
                                    <xsd:appinfo source="CIsrc">
                                       <dtyp:srcSchema form="CCSAB01E"  usage="reference">H00c_IsDWHR_Installed</dtyp:srcSchema>
                                    </xsd:appinfo>
                                 </xsd:annotation>
                              </xsd:element>
                              <xsd:element name="SHW05cc_NotApplicableMessage" >
