                                    <xsd:element name="Bat06cc_NotApplicableMessage" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Charging Efficiency</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">result = BlankGrayedOut</xsd:documentation>
                                       </xsd:annotation>
