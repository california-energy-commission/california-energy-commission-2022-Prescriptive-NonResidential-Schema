                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Battery System Certified by CEC?</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">User selects Yes or No</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="Bat09_ComplianceResultCI" type="comp:ComplianceResultCI" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Battery Compliance</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">If any user input field is blank
 result = TableIncomplete
Else If Bat03_BatteryCapacity == Bat03cc_BatteryCapacity And
Bat04_ChargingRate == Bat04cc_ChargingRate 
Or Bat04_NotApplicableMessage Is Present
result = ExactMatch
 
Else If Bat03cc_BatteryCapacity LT Bat03_BatteryCapacity And 
Bat04_ChargingRate Is Present And Bat04cc_ChargingRate LT Bat04_ChargingRate 
result = EqualBetter
 
Else If Bat03cc_BatteryCapacity GT Bat03_BatteryCapacity Or 
Bat04_ChargingRate Is Present And Bat04cc_ChargingRate GT Bat04_ChargingRate 
result = MayNotComply
End If</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
