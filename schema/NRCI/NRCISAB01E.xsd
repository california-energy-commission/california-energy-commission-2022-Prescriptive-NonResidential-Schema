                                          <xsd:documentation source="FieldText">Discharging Efficiency</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">User input</xsd:documentation>
                                       </xsd:annotation>
                                    </xsd:element>
                                    <xsd:element name="Bat08cc_NotApplicableMessage" >
                                       <xsd:annotation>
                                          <xsd:documentation source="FieldText">Battery System Certified by CEC?</xsd:documentation>
                                          <xsd:documentation source="CalculationsAndRules">result = BlankGrayedOut</xsd:documentation>
